INSERT INTO ""."examination_question1" VALUES (467, 467, '这个标志的含义是指示此处设有室内停车场', 2, '正确', '错误', , , '你看那个P字上边没有遮挡,说明是露天停车场,如果P字上边有遮挡就是室内停车场了.', 'http://images.juheapi.com/jztk/c1c2subject1/467.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (468, 468, '车辆不得在高速公路匝道上倒车.', 1, '正确', '错误', , , '规定:驶入确定匝道后,迅速提高车速,但不得超过规定速度,在匝道上,不准超车、掉头、停车、和倒车.', , 0);
INSERT INTO ""."examination_question1" VALUES (469, 469, '这是什么踏板?', 1, '加速踏板', '离合器踏板', '驻车制动器', '制动踏板', '加速踏板俗称油门,控制车速的.', 'http://images.juheapi.com/jztk/c1c2subject1/469.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (470, 470, '驾驶人在驾驶证丢失后3个月内还可以驾驶机动车.', 2, '正确', '错误', , , '不可以,没有驾驶证都算无证驾驶.', , 0);
INSERT INTO ""."examination_question1" VALUES (471, 471, '车辆涉水后,应保持低速行驶,怎样操作制动踏板,以恢复制动效果?', 4, '持续重踏', '间断重踏', '持续轻踏', '间断轻踏', '间断轻踏,给刹车片一些温度可以蒸发一些水,恢复制动.', , 0);
INSERT INTO ""."examination_question1" VALUES (472, 472, '饮酒后驾驶机动车一次记几分?', 4, '3分', '2分', '6分', '12分', '饮酒后驾驶机动车的一次记12分.', , 0);
INSERT INTO ""."examination_question1" VALUES (473, 473, '在这种情况下可加速通过交叉路口.', 2, '正确', '错误', , , '黄灯,车身在安全线里边,请停车等待.', 'http://images.juheapi.com/jztk/c1c2subject1/473.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (474, 474, '机动车在高速公路上发生交通事故无法正常行驶时,用救援车、清障车拖曳、牵引.', 1, '正确', '错误', , , '这个没有问题.', , 0);
INSERT INTO ""."examination_question1" VALUES (475, 475, '驾驶机动车在道路上向左变更车道时如何使用灯光?', 3, '提前开启右转向灯', '不用开启转向灯', '提前开启左转向灯', '提前开启近光灯', '无论左转还有右转都得提前打灯告诉其他车你的意图,这样才比较安全.', , 0);
INSERT INTO ""."examination_question1" VALUES (476, 476, '驾驶人在观察后方无来车的情况下,未开转向灯就变更车道也是合理的.', 2, '正确', '错误', , , '步骤告诉你是怎样的就怎么做,不要偷工减料！', , 0);
INSERT INTO ""."examination_question1" VALUES (477, 477, '这是什么操纵装置?', 4, '倒车灯开关', '刮水器开关', '危险报警闪光灯开关', '灯光、信号组合开关', '左手边控制灯光、信号组合开关,右手边控制刮水器开关.', 'http://images.juheapi.com/jztk/c1c2subject1/477.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (478, 478, '车辆在冰雪路面紧急制动易产生侧滑,应低速行驶,可利用发动机制动进行减速.', 1, '正确', '错误', , , '发动机制动:行车（挂上排档,不是空档）中,油门一收,便产生发动机制动！原理:主要靠（停止供油）发动机的压缩力而制止车辆的前进.行车制动:通过刹车踏板,把人体的力量,由总泵至分泵加上助力器的作用,迫使刹车片与刹车盘或刹车毂磨擦至车辆停止前进.', , 0);
INSERT INTO ""."examination_question1" VALUES (479, 479, '上路行驶的机动车未放置检验合格标志的, 交通警察可依法扣留机动车.', 1, '正确', '错误', , , '没有问题,记住吧.', , 0);
INSERT INTO ""."examination_question1" VALUES (480, 480, '上路行驶的机动车未随车携带身份证的,交通警察可依法扣留机动车.', 2, '正确', '错误', , , '上路行驶的机动车未随车携带机动车行驶证的,交通警察可依法扣留机动车.', , 0);
INSERT INTO ""."examination_question1" VALUES (481, 481, '机动车驾驶人初次申领驾驶证后的实习期是多长时间?', 3, '18个月', '16个月', '12个月', '6个月', '常识题:一年的实习期.', , 0);
INSERT INTO ""."examination_question1" VALUES (482, 482, '机动车仪表板上（如图所示）一直亮表示什么?', 4, '安全气囊处于工作状态', '安全带没有系好', '防抱死制动系统故障', '安全气囊处于故障状态', '黄色表示处于故障状态.', 'http://images.juheapi.com/jztk/c1c2subject1/482.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (483, 483, '机动车在发生碰撞时,安全带可以减轻驾乘人员伤害.', 1, '正确', '错误', , , '这个没问题,亲,不要以为安全带是装饰用的哦！', , 0);
INSERT INTO ""."examination_question1" VALUES (484, 484, '机动车仪表板上（如图所示）亮,表示行车制动系统可能出现故障.', 1, '正确', '错误', , , '记住吧.', 'http://images.juheapi.com/jztk/c1c2subject1/484.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (485, 485, '驾驶这种机动车上路行驶没有违法行为.', 2, '正确', '错误', , , '光盘遮挡号牌,新规直接扣除12分.', 'http://images.juheapi.com/jztk/c1c2subject1/485.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (486, 486, '这个标志的含义是表示车辆会车时,对方车辆应停车让行.', 2, '正确', '错误', , , '你的行驶的方向是红色的箭头,但是对方的箭头比你粗所以应该是对方车辆先行.答题技巧:谁粗谁先行.', 'http://images.juheapi.com/jztk/c1c2subject1/486.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (487, 487, '在没有中心线的道路上发现后车发出超车信号时,如果条件许可如何行驶?', 3, '保持原状态行驶', '加速行驶', '降速靠右让路', '迅速停车让行', '和谐第一,人家要超车就减速靠右给他让出超车的位置.', , 0);
INSERT INTO ""."examination_question1" VALUES (488, 488, '拼装的机动车只要认为安全就可以上路行驶.', 2, '正确', '错误', , , '机动车不跟电脑一样可以拼装,机动车都是品牌的不能动的.', , 0);
INSERT INTO ""."examination_question1" VALUES (489, 489, '对驾驶拼装机动车上路行驶的驾驶人,会受到下列哪种处罚?', 2, '依法追究刑事责任', '处200以上2000元以下罚款', '吊销机动车行驶证', '处15日以下拘留', '对驾驶拼装的机动车上道路行驶的驾驶人,处200元以上2000元以下罚款,并吊销驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (490, 490, '未取得驾驶证的学员在道路上学习驾驶技能,下列哪种做法是正确的?', 1, '使用所学车型的教练车由教练员随车指导', '使用所学车型的教练车单独驾驶学习', '使用所学车型的教练车由非教练员的驾驶人随车指导', '使用私家车由教练员随车指导', '规定:未取得驾驶证的学员在道路上学习驾驶技能,使用所学车型的教练车由教练员随车指导.', , 0);
INSERT INTO ""."examination_question1" VALUES (491, 491, '对违法驾驶发生重大交通事故且构成犯罪的,不追究其刑事责任.', 2, '正确', '错误', , , '重大交通事故且构成犯罪的,这个肯定是要追究刑事责任的.', , 0);
INSERT INTO ""."examination_question1" VALUES (492, 492, '机动车仪表板上（如图所示)亮,提示两侧车门未关闭.', 1, '正确', '错误', , , '毫无疑问俩侧的车门都没关闭', 'http://images.juheapi.com/jztk/c1c2subject1/492.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (493, 493, '驾驶人在行车中经过积水路面时,应怎样做?', 1, '减速慢行', '保持正常车速通过', '空挡滑行通过', '加速通过', '行车中经过积水路面时,应特别留意减速慢行；加速或保持正常车速通过,溅起的水花会冲击前挡风玻璃,阻挡驾驶员的视线,无法观察前方情况,极易导致车辆失控或交通事故.另外,驾驶车辆行经两侧有行人或非机动车行驶且有积水的路面时,更应减速慢行,以免泥水飞溅到行人或非机动车驾驶员身上.', , 0);
INSERT INTO ""."examination_question1" VALUES (494, 494, '旋转开关这一档控制机动车哪个部位?', 2, '近光灯', '前后雾灯', '远光灯', '左右转向灯', '此处为雾灯的控制.顺便回忆一下,哪个控制前雾灯,哪个控制后雾灯.下边的控制前雾灯,上边的控制后雾灯.', 'http://images.juheapi.com/jztk/c1c2subject1/494.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (495, 495, '驾驶机动车在冰雪道路行驶时,最高速度不能超过多少?', 3, '50公里/小时', '40公里/小时', '30公里/小时', '20公里/小时', '规定:机动车在冰雪道路行驶时,最高速度不能超过30公里/小时', , 0);
INSERT INTO ""."examination_question1" VALUES (496, 496, '开启危险报警闪光灯时,（如图所示）闪烁.', 1, '正确', '错误', , , '危险报警闪光灯,通常称为“双蹦”（红三角里有个！的标志按扭开关,俗称双闪灯 或双跳灯）,是一种提醒其他车辆与行人注意本车发生了特殊情况的信号灯.', 'http://images.juheapi.com/jztk/c1c2subject1/496.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (497, 497, '这辆在高速公路上临时停放的故障车,警告标志应该设置在车后多远处?', 1, '150米以外', '50～150米', '50米以内', '50～100米', '请看清道路,高速公路是150米是最佳距离,普通道路是50-100米.', 'http://images.juheapi.com/jztk/c1c2subject1/497.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (498, 498, '机动车在紧急制动时ABS系统会起到什么作用?', 4, '切断动力输出', '自动控制方向', '减轻制动惯性', '防止车轮抱死', 'ABS通过控制作用于车轮制动分泵上的制动管路压力,使汽车在紧急刹车时车轮不会抱死,故选D.', 'http://images.juheapi.com/jztk/c1c2subject1/498.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (499, 499, '夜间驾驶人对事物的观察能力明显比白天差,视距变短.', 1, '正确', '错误', , , '此题说的意思是,你在白天开车的时候由于光线比较充足,所以观察事物的范围比较广,观察事物的视线距离相对也就比较大,而在晚上受到光线暗的局限,相对观察事物的距离也就受到局限,所以视距比较短,而并不是说人本身的观察有问题,所以此题是正确的！', , 0);
INSERT INTO ""."examination_question1" VALUES (500, 500, '机动车仪表板上（如图所示）亮表示什么?', 4, '迎面出风', '空气外循环', '风窗玻璃除霜', '空气内循环', '此为空气内循环,当箭头是从车外进来的就是空气外循环.', 'http://images.juheapi.com/jztk/c1c2subject1/500.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (501, 501, '机动车行驶证灭失、丢失,机动车所有人要向登记地车辆管理所申请补领、换领.', 1, '正确', '错误', , , '常识题,记住.', , 0);
INSERT INTO ""."examination_question1" VALUES (502, 502, '驾驶拼装机动车上路行驶的驾驶人,除按规定接受罚款外,还要受到哪种处理?', 2, '暂扣驾驶证', '吊销驾驶证', '追究刑事责任', '处10日以下拘留', '对驾驶拼装的机动车上道路行驶的驾驶人,处200元以上2000元以下罚款,并吊销驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (503, 503, '在山区冰雪道路上行车,遇有前车正在爬坡时,后车应怎样做?', 3, '低速爬坡', '紧随其后爬坡', '选择适当地点停车,等前车通过后再爬坡', '迅速超越前车', '如果大家都开上去,路面车速低,极易熄火！造成堵塞！而且冰雪路面很滑的,搞不好往下滑发生事故.', , 0);
INSERT INTO ""."examination_question1" VALUES (504, 504, '机动车仪表板上（如图所示）亮时表示什么?', 3, '已开启前雾灯', '已开启前照灯近光', '已开启前照灯远光', '已开启后雾灯', '远光灯的灯光走向是直的.近光灯的灯光走向是朝下的.', 'http://images.juheapi.com/jztk/c1c2subject1/504.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (505, 505, '浓雾天气能见度低,开启远光灯会提高能见度.', 2, '正确', '错误', , , '不能开启远光灯,因浓雾会反射灯光.应开启雾灯.', , 0);
INSERT INTO ""."examination_question1" VALUES (506, 506, '机动车仪表板上（如图所示）亮表示什么?', 3, '驻车制动解除', '行车制动器失效', '制动系统出现异常', '制动踏板没回位', '可能出现故障仪表板上的图标才会亮.', 'http://images.juheapi.com/jztk/c1c2subject1/506.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (507, 507, '驾驶机动车在道路上违反道路交通安全法的行为,属于什么行为?', 4, '过失行为', '违规行为', '违章行为', '违法行为', '现在都是违法行为,没有违规行为和违章行为一说.', , 0);
INSERT INTO ""."examination_question1" VALUES (508, 508, '驾驶机动车在道路上超车时可以不使用转向灯.', 2, '正确', '错误', , , '超车是应该开启左转向灯.', , 0);
INSERT INTO ""."examination_question1" VALUES (509, 509, '车辆应靠高速公路右侧的路肩上行驶.', 2, '正确', '错误', , , '路肩通常是让有事故的车辆停靠维修的,你如果车辆没有问题的话这样做是不对的.', , 0);
INSERT INTO ""."examination_question1" VALUES (510, 510, '危险报警闪光灯可用于下列什么场合?', 3, '遇到道路拥堵时', '在道路上跟车行驶时', '机动车发生故障停车时', '引领后车行驶时', '危险报警闪光灯是一种提醒其他车辆与行人注意本车发生了特殊情况的信号灯.', , 0);
INSERT INTO ""."examination_question1" VALUES (511, 511, '装有ABS系统的机动车在冰雪路面上会最大限度缩短制动距离.', 2, '正确', '错误', , , '因为（ABS）是防抱死制动装置.防止侧滑,就是当它发现有轮胎抱死后,会松开刹车.以后再刹再松,直到车停下.肯定增加了制动距离！因此本题错误.', , 0);
INSERT INTO ""."examination_question1" VALUES (512, 512, '（如图所示）这个符号的开关控制什么装置?', 4, '前风窗玻璃除霜', '后风窗玻璃刮水器', '后风窗玻璃除霜', '前风窗玻璃刮水器', '此为前风窗玻璃刮水器.关于如何区分前风窗玻璃刮水器及洗涤器和后风窗玻璃刮水器及洗涤器,你只需记住一点半弧形的图标是前风窗,四四方方的图标就是后风窗.', 'http://images.juheapi.com/jztk/c1c2subject1/512.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (513, 513, '夜间行车,遇对面来车未关闭远光灯时,应减速行驶,以防两车灯光的交汇处有行人通过时发生事故.', 1, '正确', '错误', , , '灯光刺行人眼看不清会发生事故.', , 0);
INSERT INTO ""."examination_question1" VALUES (514, 514, '这是什么踏板?', 3, '离合器踏板', '加速踏板', '制动踏板', '驻车制动器', '制动踏板俗称刹车.', 'http://images.juheapi.com/jztk/c1c2subject1/514.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (515, 515, '开启前照灯近光时仪表板上（如图所示）亮起.', 2, '正确', '错误', , , '近光灯的灯光走向是朝下的.远光灯的灯光走向是直的.', 'http://images.juheapi.com/jztk/c1c2subject1/515.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (516, 516, '夜间通过无交通信号灯控制的交叉路口时,不得变换远、近光灯.', 2, '正确', '错误', , , '换远近光灯是为了让来车认识到前方有车,应注意减速慢性注意安全.', , 0);
INSERT INTO ""."examination_question1" VALUES (517, 517, '驾驶机动车通过急弯路时,最高速度不能超过多少?', 2, '20公里/小时', '30公里/小时', '40公里/小时', '50公里/小时', '规定:驾驶机动车通过急弯路时,最高速度不能超过30公里/小时', , 0);
INSERT INTO ""."examination_question1" VALUES (518, 518, '这是什么操纵装置?', 3, '驻车制动器操纵杆', '节气门操纵杆', '变速器操纵杆', '离合器操纵杆', '变速器操纵杆也就是换挡的,当我们速度越开越快的时候也要对应上相应的档位,故此为变速器操纵杆.', 'http://images.juheapi.com/jztk/c1c2subject1/518.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (519, 519, '雾天行车多使用喇叭可引起对方注意；听到对方车辆鸣喇叭,也应鸣喇叭回应.', 1, '正确', '错误', , , '既然雾天看不见,就要靠听的了.人家鸣喇叭,你当然要回一声告诉别人这里有车,以提高注意.', , 0);
INSERT INTO ""."examination_question1" VALUES (520, 520, '机动车仪表板上（如图所示）亮表示什么?', 1, '后雾灯打开', '前照灯近光打开', '前照灯远光打开', '前雾灯打开', '前雾灯朝左,后雾灯朝右.', 'http://images.juheapi.com/jztk/c1c2subject1/520.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (521, 521, '驾驶机动车通过铁路道口时,最高速度不能超过多少?', 3, '15公里/小时', '20公里/小时', '30公里/小时', '40公里/小时', '规定:驾驶机动车通过铁路道口时,最高速度不能超过30公里/小时', , 0);
INSERT INTO ""."examination_question1" VALUES (522, 522, '未上坡的车辆遇到这种情况让对向下坡车先行.', 1, '正确', '错误', , , '在狭窄的坡路,上坡的一方先行；但下坡的一方已行至中途而上坡的一方未上坡时,下坡的一方先行.由图可知下坡的一方行之中途而上坡的一方没有上坡时,下坡的一方先行！', 'http://images.juheapi.com/jztk/c1c2subject1/522.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (523, 523, '这个标志的含义是告示前方是塌方路段,车辆应绕道行驶.', 2, '正确', '错误', , , '施工标志:用以告示前方道路施工,车辆应减速慢行或绕道行驶.该标志可以作为临时标志支设在施工路段以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/523.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (524, 524, '驾驶机动车在下列哪种情形下不能超越前车?', 2, '前车减速让行', '前车正在左转弯', '前车靠边停车', '前车正在右转弯', '因为要从左侧超车正好他左转那不撞上了,所以前车正在左转弯的时候不能超车.', , 0);
INSERT INTO ""."examination_question1" VALUES (525, 525, '机动车发生碰撞时座椅安全带主要作用是什么?', 3, '保护驾乘人员颈部', '保护驾乘人员胸部', '减轻驾乘人员伤害', '保护驾乘人员腰部', '座椅安全带可以保护人身体的大部分,故选C', , 0);
INSERT INTO ""."examination_question1" VALUES (526, 526, '在路口遇有交通信号灯和交通警察指挥不一致时,按照交通信号灯通行.', 2, '正确', '错误', , , '灯是死的,人是活的嘛.', , 0);
INSERT INTO ""."examination_question1" VALUES (527, 527, '驾驶机动车在没有中心线的公路上,最高速度不能超过每小时70公里.', 2, '正确', '错误', , , '根据《中华人民共和国道路交通安全法实施条例》第四十五条:……在没有限速标志、标线的道路上,机动车不得超过下列最高行驶速度:　　(一)没有道路中心线的道路,城市道路为每小时30公里,公路为每小时40公里；　　(二)同方向只有1条机动车道的道路,城市道路为每小时50公里,公路为每小时70公里.', , 0);
INSERT INTO ""."examination_question1" VALUES (528, 528, '这种握转向盘的动作是正确的.', 2, '正确', '错误', , , '正确的握法是握住方向盘的俩边,题中的握法是驾驶的陋习,是有很大的风险的.', 'http://images.juheapi.com/jztk/c1c2subject1/528.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (529, 529, '车辆下坡行驶,要适当控制车速,充分利用发动机进行制动.', 1, '正确', '错误', , , '在下坡时,有人为了省油,就挂空档,采用惯性下坡,这样时间一长,刹车的压力就不够了,容易出现刹车失效,所以不提倡挂空档,不挂空档,就是让发动机和变速器连接,这个时候车下坡时不是靠惯性的,而是靠发动机行驶的,就好像发动机带着你走,不让你的车自己走快,这个就是发动机制动.', , 0);
INSERT INTO ""."examination_question1" VALUES (530, 530, '驾驶有ABS系统的机动车在紧急制动的同时转向可能会发生侧滑.', 1, '正确', '错误', , , 'ABS使汽车在紧急刹车时车轮不会抱死,这样就能使汽车在紧急制动时仍能保持较好的方向稳定性.由于四轮防抱死制动系统保留着控制转向的能力,因此,在制动过程中有可能绕过障碍物,避免可能发生的事故.然而,驾驶有ABS 系统的机动车在紧急制动的同时转向会发生侧滑.', , 0);
INSERT INTO ""."examination_question1" VALUES (531, 531, '驾驶机动车在道路上违反道路通行规定应当接受相应的处罚.', 1, '正确', '错误', , , '常识题,犯错就得接受相应的处罚.', , 0);
INSERT INTO ""."examination_question1" VALUES (532, 532, '机动车驾驶人造成事故后逃逸构成犯罪的,吊销驾驶证且多长时间不得重新取得驾驶证?', 4, '5年内', '10年内', '20年内', '终生', '吊二撤三醉五逃终生,这样就好理解了吧！', , 0);
INSERT INTO ""."examination_question1" VALUES (533, 533, '伪造、变造机动车驾驶证构成犯罪的将被依法追究刑事责任.', 1, '正确', '错误', , , '常识题', , 0);
INSERT INTO ""."examination_question1" VALUES (534, 534, '准驾车型为小型自动挡汽车的,可以驾驶低速载货汽车.', 2, '正确', '错误', , , '小型自动挡汽车（C2）准驾车型:小型、微型自动挡载客汽车以及轻型、微型自动挡载货汽车', , 0);
INSERT INTO ""."examination_question1" VALUES (535, 535, '在暴雨天气驾车,刮水器无法刮净雨水时,应怎样做?', 2, '集中注意力谨慎驾驶', '立即减速靠边停车', '以正常速度行驶', '减速行驶', '看清题:是暴雨导致刮不净水了,不是雨刷坏了.快停车.', , 0);
INSERT INTO ""."examination_question1" VALUES (536, 536, '在冰雪路面上行车,必须降低车速、加大安全距离.', 1, '正确', '错误', , , '冰雪路面行驶时,制动距离比平时要大.因此要考虑到紧急状况.如果出现紧急情况,需要有足够的制动距离.', , 0);
INSERT INTO ""."examination_question1" VALUES (537, 537, '安装防抱死制动装置（ABS）的机动车制动时,制动距离会大大缩短,因此不必保持安全车距.', 2, '正确', '错误', , , '这种错误认识是由于不了解ABS的工作原理导致的.ABS可以兼顾车辆行驶中的最大的纵向制动力和横向抓地力,有效地保证车辆不会发生失控状况,但不是所有配置了ABS的车辆都会缩短制动距离,如冰雪、潮湿路面上的制动距离比车轮抱死时短一些,但在一般路面上的制动距离反而更长.', , 0);
INSERT INTO ""."examination_question1" VALUES (538, 538, '如图所示,A车在此时进入左侧车道是因为进入实线区不得变更车道.', 1, '正确', '错误', , , '白实线是用来规分同方向车道的,实线是表示同方向行驶,不能越线到另一同方向车道行驶. 白虚线是虚线同方向车道的,可以越线到另一同方向车道行驶.所以在白虚线进行变道.', 'http://images.juheapi.com/jztk/c1c2subject1/538.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (539, 539, '（如图所示）这个符号的开关控制什么装置?', 3, '两侧车窗玻璃', '电动车门', '车门锁住开锁', '儿童安全锁', '你看那个长的多像一把钥匙,所以是车门锁住开锁.', 'http://images.juheapi.com/jztk/c1c2subject1/539.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (540, 540, '风、雨、雪、雾等复杂气象条件,遇前车速度较低时,应开启前照灯,连续鸣喇叭迅速超越.', 2, '正确', '错误', , , '风、雨、雪、雾,都要减速了,你还要超车.安全第一！', , 0);
INSERT INTO ""."examination_question1" VALUES (541, 541, '对未取得驾驶证驾驶机动车的,追究其法律责任.', 1, '正确', '错误', , , '这个是要追究你的法律责任的.而且出任何事故都是你全职,因为你是无证驾驶.', , 0);
INSERT INTO ""."examination_question1" VALUES (542, 542, '上下搬动这个开关前风窗玻璃刮水器开始工作.', 1, '正确', '错误', , , '正确,这个没问题,左侧的开关是管灯光的,右侧的开关是管雨刷的.', 'http://images.juheapi.com/jztk/c1c2subject1/542.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (543, 543, '机动车仪表板上（如图所示）亮表示什么?', 1, '左转向指示灯闪烁', '车前后示宽灯亮起', '车前后位置灯亮起', '右转向指示灯闪烁', '这个没有问题吧...老衲也不知如何解释这个题.', 'http://images.juheapi.com/jztk/c1c2subject1/543.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (544, 544, '驾驶机动车上道路行驶,不允许超过限速标志标明的最高时速.', 1, '正确', '错误', , , '这个没问题,按照标志行驶不会错.', , 0);
INSERT INTO ""."examination_question1" VALUES (545, 545, '正面安全气囊与什么配合才能充分发挥保护作用?', 2, '防抱死制动系统', '座椅安全带', '座椅安全头枕', '安全玻璃', '研究表明,假设气囊加安全带是100%安全的话,安全带的作用是70%,气囊的作用是30%', 'http://images.juheapi.com/jztk/c1c2subject1/545.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (546, 546, '通过漫水路时要谨慎慢行,不得空挡滑行.', 1, '正确', '错误', , , '机动车行经漫水桥路段时,应当停车察明水情,确认安全后,低速通过.', , 0);
INSERT INTO ""."examination_question1" VALUES (547, 547, '如图所示,这个标志设置在有人看守的铁路道口,提示驾驶人距有人看守的铁路道口的距离还有100米.', 2, '正确', '错误', , , '火车表示无人看守,栅栏表示有人看管！一条红杠杠表示50米,以此类推两条100米、无人看守！', 'http://images.juheapi.com/jztk/c1c2subject1/547.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (548, 548, '初次申领的机动车驾驶证的有效期为4年.', 2, '正确', '错误', , , '123号令第十条  机动车驾驶证有效期分为六年、十年和长期.没有四年一说.', , 0);
INSERT INTO ""."examination_question1" VALUES (549, 549, '如图所示,在同向三车道高速公路上行车,车速每小时115公里应在哪条行车道上行驶?', 1, '最左侧行车道', '中间行车道', '最右侧行车道', '哪条都行', '如图上面的绿色标志,左侧限速在100-120,中间是80-100,右侧是60-80,车速每小时115公里,当然在左侧车道了.', 'http://images.juheapi.com/jztk/c1c2subject1/549.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (550, 550, '如图所示,在这种情况下不能够超车的原因是什么?', 4, '前车速度过快', '我方车速不足以超越前车', '路中心为黄线', '前车正在超车', '如图所示,很明显前面蓝色的车正在超车这个时候我们是不能超车的.', 'http://images.juheapi.com/jztk/c1c2subject1/550.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (551, 551, '交通肇事致一人以上重伤,负事故全部或者主要责任,并具有下列哪种行为的,构成交通肇事罪?', 3, '未带驾驶证', '未报警', '为逃避法律追究逃离事故现场的', '未抢救受伤人员', '根据规定,交通肇事致一人以上重伤,负事故全部责任或者主要责任,为逃避法律追究逃离事故现场的,以交通肇事罪定罪处罚,在这种情形下不能再认定为“逃逸”.', , 0);
INSERT INTO ""."examination_question1" VALUES (552, 552, '机动车仪表板上（如图所示）亮时,提醒驾驶人座椅没调整好.', 2, '正确', '错误', , , '此为提醒用户没有系安全带.', 'http://images.juheapi.com/jztk/c1c2subject1/552.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (553, 553, '如图所示,在这种情况下,A车可以向左变更车道.', 2, '正确', '错误', , , '白虚线是虚线同方向车道的,可以越线到另一同方向车道行驶.但右侧刚好有车,为了安全,这个时候不能变道.', 'http://images.juheapi.com/jztk/c1c2subject1/553.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (554, 554, '记分满12分的驾驶人拒不参加学习和考试的将被公告驾驶证停止使用.', 1, '正确', '错误', , , '这个没有问题.', , 0);
INSERT INTO ""."examination_question1" VALUES (555, 555, '变更车道时只需开启转向灯,并迅速转向驶入相应的车道,以不妨碍同车道机动车正常行驶.', 2, '正确', '错误', , , '经常跟大家说,看到“只需”等太绝对的词,及“迅速”等不利安全的词,基本上都选错误.', , 0);
INSERT INTO ""."examination_question1" VALUES (556, 556, '行车中当车辆前轮爆胎已发生转向时,驾驶人应双手紧握转向盘,尽力控制车辆直线行驶.', 1, '正确', '错误', , , '此题是对的,很多人都做错了.听哥来细说:行车中当车辆前轮爆胎已发生转向时: 第一步:驾驶人应双手紧握转向盘,尽力控制车辆转回原行驶路线.为什么要这么做?如果不控制方向,很容易翻车,如果急刹车,也很容翻车. 第二步:随后开始减速打起右转向灯,然后靠右最终是车停在路肩上.', , 0);
INSERT INTO ""."examination_question1" VALUES (557, 557, '这是什么操纵装置?', 4, '转向灯开关', '前照灯开关', '刮水器开关', '除雾器开关', '你看那些曲线,代表的就是雾气.题目扩展:上边的按钮是前挡风除雾,下边的按钮是后挡风除雾.', 'http://images.juheapi.com/jztk/c1c2subject1/557.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (558, 558, '驾驶人违反交通运输管理法规发生重大事故使公私财产遭受重大损失,可能会受到什么刑罚?', 2, '处5年以上徒刑', '处3年以下徒刑或者拘役', '处3年以上徒刑', '处3年以上7年以下徒刑', '第一百三十三条　违反交通运输管理法规,因而发生重大事故,致人重伤、死亡或者使公私财产遭受重大损失的,处三年以下有期徒刑或者拘役；别逃,一逃就是3年以上了,如果因为你的逃导致人死亡了,7年以上.', , 0);
INSERT INTO ""."examination_question1" VALUES (559, 559, '机动车仪表板上（如图所示）这个符号表示什么?', 2, '行李舱开启', '发动机舱开启', '燃油箱盖开启', '一侧车门开启', '前盖为发动机舱,后边为行李舱.', 'http://images.juheapi.com/jztk/c1c2subject1/559.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (560, 560, '如图所示,A车具有优先通行权.', 2, '正确', '错误', , , '三个先行原则:转弯的机动车让直行的车辆先行,右方道路来车先行,右转弯车让左转弯车先行.', 'http://images.juheapi.com/jztk/c1c2subject1/560.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (561, 561, '安全气囊是一种什么装置?', 3, '防抱死制动系统', '电子制动力分配系统', '辅助驾乘人员保护系统', '驾驶人头颈保护系统', '安全气囊,设置在车内前方（正副驾驶位）,侧方（车内前排和后排）和车顶三个方向.装有安全气囊系统的容器外部都印有Supplemental Inflatable Restraint System,简称SRS)的字样,直译成中文为“辅助可充气约束系统”.旨在减轻乘员的伤害程度,当发生碰撞事故时,避免乘员发生二次碰撞,或车辆发生翻滚等危险情况下被抛离座位.', , 0);
INSERT INTO ""."examination_question1" VALUES (562, 562, '如图所示,机动车在这种道路上行驶,在道路中间通行的原因是什么?', 3, '在道路中间通行速度快', '在道路中间通行视线好', '给两侧的非机动车和行人留有充足的通行空间', '防止车辆冲出路外', '图中是没有中心线的道路,在中间行驶是为了给两侧的非机动车和行人流出充足的通行空间.', 'http://images.juheapi.com/jztk/c1c2subject1/562.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (563, 563, '驾驶机动车变更车道为什么要提前开启转向灯?', 4, '提示前车让行', '提示行人让行', '开阔视野,便于观察路面情况', '提示其他车辆我方准备变更车道', '转向灯其实就相当于一个信号,传达自己车辆动向的信息.', , 0);
INSERT INTO ""."examination_question1" VALUES (564, 564, '仪表显示当前冷却液的温度是90度.', 1, '正确', '错误', , , '这个没有问题.', 'http://images.juheapi.com/jztk/c1c2subject1/564.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (565, 565, '机动车驶近急弯、坡道顶端等影响安全视距的路段时,减速慢行并鸣喇叭示意是为了什么?', 2, '提示前车后方车辆准备超车', '提示对向交通参与者我方有来车', '避免行至坡道顶端车辆动力不足', '测试喇叭是否能正常使用', '视线受阻,彼此看不到,所以就要靠喇叭来提醒,路上的行人或者车辆,我方有来车需要注意安全！', , 0);
INSERT INTO ""."examination_question1" VALUES (566, 566, '机动车仪表板上（如图所示）亮时,提醒发动机需要补充机油.', 2, '正确', '错误', , , '不是机油不足,是汽油不足了.', 'http://images.juheapi.com/jztk/c1c2subject1/566.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (567, 567, '驾驶人违反交通运输管理法规发生重大事故致人重伤、死亡,可能会受到什么刑罚?', 1, '处3年以下徒刑或者拘役', '处3年以上7年以下徒刑', '处5年以上徒刑', '处7年以上徒刑', '撞死,撞伤没逃逸的,3年以下！！撞伤,或直接撞死后逃逸的3?7年！！撞伤的,没死的,逃逸后又死了,7年上！！老实人,不跑的伤（3）心一下（以下）撞死人跑了的（在中间呢不就是3?7年）见死不救的就上去（7）吧！', , 0);
INSERT INTO ""."examination_question1" VALUES (568, 568, '如图所示,在前方路口可以掉头.', 2, '正确', '错误', , , '如图所示,前方是铁道路口,根据法规,铁道路口不能掉头.', 'http://images.juheapi.com/jztk/c1c2subject1/568.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (569, 569, '非机动车驾驶人、行人故意碰撞机动车造成交通事故的,机动车一方不承担赔偿责任.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法》第七十六条明确规定交通事故的损失是由非机动车驾驶人、行人故意碰撞机动车造成的,机动车一方不承担赔偿责任.', , 0);
INSERT INTO ""."examination_question1" VALUES (570, 570, '安装防抱死制动装置（ABS）的机动车紧急制动时,可用力踏制动踏板.', 1, '正确', '错误', , , 'ABS技术的出现,就是大大地降低了侧滑,和原地掉头的现象,保证了行驶的安全.如果,你在使用带ABS的车辆,在刹车时的动作,类似不带ABS的车这样轻轻的踩下去,很可能会不能触发ABS系统,也就起不到ABS的作用了,所以,一般就是要求,刹车动作要果断,一脚就踩到底,保证ABS的正确执行.', , 0);
INSERT INTO ""."examination_question1" VALUES (571, 571, '这个标志的含义是告示车辆驾驶人应慢行或停车,确保干道车辆优先.', 1, '正确', '错误', , , '此标志是减速让行标志.', 'http://images.juheapi.com/jztk/c1c2subject1/571.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (572, 572, '如图所示,当越过停在人行横道前的A车时,B车应减速,准备停车让行.', 1, '正确', '错误', , , '如图所示,红车B前方是人行横道,应当减速行驶,当B车越过A车后,若有行人通过,必要时停车让行.', 'http://images.juheapi.com/jztk/c1c2subject1/572.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (573, 573, '如图所示,在这种情形下,对方车辆具有先行权.', 1, '正确', '错误', , , '有障碍的让无障碍的一方先行；但有障碍的一方已经驶入有障碍路段而无障碍一方未驶入的,有障碍的一方先行.本题中,本车前方有障碍,且未进入障碍路段,对方先行.', 'http://images.juheapi.com/jztk/c1c2subject1/573.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (574, 574, '如图所示,当A车后方有执行任务的救护车驶来时,以下做法正确的是什么?', 3, '立即停车让路', '向左转弯让路', '靠右减速让路', '不必理会,继续行驶', '行车中遇到执行任务的消防车、救护车、工程救险车时要及时让行.', 'http://images.juheapi.com/jztk/c1c2subject1/574.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (575, 575, '如图所示,驾驶机动车遇到没有行人通过的人行横道时不用减速慢行.', 2, '正确', '错误', , , '按规定,驾驶机动车遇到人行横道必须减速让行,必要时停车让行.行人的流动很随意,需要谨值减速慢行.', 'http://images.juheapi.com/jztk/c1c2subject1/575.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (576, 576, '如图所示,在超车过程中,遇对向有来车时要放弃超车是因为什么?', 1, '如继续超车,易与对面机动车发生刮擦、相撞', '前车车速快', '对向来车车速快', '我方车辆提速太慢', '如图所示,对向车道有来车,前方车辆左侧没有足够的安全距离,应放弃超车,避免与对面车辆发生刮擦、相撞.', 'http://images.juheapi.com/jztk/c1c2subject1/576.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (577, 577, '如图所示,在这种情况下,会车时必须减速靠右通过.', 1, '正确', '错误', , , '会车时减速靠右行驶是常识.', 'http://images.juheapi.com/jztk/c1c2subject1/577.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (578, 578, '初次申领的机动车驾驶证的有效期为多少年?', 3, '3年', '5年', '6年', '12年', '第十条  机动车驾驶证有效期分为六年、十年和长期.', , 0);
INSERT INTO ""."examination_question1" VALUES (579, 579, '怎样通过这样的路口?', 4, '不减速通过', '加速尽快通过', '空挡滑行通过', '减速或停车观察', '右侧为无人看守的铁路标志,既然是无人看守,您就得悠着点,减速或停车观察一下吧.', 'http://images.juheapi.com/jztk/c1c2subject1/579.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (580, 580, '防抱死制动系统（ABS）在什么情况下可以最大限度发挥制动器效能?', 3, '间歇制动', '持续制动', '紧急制动', '缓踏制动踏板', 'ABS的目的就是为了防止刹车的时候一脚踩死,导致翻车什么的. 不过刹车刹死就比较慢（速度N快的时候）,效果等于踩下刹车,再松下刹车,反复几次！', , 0);
INSERT INTO ""."examination_question1" VALUES (581, 581, '如图所示,在这种情况下只要后方没有来车,可以倒车.', 2, '正确', '错误', , , '箭头框子是矩形,表示单行路；单行道是不准倒车的.', 'http://images.juheapi.com/jztk/c1c2subject1/581.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (582, 582, '初次申领的机动车驾驶证的有效期为6年.', 1, '正确', '错误', , , '123号令第四十七条  机动车驾驶人在机动车驾驶证的六年有效期内,每个记分周期均未记满12分的,换发十年有效期的机动车驾驶证；在机动车驾驶证的十年有效期内,每个记分周期均未记满12分的,换发长期有效的机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (583, 583, '驾驶人在机动车驾驶证的6年有效期内,每个记分周期均未达到12分的,换发10年有效期的机动车驾驶证.', 1, '正确', '错误', , , '123号令第四十七条  机动车驾驶人在机动车驾驶证的六年有效期内,每个记分周期均未记满12分的,换发十年有效期的机动车驾驶证；在机动车驾驶证的十年有效期内,每个记分周期均未记满12分的,换发长期有效的机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (584, 584, '如图所示,当您超越右侧车辆时,应该尽快超越,减少并行时间.', 1, '正确', '错误', , , '超车时应尽且提高辆车速度差,以减少超车距离和时间.超越时间加长,危险性也会增大；并行容易引起车祸.', 'http://images.juheapi.com/jztk/c1c2subject1/584.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (585, 585, '当您即将通过交叉路口的时候,才意识到要左转而不是向前,以下说法正确的是什么?', 2, '停在交叉路口,等待安全时左转', '继续向前行驶', '在确保安全的情况下,倒车然后左转', '原地掉头后,右转', '按规定,交叉路口,不可以停车,不可以倒车,只能继续向前行驶.', , 0);
INSERT INTO ""."examination_question1" VALUES (586, 586, '机动车紧急制动时,ABS系统在提供最大制动力的同时能使车前轮保持转向能力.', 1, '正确', '错误', , , 'ABS系统是“防抱死刹车系统”,也就是防止抱死车轮,既然没有抱死轮胎那么就是具有转向能力.', , 0);
INSERT INTO ""."examination_question1" VALUES (587, 587, '驾驶人在核发地车辆管理所管辖区以外居住的,可以向居住地车辆管理所申请换证.', 1, '正确', '错误', , , '《公安部令第123号》第四十九条:机动车驾驶人户籍迁出原车辆管理所管辖区的,应当向迁入地车辆管理所申请换证.机动车驾驶人在核发地车辆管理所管辖区以外居住的,可以向居住地车辆管理所申请换证.申请时应当填写申请表,并提交第四十八条:规定的证明、凭证.', , 0);
INSERT INTO ""."examination_question1" VALUES (588, 588, '驾驶机动车前,以下说法错误的是什么?', 4, '调整驾驶座椅,保证踩踏踏板舒适', '调整安全带的松紧与高低', '调整适合驾驶的方向盘位置', '调整安全头枕高度,使头枕正对驾驶人的颈椎', '驾驶机动车前,需要:调整驾驶座椅,保证踩踏踏板舒适；调整安全带的松紧与高低；调整适合驾驶的方向盘位置.', , 0);
INSERT INTO ""."examination_question1" VALUES (589, 589, '如图所示,驾驶机动车遇到这种情况能够加速通过,是因为人行横道没有行人通过.', 2, '正确', '错误', , , '按规定,驾驶机动车遇到人行横道时必须减速让行,必要时需要停车让行.因为行人有很大的随意性,需要谨慎减速慢行.', 'http://images.juheapi.com/jztk/c1c2subject1/589.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (590, 590, '如图所示,A车要在前方掉头行驶,可以在此处变换车道,进入左侧车道准备掉头.', 2, '正确', '错误', , , '白实线是用来规分同方向车道的,实线是表示同方向行驶,不能越线到另一同方向车道行驶. 白虚线是虚线同方向车道的,可以越线到另一同方向车道行驶.', 'http://images.juheapi.com/jztk/c1c2subject1/590.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (591, 591, '机动车仪表板上（如图所示）亮时表示什么?', 2, '已开启后雾灯', '已开启前照灯近光', '已开启前照灯远光', '已开启前雾灯', '近光灯的灯光走向是朝下的.远光灯的灯光走向是直的.', 'http://images.juheapi.com/jztk/c1c2subject1/591.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (592, 592, '下列哪个指示灯亮表示车辆在使用远光灯?(图10a. 10b. 10c. 10d)', 4, '10a', '10b', '10c', '10d', '10a:车灯总开关；10b:前后雾灯；10c:驻车制动器处于制动状态；10d:远光灯.', 'http://images.juheapi.com/jztk/c1c2subject1/592.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (593, 593, '避免爆胎的错误的做法是什么?', 1, '降低轮胎气压', '定期检查轮胎', '更换有裂纹或有很深损伤的轮胎', '及时清理轮胎沟槽里的异物', '轮胎缺气行驶是爆胎的祸根.车辆的缺气（轮胎胎压低于标准胎压）行驶时,随着胎压的下降,轮胎与地面的摩擦成倍增加,胎温急剧升高,轮胎变软,强度急剧下降.这种情况下,如果车辆高速行驶,就可能导致爆胎.如果车辆在低速行驶,也会伤胎,而且潜伏期长,隐蔽性大,更有危害性,为以后高速行车时埋下爆胎隐患,所以A是错误的做法', , 0);
INSERT INTO ""."examination_question1" VALUES (594, 594, '驾驶机动车变更车道前应仔细观察,目的是判断有无变更车道的条件.', 1, '正确', '错误', , , '先观察与判断汽车侧后方的交通情况,然后打开转向灯；再次确认两侧道路没有车辆超越,并保持准备驶入的车道留有安全距离；平稳转向,再驶往所需车道,完成后关闭转向灯即可.', , 0);
INSERT INTO ""."examination_question1" VALUES (595, 595, '在以下路段不能倒车的是什么?', 4, '交叉路口', '隧道', '急弯', '以上皆是', '第五十条机动车倒车时,应当察明车后情况,确认安全后倒车.不得在铁路道口、交叉路口、单行路、桥梁、急弯、陡坡或者隧道中倒车.', , 0);
INSERT INTO ""."examination_question1" VALUES (596, 596, '机动车仪表板上（如图所示）亮表示什么?', 4, '防抱死制动系统出现故障', '驻车制动器处于解除状态', '行车制动系统出现故障', '驻车制动器处于制动状态', '驻车制动器处于制动状态,就是手刹没松开.', 'http://images.juheapi.com/jztk/c1c2subject1/596.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (597, 597, '在划有道路中心线的道路上会车时,应做到保持安全车速、不越线行驶.', 1, '正确', '错误', , , '会车时要放慢车速,临近交会时条件不良,更应控制车速,不能盲目交会,必要时应该先停车,以达到两车顺利交会的目的.', , 0);
INSERT INTO ""."examination_question1" VALUES (598, 598, '机动车仪表板上（如图所示）亮表示什么?', 2, '空气内循环', '空气外循环', '迎面吹风', '风窗玻璃除霜', '此为空气外循环,当箭头在车内转的时候是空气内循环.', 'http://images.juheapi.com/jztk/c1c2subject1/598.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (599, 599, '如图所示,铁路道口设置这个标志,是提示驾驶人前方路口有单股铁道.', 2, '正确', '错误', , , '叉行符号:多股铁路与道路相交.', 'http://images.juheapi.com/jztk/c1c2subject1/599.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (600, 600, '在行驶过程中,机动车驾驶人要注意与前车保持安全距离.', 1, '正确', '错误', , , '追尾都是后车责任,因为没有足够的安全距离.', , 0);
INSERT INTO ""."examination_question1" VALUES (601, 601, '在没有交通信号指示的交叉路口,转弯的机动车让直行的车辆和行人先行.', 1, '正确', '错误', , , '右转让左转,转弯让直行.', , 0);
INSERT INTO ""."examination_question1" VALUES (602, 602, '驾驶机动车遇到前方低速行驶的洒水车作业时,以下做法错误的是什么?', 4, '注意避让', '若洒水车有指示箭头,在确保安全的情况下按箭头指示方向变更车道', '若洒水车无指示箭头,在确保安全的情况下选择合适的车道变更', '通过洒水车时应急加速通过', '驾驶机动车遇到前方低速行驶的洒水车作业时,需要注意避让；若洒水车有指示箭头,在确保安全的情况下按箭头指示方向变更车道；若洒水车无指示箭头,在确保安全的情况下选择合适的车道变更.', , 0);
INSERT INTO ""."examination_question1" VALUES (603, 603, '对驾驶已达到报废标准的机动车上路行驶的驾驶人,会受到下列哪种处罚?', 4, '处20以上200元以下罚款', '追究刑事责任', '处15日以下拘留', '吊销机动车驾驶证', '对驾驶拼装的机动车上道路行驶的驾驶人,处200元以上2000元以下罚款,并吊销驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (604, 604, '下列哪个指示灯亮表示车辆在使用近光灯?(图9a. 9b. 9c. 9d)', 3, '9a', '9b', '9c', '9d', '9a是车廓灯,9b是前后雾灯,9c是近光灯,9d制动系统出现异常或故障.', 'http://images.juheapi.com/jztk/c1c2subject1/604.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (605, 605, '驾驶机动车在高速公路上遇到雨雪天气时,需要降低车速、保持安全距离的原因,以下说法错误的是什么?', 4, '能见度下降,驾驶人难以及时发现前方车辆', '此类天气条件下的道路上,车辆的制动距离变长', '为车辆安全行驶提供足够的安全距离', '降低恶劣天气对车辆造成的损害', '驾驶机动车在高速公路上遇到雨雪天气时能见度下降,驾驶人难以及时发现前方车辆,在此类天气条件下的道路上,车辆的制动距离变长,需要降低车速,为车辆安全行驶提供足够的安全距离.', , 0);
INSERT INTO ""."examination_question1" VALUES (606, 606, '如图所示,在这种情况下遇到红灯交替闪烁时,要尽快通过道口.', 2, '正确', '错误', , , '都红灯了,就等等吧,小命要紧.', 'http://images.juheapi.com/jztk/c1c2subject1/606.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (607, 607, '如图所示,A车可以从左侧超越B车.', 2, '正确', '错误', , , 'B车的左边为实线,B车不能压过实线超车.', 'http://images.juheapi.com/jztk/c1c2subject1/607.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (608, 608, '这段道路红车所在车道是什么车道?', 1, '快速车道', '慢速车道', '应急车道', '专用车道', '最左侧为快速车道.', 'http://images.juheapi.com/jztk/c1c2subject1/608.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (609, 609, '驾驶机动车正在被其他车辆超越时,被超车辆减速靠右侧行驶的目的是什么?', 2, '以便随时停车', '给该车让出足够的超车空间', '避让行人与非机动车', '提醒其他交通参与者注意避让', '\"《道路交通安全法实施条例》第四十七条:机动车超车时,应当提前开启左转向灯、变换使用远、近光灯或者鸣喇叭.在没有道路中心线或者同方向只有1条机动车道的道路上,前车遇后车发出超车信号时,在条件许可的情况下,应当降低速度、靠右让路.后车应当在确认有充足的安全距离后,从前车的左侧超越,在与被超车辆拉开必要的安全距离后,开启右转向灯,驶回原车道.\"', , 0);
INSERT INTO ""."examination_question1" VALUES (610, 610, '驾驶机动车在进出非机动车道时,最高速度不能超过多少?', 4, '40公里/小时', '50公里/小时', '60公里/小时', '30公里/小时', '规定:驾驶机动车在进出非机动车道时,最高速度不能超过30公里/小时', , 0);
INSERT INTO ""."examination_question1" VALUES (611, 611, '申请人患有精神病的,可以申领机动车驾驶证,但是在发病期间不得驾驶机动车.', 2, '正确', '错误', , , '《机动车驾驶证申领和使用规定》第十二条有下列情形之一的,不得申请机动车驾驶证:(一)有器质性心脏病、癫痫病、美尼尔氏症、眩晕症、癔病、震颤麻痹、精神病、痴呆以及影响肢体活动的神经系统疾病等妨碍安全驾驶疾病的;(二)三年内有吸食、注射毒品行为或者解除强制隔离戒毒措施未满三年,或者长期服用依赖性精神药品成瘾尚未戒除的;(三)造成交通事故后逃逸构成犯罪的;（四）申请人以欺骗、贿赂等不正当手段取得机动车驾驶证的；(五)醉酒驾驶营运机动车依法被吊销机动车驾驶证未满十年的.', , 0);
INSERT INTO ""."examination_question1" VALUES (612, 612, '机动车仪表板上（如图所示）亮时,不影响正常行驶.', 2, '正确', '错误', , , '此图为汽车现在有故障,不能上路.', 'http://images.juheapi.com/jztk/c1c2subject1/612.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (613, 613, '驾驶报废机动车上路行驶的驾驶人,除按规定罚款外,还要受到哪种处理?', 4, '收缴驾驶证', '撤销驾驶许可', '强制恢复车况', '吊销驾驶证', '对于驾驶已达到报废标准的机动车上路行驶的驾驶人,处200元以上2000元以下罚款,并吊销机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (614, 614, '如图所示,在这种情况下准备进入环形路口时,为了保证车后车流的通畅,应加速超越红车进入路口.', 2, '正确', '错误', , , '准备进入环形路口的让已在路口内的机动车先行.', 'http://images.juheapi.com/jztk/c1c2subject1/614.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (615, 615, '驾驶机动车遇到沙尘、冰雹、雨、雾、结冰等气象条件时应降低行驶速度.', 1, '正确', '错误', , , '安全第一,毫无疑问.', , 0);
INSERT INTO ""."examination_question1" VALUES (616, 616, '如图所示,在这种情况下驶近路口,车辆可以怎么行驶?', 2, '只能直行', '左转或者直行', '左转或右转', '直行或右转', '擦亮眼睛,注意看图！地面图示直行或者左转弯箭头！只有这两种行驶路线！', 'http://images.juheapi.com/jztk/c1c2subject1/616.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (617, 617, '雾天行车听到对方鸣喇叭,也应该鸣喇叭回应,以提示对方车辆.', 1, '正确', '错误', , , '说法正确,雾天时视线受阻,可多鸣笛提醒对方车辆.', , 0);
INSERT INTO ""."examination_question1" VALUES (618, 618, '前轮胎爆裂已出现转向时,驾驶人不要过度矫正,应在控制住方向的情况下,应怎样做,使车辆缓慢减速?', 3, '采取紧急制动', '使用驻车制动', '轻踏制动踏板', '迅速踏下制动踏板', 'A是踩脚刹,B是拉手刹,D是也是踩脚刹,都是属于迅速减速,危险.', , 0);
INSERT INTO ""."examination_question1" VALUES (619, 619, '机动车驾驶证补领后,以下说法正确的是什么?', 2, '原驾驶证继续使用', '原驾驶证作废,不得继续使用', '原驾驶证特殊情况下使用', '替换使用', '《机动车驾驶证申领和使用规定》机动车驾驶人补领机动车驾驶证后,原机动车驾驶证作废,不得继续使用.', , 0);
INSERT INTO ""."examination_question1" VALUES (620, 620, '饮酒后只要不影响驾驶操作可以短距离驾驶机动车.', 2, '正确', '错误', , , '这个不用说吧,喝了酒就不能开车,不管你喝了多少.', , 0);
INSERT INTO ""."examination_question1" VALUES (621, 621, '驾驶机动车在没有道路中心线的狭窄山路怎样会车?', 1, '不靠山体的一方先行', '靠山体的一方先行', '重车让空车先行', '速度慢的先行', '山路狭窄,这种山路一般有在靠山体一侧挖个让车位,说白了就是加宽点用来会车用的,两车交会时靠山的开进去停住,不一定有落石的, 让不靠山体车先行,是不会开到路边去的,总不能不靠山的停下来,靠山的车转弯进去再转弯出来吧,这要加宽多长哦,这样子的话就不叫狭窄山路了.', , 0);
INSERT INTO ""."examination_question1" VALUES (622, 622, '灯光开关在该位置时,前雾灯点亮.', 1, '正确', '错误', , , '你看它停的位置雾灯是冲左的,此为前雾灯,故正确.', 'http://images.juheapi.com/jztk/c1c2subject1/622.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (623, 623, '车辆后轮胎爆裂,车尾会摇摆不定,驾驶人应双手紧握转向盘,控制车辆保持直线行驶,减速停车.', 1, '正确', '错误', , , '如果突然爆胎,瞬间靠右,会导致与后方来车相撞.所以只能先保持直线行驶,再减速停车.', , 0);
INSERT INTO ""."examination_question1" VALUES (624, 624, '机动车向左转弯、向左变更车道,驶离停车地点及掉头时,提前开启左转向灯是为了什么?', 3, '提示前车,将要向左变更行驶路线', '提示前车,将要向右变更行驶路线', '提示后车,将要向左变更行驶路线', '提示后车,将要向右变更行驶路线', '开启左转向灯,主要是为了提醒后车,开启左转向灯,当然是向左变更车道了.', , 0);
INSERT INTO ""."examination_question1" VALUES (625, 625, '人行横道上禁止掉头的原因是什么?', 4, '路段有监控设备', '人行横道禁止停车', '人行横道禁止车辆通行', '避免妨碍行人正常通行,确保行人安全', '人行横道禁止掉头.虽然设有交通隔离护栏路段的人行横道不设禁止掉头交通标志,但规定严禁在人行横道掉头,这主要是为了保护人行横道上行人的安全.因为行人具有优先通行权,人行横道就视同“禁止性标志”.', , 0);
INSERT INTO ""."examination_question1" VALUES (626, 626, '其他车辆不准进入专用车道行驶,其目的是为了不影响专用车的正常通行.', 1, '正确', '错误', , , '其他车辆进入专用车道可能影响专用车道的正常通行,专用,就是专门使用.', , 0);
INSERT INTO ""."examination_question1" VALUES (627, 627, '下列哪种标志是驾驶机动车上路行驶应当在车上放置的标志?', 3, '保持车距标志', '提醒危险标志', '检验合格标志', '产品合格标志', '检验合格标志,保险标志都是必须要放置的.', , 0);
INSERT INTO ""."examination_question1" VALUES (628, 628, '如图所示,当您车速为95km/h时,您可以在哪条车道内行驶?', 2, '车道A', '车道B', '车道C', '车道D', '如图所示,道路限速标志标明车道B最高限速100KM/h,所以选择车道B.同方向3条车道的,最左侧车速为每小时110公里,中间车道最低车速为90公里.道路限速标志标明车速的车速与上述车道行驶车速规定不一样的,按照道路限速标志标明的车速行驶.', 'http://images.juheapi.com/jztk/c1c2subject1/628.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (629, 629, '超车需从前车左侧超越,以下说法正确的是什么?', 1, '便于观察,有利于安全', '右侧为快速车道', '左侧为慢速车道', '我国实行左侧通行原则', '除了“便于观察,有利于安全”其他选项表述均错.', , 0);
INSERT INTO ""."examination_question1" VALUES (630, 630, '造成交通事故后逃逸且构成犯罪的驾驶人,将吊销驾驶证且终生不得重新取得驾驶证.', 1, '正确', '错误', , , '吊二撤三醉五逃终生,这样就好理解了吧！', , 0);
INSERT INTO ""."examination_question1" VALUES (631, 631, '如图所示,在这种情况下通过前方路口,应该怎么行驶?', 1, '减速或停车避让行人', '加速通过', '赶在行人前通过', '靠左侧行驶', '前方有行人,有路口,安全第一,一定要减速或者停车避让行人！', 'http://images.juheapi.com/jztk/c1c2subject1/631.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (632, 632, '如图所示,A车在这种情况下应适当减速.', 1, '正确', '错误', , , '如图所示,A车在B车后,B车开启了右转向灯,减速右转,紧跟B车的A车需要减速,以保持安全距离.', 'http://images.juheapi.com/jztk/c1c2subject1/632.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (633, 633, '夜间会车规定150米以内使用近光灯的原因是什么?', 2, '驾驶人的操作习惯行为', '使用远光灯会造成驾驶人出现眩目,易引发危险', '两车之间相互提示', '提示后方车辆', '在夜间会车时开远光灯,容易造成对方驾驶员10米左右出现盲区,这种视线受阻一般可持续四五秒,而这四五秒极有可能酿成车祸.冬天冰雪路面光滑,会车时如果再开远光灯更容易造成事故.', , 0);
INSERT INTO ""."examination_question1" VALUES (634, 634, '年满20周岁,可以初次申请下列哪种准驾车型?', 1, '大型货车.', '大型客车', '中型客车', '牵引车', '申请城市公交车、大型货车、无轨电车或者有轨电车准驾车型的,在20周岁以上,50周岁以下；', , 0);
INSERT INTO ""."examination_question1" VALUES (635, 635, '在涉水路段跟车行驶时,应当怎样做?', 3, '紧跟其后', '并行通过', '适当增加车距', '超越前车,抢先通过', '适当增加车距才是安全文明的做法,大家懂的.', , 0);
INSERT INTO ""."examination_question1" VALUES (636, 636, '驾驶机动车在这种道路上如何通行?', 1, '在道路中间通行', '在道路两边通行', '实行分道通行', '可随意通行', '这种道路在在道路中间通行最安全', 'http://images.juheapi.com/jztk/c1c2subject1/636.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (637, 637, '这个标志的含义是告示前方道路有障碍物,车辆左侧绕行.', 1, '正确', '错误', , , '注意障碍物标志左侧绕行:用以告示前方道路有障碍物,车辆应按标志指示减速慢行.设在道路障碍物前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/637.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (638, 638, '机动车、非机动车和行人实行分道行驶,是为了规范交通秩序,提高通行效率.', 1, '正确', '错误', , , '行人、非机动车、机动车速度都是不一样的,分开以后肯定是能提高通行效率的,当然交通秩序当也能更好地规范.', , 0);
INSERT INTO ""."examination_question1" VALUES (639, 639, '如图所示,当车辆驶近这样的路口时,以下说法错误的是什么?', 4, '右前方路口视野受阻,如有突然冲出车辆,容易引发事故', '为避免车辆从路口突然冲出引发危险,应适当降低车速', '因为视野受阻,应鸣喇叭提醒侧方道路来车', '本车有优先通行权,可加速通过', '任何时候“加速”都是不对的！', 'http://images.juheapi.com/jztk/c1c2subject1/639.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (640, 640, '如图所示,在这种情况下,驾驶机动车要停车让行.', 1, '正确', '错误', , , '人行横道是生命的保护线,人行横道正有行人通行,要停车让行,这体现对生命的尊重！', 'http://images.juheapi.com/jztk/c1c2subject1/640.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (641, 641, '如图所示,机动车在这样的城市道路上行驶,最高的行驶速度不得超过50公里/小时.', 2, '正确', '错误', , , '从图中可以行出,是没有中心线的道路,按照规定,机动车在道路上行驶不得超过限速标志、标线标明的速度.在没有限速标志、标线的道路上,没有道路中心线的道路,城市道路为每小时30公里,公路为每小时40公里.', 'http://images.juheapi.com/jztk/c1c2subject1/641.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (642, 642, '灯光开关在该位置时,后雾灯点亮.', 1, '正确', '错误', , , '你看它停的位置雾灯是冲右的,此为后雾灯,故正确.', 'http://images.juheapi.com/jztk/c1c2subject1/642.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (643, 643, '牵引发生故障的机动车时,最高车速不得超过多少?', 2, '20公里/小时', '30公里/小时', '40公里/小时', '50公里/小时', '机动车行驶中遇有牵引发生故障的机动车时,最高行驶速度不得超过每小时30公里', , 0);
INSERT INTO ""."examination_question1" VALUES (644, 644, '公安机关交通管理部门对累积记分达到规定分值的驾驶人怎样处理?', 3, '处15日以下拘留', '终生禁驾', '进行法律法规教育,重新考试', '依法追究刑事责任', '满12分了你就重新去学习一下吧,再来金手指重新回锅一下.', , 0);
INSERT INTO ""."examination_question1" VALUES (645, 645, '机动车仪表板上（如图所示）亮时,提醒发动机冷却液可能不足.', 1, '正确', '错误', , , '发动机冷却液可能不足,没有问题.', 'http://images.juheapi.com/jztk/c1c2subject1/645.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (646, 646, '如图所示,B车具有优先通行权.', 2, '正确', '错误', , , '右转让左转,转弯让直行.如图所示,A车直行而B车右转,明显是A车先行,B车避让.', 'http://images.juheapi.com/jztk/c1c2subject1/646.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (647, 647, '将转向灯开关向上提,左转向灯亮.', 2, '正确', '错误', , , '转向灯操作:上右下左.', 'http://images.juheapi.com/jztk/c1c2subject1/647.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (648, 648, '准驾车型为小型自动挡汽车的,可以驾驶以下哪种车型?', 4, '低速载货汽车', '小型汽车', '二轮摩托车', '轻型自动挡载货汽车', '小型自动挡汽车（C2）准驾车型:小型、微型自动挡载客汽车以及轻型、微型自动挡载货汽车', , 0);
INSERT INTO ""."examination_question1" VALUES (649, 649, '当行人出现交通安全违法行为时,车辆可以不给行人让行.', 2, '正确', '错误', , , '只要是非机动车、行人、牲口,什么时候都得让行.', , 0);
INSERT INTO ""."examination_question1" VALUES (650, 650, '交叉路口不得倒车的原因是什么?', 1, '交通情况复杂,容易造成交通堵塞甚至引发事故', '交通监控设备多', '交通警察多', '车道数量少', '交叉路口车流最多,情况很复杂,极易造车拥堵,易发事故,当然是不允许倒车的.', , 0);
INSERT INTO ""."examination_question1" VALUES (651, 651, '交通肇事致一人以上重伤,负事故全部或者主要责任,并具有下列哪种行为的,构成交通肇事罪?', 2, '未带驾驶证', '酒后、吸食毒品后驾驶机动车辆的', '未报警', '未抢救受伤人员', '交通肇事致一人受伤(属于一般事故),负事故全部或者主要责任,并具有酒后、无证驾驶、严重超载等情形之一的,以交通肇事罪处罚.', , 0);
INSERT INTO ""."examination_question1" VALUES (652, 652, '这个标志的含义是提醒车辆驾驶人前方是急转弯路段.', 2, '正确', '错误', , , '易滑:用以促使车辆驾驶人注意慢行.设在路滑容易发生事故的路段以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/652.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (653, 653, '机动车驾驶人补领机动车驾驶证后,原机动车驾驶证作废,不得继续使用.', 1, '正确', '错误', , , '《机动车驾驶证申领和使用规定》机动车驾驶人补领机动车驾驶证后,原机动车驾驶证作废,不得继续使用.', , 0);
INSERT INTO ""."examination_question1" VALUES (654, 654, '车在这种条件的道路上,最高速度不能超过每小时50公里.', 2, '正确', '错误', , , '冰雪路面最高速度不能超过每小时30公里.', 'http://images.juheapi.com/jztk/c1c2subject1/654.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (655, 655, '夜间尾随前车行驶时,后车可以使用远光灯.', 2, '正确', '错误', , , '你会开车了体验一下就有感受了,你开的好好的,后面一辆车开着远光灯,你从后视镜里看着就不清楚,估计那时你会骂娘的.', , 0);
INSERT INTO ""."examination_question1" VALUES (656, 656, '仪表显示当前发动机转速是6000转/分钟.', 2, '正确', '错误', , , '这个是时速表.不是转速.', 'http://images.juheapi.com/jztk/c1c2subject1/656.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (657, 657, '驾驶机动车在隧道、陡坡等特殊路段不得超车.', 1, '正确', '错误', , , '隧道、陡坡等特殊路段路况那么复杂还超车,记住安全第一.', , 0);
INSERT INTO ""."examination_question1" VALUES (658, 658, '路口转弯过程中,持续开启转向灯,主要是因为什么?', 2, '完成转弯动作前,关闭转向灯是习惯动作', '让其他驾驶人知道您正在转弯', '让其他驾驶人知道您正在超车', '完成转弯动作前,关闭转向灯会对车辆造成损害', '要让道路上的行人或者车辆,你在转弯,注意安全！', , 0);
INSERT INTO ""."examination_question1" VALUES (659, 659, '如图所示,车辆在拥挤路段排队行驶时,遇有其他车辆强行穿插行驶,以下说法正确的是什么?', 4, '迅速提高车速不让其穿插', '持续鸣喇叭警告', '迅速左转躲避', '减速或停车让行', '减速停车让行,文明才是王道.', 'http://images.juheapi.com/jztk/c1c2subject1/659.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (660, 660, '机动车驾驶人在一个记分周期内累积记分达到12分,拒不参加学习和考试的,将被公安机关交通管理部门公告其驾驶证停止使用.', 1, '正确', '错误', , , '正确的,所以要及时去处理扣分.', , 0);
INSERT INTO ""."examination_question1" VALUES (661, 661, '山区道路对安全行车的主要影响是什么?', 2, '交通情况单一', '坡长弯急,视距不足', '车流密度大', '道路标志少', '最关键的一点是山区道路行驶时不容易掌握周围情况,也就是所谓视距不足.', , 0);
INSERT INTO ""."examination_question1" VALUES (662, 662, '在这段城市道路上行驶的最高速度不能超过多少?', 2, '40公里/小时', '30公里/小时', '50公里/小时', '70公里/小时', '根据《中华人民共和国道路交通安全法实施条例》第四十五条:……在没有限速标志、标线的道路上,机动车不得超过下列最高行驶速度:　　(一)没有道路中心线的道路,城市道路为每小时30公里,公路为每小时40公里；　　(二)同方向只有1条机动车道的道路,城市道路为每小时50公里,公路为每小时70公里.图为没有道路中心线的城市道路,所以最高时速为每小时30公里.', 'http://images.juheapi.com/jztk/c1c2subject1/662.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (663, 663, '驾驶机动车在雾天行车开启雾灯和危险报警闪光灯.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法实施条例》第五十八条 机动车在夜间没有路灯、照明不良或者遇有雾、雨、雪、沙尘、冰雹等低能见度情况下行驶时,应当开启前照灯、示廓灯和后位灯,但同方向行驶的后车与前车近距离行驶时,不得使用远光灯.<b>机动车雾天行驶应当开启雾灯和危险报警闪光灯</b>.', , 0);
INSERT INTO ""."examination_question1" VALUES (664, 664, '如图所示,在这种情况下通过交叉路口时,不得超车的原因是什么?', 3, '路口有交通监控设备', '路口设有信号灯', '路口内交通情况复杂,易发生交通事故', '机动车速度慢,不足以超越前车', '都是规定,交叉路口不能超车.因为交叉路口车流大,行车条件十分复杂,这个时候超车.很容易造成事故.', 'http://images.juheapi.com/jztk/c1c2subject1/664.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (665, 665, '这个导向箭头是何含义?', 1, '指示直行或右转弯', '指示向右转弯或掉头', '指示直行或向右变道', '指示直行或掉头', '直接或右转,送分题', 'http://images.juheapi.com/jztk/c1c2subject1/665.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (666, 666, '遇后车超车时,在条件许可的情况下应减速靠右让路,是为了给后车留出超车空间.', 1, '正确', '错误', , , '和谐第一,做题就要假扮圣人,人家要超车就减速靠右给他让出超车的位置.', , 0);
INSERT INTO ""."examination_question1" VALUES (667, 667, '有这种信号灯的路口允许机动车如何行驶?', 1, '向右转弯', '停车等待', '向左转弯', '直行通过', '只有右转的灯是绿灯,所以可以向右转弯.', 'http://images.juheapi.com/jztk/c1c2subject1/667.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (668, 668, '交通肇事致一人以上重伤,负事故全部或者主要责任,并具有下列哪种行为的,构成交通肇事罪?', 3, '未带驾驶证', '未及时报警', '严重超载驾驶的', '未抢救受伤人员', '交通肇事致一人受伤(属于一般事故),负事故全部或者主要责任,并具有酒后、无证驾驶(这个是没有驾驶证,而不是没带驾驶证)、严重超载等情形之一的,以交通肇事罪处罚.', , 0);
INSERT INTO ""."examination_question1" VALUES (669, 669, '灯光开关旋转到这个位置时,全车灯光点亮.', 2, '正确', '错误', , , '此表示不是全车灯亮,而是示廓灯点亮.', 'http://images.juheapi.com/jztk/c1c2subject1/669.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (670, 670, '这辆在道路上行驶的机动车有下列哪种违法行为?', 2, '未按规定悬挂号牌', '故意遮挡号牌', '占用非机动车道', '逆向行驶', '很明显的用光盘遮挡号牌了,现在遮挡号牌的直接扣12分,小心吧您.', 'http://images.juheapi.com/jztk/c1c2subject1/670.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (671, 671, '存在以下哪种行为的申请人在一年内不得再次申领机动车驾驶证?', 2, '在考试过程中出现身体不适', '在考试过程中有舞弊行为', '不能按照教学大纲认真练习驾驶技能', '未参加理论培训', '《机动车驾驶证申领和使用规定》第六章,法律责任,第七十八条,隐瞒有关情况或者提供虚假材料申领机动车驾驶证的,申请人在一年内不得再次申领机动车驾驶证.申请人在考试过程中有贿赂、舞弊行为的,取消考试资格,已经通过考试的其他科目成绩无效；申请人在一年内不得再次申领机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (672, 672, '驾驶机动车在道路上发生交通事故,任何情况下都应标明现场位置后,先行撤离现场.', 2, '正确', '错误', , , '因为根据交通安全法的规定,如果机动车在道路上面发生了事故,那么必须要马上停车保护现场,如果有人员伤亡的情况,驾驶人应该马上去抢救受伤人员.并且报告执勤的交警或交通管理部门.如果因为抢救伤员而而变动了现场的话,就要表明位置.路人或过往的车辆驾驶人或乘客都应该协助帮忙.如果在道路上发生交通事故,没有造成人员伤亡的,当事人没有对事故有任何争议的,可以撤离现场恢复交通次序,双方自行协商处理损害赔偿就行了.如果说有争议的话,那么不要撤离现场,需要马上报个相关的交警来处理.', , 0);
INSERT INTO ""."examination_question1" VALUES (673, 673, '这个标志的含义是警告车辆驾驶人前方是人行横道.', 2, '正确', '错误', , , '注意行人:用以警告车辆驾驶人减速慢行,注意行人.设在行人密集,或不易被驾驶员发现的人行横道线以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/673.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (674, 674, '在道路上发生交通事故造成人身伤亡时,要立即抢救受伤人员并迅速报警.', 1, '正确', '错误', , , '这个没问题,很和谐.', , 0);
INSERT INTO ""."examination_question1" VALUES (675, 675, '如图所示,在这种情况下跟车行驶,不能使用远光灯的原因是什么?', 3, '不利于看清远方的路况', '不利于看清车前的路况', '会影响前车驾驶人的视线', '会影响自己的视线', '当开启远光灯之后,由于远光灯的光束十分集中且亮度较高,前车的司机在看左右后视镜时,往往会受刺眼的强光影响,形成局部的“盲区”甚至短暂“失明”,无法看清车身两侧的路面情况.尤其在需要并线或者转弯时,一个不留神就极其容易发生交通意外.', 'http://images.juheapi.com/jztk/c1c2subject1/675.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (676, 676, '申请人有下列哪种行为,三年内不得再次申领机动车驾驶证?', 3, '实习期记满12分,注销驾驶证的', '申请人在考试过程中有舞弊行为的', '申请人以欺骗、贿赂等不正当手段取得机动车驾驶证的', '申请人未能在培训过程中认真练习的', '《机动车驾驶证申领和使用规定》第十二条有下列情形之一的,不得申请机动车驾驶证:(一)有器质性心脏病、癫痫病、美尼尔氏症、眩晕症、癔病、震颤麻痹、精神病、痴呆以及影响肢体活动的神经系统疾病等妨碍安全驾驶疾病的;(二)三年内有吸食、注射毒品行为或者解除强制隔离戒毒措施未满三年,或者长期服用依赖性精神药品成瘾尚未戒除的;(三)造成交通事故后逃逸构成犯罪的;（四）申请人以欺骗、贿赂等不正当手段取得机动车驾驶证的；(五)醉酒驾驶营运机动车依法被吊销机动车驾驶证未满十年的.', , 0);
INSERT INTO ""."examination_question1" VALUES (677, 677, '这个导向箭头是何含义?', 2, '指示前方可直行或掉头', '指示前方可左转或掉头', '指示前方可直行或向左变道', '指示前方可直行或左转', '其实是一个左转+掉头的综合体 orz', 'http://images.juheapi.com/jztk/c1c2subject1/677.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (678, 678, '如图所示,这这种情况下只要后方、对向无来车,可以掉头.', 1, '正确', '错误', , , '简单来说:黄线是用来画分来回车道的,黄实线是禁止越线行驶.黄虚线是在对面没车来的情况下,可以在黄虚线越过转弯或者是掉头.白实线是用来规分同方向车道的,实线是表示同方向行驶,不能越线到另一同方向车道行驶.白虚线是虚线同方向车道的,可以越线到另一同方向车道行驶.', 'http://images.juheapi.com/jztk/c1c2subject1/678.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (679, 679, '遇前方路段车道减少,车辆行驶缓慢,为保证安全有序应该怎样做?', 1, '依次交替通行', '穿插到前方排队车辆中通过', '加速从前车左右超越', '借对向车道迅速通过', '第五十三条:机动车在车道减少的路口、路段,遇有前方机动车停车排队等候或者缓慢行驶的,应当每车道一辆依次交替驶入车道减少后的路口、路段.', , 0);
INSERT INTO ""."examination_question1" VALUES (680, 680, '驾驶人的机动车驾驶证被依法扣留、暂扣的情况下不得驾驶机动车.', 1, '正确', '错误', , , '没驾驶本了还怎么开车,逮住了就是无证驾驶.', , 0);
INSERT INTO ""."examination_question1" VALUES (681, 681, '以下哪种行为处十日以下拘留,并处一千元以上二千元以下罚款,吊销机动车驾驶证?', 4, '醉酒驾驶机动车的', '故意遮挡机动车号牌的', '使用其他车辆保险标志的', '因饮酒后驾驶机动车被处罚,再次饮酒后驾驶机动车的', '依据《中华人民共和国道路交通安全法》:饮酒后驾驶机动车的,处暂扣六个月机动车驾驶证,并处一千元以上二千元以下罚款.因饮酒后驾驶机动车被处罚,再次饮酒后驾驶机动车的,处十日以下拘留,并处一千元以上二千元以下罚款,吊销机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (682, 682, '如图所示,通过有这个标志的路口时无需减速.', 2, '正确', '错误', , , '如图所示,前方标志为减速让行标志.', 'http://images.juheapi.com/jztk/c1c2subject1/682.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (683, 683, '如图所示,在这种情况下只要后方、对向无来车,可以掉头.', 2, '正确', '错误', , , '黄色的实线是不能压的,掉头必须要穿过黄线,所以,有黄色的实线是不能掉头的,强制掉头,搞不好脖子上的头真会掉的.', 'http://images.juheapi.com/jztk/c1c2subject1/683.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (684, 684, '如图所示,直行车辆遇到前方路口堵塞,以下说法正确的是什么?', 2, '可以直接驶入路口内等待通行', '等前方道路疏通后,且信号灯为绿灯时方可继续行驶', '只要信号灯为绿灯,就可通过', '等有其他机动车进入路口时跟随行驶', '第五十三条机动车遇有前方交叉路口交通阻塞时,应当依次停在路口以外等候,不得进入路口.', 'http://images.juheapi.com/jztk/c1c2subject1/684.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (685, 685, '超过机动车驾驶证有效期一年以上未换证被注销,但未超过2年的,机动车驾驶人应当如何恢复驾驶资格?', 1, '参加道路交通安全法律、法规和相关知识考试合格后', '参加场地考试合格后', '参加道路驾驶技能考试合格后', '参加安全文明驾驶常识考试合格后', '《机动车驾驶证申领和使用规定》被注销机动车驾驶证未超过两年的,机动车驾驶人考试科目一合格后,可以恢复驾驶资格.', , 0);
INSERT INTO ""."examination_question1" VALUES (686, 686, '遇前方路段车道减少,车辆行驶缓慢,为保证道路通畅,应借对向车道迅速通过.', 2, '正确', '错误', , , '第五十三条机动车遇有前方交叉路口交通阻塞时,应当依次停在路口以外等候,不得进入路口.机动车在遇有前方机动车停车排队等候或者缓慢行驶时,应当依次排队,不得从前方车辆两侧穿插或者超越行驶,不得在人行横道,网状线区域内停车等候.', , 0);
INSERT INTO ""."examination_question1" VALUES (687, 687, '驾驶证审验内容不包括以下哪一项?', 4, '道路交通安全违法行为、交通事故处理情况', '身体条件情况', '道路交通安全违法行为记分及记满12分后参加学习和考试情况', '机动车检验情况', '驾驶证审验内容包括:道路交通安全违法行为、交通事故处理情况；身体条件情况；道路交通安全违法行为记分及记满12分后参加学习和考试情况.', , 0);
INSERT INTO ""."examination_question1" VALUES (688, 688, '如图所示,以下哪种情况可以超车?(图50a.50b. 50c. 50d)', 3, '50a', '50b', '50c', '50d', '大家都知道的,实线一侧是不可以跨越的,只有虑线的一侧才可以,所以只有50c具备超车条件.', 'http://images.juheapi.com/jztk/c1c2subject1/688.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (689, 689, '持有小型汽车驾驶证的驾驶人,发生交通事故造成人员死亡承担同等以上责任未被吊销机动车驾驶证的,应当在本记分周期结束后三十日内到公安机关交通管理部门接受审验,同时应当申报身体条件情况.', 1, '正确', '错误', , , '《公安部令第123号》第六十条持有本条第三款规定以外准驾车型驾驶证的驾驶人,发生交通事故造成人员死亡承担同等以上责任未被吊销机动车驾驶证的,应当在本记分周期结束后三十日内到公安机关交通管理部门接受审验.', , 0);
INSERT INTO ""."examination_question1" VALUES (690, 690, '遇到前方车辆停车排队或者缓慢行驶时,强行穿插,以下说法正确的是什么?', 3, '允许,因为可以省油', '允许,因为可以快速的通过拥堵区', '禁止,因为这样扰乱车流,加重拥堵', '禁止,因为这样不利于省油', '机动车遇有前方车辆停车排队等候或者缓慢行驶时,不得借道超车或者占用对面车道,不得穿插等候的车辆,应当依次排队.所以,C是正确选项！', , 0);
INSERT INTO ""."examination_question1" VALUES (691, 691, '驾驶机动车在隧道内行驶时,可以临时停车.', 2, '正确', '错误', , , '机动车在交叉路口、铁路道口、急弯路、宽度不足4米的窄路、桥梁、陡坡、隧道以及距离上述地点50米以内的路段,不得停车.', , 0);
INSERT INTO ""."examination_question1" VALUES (692, 692, '路面上导向箭头是何含义?', 3, '提示前方有左弯或需向左绕行', '提示前方有障碍需向左合流', '提示前方有右弯或需向右合流', '提示前方有左弯或需向左合流', '按图箭头意思就是要向右小转变,可能是道路右转,可能是两个不同的车道要汇合', 'http://images.juheapi.com/jztk/c1c2subject1/692.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (693, 693, '雾天行车为了提高能见度,应该开启远光灯.', 2, '正确', '错误', , , '在行驶中应该打开雾灯、尾灯、示廓灯和前照灯(近光),不能使用远光灯.因远光灯光轴偏上,射出的光线被雾气漫反射,在车前形成白茫茫一片,如同隔着磨砂玻璃一样,反而什么都看不见.雾天行车可以靠路面车道标线以及前车的红色尾灯来行车.', , 0);
INSERT INTO ""."examination_question1" VALUES (694, 694, '这个标志的含义是提醒车辆驾驶人前方路段通过村庄或集镇.', 1, '正确', '错误', , , '村庄:用以提醒车辆驾驶人小心驾驶.设在紧靠村庄、集镇且视线不良的路段以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/694.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (695, 695, '这个路面标记是什么标线?', 4, '禁驶区', '网状线', '导流线', '中心圈', '中心圈,设置在交叉路口中心的白色圆形或菱形区域,用以区分车辆大、小转弯,及交叉路口车辆左右转弯的指示,车辆不得压线行驶.机动车向左转弯时,必须紧靠中心圈小转弯.', 'http://images.juheapi.com/jztk/c1c2subject1/695.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (696, 696, '驾驶证丢失后,驾驶人可以继续驾驶机动车.', 2, '正确', '错误', , , '驾驶证丢失后,驾驶人不可继续驾驶机动车,应该向核发地车辆管理所申请补发.', , 0);
INSERT INTO ""."examination_question1" VALUES (697, 697, '这个仪表是何含义?', 1, '发动机转速表', '行驶速度表', '区间里程表', '百公里油耗表', '此为发动机转速表,跟时速表对比记忆.', 'http://images.juheapi.com/jztk/c1c2subject1/697.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (698, 698, '这个标志的含义是提醒车辆驾驶人前方有很强的侧向风.', 1, '正确', '错误', , , '注意横风:用以提醒车辆驾驶人小心驾驶.设在经常有很强的侧向风的路段以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/698.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (699, 699, '在狭窄的山路会车,规定不靠山体的一方优先行驶的原因是什么?', 1, '靠山体的一方相对安全', '不靠山体的一方车速较快', '靠山体的一方视野宽阔', 'B和C都正确', '不靠山体的一方是悬崖,更危险,所以先行.反过来靠山体的一方是相对安全的.', , 0);
INSERT INTO ""."examination_question1" VALUES (700, 700, '夜间行车,可选择下列哪个地段超车?', 3, '交叉路口', '窄路窄桥', '路宽车少', '弯道陡坡', '超车前,应选择视线良好、平直宽阔、左右均无障碍且前方路段200米范围内没有来车的道路,在保证安全的前提下方能超车.千万不要盲目超车.几个选项中,当然是选路宽车少的路段了.', , 0);
INSERT INTO ""."examination_question1" VALUES (701, 701, '专用车道规定的专用使用时间之外,其他车辆可以进入专用车道行驶.', 1, '正确', '错误', , , '当然啦,在规定的时间内,给专用车行驶时,咱不可以乱入；规定的时间以为,可以光明正大的行驶哦！', , 0);
INSERT INTO ""."examination_question1" VALUES (702, 702, '超车时,如果无法保证与被超车辆的安全间距,应主动放弃超车.', 1, '正确', '错误', , , '当前方或后方有情况发生时,情况不允许,应放弃超车,等待时机.', , 0);
INSERT INTO ""."examination_question1" VALUES (703, 703, '申请人在考试过程中有贿赂、舞弊行为的,申请人在多少年内不得再次申领机动车驾驶证?', 1, '1年', '2年', '3年', '4年', '根据123号令第六章法律责任的规定:申请人在考试过程中有贿赂、舞弊行为的,取消考试资格,已经通过考试的其他科目成绩无效；申请人在一年内不得再次申领机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (704, 704, '如图所示,驾驶机动车遇到这种情况时,以下做法正确的是什么? ', 1, '应停车察明水情,确认安全后,低速通过', '应停车察明水情,确认安全后,快速通过', '应减速观察水情,然后加速行驶通过', '可随意通行', '图中所示,河水已经漫过路面,驾驶机动车遇到这种情况时,应停车察明水情,确认安全后,低速通过；不可随意通行.', 'http://images.juheapi.com/jztk/c1c2subject1/704.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (705, 705, '如图所示,在这种雾天情况下,通过交叉路口时必须开灯、鸣喇叭,加速通过,以免造成交通拥堵.', 2, '正确', '错误', , , '施主咱聊聊,不要再做no zuo no die(不作不死)的事情了好不好?雾天亲你就别加速了好不好?', 'http://images.juheapi.com/jztk/c1c2subject1/705.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (706, 706, '如图所示,A车具有优先通行权.', 1, '正确', '错误', , , '左转让直行！右转让左转！', 'http://images.juheapi.com/jztk/c1c2subject1/706.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (707, 707, '在后方无来车的情况下,在隧道中倒车应靠边行驶.', 2, '正确', '错误', , , '这题前面的第一句话就是怕你答对迷惑你的,隧道不能调头', , 0);
INSERT INTO ""."examination_question1" VALUES (708, 708, '如图所示,在这种道路上行驶,应在道路中间通行的主要原因是在道路中间通行速度快.', 2, '正确', '错误', , , '实际中,有划分车道的,分道通行.没有划分车道的,在遵循右侧通行前提下,混合通行.行人在最右侧通行,非机动车在靠右侧,机动车在中间偏右的位置通行.因为对向来车也是如此,所以对整条路而言,就是机动车在中间,非机动车在两边.给两侧的非机动车和行人留有充足的通行空间,相对来讲,本身也是最安全的！', 'http://images.juheapi.com/jztk/c1c2subject1/708.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (709, 709, '如图所示,驾驶机动车接打电话容易导致发生交通事故.', 1, '正确', '错误', , , '驾驶机动车接打电话会分散驾驶员的注意力,容易导致发生交通事故.', 'http://images.juheapi.com/jztk/c1c2subject1/709.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (710, 710, '如图所示,在这种情况下,您应该轻踩制动踏板减速.', 1, '正确', '错误', , , '如图所示,前方道路限速60公里,当车速大于或等于限速时需要踩制动踏板减速.', 'http://images.juheapi.com/jztk/c1c2subject1/710.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (711, 711, '车辆在通过山区道路弯道时,要做到“减速、鸣喇叭、靠右行”.', 1, '正确', '错误', , , '比如南方的山区道路,你那怕只走过一次,对这个做法就会深表认同,因为这个真是太重要了.', , 0);
INSERT INTO ""."examination_question1" VALUES (712, 712, '初次申领机动车驾驶证的,可以申请下列哪种准驾车型?', 3, '中型客车', '大型客车', '普通三轮摩托车', '牵引车', '123号令第十三条  初次申领机动车驾驶证的,可以申请准驾车型为城市公交车、大型货车、小型汽车、小型自动挡汽车、低速载货汽车、三轮汽车、残疾人专用小型自动挡载客汽车、普通三轮摩托车、普通二轮摩托车、轻便摩托车、轮式自行机械车、无轨电车、有轨电车的机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (713, 713, '这个仪表是何含义?', 1, '速度和里程表', '发动机转速表', '最高时速值表', '百公里油耗表', '图为时速表,看到那个KM没?那个是公里的意思,也就是里程表的意思.', 'http://images.juheapi.com/jztk/c1c2subject1/713.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (714, 714, '如图所示,在高速公路同方向两条机动车道左侧车道行驶,应保持什么车速?', 2, '110公里/小时～130公里/小时', '100 公里/小时～120公里/小时', '90 公里/小时～110公里/小时', '60 公里/小时～120公里/小时', '高速路同方向有2条车道,左侧车的速度范围为100公里到120公里,右侧车的速度范围为60公里到100公里.', 'http://images.juheapi.com/jztk/c1c2subject1/714.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (715, 715, '道路交通标线分为指示标线、警告标线、禁止标线.', 1, '正确', '错误', , , '没有问题记住吧.', , 0);
INSERT INTO ""."examination_question1" VALUES (716, 716, '同车道行驶的车辆前方遇到下列哪种车辆不得超车?', 2, '大型客货车', '执行任务的消防车', '公共汽车', '出租汽车', '亲,人命关天,您就别超了.', , 0);
INSERT INTO ""."examination_question1" VALUES (717, 717, '夜间行车,需要超车时,变换远近光灯示意是为了提示前车.', 1, '正确', '错误', , , '在确保安全和不违规的情况下都可以超车.变换远近灯光是为了提醒前方车辆的注意,虽然只是一个动作,但对于保证安全来说却是很重要的.', , 0);
INSERT INTO ""."examination_question1" VALUES (718, 718, '这个仪表是何含义?', 4, '压力表', '电流表', '水温表', '燃油表', '汽油不够了,看到那个加油站常见的机器没.', 'http://images.juheapi.com/jztk/c1c2subject1/718.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (719, 719, '牵引故障车时,牵引与被牵引的机动车,在行驶中都要开启危险报警闪光灯.', 1, '正确', '错误', , , '出现故障了,就要开启危险报警闪光灯,以提醒过往车辆及行人的注意,特别是后方行驶的车辆,保持应有的安全距离和必要的安全车速,避免紧急刹引起追尾.', , 0);
INSERT INTO ""."examination_question1" VALUES (720, 720, '这个路面标记是什么标线?', 3, '禁驶区', '网状线', '中心圈', '导流线', '中心圈,设置在交叉路口中心的白色圆形或菱形区域,用以区分车辆大、小转弯,及交叉路口车辆左右转弯的指示,车辆不得压线行驶.机动车向左转弯时,必须紧靠中心圈小转弯.', 'http://images.juheapi.com/jztk/c1c2subject1/720.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (721, 721, '已注册登记的机动车达到国家规定的强制报废标准的,应当向登记地车辆管理所申请注销登记.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法》第二章第十四条规定:应当报废的机动车必须及时办理注销登记.达到报废标准的机动车不得上道路行驶.报废的大型客、货车及其他营运车辆应当在公安机关交通管理部门的监督下解体.', , 0);
INSERT INTO ""."examination_question1" VALUES (722, 722, '安全头枕用于在发生追尾事故时保护驾驶人的头部不受伤害.', 2, '正确', '错误', , , '安全头枕是保护颈椎的.', , 0);
INSERT INTO ""."examination_question1" VALUES (723, 723, '如图所示,在这种情况下遇右侧车辆变更车道,应减速保持间距,注意避让.', 1, '正确', '错误', , , '你懂的,安全文明第一.', 'http://images.juheapi.com/jztk/c1c2subject1/723.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (724, 724, '遇到这种情形怎样行驶?', 1, '停车让对方车辆通过', '开启左转向灯向左行驶', '开前照灯告知对方让行', '加速超越障碍后会车', '人家已经在路上了,你在过去,谁都别走了.', 'http://images.juheapi.com/jztk/c1c2subject1/724.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (725, 725, '路中心的双黄实线作用是分隔对向交通流,在保证安全的前提下,可越线超车或转弯.', 2, '正确', '错误', , , '双黄实线严禁碾压.', 'http://images.juheapi.com/jztk/c1c2subject1/725.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (726, 726, '交通信号灯由红灯、绿灯和黄灯组成.', 1, '正确', '错误', , , '嗯,目前我还没见到过其他颜色的灯.', , 0);
INSERT INTO ""."examination_question1" VALUES (727, 727, '机动车上路行驶时,前排乘车人可不系安全带.', 2, '正确', '错误', , , '系安全带分什么前排后排,都得系.', , 0);
INSERT INTO ""."examination_question1" VALUES (728, 728, '驾驶机动车发生交通事故后当事人故意破坏、伪造现场、毁灭证据的,应当承担什么责任?', 4, '主要责任', '次要责任', '同等责任', '全部责任', '发生交通事故后当事人逃逸的,故意破坏、伪造现场、毁灭证据的,均承担全部责任.', , 0);
INSERT INTO ""."examination_question1" VALUES (729, 729, '机动车仪表板上（如图所示）一直亮,表示发动机控制系统故障.', 1, '正确', '错误', , , '你看它长的多像发动机.', 'http://images.juheapi.com/jztk/c1c2subject1/729.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (730, 730, '年龄在60周岁以上,在一个记分周期结束后一年内未提交身体条件证明的,其机动车驾驶证将会被车辆管理所注销.', 1, '正确', '错误', , , '根据《机动车驾驶证申领和使用规定》第42条第6项:年龄在60周岁以上,在一个记分周期结束后一年内未提交身体条件证明的；或者持有大型客车、牵引车、城市公交车、中型客车、大型货车、无轨电车、有轨电车准驾车型,在两个记分周期结束后一年内未提交身体条件证明的；或者持有残疾人专用小型自动挡载客汽车准驾车型,在三个记分周期结束后一年内未提交身体条件证明的,车辆管理所应当注销其机动车驾驶证.被注销机动车驾驶证未超过两年的,机动车驾驶人考试科目一合格后,可以恢复驾驶资格.', , 0);
INSERT INTO ""."examination_question1" VALUES (731, 731, '如图所示,在高速公路同方向三条机动车道中间车道行驶,车速不能低于多少?', 2, '100公里/小时', '90公里/小时', '110公里/小时', '60公里/小时', '60到90  90到110  110到120', 'http://images.juheapi.com/jztk/c1c2subject1/731.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (732, 732, '这个标志的含义是告示前方道路是单向通行路段.', 2, '正确', '错误', , , '注意障碍物标志右侧绕行:用以告示前方道路有障碍物,车辆应按标志指示减速慢行.设在道路障碍物前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/732.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (733, 733, '夜间在道路上会车时,距离对向来车多远将远光灯改用近光灯?', 2, '不必变换灯光', '150米以外', '100米以内', '50米以内', '夜间会车应当在距相对方向来车150米以外改用近光灯,在窄路、窄桥与非机动车会车时应当使用近光灯.', , 0);
INSERT INTO ""."examination_question1" VALUES (734, 734, '在这种雨天跟车行驶使用灯光,以下做法正确的是?', 3, '使用远光灯', '不能使用近光灯', '不能使用远光灯', '使用雾灯', '开远光会影响前车驾驶员的视线,故不能使用远光灯.', 'http://images.juheapi.com/jztk/c1c2subject1/734.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (735, 735, '安全头枕在发生追尾事故时,能有效保护驾驶人的什么部位?', 4, '腰部', '胸部', '头部', '颈部', '安全头枕是保护颈椎滴.', , 0);
INSERT INTO ""."examination_question1" VALUES (736, 736, '行车中要文明驾驶,礼让行车,做到不开英雄车、冒险车、赌气车和带病车.', 1, '正确', '错误', , , '基本常识,安全第一', , 0);
INSERT INTO ""."examination_question1" VALUES (737, 737, '行车中突遇对方车辆强行超车,占据自己车道,正确的做法是什么?', 3, '挡住其去路', '保持原车速行驶', '尽可能减速避让、直至停车', '加速行驶', '做文明开车人吧,不要抱怨太多,安全才重要,“宁停三分,不抢一秒”嘛！', , 0);
INSERT INTO ""."examination_question1" VALUES (738, 738, '机动车仪表板上（如图所示）这个符号表示什么?', 3, '远光灯开关', '近光灯开关', '车灯总开关', '后雾灯开关', '此为车灯总开关.', 'http://images.juheapi.com/jztk/c1c2subject1/738.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (739, 739, '机动车仪表板上（如图所示）亮表示什么?', 4, '洗涤液不足', '制动液不足', '冷却系故障', '冷却液不足', 'A,B,C都巨不靠谱,只有D最合理.', 'http://images.juheapi.com/jztk/c1c2subject1/739.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (740, 740, '（如图所示）这个符号的开关控制什么装置?', 3, '前风窗玻璃除霜或除雾', '后风窗玻璃刮水器及洗涤器', '前风窗玻璃刮水器及洗涤器', '后风窗玻璃除霜或除雾', '此为前风窗玻璃刮水器及洗涤器.关于如何区分前风窗玻璃刮水器及洗涤器和后风窗玻璃刮水器及洗涤器,你只需记住一点半弧形的图标是前风窗,四四方方的图标就是后风窗.', 'http://images.juheapi.com/jztk/c1c2subject1/740.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (741, 741, '机动车仪表板上（如图所示）亮表示什么?', 1, '地板及迎面出风', '空气内循环', '空气外循环', '侧面及地板出风', '亲,这个真的选A.', 'http://images.juheapi.com/jztk/c1c2subject1/741.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (742, 742, '机动车仪表板上（如图所示）亮表示什么?', 3, '发动机舱开启', '燃油箱盖开启', '两侧车门开启', '行李舱开启', '这个毫无疑问俩边门都开了.', 'http://images.juheapi.com/jztk/c1c2subject1/742.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (743, 743, '路中心黄色虚线的含义是分隔对向交通流,在保证安全的前提下,可越线超车或转弯.', 1, '正确', '错误', , , '黄色虚线是中心线,可以跨越；黄色单实线就已经不能跨越了；黄色双实线是严格禁止压线或者跨越', 'http://images.juheapi.com/jztk/c1c2subject1/743.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (744, 744, '驾驶人要按照驾驶证载明的准驾车型驾驶车辆.', 1, '正确', '错误', , , '常识题,你c1的本肯定不能去开大货和大客.', , 0);
INSERT INTO ""."examination_question1" VALUES (745, 745, '在什么情况下不得行车?', 2, '车窗没关闭', '车门没关闭', '音响没关闭', '顶窗没关闭', '肯定是门没关好.', , 0);
INSERT INTO ""."examination_question1" VALUES (746, 746, '机动车仪表板上（如图所示）亮表示什么?', 4, '充电电流过大', '蓄电池损坏', '电流表故障', '充电电路故障', '此为充电电路故障,一般没有故障这些都不会亮的,只有出故障了才会亮.', 'http://images.juheapi.com/jztk/c1c2subject1/746.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (747, 747, '进入这个路口应该怎样做?', 3, '交替变换远近光灯提醒路口内车辆让行', '从路口内车辆前迅速插入', '让已在路口内的车辆先行', '鸣喇叭直接进入路口', '和谐第一.', 'http://images.juheapi.com/jztk/c1c2subject1/747.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (748, 748, '这个开关控制机动车哪个部位?', 2, '风窗玻璃除雾器', '风窗玻璃刮水器', '危险报警闪光灯', '照明、信号装置', '这个控制风窗玻璃刮水器的,木问题.风窗玻璃除雾器和危险报警闪光灯都是按钮形式的,照明、信号装置在你的左手边.', 'http://images.juheapi.com/jztk/c1c2subject1/748.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (749, 749, '如图所示,驾驶机动车行驶至桥梁涵洞时,以下做法正确的是什么?', 2, '加速,在对向车到达前通过', '减速靠右通过', '保持原速继续正常行驶', '鸣喇叭后加速通过', '驾驶机动车行驶至桥梁涵洞时,做法正确的是:减速靠右通过.', 'http://images.juheapi.com/jztk/c1c2subject1/749.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (750, 750, '在这种天气条件下行车如何使用灯光?', 1, '使用近光灯', '不使用灯光', '使用远光灯', '使用雾灯', '一般雪天能见度低,应该注意灯光的正确使用.一是开启近光灯,帮助驾驶员?望前方情况,使驾驶员对前方情况看得清楚一些.二是开启示廓灯和前后位灯,示意自己车辆的存在,使前后行驶的车辆,能够看到自己的具体位置及通行情况,方便他们采取相应的措施.', 'http://images.juheapi.com/jztk/c1c2subject1/750.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (751, 751, '如图所示,驾驶机动车遇前方车流行驶缓慢时,借用公交专用道超车是正确的.', 2, '正确', '错误', , , '机动车不得借用公交专用道行驶或超车.', 'http://images.juheapi.com/jztk/c1c2subject1/751.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (752, 752, '点火开关在LOCK位置拔出钥匙转向盘会锁住.', 1, '正确', '错误', , , 'LOCK的中文意思就是锁,一般从这里拔出钥匙方向盘也会锁死.', 'http://images.juheapi.com/jztk/c1c2subject1/752.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (753, 753, '在这个路口怎样左转弯?', 2, '靠路口中心点右侧转弯', '靠路口中心点左侧转弯', '骑路口中心点转弯', '不能左转弯', '这个是规定,左转要靠路口中心点左侧转弯.', 'http://images.juheapi.com/jztk/c1c2subject1/753.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (754, 754, '驾驶机动车在道路上发生交通事故,当事人不能自行移动车辆的,应当保护现场并立即报警.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法》中第七十条第一款规定,在道路上发生交通事故,车辆驾驶人应当立即停车,保护现场.也就是说,司机发生交通事故,不论损失大小,首先必须要停车,保护现场.然后根据损失的大小确定下一步应当做什么.', , 0);
INSERT INTO ""."examination_question1" VALUES (755, 755, '在高速公路上驾驶机动车,车辆发生故障后的处置方法,以下说法错误的是什么?', 3, '打开危险报警闪光灯,夜间还应开启示廓灯、后位灯', '在车后150米以外设置安全警告标志', '车内乘员应下车辅助将故障车辆推移到紧急停车带上', '所有人员需离开故障车辆,在紧急停车带或护栏以外安全位置报警并等候救援', '人员安全第一,人员停留在高速公路上推车容易造成危险.', , 0);
INSERT INTO ""."examination_question1" VALUES (756, 756, '夜间驾驶机动车在没有中心隔离设施或者没有中心线的道路上行驶,以下哪种情况下应当改用近光灯?', 2, '接近没有交通信号灯控制的交叉路口时', '与对向机动车会车时', '接近人行横道时', '城市道路照明条件不良时', '记住,只有在没有路灯或者没有照明的情况下才能用远光灯,一般都用近光灯.窄路窄桥,你若用远光灯,会让对方完全看不清,掉到桥下去的.夜间驾驶机动车遇到对向来车未关闭远光灯时,变换使用远近光灯提示.', , 0);
INSERT INTO ""."examination_question1" VALUES (757, 757, '这个标志的含义是警告车辆驾驶人前方是学校区域.', 2, '正确', '错误', , , '注意儿童:用以警告车辆驾驶人减速慢行,注意儿童.设在小学、幼儿园、少年宫等儿童经常出入地点前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/757.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (758, 758, '年龄在60周岁以上的驾驶人多长时间提交一次身体条件证明?', 4, '每3年', '每2年', '每6个月', '每1年', '记住吧', , 0);
INSERT INTO ""."examination_question1" VALUES (759, 759, '驾驶这种机动车上路行驶属于什么行为?', 3, '违章行为', '违规行为', '违法行为', '犯罪行为', '现在不按规定行车的都是违法行为.已经没有违章行为和违规行为一说了.', 'http://images.juheapi.com/jztk/c1c2subject1/759.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (760, 760, '持有大型客车、牵引车、城市公交车、中型客车、大型货车驾驶证的驾驶人从业单位等信息发生变化的,应当在信息变更后三十日内,向驾驶证核发地车辆管理所备案.', 1, '正确', '错误', , , '公安部123号令规定:“机动车驾驶人联系电话、联系地址等信息发生变化,以及持有大型客车、牵引车、城市公交车、中型客车、大型货车准驾车型驾驶证的驾驶人从业单位等信息发生变化,应在信息变更后三十日内,向驾驶证核发地车辆管理所备案”.', , 0);
INSERT INTO ""."examination_question1" VALUES (761, 761, '驾驶人的驾驶证损毁后不得驾驶机动车.', 1, '正确', '错误', , , '请保证驾驶证的整洁,不然警察叔叔都不能确定是不是你了,给你算无证驾驶了', , 0);
INSERT INTO ""."examination_question1" VALUES (762, 762, '持小型汽车驾驶证的驾驶人在下列哪种情况下需要接受审验?', 2, '一个记分周期末', '有效期满换发驾驶证时', '记分周期未满分', '记分周期满12分', '换发驾驶证的时候才接受审验,积分周期满12分的接受理论一科目再教育.', , 0);
INSERT INTO ""."examination_question1" VALUES (763, 763, '驾驶机动车应在变更车道的同时开启转向灯.', 2, '正确', '错误', , , '机动车超车时,应当提前开启左转向灯、变换使用远、近光灯或者鸣喇叭.', , 0);
INSERT INTO ""."examination_question1" VALUES (764, 764, '驾驶机动车发生故障或事故不能正常行驶时,应立即打开危险报警闪光灯.', 1, '正确', '错误', , , '第一时间打开危险报警闪光灯,警示其他车辆,是正确的.', , 0);
INSERT INTO ""."examination_question1" VALUES (765, 765, '酒后驾驶发生重大交通事故被依法追究刑事责任的人不能申请机动车驾驶证.', 1, '正确', '错误', , , '“马路杀手”先生,你这样的人真的不配再拿驾驶证了.', , 0);
INSERT INTO ""."examination_question1" VALUES (766, 766, '机动车登记证书丢失后应及时补办,避免被不法分子利用.', 1, '正确', '错误', , , '机动车登记证书丢失后应尽快补办,是正确的.', , 0);
INSERT INTO ""."examination_question1" VALUES (767, 767, '在车门、车厢没有关好时不要驾驶机动车起步.', 1, '正确', '错误', , , '安全第一.', , 0);
INSERT INTO ""."examination_question1" VALUES (768, 768, '驾驶人连续驾驶不得超过多长时间?', 4, '6小时', '8小时', '10小时', '4小时', '规定的4小时以上是疲劳驾驶.', , 0);
INSERT INTO ""."examination_question1" VALUES (769, 769, '如图所示,在高速公路同方向三条机动车道右侧车道行驶,车速不能低于多少?', 4, '80公里/小时', '100公里/小时', '110公里/小时', '60 公里/小时', '《中华人民共和国道路交通安全法实施条例》第七十八条:高速公路应当标明车道的行驶速度,最高车速不得超过每小时120公里,最低车速不得低于每小时60公里.', 'http://images.juheapi.com/jztk/c1c2subject1/769.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (770, 770, '机动车在道路上变更车道需要注意什么?', 3, '开启转向灯迅速向左转向', '进入左侧车道时适当减速', '不能影响其他车辆正常行驶', '尽快加速进入左侧车道', '只有C最和谐.', , 0);
INSERT INTO ""."examination_question1" VALUES (771, 771, '点火开关在START位置起动机起动.', 1, '正确', '错误', , , 'START是出发的意思,所以他就是点火的开关.', 'http://images.juheapi.com/jztk/c1c2subject1/771.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (772, 772, '违反交通信号灯通行的一次记6分.', 1, '正确', '错误', , , '新修订的《机动车驾驶证申领和使用规定》出台将违反道路交通信号灯通行等违法记分由3分提高到6分.', , 0);
INSERT INTO ""."examination_question1" VALUES (773, 773, '行至漫水路时,应当怎样做?', 2, '空挡滑行', '低速通过涉水路段', '高速通过,减少涉水时间', '高挡位低速通过', '《中华人民共和国道路交通安全法实施条例》第六十四条:机动车行经漫水路或者漫水桥时,应当停车察明水情,确认安全后,低速通过.', , 0);
INSERT INTO ""."examination_question1" VALUES (774, 774, '如图所示,通过有这个标志的路口时应该减速让行.', 2, '正确', '错误', , , '这个是停车让行的标志,光减速是不够的.另外提示一下,三角的是减速让行,八角的是停车让行.', 'http://images.juheapi.com/jztk/c1c2subject1/774.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (775, 775, '机动车仪表板上（如图所示）亮,提示发电机向蓄电池充电.', 2, '正确', '错误', , , '提示发电机不向蓄电池充电了,充电系统有故障了！', 'http://images.juheapi.com/jztk/c1c2subject1/775.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (776, 776, '驾驶机动车向右变更车道前应仔细观察右侧车道车流情况的原因是什么?', 2, '准备抢行', '判断有无变更车道的条件', '迅速变更车道', '准备迅速停车', '观察当然是为了判断有无变更车道的条件了.', , 0);
INSERT INTO ""."examination_question1" VALUES (777, 777, '如图所示,以下哪种情况可以超车?(图51a.51b. 51c. 51d)', 1, '51a', '51b', '51c', '51d', '如图所示,能够超车的必须满足两个条件:一是必须在虚线的一侧,而是对向没有来车.满足这个两个条件的就只有51a了.', 'http://images.juheapi.com/jztk/c1c2subject1/777.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (778, 778, '如图所示,机动车遇行人正在通过人行横道时,要停车让行,是因为行人享有优先通行权.', 1, '正确', '错误', , , '人行横道上,行人享有优先通过权,这叫强龙不压地头蛇！', 'http://images.juheapi.com/jztk/c1c2subject1/778.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (779, 779, '年龄在50周岁以上的机动车驾驶人,应当每年进行一次身体检查,并向公安机关交通管理部门申报身体条件情况.', 2, '正确', '错误', , , '根据《机动车驾驶证申领使用规定》第四十八条规定年龄在60周岁以上或者持有大型客车、牵引车、城市公交车、中型客车、大型货车、无轨电车、有轨电车准驾车型的机动车驾驶人,应当每年进行一次身体检查,在记分周期结束后十五日内,提交县级或者部队团级以上医疗机构出具的有关身体条件的证明.', , 0);
INSERT INTO ""."examination_question1" VALUES (780, 780, '驾驶机动车发生交通事故未造成人身伤亡的,责任明确双方无争议时,应当如何处置?', 4, '保护好现场再协商', '不要移动车辆', '疏导其他车辆绕行', '撤离现场自行协商', '《中华人民共和国道路交通安全法》第七十条明确规定在道路上发生交通事故,未造成人身伤亡,当事人对事实及成因无争议的,可以即行撤离现场,恢复交通,自行协商处理损害赔偿事宜；不即行撤离现场的,应当迅速报告执勤的交通警察或者公安机关交通管理部门.', , 0);
INSERT INTO ""."examination_question1" VALUES (781, 781, '行驶过程中发现车门未关好,应及时关闭车门,否则车辆在转弯等激烈运动过程中会造成人员或货物被甩到车外.', 1, '正确', '错误', , , '行车过程中,车门要关紧,否则会增加行车风险.', , 0);
INSERT INTO ""."examination_question1" VALUES (782, 782, '驾驶机动车超车时,可以鸣喇叭替代开启转向灯.', 2, '正确', '错误', , , '机动车超车时,应当提前开启左转向灯、变换使用远、近光灯.', , 0);
INSERT INTO ""."examination_question1" VALUES (783, 783, '交通肇事致一人以上重伤,负事故全部或者主要责任,并具有下列哪种行为的,构成交通肇事罪?', 3, '未带驾驶证', '未报警', '无驾驶资格驾驶机动车辆的', '未抢救受伤人员', '可能构成交通壁事罪.例如,交通壁事致一人受伤(属于一般事故),负事故全部或者主要责任,并具有酒后、无证驾驶、严重超载等情形之一的,以交通肇事罪处罚.', , 0);
INSERT INTO ""."examination_question1" VALUES (784, 784, '这个导向箭头是何含义?', 4, '指示向左变道', '指示前方直行', '指示前方掉头', '指示前方右转弯', '请记住了哦', 'http://images.juheapi.com/jztk/c1c2subject1/784.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (785, 785, '在暂住地初次申领机动车驾驶证的,不能直接申领大型货车驾驶证.', 1, '正确', '错误', , , '在暂住地初次申领机动车驾驶证的,可以申请准驾车型为小型汽车、小型自动挡汽车、低速载货汽车、三轮汽车、残疾人专用小型自动挡载客汽车、普通三轮摩托车、普通二轮摩托车、轻便摩托车的机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (786, 786, '驾驶人户籍迁出原车辆管理所需要向什么地方的车辆管理所提出申请?', 4, '迁出地', '居住地', '所在地', '迁入地', '《公安部令第123号》第四十九条:机动车驾驶人户籍迁出原车辆管理所管辖区的,应当向迁入地车辆管理所申请换证.', , 0);
INSERT INTO ""."examination_question1" VALUES (787, 787, '道路上划设这种标线的车道内允许下列哪类车辆通行?', 1, '公交车', '私家车', '出租车', '公务用车', '看见上边的大字没,公交专用车道,你在不选A,你选谁.', 'http://images.juheapi.com/jztk/c1c2subject1/787.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (788, 788, '驾驶机动车通过未设置交通信号灯的交叉路口时,下列说法错误的是什么?', 4, '转弯的机动车让直行的车辆、行人先行', '没有交通标志、标线控制时,在进入路口前停?望,让右方道路的来车先行', '相对方向行驶的右转弯机动车让左转弯的车辆先行', '相对方向行驶的左转弯机动车让右转弯的车辆先行', '驾驶机动车通过未设置交通信号灯的交叉路口时,转弯的机动车应当让直行的车辆、行人先行；没有交通标志、标线控制时,在进入路口前停车?望,让右方道路的来车先行；相对方向行驶的右转弯机动车让左转弯的车辆先行.', , 0);
INSERT INTO ""."examination_question1" VALUES (789, 789, '如图所示,在这起交通事故中,以下说法正确的是什么? ', 1, 'A车负全部责任', 'B车负全部责任', '各负一半的责任', 'B车负主要责任', '这起交通事故主要是因为A车绕开障碍物时未提前观察减速为B车让行造成的,所以A车全责.', 'http://images.juheapi.com/jztk/c1c2subject1/789.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (790, 790, '如图所示,驾驶机动车遇到这种情况,可以轻按喇叭提醒前方非机动车和行人后方有来车.', 1, '正确', '错误', , , '前方有行人和自行车,道路较窄,驾驶机动车遇到这种情况,可以轻按喇叭提醒前方非机动车和行人后方有来车,这样更加安全.', 'http://images.juheapi.com/jztk/c1c2subject1/790.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (791, 791, '前方路口这种信号灯亮表示什么意思?', 3, '加速左转', '禁止右转', '路口警示', '加速直行', '黄灯为警示灯,所以是路口警示.', 'http://images.juheapi.com/jztk/c1c2subject1/791.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (792, 792, '机动车驾驶证被依法扣押、扣留、暂扣期间能否申请补发?', 4, '可以申请', '扣留期间可以临时申请', '暂扣期间可以临时申请', '不得申请补发', '机动车驾驶证被依法扣押、扣留或者暂扣期间,机动车驾驶人不得申请补发.', , 0);
INSERT INTO ""."examination_question1" VALUES (793, 793, '这个标志的含义是提醒车辆驾驶人前方是桥头跳车较严重的路段.', 2, '正确', '错误', , , '驼峰桥:用以提醒车辆驾驶人谨慎驾驶.设在拱度很大,影响视距的驼峰桥以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/793.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (794, 794, '驾驶机动车行驶过程中,遇道路养护车辆从本车道逆向驶来时,以下做法正确的是什么?', 1, '靠边减速或停车让行', '在原车道继续行驶', '占用非机动车道行驶', '鸣喇叭示意其让道', '道路养护车辆有优先权,一定要让行,且要注意A中有“靠边”二字.', , 0);
INSERT INTO ""."examination_question1" VALUES (795, 795, '如图所示,在环岛交叉路口发生的交通事故中,应由A车负全部责任.', 1, '正确', '错误', , , '环岛行驶车辆应让行出岛车辆,因此A车应让B车先行,而此时事故发生,A车应负全责.', 'http://images.juheapi.com/jztk/c1c2subject1/795.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (796, 796, '申请小型汽车汽车驾驶证的,年龄应在18周岁以上70周岁以下.', 1, '正确', '错误', , , '《公安部令第123号》第十一条:1、申请小型汽车等准驾车型在18周岁以上、70周岁以下；2、申请低速载货汽车、三轮汽车等准驾车型的,在18周岁以上,60周岁以下.此题正确.本题车管所已经修正.', , 0);
INSERT INTO ""."examination_question1" VALUES (797, 797, '上道路行驶的机动车驾驶人未携带机动车驾驶证、行驶证的,除扣留机动车外,并受到什么处罚?', 2, '警告', '罚款', '拘留', '吊销驾驶证', '《道路交通安全违法行为处理程序规定》第十三条“有下列情形之一的,因无其他机动车驾驶人代替驾驶、违法行为尚未消除、需要调查或者证据保全等原因不能立即放行的,可以扣留车辆:（一）未悬挂机动车号牌,未放置检验合格标志、保险标志,或者未携带行驶证、机动车驾驶证的.', , 0);
INSERT INTO ""."examination_question1" VALUES (798, 798, '车辆进入山区道路后,要特别注意“连续转弯”标志,并主动避让车辆及行人,适时减速和提前鸣喇叭.', 1, '正确', '错误', , , '山区本来就是有山的地势,自然就会有坡度,以及拐弯,连续的弯路,容易影响行车的视线,所以要注意“连续转弯”的标志,以防在转弯处突然出现对向来车,为了安全,当然要适时减速和提前鸣喇叭,以提醒车辆与行人.', , 0);
INSERT INTO ""."examination_question1" VALUES (799, 799, '仪表显示当前车速是20公里/小时.', 2, '正确', '错误', , , '这个是转速表,不是时速.', 'http://images.juheapi.com/jztk/c1c2subject1/799.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (800, 800, '如图所示,A车货物掉落,导致B车与掉落货物发生碰撞,以下说法正确的是什么?', 2, 'B车自负责任', 'A车负全部责任', '各负一半责任', '偶然事件,不可避免', '货车散落货物,导致后面的车辆撞上,货车全责.', 'http://images.juheapi.com/jztk/c1c2subject1/800.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (801, 801, '如图所示,铁路道口禁止掉头的原因是什么?', 4, '有铁路道口标志', '有铁路道口信号灯', '铁路道口车流量大', '容易引发事故', '铁道路口行车条件比较复杂,掉头容易引发事故.', 'http://images.juheapi.com/jztk/c1c2subject1/801.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (802, 802, '为什么规定辅路车让主路车先行?', 1, '主路车流量大、速度快', '辅路车便于观察', '主路车流量小、速度快', '辅路车速度快', '正因为车流最大才成为主路的吧,所以别犹豫,选【主路车流最大、速度快】.', , 0);
INSERT INTO ""."examination_question1" VALUES (803, 803, '发动机起动后仪表板上（如图所示）亮表示什么?', 2, '发动机主油道堵塞', '发动机机油压力过低', '发动机曲轴箱漏气', '发动机机油压力过高', '像茶壶一样的就是机油的标志,各位记住就是啦. 该指示灯用来显示发动机内机油的压力状况.打开钥匙门,车辆开始自检时,指示灯点亮,启动后熄灭.该指示灯常亮,说明该车发动机机油压力低于规定标准,需要维修.', 'http://images.juheapi.com/jztk/c1c2subject1/803.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (804, 804, '如图所示,驾驶机动车遇到这种情况,以下做法正确的是什么?', 1, '减速慢行、鸣喇叭示意', '为拓宽视野,临时占用左侧车道行驶', '加速行驶', '停车观察', '驾驶机动车遇到这种情况,应该:减速慢行、鸣喇叭示意.', 'http://images.juheapi.com/jztk/c1c2subject1/804.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (805, 805, '驾驶机动车变更车道时,以下做法正确的是什么?', 2, '开启转向灯的同时变更车道', '在道路同方向划有2条以上机动车道的,不得影响相关车道内行驶的机动车的正常行驶', '在车辆较少路段,可以随意变更车道', '遇前方道路拥堵,可以向应急车道变更', '驾驶机动车变更车道时,在道路同方向划有2条以上机动车道的,不得影响相关车道内行驶的机动车的正常行驶.', , 0);
INSERT INTO ""."examination_question1" VALUES (806, 806, '驾驶机动车下长坡时,利用惯性滑行可以减少燃油消耗,值得提倡.', 2, '正确', '错误', , , '发动机制动是利用发动机的牵阻作用减慢车速,档位越低牵阻越明显,制动性越强.在下长坡道路行驶,挂入低速档利用发动机的牵阻作用可以减少制动器的负担和减少制动次数,防止制动过热引起制动力热衰减；在冰雪、泥泞的路面上行驶,应用发动机牵阻制动可以防止侧滑.', , 0);
INSERT INTO ""."examination_question1" VALUES (807, 807, '这个标志的含义是提醒车辆驾驶人前方是过水路面或漫水桥路段.', 1, '正确', '错误', , , '过水路面:用以提醒车辆驾驶人谨慎慢行.设在过水路面或漫水桥路段以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/807.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (808, 808, '因避让特种车辆而发生违法行为,被电子警察拍到时,可向交管部门复议.', 1, '正确', '错误', , , '避让特种车辆而发生违法行为可向交管部门复议,不用担心做好事还受罚了.', , 0);
INSERT INTO ""."examination_question1" VALUES (809, 809, '驾驶机动车上路行驶应当按规定悬挂号牌.', 1, '正确', '错误', , , '常识题.', , 0);
INSERT INTO ""."examination_question1" VALUES (810, 810, '已经达到报废标准的机动车经大修后可以上路行驶.', 2, '正确', '错误', , , '已经达到报废标准的机动车再怎么修也不能上路.最新出台的机动车报废标准,不限年限,历程到达60万公里就得报废.', , 0);
INSERT INTO ""."examination_question1" VALUES (811, 811, '打开左转向灯开关,（如图所示）亮起.', 2, '正确', '错误', , , '亲,这个是右转向灯.', 'http://images.juheapi.com/jztk/c1c2subject1/811.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (812, 812, '发生交通事故后,当事人故意破坏、伪造现场、毁灭证据的,承担全部责任.', 1, '正确', '错误', , , '发生交通事故后当事人逃逸的,故意破坏、伪造现场、毁灭证据的,均承担全部责任.', , 0);
INSERT INTO ""."examination_question1" VALUES (813, 813, '驾驶机动车在没有道路中心线的道路上行驶,应该在道路的左侧通行', 2, '正确', '错误', , , '《交通安全法》第三十六条:根据道路条件和通行需要,道路划分为机动车道、非机动车道和人行道的,机动车、非机动车、行人实行分道通行.没有划分机动车道、非机动车道和人行道的,机动车在道路中间通行,非机动车和行人在道路两侧通行.', , 0);
INSERT INTO ""."examination_question1" VALUES (814, 814, '发动机号码、车辆识别代号因磨损、锈蚀、事故等原因辨认不清或者损坏的,可以向登记地车辆管理所申请备案.', 1, '正确', '错误', , , '《机动车登记规定》:发动机号码、车辆识别代号因磨损、锈蚀、事故等原因辨认不清或者损坏的,可以向登记地车辆管理所申请备案.', , 0);
INSERT INTO ""."examination_question1" VALUES (815, 815, '机动车仪表板上（如图所示）亮,表示驻车制动器操纵杆可能没松到底.', 2, '正确', '错误', , , '此图表示行车制动系统可能出现故障.', 'http://images.juheapi.com/jztk/c1c2subject1/815.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (816, 816, '车辆在山区上坡路驾驶,减挡要及时、准确、迅速,避免拖挡行驶导致发动机动力不足.', 1, '正确', '错误', , , '不换档你的变速箱都会坏掉.货车上坡是要减挡的兄弟,最好是用低速档上坡.', , 0);
INSERT INTO ""."examination_question1" VALUES (817, 817, '机动车仪表板上（如图所示）亮表示什么?', 1, '没有系好安全带', '安全带出现故障', '安全带系得过松', '已经系好安全带', '此为没有系好安全带.', 'http://images.juheapi.com/jztk/c1c2subject1/817.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (818, 818, '年龄在60周岁以上的机动车驾驶人,应当每年进行一次身体检查的目的是什么?', 4, '体现对老年人的关心', '例行程序仅供参考', '检查是否患有老年常见病', '检查是否患有妨碍安全驾驶的疾病', '根据《机动车驾驶证申领使用规定》第四十八条规定年龄在60周岁以上或者持有大型客车、牵引车、城市公交车、中型客车、大型货车、无轨电车、有轨电车准驾车型的机动车驾驶人,应当每年进行一次身体检查,在记分周期结束后十五日内,提交县级或者部队团级以上医疗机构出具的有关身体条件的证明,检查是否患有妨碍安全驾驶的疾病.', , 0);
INSERT INTO ""."examination_question1" VALUES (819, 819, '这个标志的含义是提醒车辆驾驶人前方路面颠簸或有桥头跳车现象.', 1, '正确', '错误', , , '路面低洼:用以提醒车辆驾驶人减速慢行.设在路面突然低洼以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/819.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (820, 820, '图中圈内的路面标记是什么标线?', 1, '人行横道线', '减速让行线', '停车让行线', '路口示意线', '人行横道 小孩子都知道的', 'http://images.juheapi.com/jztk/c1c2subject1/820.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (821, 821, '仪表显示油箱内存油量已在警告线以内.', 1, '正确', '错误', , , '看到加油站那个机油的机器没,而且指针都到红色区了,说明油量不足了.', 'http://images.juheapi.com/jztk/c1c2subject1/821.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (822, 822, '遇到这种情况可以优先通行.', 1, '正确', '错误', , , '你要不过去大家都堵了.', 'http://images.juheapi.com/jztk/c1c2subject1/822.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (823, 823, '驾驶机动车在高速公路发生故障,需要停车排除故障时,以下做法先后顺序正确的是? ①放置警告标志,转移乘车人员至安全处,迅速报警②开启危险报警闪光灯③将车辆移至不妨碍交通的位置④等待救援', 4, '④③①②', '①②③④', '③②①④', '②③①④', '驾驶机动车在高速公路发生故障,需要停车排除故障时,应当进行如下顺序的操作:开启危险报警闪光灯；将车辆移至不妨碍交通的位置；放置警告标志,转移乘车人员至安全处,迅速报警；等待救援.', , 0);
INSERT INTO ""."examination_question1" VALUES (824, 824, '行车中仪表板上（如图所示）亮表示什么?', 1, '发动机温度过高', '发动机冷却系故障', '发动机润滑系故障', '发动机温度过低', '发动机冷却液可能不足,所以导致发动机温度过高.', 'http://images.juheapi.com/jztk/c1c2subject1/824.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (825, 825, '如图所示,在高速公路最左侧车道行驶,想驶离高速公路,以下说法正确的是什么?', 3, '立即减速后右变更车道', '找准机会一次变更到最右侧车道', '每次变更一条车道,直到最右侧车道', '为了快速变更车道,可以加速超越右侧车辆后变更车道', '变更车道,每次只能变更一条车道,变更之后需行驶一段距离,再变道.不可以连续变更车道.', 'http://images.juheapi.com/jztk/c1c2subject1/825.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (826, 826, '驾驶机动车下陡坡、转弯、掉头时,最高速度不能超过多少?', 3, '50公里/小时', '60公里/小时', '30公里/小时', '40公里/小时', '规定:机动车下陡坡、转弯、掉头时,最高速度不能超过30公里/小时', , 0);
INSERT INTO ""."examination_question1" VALUES (827, 827, '驾驶机动车行经漫水路或者漫水桥时,应当停车察明水情.', 1, '正确', '错误', , , '机动车行经漫水路或者漫水桥时,应当停车察明水情,确认安全后,低速通过.', , 0);
INSERT INTO ""."examination_question1" VALUES (828, 828, '关于机动车灯光的使用,以下说法正确的是什么?', 2, '机动车灯光的作用仅仅是为了在夜间照明', '机动车灯光一个重要的作用是提示其他机动车驾驶人和行人', '夜间驾驶机动车在照明条件良好的路段可以不使用灯光', '夜间驾驶机动车在照明条件良好的路段必须使用远光灯', '车辆的灯光不仅仅是在光线不足的情况下提供照明,而更重要的是起到警示作用,即使在白天,人们也会首先注意到亮着灯的车辆,不管是行人还是其他车辆的司机.', , 0);
INSERT INTO ""."examination_question1" VALUES (829, 829, '打开右转向灯开关,（如图所示）亮起.', 2, '正确', '错误', , , '亲,这个是左转向灯的.', 'http://images.juheapi.com/jztk/c1c2subject1/829.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (830, 830, '在超越前车时,提前开启左转向灯,变换使用远、近光灯或者鸣喇叭是为了什么?', 3, '仅提醒前车', '仅提醒后车', '提醒后车以及前车', '提醒行人', '超车时使用变换远、近光灯是提醒前车和后车的意思.', , 0);
INSERT INTO ""."examination_question1" VALUES (831, 831, '机动车仪表板上（如图所示）亮时,提醒驾驶人安全带插头未插入锁扣.', 1, '正确', '错误', , , '安全带很明显,亲,开车一定要系安全带,而且不系安全带是要扣分的.', 'http://images.juheapi.com/jztk/c1c2subject1/831.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (832, 832, '如图所示,在这种情况下通过路口,交替使用远近光灯的目的是什么?', 1, '提示其他交通参与者注意来车', '检查灯光是否能正常使用', '准备变更车道', '超车前提示前车', '大晚上的,又是十字路口,路上不仅仅有车说不定还有人,所以要选A,提醒大家注意安全！', 'http://images.juheapi.com/jztk/c1c2subject1/832.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (833, 833, '驾驶机动车下陡坡时不得有哪些危险行为?', 1, '空挡滑行', '低挡行驶', '制动减速', '提前减挡', '空挡滑行只能用刹车系统减速,如果不用发动机制动控制车速会让刹车片过热使得刹车失灵,所以是危险行为.', , 0);
INSERT INTO ""."examination_question1" VALUES (834, 834, '驾驶机动车下长坡时,仅靠行车制动器制动,容易引起行车制动器失灵.', 1, '正确', '错误', , , '汽车制动分为三种:1行车制动,就是脚下的制动踏板,控制四轮；2驻车制动,就是手刹车,停车时防止前滑后溜；3发动机制动,也就是抢档,用于刹车失灵,在行车制动失灵时用,一级一级减档,从高到底,利用发动机变速箱的原理使车速降下来.下长坡时,驾驶员长时间使用行车制动器,易造成制动器热衰退,汽车的制动效能变差.', , 0);
INSERT INTO ""."examination_question1" VALUES (835, 835, '驾驶机动车造成重大交通事故后逃逸,构成犯罪的,由公安机关交通管理部门吊销机动车驾驶证,且终生不得重新取得机动车驾驶证.', 1, '正确', '错误', , , '《道路交通安全法》第一百零一条规定,造成交通事故后逃逸的,由公安机关交通管理部门吊销机动车驾驶证,且终身不得重新取得机动车驾驶证.上述规定泛指各类交通事故,只要是造成交通事故后逃逸的,就应当由公安机关交通管理部门吊销其机动车驾驶证,并且终身不得重新取得驾驶证,而不论其造成交通事故后果的大小.也就是说,法律规定惩罚的交通肇事逃逸行为,与驾驶人造成交通事故后果的轻重无关；构成犯罪的,依法追究刑事责任.', , 0);
INSERT INTO ""."examination_question1" VALUES (836, 836, '这个标志是何含义?', 1, '向左转弯', '禁止直行', '直行车道', '单行路', '向左转弯:表示只准一切车辆向左转弯.此标志设在车辆必须向左转弯的路口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/836.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (837, 837, '这个标志的含义是警告车辆驾驶人注意前方设有信号灯.', 1, '正确', '错误', , , '注意信号灯:用以警告车辆驾驶人注意前方路段设有信号灯,应依信号灯指示行车.', 'http://images.juheapi.com/jztk/c1c2subject1/837.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (838, 838, '驾驶人在下列哪种情况下不能驾驶机动车?', 3, '喝咖啡后', '喝牛奶后', '饮酒后', '喝茶后', '很明显,喝酒后不能开车.', , 0);
INSERT INTO ""."examination_question1" VALUES (839, 839, '驾驶人有下列哪种违法行为一次记6分?', 3, '使用其他车辆行驶证', '车速超过规定时速50%以上', '违法占用应急车道行驶', '饮酒后驾驶机动车', 'A,B,D都是一次记12分.C选择是一次记6分.', , 0);
INSERT INTO ""."examination_question1" VALUES (840, 840, '这个标志是何含义?', 2, '注意长时鸣喇叭', '无人看守铁路道口', '有人看守铁路道口', '多股铁路与道路相交', '有火车形状的,为无人看守的铁路口,还有一个栅栏形式的图片为有人看守的铁路口.', 'http://images.juheapi.com/jztk/c1c2subject1/840.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (841, 841, '机动车仪表板上（如图所示）亮表示发动机可能机油量不足.', 1, '正确', '错误', , , '没有问题.', 'http://images.juheapi.com/jztk/c1c2subject1/841.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (842, 842, '不得驾驶具有安全隐患的机动车上道路行驶.', 1, '正确', '错误', , , '常识题.', , 0);
INSERT INTO ""."examination_question1" VALUES (843, 843, '服用国家管制的精神药品可以短途驾驶机动车.', 2, '正确', '错误', , , '涉嫌服用国家管制的精神药品、麻醉药品后不得驾驶车辆.', , 0);
INSERT INTO ""."examination_question1" VALUES (844, 844, '已注册登记的机动车,机动车所有人住所在车辆管理所管辖区域内迁移或者机动车所有人姓名（单位名称）、联系方式变更的,应当向登记地车辆管理所备案.', 1, '正确', '错误', , , '修订后的《机动车登记规定》已经2008年4月21日公安部部长办公会议通过,自2008年10月1日起施行.第十七条规定:已注册登记的机动车,机动车所有人住所在车辆管理所管辖区域内迁移或者机动车所有人姓名(单位名称)、联系方式变更的,应当向登记地车辆管理所备案.', , 0);
INSERT INTO ""."examination_question1" VALUES (845, 845, '驾驶人违反交通运输管理法规发生重大事故致人死亡的处3年以上有期徒刑.', 2, '正确', '错误', , , '第一百三十三条　违反交通运输管理法规,因而发生重大事故,致人重伤、死亡或者使公私财产遭受重大损失的,处三年以下有期徒刑或者拘役；别逃,一逃就是3年以上了,如果因为你的逃导致人死亡了,7年以上.', , 0);
INSERT INTO ""."examination_question1" VALUES (846, 846, '驾驶机动车在高速公路上车辆发生故障时,为获得其他车辆的帮助,可将警告标志放置在其他车道.', 2, '正确', '错误', , , '应在本车道放置警告标志,且放置距离为:高速公路是150米以外,道路是50--100米.', , 0);
INSERT INTO ""."examination_question1" VALUES (847, 847, '如图所示,在这起交通事故中,以下说法正确的是什么?', 2, 'A车负全部责任', 'B车负全部责任', '都无责任,后果自行承担', '各负一半责任', 'A车在车道上正常行驶,B车掉头导致事故发生,因此,B车负全责.', 'http://images.juheapi.com/jztk/c1c2subject1/847.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (848, 848, '机动车参加安全技术检验的主要目的是检查车辆各项性能系数,及时消除车辆安全隐患,减少事故发生.', 1, '正确', '错误', , , '机动车参加安全技术检验的主要目的,都是为了降低交通安全隐患,减少交通事故.', , 0);
INSERT INTO ""."examination_question1" VALUES (849, 849, '如图所示,驾驶机动车时,前风窗玻璃处悬挂放置干扰视线的物品是错误的.', 1, '正确', '错误', , , '物品干扰开车视线增大了行车危险,容易引发交通事故.', 'http://images.juheapi.com/jztk/c1c2subject1/849.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (850, 850, '驾驶机动车在路口遇到这种信号灯亮时,不能右转弯.', 2, '正确', '错误', , , '右转灯不是红灯,右转没问题.', 'http://images.juheapi.com/jztk/c1c2subject1/850.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (851, 851, '机动车驾驶人联系电话、联系地址等信息发生变化,应当在信息变更后三十日内,向驾驶证核发地车辆管理所备案.', 1, '正确', '错误', , , '持有大型客车、牵引车、城市公交车、中型客车、大型货车驾驶证的驾驶人联系电话、从业单位等信息发生变化未及时申报变更信息的,公安机关交通管理部门处二十元以上二百元以下罚款.机动车驾驶人联系电话、联系地址等信息发生变化,应当在信息变更后三十日内,向驾驶证核发地车辆管理所备案.', , 0);
INSERT INTO ""."examination_question1" VALUES (852, 852, '公安机关交通管理部门对驾驶人的交通违法行为除依法给予行政处罚外,实行下列哪种制度?', 3, '违法登记制度', '奖励里程制度', '累积记分制度', '强制报废制度', '《道路交通安全法》第二十四条:公安机关交通管理部门对机动车驾驶人违反道路交通安全法律、法规的行为,除依法给予行政处罚外,实行累积记分制度.', , 0);
INSERT INTO ""."examination_question1" VALUES (853, 853, '大雾天行车,多鸣喇叭是为了什么?', 1, '引起对方注意,避免发生危险', '催促前车提速,避免发生追尾', '准备超越前车', '催促前车让行', '都说了是大雾天,肯定能见度很低啦,所以要靠耳朵来听.多响几声喇叭,就是要告诉别人,这里有车,提高注意！', , 0);
INSERT INTO ""."examination_question1" VALUES (854, 854, '这个标志是何含义?', 2, '学校区域', '注意儿童', '人行横道', '注意行人', '注意儿童:用以警告车辆驾驶人减速慢行,注意儿童.设在小学、幼儿园、少年宫等儿童经常出入地点前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/854.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (855, 855, '持有大型客车、牵引车、城市公交车、中型客车、大型货车驾驶证的驾驶人联系电话、从业单位等信息发生变化未及时申报变更信息的,公安机关交通管理部门处二十元以上二百元以下罚款.', 1, '正确', '错误', , , '持有大型客车、牵引车、城市公交车、中型客车、大型货车驾驶证的驾驶人联系电话、从业单位等信息发生变化未及时申报变更信息的,公安机关交通管理部门处二十元以上二百元以下罚款.机动车驾驶人联系电话、联系地址等信息发生变化,应当在信息变更后三十日内,向驾驶证核发地车辆管理所备案.', , 0);
INSERT INTO ""."examination_question1" VALUES (856, 856, '路面由白色虚线和三角地带标线组成的是什么标线?', 4, '道路入口标线', '可跨越式分道线', '道路出口减速线', '道路出口标线', '道路出口标线为白色虚线,主要用于高速公路和其他采用立体交叉道路.该标线作用为,高速公路出入口横向标线或三角地带标线是为驶入或驶出匝道车辆提供安全交汇引导,减少与突出部缘石碰撞.', 'http://images.juheapi.com/jztk/c1c2subject1/856.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (857, 857, '驾驶机动车在高速公路上行驶,能见度小于200米时,车速不得超过每小时60公里.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法实施条例》第八十一条:机动车在高速公路上行驶,遇有雾、雨、雪、沙尘、冰雹等低能见度气象条件时,应当遵守下列规定:能见度小于200米时,开启雾灯、近光灯、示廓灯和前后位灯,车速不得超过每小时60公里,与同车道前车保持100米以上的距离.', , 0);
INSERT INTO ""."examination_question1" VALUES (858, 858, '这个标志是何含义?', 2, '路面不平', '路面低洼', '路面高突', '有驼峰桥', '路面低洼:用以提醒车辆驾驶人减速慢行.设在路面突然低洼以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/858.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (859, 859, '这个标志是何含义?', 2, '预告宽度为3米', '限制宽度为3米', '解除3米限宽', '限制高度为3米', '限制宽度为3米:表示禁止装载宽度超过标志所示数值的车辆通行.此标志设在最大允许宽度受限制的地方. 以图为例:装载宽度不得超过3米', 'http://images.juheapi.com/jztk/c1c2subject1/859.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (860, 860, '这个标志是何含义?', 4, '电动自行车行驶', '非机动车停车位', '非机动车停放区', '非机动车行驶', '看到自行车你就想他是非机动车就行了.', 'http://images.juheapi.com/jztk/c1c2subject1/860.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (861, 861, '这个标志是何含义?', 1, '解除禁止超车', '准许变道行驶', '解除禁止变道', '解除禁止借道', '解除禁止超车:表示禁止超车路段结束.此标志设在禁止超车的终点.', 'http://images.juheapi.com/jztk/c1c2subject1/861.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (862, 862, '这个标志是何含义?', 1, '右转车道', '掉头车道', '左转车道', '分向车道', '表示只准一切车辆向右转弯.此标志设在车辆必须向右转弯的路口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/862.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (863, 863, '这个标志是何含义?', 2, '禁止驶入路口', '禁止向右转弯', '禁止变更车道', '禁止车辆掉头', '禁止向右转弯:表示前方路口禁止一切车辆向右转弯.此标志设在禁止向右转弯的路口前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/863.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (864, 864, '这个标志是何含义?', 4, '禁止直行和向左变道', '禁止直行和向左转弯', '允许直行和向左变道', '禁止直行和向右转弯', '禁止直行和向右转弯:表示前方路口禁止一切车辆直行和向右转弯.此标志设在禁止直行和向右转弯的路口前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/864.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (865, 865, '驾驶机动车由加速车道进入高速公路行驶,以下做法错误的是什么?', 4, '在加速车道上加速,同时要开启左转向灯', '密切注意左侧行车道的车流状态,同时用后视镜观察后方的情况', '充分利用加速车道的长度加速,确认安全后,平顺地进入行车道', '经加速车道充分加速后,可直接驶入最左侧车道', '《中华人民共和国道路交通安全法实施条例》第七十八条:高速公路应当标明车道的行驶速度,最高车速不得超过每小时120公里,最低车速不得低于每小时60公里.在高速公路上行驶的小型载客汽车最高车速不得超过每小时120公里,其他机动车不得超过每小时100公里,摩托车不得超过每小时80公里.同方向有2条车道的,左侧车道的最低车速为每小时100公里；同方向有3条以上车道的,最左侧车道的最低车速为每小时110公里,中间车道的最低车速为每小时90公里.因此,驾车进入高速公路后,不能直接驶入最左车道,要循序渐进.', , 0);
INSERT INTO ""."examination_question1" VALUES (866, 866, '持有大型客车、牵引车、城市公交车、中型客车、大型货车驾驶证的驾驶人,记分周期内有记分的,应当在记分周期结束后三十日内到公安机关交通管理部门接受审验,同时还应当申报身体条件情况.', 1, '正确', '错误', , , '《公安部令第123号》第六十条:持有大型客车、牵引车、城市公交车、中型客车、大型货车驾驶证的驾驶人,应当在每个记分周期结束后三十日内到公安机关交通管理部门接受审验.但在一个记分周期内没有记分记录的,免予本记分周期审验.', , 0);
INSERT INTO ""."examination_question1" VALUES (867, 867, '申请小型汽车准驾车型驾驶证的人年龄条件是多少?', 2, '18周岁以上60周岁以下', '18周岁以上70周岁以下', '24周岁以上70周岁以下', '21周岁以上50周岁以下', '申请小型汽车、小型自动挡汽车、残疾人专用小型自动挡载客汽车、轻便摩托车准驾车型的,在18周岁以上、70周岁以下.', , 0);
INSERT INTO ""."examination_question1" VALUES (868, 868, '这个标志是何含义?', 3, '不准让行', '会车让行', '减速让行', '停车让行', '减速让行:表示车辆应减速让行,告示车辆驾驶员必须慢行或停车,观察干路行车情况,在确保干道车辆优先的前提下,认为安全时方可续行.此标志设在视线良好交叉道路的次要路口.', 'http://images.juheapi.com/jztk/c1c2subject1/868.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (869, 869, '这个标志是何含义?', 4, '两侧变窄路段', '车速测试路段', '车距确认路段', '注意保持车距', '车距确认标线作为车辆驾驶人保持行车安全距离的参考,视需要设于较长直线段、易发生追尾事故或其他需要的路段,应与车距确认标志配合使用.', 'http://images.juheapi.com/jztk/c1c2subject1/869.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (870, 870, '驾驶机动车在高速公路上行驶,能见度小于200米时,与同车车道前车应保持100米以上的距离.', 1, '正确', '错误', , , '能见度小于200米时,开启雾灯、近光灯、示廓灯和前后位灯,车速不得超过每小时60公里,与同车道前车保持100米以上的距离；能见度小于100米时,开启雾灯、近光灯、示廓灯、前后位灯和危险报警闪光灯,车速不得超过每小时40公里,与同车道前车保持50米以上的距离；能见度小于50米时,开启雾灯、近光灯、示廓灯、前后位灯和危险报警闪光灯,车速不得超过每小时20公里,并从最近的出口尽快驶离高速公路.', , 0);
INSERT INTO ""."examination_question1" VALUES (871, 871, '机动车所有人申请转移登记前,应当将涉及该车的道路交通安全违法行为和交通事故处理完毕.', 1, '正确', '错误', , , '第十八条已注册登记的机动车所有权发生转移的,现机动车所有人应当自机动车交付之日起三十日内向登记地车辆管理所申请转移登记.机动车所有人申请转移登记前,应当将涉及该车的道路交通安全违法行为和交通事故处理完毕.', , 0);
INSERT INTO ""."examination_question1" VALUES (872, 872, '这个标志的含义是提醒车辆驾驶人前方是车辆渡口.', 1, '正确', '错误', , , '渡口:用以提醒车辆驾驶人谨慎驾驶.设在车辆渡口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/872.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (873, 873, '这个标志是何含义?', 3, '最高速度', '限制速度', '建议速度', '最低速度', '常识题,记住.', 'http://images.juheapi.com/jztk/c1c2subject1/873.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (874, 874, '这个标志是何含义?', 4, '禁止长时鸣喇叭', '断续鸣喇叭', '减速鸣喇叭', '禁止鸣喇叭', '禁止鸣喇叭:表示禁止鸣喇叭.此标志设在需要禁止鸣喇叭的地方.禁止鸣喇叭的时间和范围可用辅助标志说明.', 'http://images.juheapi.com/jztk/c1c2subject1/874.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (875, 875, '如图所示,驾驶机动车经过这种道路时,如果前方没有其他交通参与者,可在道路上随意通行. ', 2, '正确', '错误', , , '驾驶机动车经过这种道路时,如果前方没有其他交通参与者,也不可在道路上随意通行,安全第一.', 'http://images.juheapi.com/jztk/c1c2subject1/875.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (876, 876, '这个标志是何含义?', 1, '省道编号', '国道编号', '县道编号', '乡道编号', '乡道是白底Y字开头的,县道是白底X开头的,省道是黄底的,国道是红底的.', 'http://images.juheapi.com/jztk/c1c2subject1/876.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (877, 877, '这种白色矩形标线框含义是什么?', 2, '长时停车位', '限时停车位', '免费停车位', '专用停车位', '有时限的停车位~~', 'http://images.juheapi.com/jztk/c1c2subject1/877.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (878, 878, '这个标志是何含义?', 3, '泥泞道路', '低洼路面', '过水路面', '渡口', '过水路面指的是通过平时无水或流水很少的宽浅河流而修筑的在洪水期间容许水流浸过的路面.', 'http://images.juheapi.com/jztk/c1c2subject1/878.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (879, 879, '驾驶机动车在山路行驶时,为了减少油耗,下坡时可以空挡滑行,并使用行车制动器控制速度.', 2, '正确', '错误', , , '有些驾驶员为了节油,在高速行驶或下坡时,将换挡杆拨到N挡滑行,这很可能烧坏变速器.因为此时变速器输出轴转速很高,而发动机怠速运转,变速器油泵供油不足,润滑状况恶化,而且对变速器内部的多片离合器来说,虽然动力已经切断,但其被动片在车轮带动下高速运转,发动机驱动的主动片转速很低,两者间隙又很小,容易引起共振和打滑现象,产生不良后果.当下长坡确需滑行时,可将换挡杆保持在D挡滑行,但不可使发动机熄火.', , 0);
INSERT INTO ""."examination_question1" VALUES (880, 880, '驾驶机动车在交叉路口前变更车道时,应在进入实线区后,开启转向灯,变更车道.', 2, '正确', '错误', , , '在虚线区就要完成车道变更选择而并更车道要提前开启转向灯的,实线区才开转向灯就不能做到提前开启转向灯的要求了,而且实线区禁止变更车道.', , 0);
INSERT INTO ""."examination_question1" VALUES (881, 881, '路中心的黄色斜线填充是何含义?', 2, '单向行驶车道分界线', '禁止跨越对向车行道分界线', '双侧可跨越同向车道分界线', '可跨越对向车道分界线', '相对车道之间不能跨越黄线,一过就你就成马路杀手了.', 'http://images.juheapi.com/jztk/c1c2subject1/881.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (882, 882, '驾驶机动车经过无划分车道的道路时,可以随意通行.', 2, '正确', '错误', , , '驾驶机动车经过无划分车道的道路时,机动车在道路中间通行,非机动车和行人在道路两侧通行.', , 0);
INSERT INTO ""."examination_question1" VALUES (883, 883, '这个标志是何含义?', 3, '距无人看守铁路道口100米', '距有人看守铁路道口100米', '距无人看守铁路道口50米', '距有人看守铁路道口50米', '一道红线是50米,一道50米.', 'http://images.juheapi.com/jztk/c1c2subject1/883.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (884, 884, '如图所示,夜间驾驶机动车与同方向行驶的前车距离较近时,以下做法正确的是什么?', 2, '使用远光灯,有利于观察路面情况', '禁止使用远光灯,避免灯光照射至前车后视镜造成前车驾驶人眩目', '使用远光灯,有利于告知前方驾驶人后方有来车', '禁止使用远光灯,避免灯光照射至前车后视镜造成自己眩目', '夜间驾驶机动车与同方向行驶的前车距离较近时,禁止使用远光灯,避免灯光照射至前车后视镜造成前车驾驶人眩目.', 'http://images.juheapi.com/jztk/c1c2subject1/884.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (885, 885, '这一组交通警察手势是什么信号?', 3, '右转弯信号', '变道信号', '减速慢行信号', '靠边停车信号', '如果看到别人发火,我们会做由上往下压的动作让他压压火别爆发,同理,由下往下压是让你减速慢行', 'http://images.juheapi.com/jztk/c1c2subject1/885.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (886, 886, '如图所示,驾驶机动车在路口遇到这种交通信号时,右转弯的车辆在不妨碍被放行的车辆、行人的情况下,可以通行.', 1, '正确', '错误', , , '驾驶机动车在路口遇到这种交通信号时,右转弯的车辆在不妨碍被放行的车辆、行人的情况下,可以通行.', 'http://images.juheapi.com/jztk/c1c2subject1/886.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (887, 887, '经购买、调拨、赠予等方式获得机动车后尚未注册登记的,向车辆管理所申领临时行驶车号牌后,方可临时上道路行驶.', 1, '正确', '错误', , , '经购买、调拨、赠予等方式获得机动车后尚未注册登记的,向车辆管理所申领临时行驶车号牌后,方可临时上道路行驶.', , 0);
INSERT INTO ""."examination_question1" VALUES (888, 888, '发生交通事故时,下列哪种情况下当事人应当保护现场并立即报警?', 4, '未造成人员伤亡的', '未发生财产损失事故', '未损害公共设施及建筑物的', '驾驶人有酒后驾驶嫌疑的', '酒后驾驶属违法行为,应及时报警.', , 0);
INSERT INTO ""."examination_question1" VALUES (889, 889, '驾驶机动车遇紧急事务,可以边开车边接打电话.', 2, '正确', '错误', , , '新修订的《机动车驾驶证申领和使用规定》公布后,2013年1月1日起施行,其中规定:驾驶机动车有拨打、接听手持电话等妨碍安全驾驶的行为的一次记2分.此外,边开车边打电话也会危害人身安全.', , 0);
INSERT INTO ""."examination_question1" VALUES (890, 890, '如图所示,A车具有优先通行权.', 1, '正确', '错误', , , '直行先行,但若让A车等待B车,则时间更长,所以都是直行的情况下,A车先行.', 'http://images.juheapi.com/jztk/c1c2subject1/890.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (891, 891, '如图所示,驾驶机动车驶出这个路口时应当怎样使用灯光?', 1, '开启右转向灯', '开启危险报警闪光灯', '不用开启转向灯', '开启左转向灯', '驶入开左转向灯,驶出开右转向灯.', 'http://images.juheapi.com/jztk/c1c2subject1/891.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (892, 892, '这个标志是何含义?', 1, '右侧绕行', '单向通行', '注意危险', '左侧绕行', '右侧绕行:用以告示前方道路有障碍物,车辆应按标志指示减速慢行,右侧绕行通过.', 'http://images.juheapi.com/jztk/c1c2subject1/892.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (893, 893, '车辆在交叉路口有优先通行权的,遇有车辆抢行时,应怎样做?', 3, '抢行通过', '提前加速通过', '减速避让,必要时停车让行', '按优先权规定正常行驶不予避让', '即使你有优先权,还是要安全第一.', , 0);
INSERT INTO ""."examination_question1" VALUES (894, 894, '这个标志是何含义?', 2, '海关检查', '停车检查', '边防检查', '禁止通行', '停车检查:表示机动车必须停车接受检查.此标志设在关卡将近处,以便要求车辆接受检查或缴费等手续.标志中可加注说明检查事项.', 'http://images.juheapi.com/jztk/c1c2subject1/894.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (895, 895, '如图所示,驾驶机动车在路口前遇到这种情况时,A车具有优先通行权.', 1, '正确', '错误', , , '直行优先.', 'http://images.juheapi.com/jztk/c1c2subject1/895.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (896, 896, '当驾驶人的血液中酒精含量为100毫克/100毫升时,属于醉酒驾驶.', 1, '正确', '错误', , , '醉酒驾驶是指车辆驾驶人员血液中的酒精含量大于或者等于80mg/100mL的驾驶行为.', , 0);
INSERT INTO ""."examination_question1" VALUES (897, 897, '申请人患有癫痫病的,可以申领机动车驾驶证,但是驾驶时必须有人陪同.', 2, '正确', '错误', , , '申请人患有癫痫病的,不可以申领机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (898, 898, '驾驶机动车下长坡时,空挡滑行会导致再次挂挡困难.', 1, '正确', '错误', , , '驾驶机动车下长坡时,空挡滑行会导致再次挂挡困难.', , 0);
INSERT INTO ""."examination_question1" VALUES (899, 899, '驾驶机动车在道路上掉头时,应当提前开启左转向灯.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法实施条例》第五十七条机动车应当按照下列规定使用转向灯:（一）向左转弯、向左变更车道、准备超车、驶离停车地点或者掉头时,应当提前开启左转向灯；（二）向右转弯、向右变更车道、超车完毕驶回原车道、靠路边停车时,应当提前开启右转向灯.', , 0);
INSERT INTO ""."examination_question1" VALUES (900, 900, '机动车驾驶人造成重大交通事故后逃逸,构成犯罪的,十年内不能申请机动车驾驶证.', 2, '正确', '错误', , , '《交通法》第一百零一条:违反道路交通安全法律、法规的规定,发生重大交通事故,构成犯罪的,依法追究刑事责任,并由公安机关交通管理部门吊销机动车驾驶证.造成交通事故后逃逸的,由公安机关交通管理部门吊销机动车驾驶证,且终生不得重新取得机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (901, 901, '警车、消防车、救护车、工程救险车执行紧急任务时,耽误或影响其通行可能会导致严重后果,所以其他车辆和行人应当主动让行.', 1, '正确', '错误', , , '警车、消防车、救护车、工程救险车执行紧急任务时,耽误或影响其通行可能会导致严重后果,所以其他车辆和行人应当主动让行.', , 0);
INSERT INTO ""."examination_question1" VALUES (902, 902, '车速超过规定时速达到50%的一次记3分.', 2, '正确', '错误', , , '车速超过规定时速达到50%的直接扣12分', , 0);
INSERT INTO ""."examination_question1" VALUES (903, 903, '两辆机动车发生轻微碰擦事故后,为保证理赔,必须等保险公司人员到场鉴定后才能撤离现场.', 2, '正确', '错误', , , '《中华人民共和国道路交通安全法》中第七十条第二、三款规定,在道路上发生交通事故,未造成人身伤亡,当事人对事实及成因无争议的,可以即行撤离现场,恢复交通,自行协商处理损害赔偿事宜;不即行撤离现场的,应当迅速报告执勤的交通警察或者公安机关交通管理部门.在道路上发生交通事故,仅造成轻微财产损失,并且基本事实清楚的,当事人应当先撤离现场再进行协商处理.', , 0);
INSERT INTO ""."examination_question1" VALUES (904, 904, '补领机动车驾驶证应到以下哪个地方办理?', 2, '所学驾校', '驾驶证核发地车辆管理所', '派出所', '全国任何地方公安机关交通管理部门', '如果出现驾驶证丢失需补办的情况,需要到驾驶证核发地车辆管理所补领机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (905, 905, '机动车驾驶人一次有两个以上违法行为记分的,应当分别计算累加分值.', 1, '正确', '错误', , , '《机动车驾驶证申领和使用规定》第五十六条对机动车驾驶人的道路交通安全违法行为,处罚与记分同时执行.机动车驾驶人一次有两个以上违法行为记分的,应当分别计算,累加分值.', , 0);
INSERT INTO ""."examination_question1" VALUES (906, 906, '隐瞒有关情况或者提供虚假材料申领机动车驾驶证的,申请人在1年内不得再次申领机动车驾驶证.', 1, '正确', '错误', , , '《机动车驾驶证申领和使用规定》第七十八条:隐瞒有关情况或者提供虚假材料申领机动车驾驶证的,申请人在一年内不得再次申领机动车驾驶证.申请人在考试过程中有贿赂、舞弊行为的,取消考试资格,已经通过考试的其他科目成绩无效；申请人在一年内不得再次申领机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (907, 907, '道路没有划分机动车道、非机动车道和人行道的,以下说法正确的是什么?', 3, '机动车在道路左侧通行,非机动车和行人随意通行', '机动车在道路左侧通行,非机动车和行人在道路两侧通行', '机动车在道路中间通行,非机动车和行人在道路两侧通行', '机动车、非机动车和行人可随意通行', '驾驶机动车经过无划分车道的道路时,机动车在道路中间通行,非机动车和行人在道路两侧通行.', , 0);
INSERT INTO ""."examination_question1" VALUES (908, 908, '驾驶机动车在高速公路上行驶,遇低能见度气象条件时,能见度在200米以下,车速不得超过每小时多少公里,与同车道前车至少保持多少米的距离?', 1, '60,100', '70,100', '40,80', '30,80', '驾驶机动车在高速公路上行驶,遇低能见度气象条件时,能见度在200米以下,车速不得超过60公里/小时,与同车道前车至少保持100米的车距以保证安全.', , 0);
INSERT INTO ""."examination_question1" VALUES (909, 909, '驾驶机动车遇到前方道路拥堵时,可以借用无人通行的非机动车道行驶.', 2, '正确', '错误', , , '机动车不得借用非机动车道行驶.', , 0);
INSERT INTO ""."examination_question1" VALUES (910, 910, '关于醉酒驾驶机动车的处罚,以下说法错误的是什么?', 4, '公安机关交通管理部门约束至酒醒', '吊销驾驶证', '五年内不得重新取得机动车驾驶证', '记6分', '醉酒驾驶机动车的,由公安机关交通管理部门约束至酒醒,吊销机动车驾驶证,依法追究刑事责任；五年内不得重新取得机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (911, 911, '如图所示,驾驶机动车直行遇前方道路堵塞时,车辆可以在黄色网格线区域临时停车等待,但不得在人行横道停车.', 2, '正确', '错误', , , '驾驶机动车直行遇前方道路堵塞时,车辆不可在黄色网格线区域临时停车等待,也不可在人行横道停车.', 'http://images.juheapi.com/jztk/c1c2subject1/911.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (912, 912, '以下哪种违法行为的机动车驾驶人将被一次记6分?', 3, '驾驶与准驾车型不符的机动车', '车速超过规定时速50%以上', '驾驶机动车违反道路交通信号灯', '未取得校车驾驶资格驾驶校车', '机动车驾驶员有下列违章行为之一的,一次记6分:(一)不按规定停车或者车辆发生故障不立即将车移开,造成交通严重堵塞的；(二)逆向行驶的；(三)饮酒后驾驶车辆的；(四)驾车穿插、超越警车护卫的车队的；(五)驾驶与驾驶证准驾车型不相符合的车辆的；(六)驾车下陡坡时熄火、空挡滑行的；(七)行经铁路道口不按规定行车或者停车的；(八)客车载人超过核定人数20%以上的；(九)在高速公路上客车载人超过核定人数的；(十)在高速公路上货车载物超过核定载质量30%以上的；(十一)在高速公路上不按规定超车或者变更车道的；(十二)在高速公路上驾驶转向器、制动器、灯光装置等机件不符合安全要求的车辆的；(十三)在高速公路上载运危险物品未经审批或者未按规定行驶的.', , 0);
INSERT INTO ""."examination_question1" VALUES (913, 913, '驾驶机动车发生以下交通事故,哪种情况适用自行协商解决?', 3, '对方饮酒的', '对事实及成因有争议的', '未造成人身伤亡,对事实及成因无争议的', '造成人身伤亡的', '《中华人民共和国道路交通安全法实施条例》第八十六条规定:“机动车与机动车、机动车与非机动车在道路上发生未造成人身伤亡的交通事故,当事人对事实及成因无争议的,在记录交通事故的时间、地点、对方当事人的姓名和联系方式、机动车牌号、驾驶证号、保险凭证号、碰撞部位,并共同签名后,撤离现场,自行协商损害赔偿事宜.当事人对交通事故事实及成因有争议的,应当迅速报警.”', , 0);
INSERT INTO ""."examination_question1" VALUES (914, 914, '如图所示,驾驶机动车遇到这种情况时,以下做法正确的是什么? ', 2, '应减速观察水情,然后加速行驶通过', '应停车察明水情,确认安全后,低速通过', '应停车察明水情,确认安全后,快速通过', '可随意通行', '机动车在有积水较深的桥下通过行车时,应当停车察明水情,确认安全后,低速通过.', 'http://images.juheapi.com/jztk/c1c2subject1/914.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (915, 915, '避让特种车辆使其顺利通过后,车辆应有序回到原车道继续行驶,不要尾随特种车辆,以免发生交通事故.', 1, '正确', '错误', , , '司机在行驶中需要保持一定的车距,即使在拥堵的情况下也需要保持一定的车距,一是保障自身行车安全,二是万一发生险情也可以顺利变道,避让特种车辆.同时,在特种车辆顺利通过后,车辆应有序回到原位继续行驶,不要趁机抢车位甚至跟随特种车辆,以免发生交通事故.', , 0);
INSERT INTO ""."examination_question1" VALUES (916, 916, '实习期驾驶人驾驶机动车上高速公路行驶,以下做法正确的是什么?', 3, '任何情况下都不允许上高速', '不需要其他人员陪同', '需要持有相应或者更高准驾车型驾驶证三年以上的驾驶人陪同', '需要持有相应或者更高准驾车型驾驶证、同在实习期内的驾驶人陪同', '相关法律规定,实习期驾驶人驾驶机动车上高速公路行驶,应当由持相应或者更高准驾车型驾驶证三年以上的驾驶人陪同.', , 0);
INSERT INTO ""."examination_question1" VALUES (917, 917, '驾驶机动车行经漫水路或者漫水桥时,应当停车察明水情,快速通过.', 2, '正确', '错误', , , '《中华人民共和国道路交通管理条例》的第四十六条规定:车辆行经漫水路或漫水桥时,必须停车察明水情,确认安全后,低速通过.', , 0);
INSERT INTO ""."examination_question1" VALUES (918, 918, '驾驶机动车在路口右转弯时,应提前开启右转向灯,不受信号灯限制,不受车速限制,迅速通过,防止路口堵塞.', 2, '正确', '错误', , , '驾驶机动车在路口右转弯时,应提前开启右转向灯,不受信号灯限制,必须限制车速,注意观察行人及来往车辆,安全通过.', , 0);
INSERT INTO ""."examination_question1" VALUES (919, 919, '下列哪种情况可以向机动车驾驶证核发地车辆管理所申请补发?', 3, '驾驶证被扣押', '驾驶证被扣留', '驾驶证遗失', '驾驶证被暂扣', '驾驶证被扣留、扣押及暂扣都可再次取回,机动车驾驶证遗失、损毁无法辨认时,机动车驾驶人应当向机动车驾驶证核发地车辆管理所申请补发.', , 0);
INSERT INTO ""."examination_question1" VALUES (920, 920, '这个标志是何含义?', 3, '施工路段绕行', '双向交通', '左右绕行', '注意危险', '左右绕行:用以告示前方道路有障碍物,车辆应按标志指示减速慢行,绕行通过.', 'http://images.juheapi.com/jztk/c1c2subject1/920.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (921, 921, '自愿降级的驾驶人需要到车辆管理所申请换领驾驶证.', 1, '正确', '错误', , , '自愿降级的驾驶人,由本人带驾驶证、身份证去车管所提出申请,并换领驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (922, 922, '发生无人员伤亡的、财产轻微损失的交通事故后,以下做法正确的是什么?', 3, '必须报警,等候警察处理', '开车离开现场', '确保安全的情况下,对现场拍照,然后将车辆移至路边等不妨碍交通的地点', '停在现场保持不动', '《中华人民共和国道路交通安全法实施条例》第八十六条规定:“机动车与机动车、机动车与非机动车在道路上发生未造成人身伤亡的交通事故,当事人对事实及成因无争议的,在记录交通事故的时间、地点、对方当事人的姓名和联系方式、机动车牌号、驾驶证号、保险凭证号、碰撞部位,并共同签名后,撤离现场,自行协商损害赔偿事宜.当事人对交通事故事实及成因有争议的,应当迅速报警.”', , 0);
INSERT INTO ""."examination_question1" VALUES (923, 923, '驾驶机动车在高速公路上行驶,错过出口时,如果确认后方无来车,可以倒回出口驶离高速公路.', 2, '正确', '错误', , , '错过出口时,应在下一个出口出去,不能倒回.', , 0);
INSERT INTO ""."examination_question1" VALUES (924, 924, '路中心双黄实线是何含义?', 1, '禁止跨越对向车行道分界线', '可跨越对向车道分界线', '双侧可跨越同向车道分界线', '单向行驶车道分界线', '哥木了,黄线不能压,两黄线就是左右不能过界了－－写解释的孩子伤不起', 'http://images.juheapi.com/jztk/c1c2subject1/924.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (925, 925, '如图所示,驾驶机动车遇到右侧车辆强行变道,应减速慢行,让右前方车辆顺利变道.', 1, '正确', '错误', , , '驾驶机动车遇到右侧车辆强行变道,应减速慢行,让右前方车辆顺利变道.', 'http://images.juheapi.com/jztk/c1c2subject1/925.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (926, 926, '驾驶机动车遇前方交叉路口交通阻塞时,路口内无网状线的,可停在路口内等候.', 2, '正确', '错误', , , '《中华人民共和国道路交通安全法实施条例》第五十三条:机动车遇有前方交叉路口交通阻塞时,应当依次停在路口以外等候,不得进入路口.', , 0);
INSERT INTO ""."examination_question1" VALUES (927, 927, '驾驶机动车超车时,被超越车辆未减速让路,应迅速提速超越前方车辆完成超车.', 2, '正确', '错误', , , '驾驶机动车超车时,被超越车辆未减速让路,应停止超车.', , 0);
INSERT INTO ""."examination_question1" VALUES (928, 928, '隐瞒有关情况或者提供虚假材料申请机动车驾驶证,申请人在多少年内不得再次申领机动车驾驶证?', 1, '1年', '2年', '3年', '4年', '机动车驾驶证申领和使用规定》第六章,法律责任,第七十八条隐瞒有关情况或者提供虚假材料申领机动车驾驶证的,申请人在一年内不得再次申领机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (929, 929, '在交叉路口、隧道内均不能倒车.', 1, '正确', '错误', , , '根据交通法,机动车在交叉路口、隧道内均不能倒车.', , 0);
INSERT INTO ""."examination_question1" VALUES (930, 930, '以下哪种行为机动车驾驶人将被一次记12分?', 4, '驾驶机动车不按规定避让校车的', '驾驶证被暂扣期间驾驶机动车的', '驾驶机动车违反道路交通信号灯通行的', '驾驶与准驾车型不符的机动车的', '机动车驾驶人有下列违法行为之一,一次记12分:（一）醉酒后驾驶机动车的；（二）机动车驾驶证被暂扣期间驾驶机动车的；（三）造成交通事故后逃逸,尚不构成犯罪的；（四）违反交通管制的规定强行通行,不听劝阻的；（五）使用他人机动车驾驶证驾驶机动车的；（六）驾驶与准驾车型不符的机动车的；（七）超过三个月不缴纳罚款或者连续两次逾期不缴纳罚款的.', , 0);
INSERT INTO ""."examination_question1" VALUES (931, 931, '驾驶机动车发生交通事故,仅造成财产损失的,但是对交通事故事实及成因有争议的,应当怎么处理?', 1, '迅速报警', '占道继续和对方争辩', '找中间人帮忙解决', '自行协商损害赔偿事宜', '对于事故成因不明的交通事故,应及时报警.', , 0);
INSERT INTO ""."examination_question1" VALUES (932, 932, '这个标志是何含义?', 3, '高速公路出口预告', '高速公路入口预告', '高速公路终点预告', '高速公路起点预告', '高速公路终点预告:用于指示高速公路或城市快速路终点,设在高速公路或城市快速路终点处.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/932.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (933, 933, '以下不属于机动车驾驶证审验内容的是什么?', 4, '道路交通安全违法行为、交通事故处理情况', '驾驶人身体条件', '记满12分后参加学习和考试情况', '驾驶车辆累计行驶里程', '机动车驾驶证审验内容:（一）道路交通安全违法行为、交通事故处理情况；（二）身体条件情况；（三）道路交通安全违法行为记分及记满12分后参加学习和考试情况.对交通违法行为或者交通事故未处理完毕的、身体条件不符合驾驶许可条件的、未按照规定参加学习、教育和考试的,不予通过审验.', , 0);
INSERT INTO ""."examination_question1" VALUES (934, 934, '如图所示,红圈标注的深色车辆的做法是违法的.', 1, '正确', '错误', , , '白色实线不允许便道,深色车辆在白色实线上变更车道,属于违法行驶.', 'http://images.juheapi.com/jztk/c1c2subject1/934.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (935, 935, '机动车驾驶证有效期超过一年以上未换证的,驾驶证将被注销.', 1, '正确', '错误', , , '《机动车驾驶证申领和使用规定》第四十二条机动车驾驶人具有下列情形之一的,车辆管理所应当注销其机动车驾驶证:(一)死亡的;(二)身体条件不适合驾驶机动车的;(三)提出注销申请的;(四)丧失民事行为能力,监护人提出注销申请的;(五)超过机动车驾驶证有效期一年以上未换证的.', , 0);
INSERT INTO ""."examination_question1" VALUES (936, 936, '上道路行驶的机动车未悬挂机动车号牌的一次记几分?', 2, '6分', '12分', '2分', '3分', '上道路行驶的机动车未悬挂机动车号牌的,或者故意遮挡、污损、不按规定安装机动车号牌的一次记12分.', , 0);
INSERT INTO ""."examination_question1" VALUES (937, 937, '机动车驾驶人补领机动车驾驶证后,使用原机动车驾驶证驾驶的,除由公安机关交通管理部门收回原机动车驾驶证外,还应当受到何种处罚?', 4, '吊销驾驶证', '拘留驾驶人', '警告', '罚款', '机动车驾驶人补领机动车驾驶证后,继续使用原机动车驾驶证的,公安机关交通管理部门可处二十元以上二百元以下罚款.', , 0);
INSERT INTO ""."examination_question1" VALUES (938, 938, '驾驶机动车在高速公路上行驶,遇有雾、雨、雪、沙尘、冰雹等低能见度气象条件下,能见度在100米以下时,车速不得超过每小时多少公里,与同车道前车至少保持多少米的距离?', 1, '40,50', '40,40', '50,40', '50,30', '能见度小于200米时,开启雾灯、近光灯、示廓灯和前后位灯,车速不得超过每小时60公里,与同车道前车保持100米以上的距离；能见度小于100米时,开启雾灯、近光灯、示廓灯、前后位灯和危险报警闪光灯,车速不得超过每小时40公里,与同车道前车保持50米以上的距离；能见度小于50米时,开启雾灯、近光灯、示廓灯、前后位灯和危险报警闪光灯,车速不得超过每小时20公里,并从最近的出口尽快驶离高速公路.', , 0);
INSERT INTO ""."examination_question1" VALUES (939, 939, '使用伪造、变造的驾驶证一次记12分.', 1, '正确', '错误', , , '使用伪造、变造的机动车号牌、行驶证、驾驶证、校车标牌或者使用其他机动车号牌、行驶证的一次记12分.', , 0);
INSERT INTO ""."examination_question1" VALUES (940, 940, '驾驶拼装的机动车上道路行驶的,公安机关交通管理部门应当予以收缴,强制报废,并吊销机动车驾驶证.', 1, '正确', '错误', , , '驾驶拼装的机动车或者已达到报废标准的机动车上道路行驶的,公安机关交通管理部门应当予以收缴,强制报废.对驾驶前款所列机动车上道路行驶的驾驶人,处二百元以上二千元以下罚款,并吊销机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (941, 941, '如图所示,驾驶机动车遇左侧车道有车辆正在超车时,可以迅速变道,伺机反超.', 2, '正确', '错误', , , '遇到左侧车辆超车,应当减速慢行,靠右边行,以保障行车安全.', 'http://images.juheapi.com/jztk/c1c2subject1/941.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (942, 942, '这辆小型载客汽车驶离高速公路行车道的方法是正确的.', 1, '正确', '错误', , , '这个车是按规定驶离高速的,没有问题.', 'http://images.juheapi.com/jztk/c1c2subject1/942.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (943, 943, '道路交通安全违法行为累积记分的周期是多长时间?', 1, '12个月', '24个月', '3个月', '6个月', '《机动车驾驶证申领和使用规定》（公安部令第123号）第五十五条  道路交通安全违法行为累积记分周期（即记分周期）为12个月,满分为12分,从机动车驾驶证初次领取之日起计算.', , 0);
INSERT INTO ""."examination_question1" VALUES (944, 944, '如图所示,红圈中标记车辆使用灯光的方法是正确的.', 2, '正确', '错误', , , '《中华人民共和国道路交通安全法》第四十八条规定:在没有中心隔离设施或者没有中心线的道路上,夜间会车应当在距相对方向来车150米以外改用近光灯.', 'http://images.juheapi.com/jztk/c1c2subject1/944.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (945, 945, '机动车驾驶人驾驶证有效期满换领驾驶证时,须提交县级以上医疗机构出具的身体条件证明.', 1, '正确', '错误', , , '《道路运输从业人员管理规定》规定:机动车驾驶人驾驶证有效期满换领驾驶证时,须提交县级以上医疗机构出具的身体条件证明.', , 0);
INSERT INTO ""."examination_question1" VALUES (946, 946, '行人参与道路交通的主要特点除了行走随意性大、方向多变以外,还喜欢聚集、围观.', 1, '正确', '错误', , , '行人参与道路交通的主要特点除了行走随意性大、方向多变以外,还喜欢聚集、围观.', , 0);
INSERT INTO ""."examination_question1" VALUES (947, 947, '这种黄黑相间的倾斜线条是什么标记?', 3, '实体标记', '突起标记', '立面标记', '减速标记', '那是用来标记物体表面的－－简称“立面”,别去撞哟~~', 'http://images.juheapi.com/jztk/c1c2subject1/947.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (948, 948, '驾驶机动车在道路上发生交通事故造成人身伤亡的,驾驶人必须报警.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法》第七十条规定:在道路上发生交通事故,车辆驾驶人应当立即停车,保护现场；造成人身伤亡的,车辆驾驶人应当立即抢救受伤人员,并迅速报告执勤的交通警察或者公安机关交通管理部门.因抢救受伤人员变动现场的,应当标明位置.乘车人、过往车辆驾驶人、过往行人应当予以协助.', , 0);
INSERT INTO ""."examination_question1" VALUES (949, 949, '已注册登记的机动车,改变机动车车身颜色的应到公安交通管理部门申请变更登记.', 1, '正确', '错误', , , '《中华人民共和国道路运输条例》说明,已注册登记的机动车,改变机动车车身颜色的应到公安交通管理部门申请变更登记.', , 0);
INSERT INTO ""."examination_question1" VALUES (950, 950, '这个标志是何含义?', 4, '高速公路终点地名预告', '高速公路行驶路线预告', '高速公路行驶方向预告', '高速公路地点距离预告', '高速公路地点距离预告:预告高速公路或城市快速路前方所要经过的重要的地点、道路的名称和距离,设置在互通式立体交叉加速车道的渐变段终点以后1 km以上路段的合适位置处.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/950.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (951, 951, '如图所示,驾驶机动车行驶至车道减少的路段时,遇前方机动车排队等候或行驶缓慢时,以下做法正确的是什么?', 2, '右侧车让左侧车先行', '每车道一辆依次交替驶入左侧车道', '左侧车让右侧车先行', '右侧车寻找空隙提前进入左侧车道', '驾驶机动车行驶至车道减少的路段时,前方机动车排队等候或行驶缓慢时,应当每车道一辆依次交替驶入左侧车道.', 'http://images.juheapi.com/jztk/c1c2subject1/951.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (952, 952, '驾驶机动车在高速公路上行驶,遇有雾、雨、雪、沙尘、冰雹等低能见度气象条件时,能见度在50米以下时,以下做法正确的是什么?', 4, '加速驶离高速公路', '在应急车道上停车等待', '可以继续行驶,但车速不得超过每小时40公里', '以不超过每小时20公里的车速从最近的出口尽快驶离高速公路', '机动车在高速公路上行驶,遇有雾、雨、雪、沙尘、冰雹等低能见度气象条件时,能见度小于50米时,开启雾灯、近光灯、示廓灯、前后位灯和危险报警闪光灯,车速不得超过每小时20公里,并从最近的出口尽快驶离高速公路.', , 0);
INSERT INTO ""."examination_question1" VALUES (953, 953, '以下哪项行为可构成危险驾驶罪?', 4, '闯红灯', '无证驾驶', '疲劳驾驶', '醉驾', '危险驾驶罪是指在道路上醉酒驾驶机动车或者在道路上驾驶机动车追逐竞驶,情节恶劣的行为.', , 0);
INSERT INTO ""."examination_question1" VALUES (954, 954, '将机动车交由未取得机动车驾驶证的人驾驶的,由公安交通管理部门处二百元以上二千元以下罚款,可以并处以下哪种处罚?', 2, '15日以下拘留', '吊销驾驶证', '扣留车辆', '5年不得重新取得新驾驶证', '将机动车交由未取得机动车驾驶证的人驾驶的,由公安交通管理部门处二百元以上二千元以下罚款,并处以吊销驾驶证的处罚.', , 0);
INSERT INTO ""."examination_question1" VALUES (955, 955, '机动车发生轻微财产损失的交通事故,对应当自行撤离现场而未撤离的,交通警察有权责令当事人撤离现场.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法》中第七十条第二、三款规定,在道路上发生交通事故,未造成人身伤亡,当事人对事实及成因无争议的,可以即行撤离现场,恢复交通,自行协商处理损害赔偿事宜;不即行撤离现场的,应当迅速报告执勤的交通警察或者公安机关交通管理部门.在道路上发生交通事故,仅造成轻微财产损失,并且基本事实清楚的,当事人应当先撤离现场再进行协商处理.根据《中华人民共和国道路交通安全法》的有关规定精神,仅造成车物损失或者人体伤情轻微的交通事故,可由一名交通警察按照简易程序处理.', , 0);
INSERT INTO ""."examination_question1" VALUES (956, 956, '机动车在抵押登记、质押备案期间不可以办理转移登记.', 1, '正确', '错误', , , '第二十条有下列情形之一的,不予办理转移登记:(一)机动车与该车档案记载内容不一致的；(二)属于海关监管的机动车,海关未解除监管或者批准转让的；(三)机动车在抵押登记、质押备案期间的；(四)有本规定第九条第(一)项、第(二)项、第(七)项、第(八)项、第(九)项规定情形的.', , 0);
INSERT INTO ""."examination_question1" VALUES (957, 957, '如图所示,夜间驾驶机动车遇对方使用远光灯,无法看清前方路况时,以下做法正确的是什么? ', 3, '保持行驶方向和车速不变', '自己也打开远光灯行驶', '降低车速,谨慎会车', '加速通过,尽快摆脱眩目光线', '夜间驾驶机动车遇对方使用远光灯,无法看清前方路况时,降低车速,谨慎会车,以免发生车祸、危险.', 'http://images.juheapi.com/jztk/c1c2subject1/957.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (958, 958, '如图所示,驾驶机动车经过这种道路时,应降低车速在道路中间通行.', 1, '正确', '错误', , , '图中道路两侧都有行人,驾驶机动车经过这种道路时,需要降低车速在道路中间通行.', 'http://images.juheapi.com/jztk/c1c2subject1/958.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (959, 959, '如图所示,驾驶机动车遇到这种情况,以下做法正确的是什么?', 3, '长鸣喇叭催促行人快速通过', '开启远光灯警示行人有车辆驶近', '降低行驶速度,避让行人', '适当加速从行人前方绕行', '驾驶机动车遇到图中这种情况,应当降低行驶速度,避让行人.', 'http://images.juheapi.com/jztk/c1c2subject1/959.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (960, 960, '年龄在60周岁以上的机动车驾驶人,应当每年进行一次身体检查.', 1, '正确', '错误', , , '年龄在60周岁以上的机动车驾驶人,应当每年进行一次身体检查,以保证行车安全.', , 0);
INSERT INTO ""."examination_question1" VALUES (961, 961, '如图所示,驾驶机动车行驶至此位置时,以下做法正确的是什么?', 3, '观察左侧无车后,可以左转', '从该处直接左转', '不得左转,应当直行', '倒车退到虚线处换到左转车道', '驾驶机动车行驶至此位置时,以下做法正确的是:不得左转,应当直行.', 'http://images.juheapi.com/jztk/c1c2subject1/961.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (962, 962, '如图所示,驾驶机动车在这种情况下,当C车减速让超车时,A车应该如何行驶?', 1, '放弃超越C车', '加速超越C车', '鸣喇叭示意B车让行后超车', '直接向左变更车道,迫使B车让行', '因为C车减速让超车的同时又来向B车进行会车,此时继续超车容易造成危险,所以应当放弃超越C车.', 'http://images.juheapi.com/jztk/c1c2subject1/962.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (963, 963, '使用伪造、变造的行驶证一次记几分?', 4, '6分', '3分', '2分', '12分', '使用伪造、变造的机动车号牌、行驶证、驾驶证、校车标牌或者使用其他机动车号牌、行驶证的一次记12分.', , 0);
INSERT INTO ""."examination_question1" VALUES (964, 964, '申请人以不正当手段取得机动车驾驶证的,公安机关交通管理部门收缴机动车驾驶证,撤销机动车驾驶许可,申请人在3年内不得再次申领机动车驾驶证.', 1, '正确', '错误', , , '《中华人民共和国道路运输条例》说明,申请人以不正当手段取得机动车驾驶证的,公安机关交通管理部门收缴机动车驾驶证,撤销机动车驾驶许可,申请人在3年内不得再次申领机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (965, 965, '驾驶机动车时接打电话容易引发事故,以下原因错误的是什么?', 3, '单手握方向盘,对机动车控制力下降', '驾驶人注意力不集中,不能及时判断危险', '电话的信号会对汽车电子设备的运行造成干扰', '驾驶人对路况观察不到位,容易导致操作失误', '驾驶机动车时接打电话容易引发事故,主要是因为接电话时单手握方向盘,对机动车控制力下降；驾驶人注意力不集中,不能及时判断危险；驾驶人对路况观察不到位,容易导致操作失误.', , 0);
INSERT INTO ""."examination_question1" VALUES (966, 966, '机动车驾驶证遗失的,机动车驾驶人应当向哪里的车辆管理所申请补发?', 1, '核发地', '暂住地', '居住地', '户籍地', '驾驶证遗失、损毁无法辨认时驾驶员应当向驾驶证核发地车辆管理所申请补发.', , 0);
INSERT INTO ""."examination_question1" VALUES (967, 967, '驾驶人在驾驶证有效期满前多长时间申请换证?', 3, '60日内', '30日内', '90日内', '6个月内', '《机动车驾驶证申领和使用规定》（公安部令第123号）第四十八条  机动车驾驶人应当于机动车驾驶证有效期满前九十日内,向机动车驾驶证核发地车辆管理所申请换证.', , 0);
INSERT INTO ""."examination_question1" VALUES (968, 968, '已注册登记的机动车,改变车身颜色,机动车所有人不需要向登记地车辆管理所申请变更登记.', 2, '正确', '错误', , , '《中华人民共和国道路运输条例》说明,已注册登记的机动车,改变机动车车身颜色的应到公安交通管理部门申请变更登记.', , 0);
INSERT INTO ""."examination_question1" VALUES (969, 969, '造成交通事故后逃逸,尚不构成犯罪的一次记12分.', 1, '正确', '错误', , , '发生事故一定别跑,你是跑不掉的.造成交通事故后逃逸,尚不构成犯罪的一次记12分.', , 0);
INSERT INTO ""."examination_question1" VALUES (970, 970, '驾驶人将机动车交给驾驶证被暂扣的人驾驶的,交通警察给予口头警告.', 2, '正确', '错误', , , '规定是:驾驶人将机动车交给驾驶证被暂扣的人驾驶的将被处200元以上2000元以下罚款,可以并处吊销机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (971, 971, '驾驶机动车与行人之间发生交通事故造成人身伤亡、财产损失的,机动车一方没有过错的,不承担赔偿责任.', 2, '正确', '错误', , , '非机动车之间、非机动车与行人之间发生交通事故造成人身伤亡、财产损失的,当事人对造成损害都没有过错的,可以根据实际情况,由当事人分担赔偿责任.', , 0);
INSERT INTO ""."examination_question1" VALUES (972, 972, '这个标志是何含义?', 3, '向左绕行', '连续弯路', '向左急转弯', '向右急转弯', '向左急转弯:向左急弯路标志设在左急转弯的道路前方适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/972.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (973, 973, '机动车驾驶人有以下哪种违法行为的,暂扣六个月机动车驾驶证?', 3, '醉酒后驾驶机动车的', '伪造、变造机动车驾驶证的', '饮酒后驾驶机动车的', '使用伪造、变造机动车驾驶证的', '饮酒驾驶机动车辆,罚款1000元?2000元、记12分并暂扣驾照6个月；饮酒驾驶营运机动车,罚款5000元,记12分,处以15日以下拘留,并且5年内不得重新获得驾照.醉酒驾驶机动车辆,吊销驾照,5年内不得重新获取驾照,经过判决后处以拘役,并处罚金；醉酒驾驶营运机动车辆,吊销驾照,10年内不得重新获取驾照,终生不得驾驶营运车辆,经过判决后处以拘役,并处罚金.', , 0);
INSERT INTO ""."examination_question1" VALUES (974, 974, '以下哪种身体条件,不可以申请机动车驾驶证?', 2, '糖尿病', '红绿色盲', '高血压', '怀孕', '红绿色盲会影响观察红绿灯,不可以申请机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (975, 975, '这个标志是何含义?', 3, '靠右侧行驶', '不允许直行', '直行单行路', '直行车让行', '表示一切车辆单向行驶.此标志设在单行路的路口和入口处的适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/975.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (976, 976, '饮酒后或者醉酒驾驶机动车发生重大交通事故构成犯罪的,依法追究刑事责任,吊销机动车驾驶证,将多少年内不得申请机动车驾驶证?', 4, '五年', '十年', '二十年', '终生', '饮酒驾驶机动车辆,罚款1000元?2000元、记12分并暂扣驾照6个月；饮酒驾驶营运机动车,罚款5000元,记12分,处以15日以下拘留,并且5年内不得重新获得驾照.醉酒驾驶机动车辆,吊销驾照,5年内不得重新获取驾照,经过判决后处以拘役,并处罚金；醉酒驾驶营运机动车辆,吊销驾照,10年内不得重新获取驾照,终生不得驾驶营运车辆,经过判决后处以拘役,并处罚金.', , 0);
INSERT INTO ""."examination_question1" VALUES (977, 977, '机动车驾驶人血液中酒精含量大于或者等于多少可认定为醉驾?', 3, '20毫克/100毫升', '60毫克/100毫升', '80毫克/100毫升', '50毫克/100毫升', '机动车驾驶人血液中酒精含量大于或者等于80毫克/100毫升会被判定为醉驾.', , 0);
INSERT INTO ""."examination_question1" VALUES (978, 978, '机动车驾驶人初次申请机动车驾驶证和增加准驾车型后的多长时间为实习期?', 2, '6个月', '12个月', '2年', '3个月', '常识题', , 0);
INSERT INTO ""."examination_question1" VALUES (979, 979, '驾驶机动车碰撞建筑物、公共设施后可即行撤离现场.', 2, '正确', '错误', , , '当然不行的.要负责任的亲.', , 0);
INSERT INTO ""."examination_question1" VALUES (980, 980, '如图所示,驾驶机动车行经该路段时,应减速慢行,避免因眩目导致的交通事故.', 1, '正确', '错误', , , '该路段为隧道出口,驾驶机动车行经该路段时,应减速慢行,避免因光线突然变化,阳光眩目导致的交通事故.', 'http://images.juheapi.com/jztk/c1c2subject1/980.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (981, 981, '如图所示,驾驶机动车遇到这种情况,不仅要控制车辆留出会车空间,而且要注意与右侧的儿童保持足够的安全距离.', 1, '正确', '错误', , , '窄路路边有儿童,这种情况应该控制车速为会车留出足够的空间,同时注意与右侧的儿童保持足够的安全距离.', 'http://images.juheapi.com/jztk/c1c2subject1/981.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (982, 982, '机动车在高速公路上发生故障时,将车上人员迅速转移到右侧路肩上或者应急车道内,并且迅速报警.', 1, '正确', '错误', , , '一定要移到里边,放着车道上太危险.', , 0);
INSERT INTO ""."examination_question1" VALUES (983, 983, '驾驶人有下列哪种违法行为一次记12分?', 2, '违反交通信号灯', '使用伪造机动车号牌', '拨打、接听手机的', '违反禁令标志指示', '使用伪造、变造的机动车号牌、行驶证、驾驶证、校车标牌或者使用其他机动车号牌、行驶证的一次记12分.', , 0);
INSERT INTO ""."examination_question1" VALUES (984, 984, '道路交通事故中,驾驶人有饮酒、醉酒嫌疑时,要保护现场并立即报警.', 1, '正确', '错误', , , '就该对这些人给予教训,不仅拿自己的生命开玩笑,还拿别人的生命开玩笑,报警给予丫惩罚.', , 0);
INSERT INTO ""."examination_question1" VALUES (985, 985, '驾驶机动车造成交通事故后逃逸,尚不构成犯罪的,由公安机关交通管理部门处二百元以上二千元以下罚款,可以并处15日以下拘留.', 1, '正确', '错误', , , '造成交通事故后逃逸,尚不构成犯罪的由公安机关交通管理部门处二百元以上二千元以下罚款,可以并处15日以下拘留.', , 0);
INSERT INTO ""."examination_question1" VALUES (986, 986, '驾驶机动车遇到这种信号灯,可在对面直行车前直接向左转弯.', 2, '正确', '错误', , , '左转让直行.', 'http://images.juheapi.com/jztk/c1c2subject1/986.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (987, 987, '机动车达到国家规定的强制报废标准的不能办理注册登记.', 1, '正确', '错误', , , '常识题', , 0);
INSERT INTO ""."examination_question1" VALUES (988, 988, '驾驶机动车在高速公路上车辆发生故障时,若车辆可以移动至应急车道内,只需开启危险报警闪光灯,警告标志可根据交通流情况选择是否放置.', 2, '正确', '错误', , , '驾驶机动车在高速公路上车辆发生故障时,若车辆可以移动至应急车道内,应开启危险报警闪光灯,放置警告标志.', , 0);
INSERT INTO ""."examination_question1" VALUES (989, 989, '这个标志是何含义?', 2, '左侧变窄', '窄桥', '窄路', '右侧变窄', '窄桥:用以警告车辆驾驶人注意前方桥面宽度变窄,应谨慎驾驶.设在桥面净宽较两端路面宽度变窄,且桥的净宽小于6 m的桥梁以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/989.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (990, 990, '这个标志是何含义?', 4, '高速公路缴费车道', '高速公路检查车道', '高速公路领卡车道', '高速公路ETC车道', '高速公路ETC车道:用于指示电子不停车收费车道,设在收费广场渐变段起点前300 m处.表示单向3个收费车道时ETC车道指示标志.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/990.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (991, 991, '这个标志的含义是提醒车辆驾驶人前方是单向行驶并且照明不好的涵洞.', 2, '正确', '错误', , , '隧道:用以提醒车辆驾驶人注意慢行.设在双向行驶并且照明不好的隧道口前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/991.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (992, 992, '有吸食、注射毒品后驾驶驾车行为的机动车驾驶人,不会被注销驾驶证.', 2, '正确', '错误', , , '被查获有吸食、注射毒品后驾驶机动车行为,正在执行社区戒毒、强制隔离戒毒、社区康复措施,或者长期服用依赖性精神药品成瘾尚未戒除的,车辆管理所应当注销其机动车驾驶证.三年内有吸食、注射毒品行为,或者解除强制隔离戒毒措施未满三年,或者长期服用依赖性精神药品成瘾尚未戒除的,不得申请机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (993, 993, '这个标志是何含义?', 3, '高速公路客车站', '高速公路休息区', '高速公路加油站', '高速公路服务区', '高速公路加油站:用于指示加油站的位置,设在通往加油站的入口附近.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/993.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (994, 994, '这种停车标线含义是什么?', 4, '专用待客停车位', '专用上下客停车位', '机动车限时停车位', '固定停车方向停车位', '这是停车位,车头要按箭头位置,停反了肯定会有人找你事儿~~', 'http://images.juheapi.com/jztk/c1c2subject1/994.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (995, 995, '路中心的双黄实线属于哪一类标线?', 3, '辅助标线', '警告标志', '禁止标线', '指示标线', '禁止压线哦亲~~', 'http://images.juheapi.com/jztk/c1c2subject1/995.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (996, 996, '驾驶机动车在上道路行驶前驾驶人要按规定系好安全带.', 1, '正确', '错误', , , '这个没问题,都得系安全带.', , 0);
INSERT INTO ""."examination_question1" VALUES (997, 997, '《道路交通安全法实施条例》规定,高速公路上最高时速不得超过120公里.因此在高速公路上行驶只要时速不超过120公里就不违法.', 2, '正确', '错误', , , '《道路交通安全法实施条例》规定高速公路,有最低速度60公里,最高速度120公里,这道题只说了不超过120公里,没说不能低于60公里. 所以是错的~', , 0);
INSERT INTO ""."examination_question1" VALUES (998, 998, '在这段高速公路上行驶的最高车速是多少?', 4, '60公里/小时', '90公里/小时', '100公里/小时', '120公里/小时', '那个120的牌子就是最高车速限制.', 'http://images.juheapi.com/jztk/c1c2subject1/998.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (999, 999, '车辆在主干道上行驶,驶近主支干道交汇处时,为防止与从支路突然驶入的车辆相撞,应怎样做?', 2, '保持正常速度行驶', '提前减速、观察,谨慎驾驶', '鸣喇叭,迅速通过', '提前加速通过', '常识题,安全第一.', , 0);
INSERT INTO ""."examination_question1" VALUES (1000, 1000, '这种白色矩形标线框含义是什么?', 2, '出租车专用上下客停车位', '平行式停车位', '倾斜式停车位', '垂直式停车位', '路边的平行的停车位,亲 侧方停车以后要好好学哦~', 'http://images.juheapi.com/jztk/c1c2subject1/1000.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1001, 1001, '车辆不得在高速公路匝道上掉头.', 1, '正确', '错误', , , '规定:驶入确定匝道后,迅速提高车速,但不得超过规定速度,在匝道上,不准超车、掉头、停车、和倒车.', , 0);
INSERT INTO ""."examination_question1" VALUES (1002, 1002, '机动车在高速公路上遇前方交通受阻时,应当跟随前车顺序排队,并立即开启危险报警闪光灯,防止追尾.', 1, '正确', '错误', , , '危险报警闪光灯,通常称为双闪,是一种提醒其他车辆与行人注意本车发生了特殊情况的信号灯.', , 0);
INSERT INTO ""."examination_question1" VALUES (1003, 1003, '机动车在道路上发生故障,难以移动时下列做法正确的是什么?', 1, '开启危险报警闪光灯', '开启车上所有灯光', '禁止车上人员下车', '在车前方设置警告标志', '机动车在道路上发生故障,难以移动时应该立即把车上的人疏散到安全栏外,开启危险报警闪光灯,在车的后方设置警告标志.', , 0);
INSERT INTO ""."examination_question1" VALUES (1004, 1004, '车辆在交叉路口绿灯亮后,遇非机动车抢道行驶时,可以不让行.', 2, '正确', '错误', , , '只要是非机动车、行人、牲口,什么时候都得让行.', , 0);
INSERT INTO ""."examination_question1" VALUES (1005, 1005, '遇到这种情况时,中间车道不允许车辆通行.', 1, '正确', '错误', , , '中间车道是红灯故不允许车辆通行.', 'http://images.juheapi.com/jztk/c1c2subject1/1005.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1006, 1006, '车辆在高速公路以每小时100公里的速度行驶时,距同车道前车100米以上为安全距离.', 1, '正确', '错误', , , '横向是1.5米最安全,跟前车100米是安全距离.', , 0);
INSERT INTO ""."examination_question1" VALUES (1007, 1007, '驾驶机动车在高速公路上倒车、逆行、穿越中央分隔带掉头的一次记6分.', 2, '正确', '错误', , , '驾驶机动车在高速公路上倒车、逆行、穿越中央分隔带掉头的一次记12分.', , 0);
INSERT INTO ""."examination_question1" VALUES (1008, 1008, '驾驶人在实习期内驾驶机动车时,应当在车身后部粘贴或者悬挂统一式样的实习标志.', 1, '正确', '错误', , , '驾驶机动车未按规定粘贴、悬挂实习标志或者残疾人机动车专用标志的；由公安机关交通管理部门处二十元以上二百元以下罚款', , 0);
INSERT INTO ""."examination_question1" VALUES (1009, 1009, '这样停放机动车什么违法行为?', 3, '在非机动车道停车', '在有禁停标志路段停车', '在公共汽车站停车', '停车占用人行道', '图为公共汽车站.公共汽车站、急救站、加油站、消防栓或者消防队（站）门前以及距离上述地点３０米以内的路段,除使用上述设施的以外,不得停车.', 'http://images.juheapi.com/jztk/c1c2subject1/1009.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1010, 1010, '机动车停稳前不能开车门和上下人员.', 1, '正确', '错误', , , '安全第一.', , 0);
INSERT INTO ""."examination_question1" VALUES (1011, 1011, '驾驶机动车在高速公路遇到能见度低于200米的气象条件时,最高车速是多少?', 3, '不得超过100公里/小时', '不得超过90公里/小时', '不得超过60公里/小时', '不得超过80公里/小时', '规定,记住吧.', , 0);
INSERT INTO ""."examination_question1" VALUES (1012, 1012, '在这种情形中前车怎样行驶?', 2, '正常行驶', '及时让行', '开启危险报警闪光灯行驶', '不得变更车道', '警察叔叔在执行公务,我们应该礼让.', 'http://images.juheapi.com/jztk/c1c2subject1/1012.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1013, 1013, '遇后车发出超车信号后,只要具备让超条件应怎样做?', 2, '靠道路右侧加速行驶', '主动减速并靠右侧行驶', '让出适当空间加速行驶', '迅速减速或紧急制动', '文明礼让,和谐驾驶.', , 0);
INSERT INTO ""."examination_question1" VALUES (1014, 1014, '机动车驾驶人在实习期内有记满12分记录的,注销其实习的准驾车型驾驶资格.', 1, '正确', '错误', , , '规定.', , 0);
INSERT INTO ""."examination_question1" VALUES (1015, 1015, '驾驶机动车在高速公路违法占用应急车道行驶的一次记6分.', 1, '正确', '错误', , , '新年新规定,记住吧.', , 0);
INSERT INTO ""."examination_question1" VALUES (1016, 1016, '驾驶机动车在这种信号灯亮的路口,可以右转弯.', 1, '正确', '错误', , , '没有问题,只要安全的情况下就可以右转,前边的红灯是管直行的.', 'http://images.juheapi.com/jztk/c1c2subject1/1016.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1017, 1017, '夜间驾驶机动车在窄路、窄桥会车怎样使用灯光?', 2, '关闭所有灯光', '开启近光灯', '关闭前照灯', '开启远光灯', '记住,只有在没有路灯或者没有照明的情况下才能用远光灯,一般都用近光灯.', , 0);
INSERT INTO ""."examination_question1" VALUES (1018, 1018, '驾驶车辆进入交叉路口前,应降低行驶速度,注意观察,确认安全.', 1, '正确', '错误', , , '常识题,必须遵守的安全行车准则.', , 0);
INSERT INTO ""."examination_question1" VALUES (1019, 1019, '行车中前方遇自行车影响通行时,可鸣喇叭提示,加速绕行.', 2, '正确', '错误', , , '减速紧随其后,道路宽度允许通过时谨慎通过.', , 0);
INSERT INTO ""."examination_question1" VALUES (1020, 1020, '驾驶机动车通过窄路、窄桥时,最高速度不能超过多少?', 3, '50公里/小时', '40公里/小时', '30公里/小时', '60公里/小时', '规定:驾驶机动车通过窄路、窄桥时,最高速度不能超过30公里/小时', , 0);
INSERT INTO ""."examination_question1" VALUES (1021, 1021, '在这个路口可以掉头.', 2, '正确', '错误', , , '都禁止左转了,你还掉什么头啊亲.', 'http://images.juheapi.com/jztk/c1c2subject1/1021.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1022, 1022, '夜间起步前,应当先开启近光灯.', 1, '正确', '错误', , , '先开近光灯,把车前路面的状况给看清楚,是否有坑啊什么的！在确保安全后开车,所以是近光灯.', , 0);
INSERT INTO ""."examination_question1" VALUES (1023, 1023, '在山区道路超车时,应怎样超越?', 2, '抓住任何机会尽量', '选择宽阔的缓上坡路段', '选择较长的下坡路', '选择较缓的下坡路', '上坡有阻力,不宜滑车,也不宜撞车,所以上坡宽阔地可以超车！', , 0);
INSERT INTO ""."examination_question1" VALUES (1024, 1024, '在高速公路变更车道时,应提前开启转向灯,观察情况,确认安全后,驶入需要变更的车道.', 1, '正确', '错误', , , '安全第一,没问题.', , 0);
INSERT INTO ""."examination_question1" VALUES (1025, 1025, '驾驶车辆时,长时间左臂搭在车门窗上,或者长时间右手抓住变速器操纵杆,是一种驾驶陋习.', 1, '正确', '错误', , , '这是一种陋习,但还算不上是违法.因为这样久了,会导致血脉不通,手臂麻木,容易出事！', , 0);
INSERT INTO ""."examination_question1" VALUES (1026, 1026, '驾驶机动车找不到停车位时可以借人行道停放.', 2, '正确', '错误', , , '这个当然是不可以的,人行横道禁止停车.', , 0);
INSERT INTO ""."examination_question1" VALUES (1027, 1027, '在道路上发生未造成人员伤亡且无争议的轻微交通事故如何处置?', 3, '不要移动车辆', '疏导其他车辆绕行', '撤离现场自行协商', '保护好现场再协商', '在道路上发生未造成人员伤亡且无争议的就赶紧闪开吧,别堵路了.', , 0);
INSERT INTO ""."examination_question1" VALUES (1028, 1028, '机动车在高速公路上发生故障或交通事故无法正常行驶时由什么车拖曳或牵引?', 4, '过路车', '大客车', '同行车', '清障车', '应该有专门的清障车来拖拽和牵引.', , 0);
INSERT INTO ""."examination_question1" VALUES (1029, 1029, '驾驶人一边驾车,一边吸烟对安全行车无影响.', 2, '正确', '错误', , , '一心两用妨碍安全驾驶,容易发生危险.', , 0);
INSERT INTO ""."examination_question1" VALUES (1030, 1030, '驾驶人有使用其他车辆保险标志嫌疑的,交通警察可依法扣留车辆.', 1, '正确', '错误', , , '交通警察可依法扣留 车辆,等他们查清楚车的来由,当场教导让其车在指定时间内接受车辆检验并放行.', , 0);
INSERT INTO ""."examination_question1" VALUES (1031, 1031, '这辆红色轿车变更车道的方法和路线是正确的.', 2, '正确', '错误', , , '红车明显就是没留出足够安全的行车距离强行并线,也没打灯.', 'http://images.juheapi.com/jztk/c1c2subject1/1031.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1032, 1032, '机动车在高速公路上发生故障时错误的做法是什么?', 2, '按规定设置警告标志', '车上人员不能下车', '迅速报警', '开启危险报警闪光灯', '车上的人员应该立即撤到安全栏外.', , 0);
INSERT INTO ""."examination_question1" VALUES (1033, 1033, '行车中遇残疾人影响通行时,应主动减速礼让.', 1, '正确', '错误', , , '要是不让,撞了在导致残疾,残疾再加上残疾,估计人家都不活了.', , 0);
INSERT INTO ""."examination_question1" VALUES (1034, 1034, '夜间驾驶机动车在窄路、窄桥会车时正确的做法是使用远光灯.', 2, '正确', '错误', , , '记住,只有在没有路灯或者没有照明的情况下才能用远光灯,一般都用近光灯.', , 0);
INSERT INTO ""."examination_question1" VALUES (1035, 1035, '在这段道路上不能掉头.', 1, '正确', '错误', , , '黄色实线是不可以压的,况且是弯道不能掉头.', 'http://images.juheapi.com/jztk/c1c2subject1/1035.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1036, 1036, '将点火开关转到ACC位置起动机工作.', 2, '正确', '错误', , , '点火锁开关档位设置方式.其含义是:LOCK、切断电源,锁定方向盘；ACC、接通附件电源（比如收音机等附件）ON、接通除起动机外的全车全部电源；START、接通起动机电源,起动发动机. （起动后松手钥匙,会自动弹回ON档位）供你参考.', 'http://images.juheapi.com/jztk/c1c2subject1/1036.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1037, 1037, '行车中当驾驶人意识到车辆爆胎时,应在控制住方向的情况下采取紧急制动,迫使车辆迅速停住.', 2, '正确', '错误', , , '不要紧急刹车,容易出现侧翻事故的,应该是握紧方向,同时控制车速,慢慢停车.', , 0);
INSERT INTO ""."examination_question1" VALUES (1038, 1038, '驶入高速公路的收费口时,应选择怎样的入口?', 3, '车辆多', '红灯亮', '绿灯亮', '暂停服务', '跟红灯停绿灯行一个样,不解释.', , 0);
INSERT INTO ""."examination_question1" VALUES (1039, 1039, '通过急转弯路段时,在车辆较少的情况下可以超车.', 2, '正确', '错误', , , '危险动作,你不知道急转弯后面的路况,如果突然从对方车道窜出一辆车,不是两车相撞了?', , 0);
INSERT INTO ""."examination_question1" VALUES (1040, 1040, '在这段道路上,只要不影响其他车辆通行的前提下可以掉头.', 1, '正确', '错误', , , '黄色虚线,是可以掉头的,如果是实线就不可以了.', 'http://images.juheapi.com/jztk/c1c2subject1/1040.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1041, 1041, '驾驶机动车超车后立即开启右转向灯驶回原车道.', 2, '正确', '错误', , , '应该跟后车有了安全距离并且确保安全的情况下在开启右转向灯驶回原车道.', , 0);
INSERT INTO ""."examination_question1" VALUES (1042, 1042, '行车中,发现行人突然横过道路时,应迅速减速避让.', 1, '正确', '错误', , , '碰到行人的一定避让,减速,甚至停车.', , 0);
INSERT INTO ""."examination_question1" VALUES (1043, 1043, '驾驶机动车行经下列哪种路段时不得超车?', 2, '高架路', '交叉路口', '中心街道', '环城高速', '《中华人民共和国道路交通管理条例》第六章 第五十条 机动车超车,必须遵守下列规定:行经交叉路口、人行横道、漫水路、漫水桥等,不准超车.', , 0);
INSERT INTO ""."examination_question1" VALUES (1044, 1044, '遇到这种情况下可以从右侧超车.', 2, '正确', '错误', , , '超车只能从左侧超车.', 'http://images.juheapi.com/jztk/c1c2subject1/1044.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1045, 1045, '通过山区危险路段,尤其是通过经常发生塌方、泥石流的山区地段,应谨慎驾驶,避免停车.', 1, '正确', '错误', , , '是对的！谨慎驾驶 有塌方 就避免停车 ！！！ 又不是说不能停车,只是避免！', , 0);
INSERT INTO ""."examination_question1" VALUES (1046, 1046, '机动车仪表板上（如图所示）亮,提示发动机舱开启.', 2, '正确', '错误', , , '应该可以看出是行李箱（后备箱）吧.', 'http://images.juheapi.com/jztk/c1c2subject1/1046.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1047, 1047, '下长坡时,控制车速除了刹车制动以外还有什么有效的辅助方法?', 2, '挂入空挡滑行', '利用发动机制动', '关闭发动机熄火滑行', '踏下离合器滑行', '发动机制动就是挂低档,让车利用低档位的惯性制动,你可以试一下,你在3挡的速度,直接挂1挡,记住不要踩刹车,直接挂档,你看效果如何,车身会震一下,速度下降的很快', , 0);
INSERT INTO ""."examination_question1" VALUES (1048, 1048, '如何通过这种交叉路口?', 3, '保持速度通过', '鸣笛催促', '减速慢行', '加速通过', '这么多的人要减速慢行啊,安全第一.', 'http://images.juheapi.com/jztk/c1c2subject1/1048.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1049, 1049, '驾驶机动车超车应该提前开启左转向灯、变换使用远近光灯或鸣喇叭.', 1, '正确', '错误', , , '这个没有问题.', , 0);
INSERT INTO ""."examination_question1" VALUES (1050, 1050, '在这种环境里行车使用近光灯.', 1, '正确', '错误', , , '近光灯不会影响人家的视线.', 'http://images.juheapi.com/jztk/c1c2subject1/1050.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1051, 1051, '驾驶机动车在沙尘天气条件下行车不用开启前照灯、示廓灯和后位灯.', 2, '正确', '错误', , , '沙尘天气条件下能见度低,要开启前照灯、示廓灯和后位灯防止出现事故.', , 0);
INSERT INTO ""."examination_question1" VALUES (1052, 1052, '在这个路口左转弯选择哪条车道?', 1, '最左侧车道', '中间车道', '不用变道', '最右侧车道', '只有最左侧车道有左转的标志,按照标志行车没错.', 'http://images.juheapi.com/jztk/c1c2subject1/1052.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1053, 1053, '倒车过程中要缓慢行驶,注意观察车辆两侧和后方的情况,随时做好停车准备.', 1, '正确', '错误', , , '相同车道上前面是没车,车都是从后面来的.另外题目中并没有说不要看前面,只是说注意观察两侧和后方的情况.', , 0);
INSERT INTO ""."examination_question1" VALUES (1054, 1054, '行车中遇到后方车辆要求超车时,应怎样做?', 2, '保持原有车速行驶', '及时减速、观察后靠右行驶让行', '靠右侧加速行驶', '不让行', '基于安全和和谐的因素,所以要及时减速让行.', , 0);
INSERT INTO ""."examination_question1" VALUES (1055, 1055, '驾驶机动车在没有交通信号的路口遇到前方车辆缓慢行驶时要依次交替通行.', 1, '正确', '错误', , , '这个没问题.', , 0);
INSERT INTO ""."examination_question1" VALUES (1056, 1056, '同车道行驶的车辆遇前车有下列哪种情形时不得超车?', 3, '减速让行', '正常行驶', '正在超车', '正在停车', '同车道行驶的机动车,后车应当与前车保持足以采取紧急制动措施的安全距离.有下列情形之一的,不得超车:  ①《《前车正在左转弯、掉头、超车的》》;  ②与对面来车有会车可能的;  ③前车为执行紧急任务的警车、消防车、救护车、工程救险车的;  ④行经铁路道口、交叉路口、窄桥、弯道、陡坡、隧道、人行横道、市区交通流量大的路段等没有超车条件的.', , 0);
INSERT INTO ""."examination_question1" VALUES (1057, 1057, '（如图所示）这个符号的开关控制什么装置?', 3, '前风窗玻璃刮水器及洗涤器', '后风窗玻璃除霜或除雾', '后风窗玻璃刮水器及洗涤器', '前风窗玻璃除霜或除雾', '此为后风窗玻璃刮水器及洗涤器.关于如何区分前风窗玻璃刮水器及洗涤器和后风窗玻璃刮水器及洗涤器,你只需记住一点半弧形的图标是前风窗,四四方方的图标就是后风窗.', 'http://images.juheapi.com/jztk/c1c2subject1/1057.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1058, 1058, '通过铁路道口时,不得超车.', 1, '正确', '错误', , , '安全第一！通过铁道路口,隧道,窄桥,弯道都是不可以超车的.', , 0);
INSERT INTO ""."examination_question1" VALUES (1059, 1059, '车辆在下坡行驶时,可充分利用空挡滑行.', 2, '正确', '错误', , , '危险动作,下坡时,不准熄火或用空挡.如果下坡空档滑行,只靠刹车来控制车速,遇到连续下坡的时候可能会造成刹车负担过重,刹车片过热而失灵.', , 0);
INSERT INTO ""."examination_question1" VALUES (1060, 1060, '机动车仪表板上（如图所示）一直亮,表示安全气囊处于工作状态.', 2, '正确', '错误', , , '此图表示安全气囊现在处于故障状态.', 'http://images.juheapi.com/jztk/c1c2subject1/1060.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1061, 1061, '机动车驾驶人在实习期内驾驶机动车不得牵引挂车.', 1, '正确', '错误', , , '常识题,小车都开不好,还要牵引挂车,安全第一.', , 0);
INSERT INTO ""."examination_question1" VALUES (1062, 1062, '驾驶机动车在道路上靠路边停车过程中如何使用灯光?', 2, '开启危险报警闪光灯', '提前开启右转向灯', '变换使用远近光灯', '不用指示灯提示', '靠边停车首先开启右转向灯然后再打方向盘停车.', , 0);
INSERT INTO ""."examination_question1" VALUES (1063, 1063, '车辆在高速公路匝道提速到每小时60公里以上时,可直接驶入行车道.', 2, '正确', '错误', , , '应该是将车速提高到80公里每小时,开启左转向灯,观察后面无车后在驶入行车道.进入高速公路时才是60公里.', , 0);
INSERT INTO ""."examination_question1" VALUES (1064, 1064, '遇到路口情况复杂时,应做到“宁停三分,不抢一秒”.', 1, '正确', '错误', , , '这个没有问题,安全第一.', , 0);
INSERT INTO ""."examination_question1" VALUES (1065, 1065, '驾驶机动车在道路上超车完毕驶回原车道时开启右转向灯.', 1, '正确', '错误', , , '转向灯就是行车过程中的语言,一定要遵守规则.', , 0);
INSERT INTO ""."examination_question1" VALUES (1066, 1066, '（如图所示）这个符号的开关控制什么装置?', 1, '儿童安全锁', '两侧车窗玻璃', '电动车门', '车门锁住开锁', '你看里边长的多像一个小孩,故选A最靠谱.', 'http://images.juheapi.com/jztk/c1c2subject1/1066.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1067, 1067, '通过窄路、窄桥时,不得超车.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法》第四十三条 同车道行驶的机动车,后车应当与前车保持足以采取紧急制动措施的安全距离.有下列情形之一的,不得超车:（四）行经铁路道口、交叉路口、窄桥、弯道、陡坡、隧道、人行横道、市区交通流量大的路段等没有超车条件的. 通过隧道,铁路道口,急转弯路段,窄路,窄桥或遇前方道路上有车辆掉头时,不得超车.', , 0);
INSERT INTO ""."examination_question1" VALUES (1068, 1068, '在这种天气条件下行车如何使用灯光?', 2, '使用远光灯', '使用雾灯', '不使用灯光', '开启右转向灯', '能见度这么低一定要开启雾灯.', 'http://images.juheapi.com/jztk/c1c2subject1/1068.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1069, 1069, '车辆驶入双向行驶隧道前,应开启什么灯?', 1, '示廓灯或近光灯', '雾灯', '远光灯', '危险报警闪光灯', '凡是车辆进入双向行驶隧道,一般都应该开启近光灯,而不要开启远光灯.若是隧道本身没有灯光的,可以变换灯光行驶,但是会车前要转为近光灯.', , 0);
INSERT INTO ""."examination_question1" VALUES (1070, 1070, '在这段路的最高时速为每小时50公里.', 2, '正确', '错误', , , '看右侧的标志,此路段最高车速是80公里每小时,最低车速是50公里每小时.', 'http://images.juheapi.com/jztk/c1c2subject1/1070.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1071, 1071, '当驾驶车辆行经两侧有非机动车行驶且有积水的路面时,应怎样做?', 1, '减速慢行', '正常行驶', '加速通过', '连续鸣喇叭', '常识题,减速可以防止溅水,按喇叭有提醒的作用,但不能防止溅水. 想想我们没车的时候是不是很讨厌那些在积水路面开快车的?好吧,我们还是不要做让别人讨厌的人了吧！', , 0);
INSERT INTO ""."examination_question1" VALUES (1072, 1072, '行驶车道绿灯亮时,但车辆前方人行横道仍有行人行走,应怎样做?', 3, '直接起步通过', '起步后从行人后方绕过', '等行人通过后再起步', '起步后从行人前方绕过', '以人为本,安全第一,礼让行人.', , 0);
INSERT INTO ""."examination_question1" VALUES (1073, 1073, '这个标志的含义是提醒车辆驾驶人前方是堤坝路段.', 2, '正确', '错误', , , '傍山险路:用以提醒车辆驾驶人小心驾驶.设在傍山险路路段以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/1073.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1074, 1074, '机动车仪表板上（如图所示）亮,提示右侧车门未关闭.', 2, '正确', '错误', , , '很明显他是左侧的', 'http://images.juheapi.com/jztk/c1c2subject1/1074.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1075, 1075, '驾驶机动车通过没有交通信号和管理人员的铁路道口怎样通过?', 3, '适当减速通过', '空挡滑行通过', '停车确认安全后通过', '加速尽快通过', '别跟火车过意不去,停车确认安全后在通过.', , 0);
INSERT INTO ""."examination_question1" VALUES (1076, 1076, '雨天行车,遇撑雨伞和穿雨衣的行人在公路上行走时,应怎样做?', 3, '持续鸣喇叭示意其让道', '加速绕行', '提前鸣喇叭,并适当降低车速', '以正常速度行驶', '只有C最和谐.', , 0);
INSERT INTO ""."examination_question1" VALUES (1077, 1077, '驾驶机动车在高速公路遇到能见度低于100米的气象条件时,最高车速是多少?', 1, '不得超过40公里/小时', '不得超过60公里/小时', '不得超过80公里/小时', '不得超过90公里/小时', '这是规定,没什么好解释的.', , 0);
INSERT INTO ""."examination_question1" VALUES (1078, 1078, '在这种情况的交叉路口转弯要让直行车先行.', 1, '正确', '错误', , , '左转让直行.', 'http://images.juheapi.com/jztk/c1c2subject1/1078.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1079, 1079, '设有安全带装置的车辆,应要求车内乘员系安全带.', 1, '正确', '错误', , , '请认真看题,是“设有”,不是“没有”.', , 0);
INSERT INTO ""."examination_question1" VALUES (1080, 1080, '行车中从其他道路汇入车流前,应注意观察侧后方车辆的动态.', 1, '正确', '错误', , , '这题正确的.', , 0);
INSERT INTO ""."examination_question1" VALUES (1081, 1081, '夜间会车时,若对方车辆不关闭远光灯,可变换灯光提示对向车辆,同时减速靠右侧行驶或停车.', 1, '正确', '错误', , , '夜间安全驾驶知识中第三条就有:“夜间会车应当在距对方来车150米以外改用近光灯,若对方车辆不关闭远光灯,可连续变换灯光提示对向车辆.当遇对面来车仍不关闭远光灯是,应及时减速靠右侧行驶或停车让行.”', , 0);
INSERT INTO ""."examination_question1" VALUES (1082, 1082, '驾驶机动车在道路上掉头时提前开启左转向灯.', 1, '正确', '错误', , , '这个没有问题.', , 0);
INSERT INTO ""."examination_question1" VALUES (1083, 1083, '机动车仪表板上（如图所示）亮时,表示驻车制动器处于制动状态.', 1, '正确', '错误', , , '正确,驻车制动器俗称手刹.', 'http://images.juheapi.com/jztk/c1c2subject1/1083.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1084, 1084, '驾驶人发现轮胎漏气,将车辆驶离主车道时,不要采用紧急制动,以免造成翻车或后车采取制动不及时导致追尾事故.', 1, '正确', '错误', , , '连续轻踏制动,缓慢将车停下,更换轮胎.', , 0);
INSERT INTO ""."examination_question1" VALUES (1085, 1085, '车辆行至泥泞或翻浆路段时,应停车观察,选择平整、坚实的路段缓慢通过.', 1, '正确', '错误', , , '常识题,注意选择比较坚硬的地方通行,不要选择泥泞地方.', , 0);
INSERT INTO ""."examination_question1" VALUES (1086, 1086, '车辆下长坡时要减挡行驶,以充分利用发动机的制动作用.', 1, '正确', '错误', , , '变速箱高档位高速持续运转中,迅速降低变速箱的档位转速,使变速箱的低档位转速对车辆本身的惯性高速产生抑制,从而产生短时间内的所谓发动机制动(变速箱制动).', , 0);
INSERT INTO ""."examination_question1" VALUES (1087, 1087, '驾驶机动车掉头、转弯、下陡坡时的最高速度不能超过每小时40公里.', 2, '正确', '错误', , , '机动车行驶中遇有下列情形之一的,最高行驶速度不得超过每小时30公里,其中拖拉机、电瓶车、轮式专用机械车不得超过每小时15公里:　　 （一）进出非机动车道,通过铁路道口、急弯路、窄路、窄桥时；　　 （二）掉头、转弯、下陡坡时；　　 （三）遇雾、雨、雪、沙尘、冰雹,能见度在50米以内时；　　 （四）在冰雪、泥泞的道路上行驶时；', , 0);
INSERT INTO ""."examination_question1" VALUES (1088, 1088, '预计在超车过程中与对面来车有会车可能时,应提前加速超越.', 2, '正确', '错误', , , '预计在超车过程中与对面来车有会车可能时,应当放弃超车.若加速超越,有可能会引起不必要的事故.', , 0);
INSERT INTO ""."examination_question1" VALUES (1089, 1089, '下列哪种行为会受到200元以上2000元以下罚款,并处吊销机动车驾驶证?', 2, '违反道路通行规定', '超过规定时速50%', '造成交通事故后逃逸', '驾车没带驾驶证', , , 0);
INSERT INTO ""."examination_question1" VALUES (1090, 1090, '轮胎气压过低时,高速行驶轮胎会出现波浪变形温度升高而导致什么?', 3, '气压更低', '行驶阻力增大', '爆胎', '气压不稳', '轮胎气不足,就会造成轮胎与地面的摩擦范围增大,所以会使温度升高,从而爆胎.', , 0);
INSERT INTO ""."examination_question1" VALUES (1091, 1091, '驾驶机动车在路口遇到这种情况如何行驶?', 1, '停车等待', '遵守交通信号灯', '靠右侧直行', '可以向右转弯', '看警察叔叔的手,让你停了.在有警察和指示灯的路口,当然听警察的.', 'http://images.juheapi.com/jztk/c1c2subject1/1091.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1092, 1092, '行车中当驾驶人意识到爆胎时,应在控制住方向的情况下,轻踏制动踏板,使车辆缓慢减速,逐渐平稳地停靠于路边.', 1, '正确', '错误', , , '如果写的意识到爆胎,说明还没爆,可以轻刹；如果已经爆了,刹车容易翻车,所以抢档好些.', , 0);
INSERT INTO ""."examination_question1" VALUES (1093, 1093, '在这条公路上行驶的最高速度不能超过多少?', 3, '70公里/小时', '50公里/小时', '40公里/小时', '30公里/小时', '根据《中华人民共和国道路交通安全法实施条例》第四十五条:……在没有限速标志、标线的道路上,机动车不得超过下列最高行驶速度:　　(一)没有道路中心线的道路,城市道路为每小时30公里,公路为每小时40公里；　　(二)同方向只有1条机动车道的道路,城市道路为每小时50公里,公路为每小时70公里.图为没有道路中心线的公路,所以最高时速为40公里每小时.', 'http://images.juheapi.com/jztk/c1c2subject1/1093.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1094, 1094, '机动车 仪表板上（如图所示）亮表示发动机可能机油压力过高.', 2, '正确', '错误', , , '图片表示发动机可能机油压力不足.', 'http://images.juheapi.com/jztk/c1c2subject1/1094.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1095, 1095, '开启前照灯远光时仪表板上（如图所示）亮起.', 2, '正确', '错误', , , '远光灯的灯光走向是直的.近光灯的灯光走向是朝下的.', 'http://images.juheapi.com/jztk/c1c2subject1/1095.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1096, 1096, '驾驶车辆汇入车流时,应提前开启转向灯,保持直线行驶,通过后视镜观察左右情况,确认安全后汇入合流.', 1, '正确', '错误', , , '在汇入车流是先开转向灯的,是为了提示后面的车避让留下进入车流的空隙,保持直行一段,因为是汇入合流,要确认好车与车横向空隙就要用后视镜看左右了', , 0);
INSERT INTO ""."examination_question1" VALUES (1097, 1097, '红色圆圈内标线含义是什么?', 2, '临时停靠站', '港湾式停靠站', '应急停车带', '公交车停靠站', '请记住', 'http://images.juheapi.com/jztk/c1c2subject1/1097.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1098, 1098, '驾驶机动车通过交叉路口要遵守交通信号.', 1, '正确', '错误', , , '这个最基本的,没什么解释的.', , 0);
INSERT INTO ""."examination_question1" VALUES (1099, 1099, '在这种情况的铁路道口要加速通过.', 2, '正确', '错误', , , '前边是火车道啊,一定要观察后在慢慢的通过', 'http://images.juheapi.com/jztk/c1c2subject1/1099.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1100, 1100, '遇到这种情况的路口怎样通过?', 1, '确认安全后通过', '右转弯加速通过', '加速直行通过', '左转弯加速通过', '前方是警告性的黄灯,所以此路口是确认安全后通过.', 'http://images.juheapi.com/jztk/c1c2subject1/1100.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1101, 1101, '这个仪表是何含义?', 1, '水温表', '燃油表', '电流表', '压力表', '看到那个温度计没说明他是管温度的.', 'http://images.juheapi.com/jztk/c1c2subject1/1101.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1102, 1102, '在这个路口右转弯如何通行?', 4, '直接向右转弯', '抢在对面车前右转弯', '鸣喇叭催促', '先让对面车左转弯', '人家都到路中间了,你还不让人间转完了.', 'http://images.juheapi.com/jztk/c1c2subject1/1102.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1103, 1103, '在交叉路口遇到这种情况享有优先通行权.', 1, '正确', '错误', , , '右转让左转,没有问题.', 'http://images.juheapi.com/jztk/c1c2subject1/1103.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1104, 1104, '在这条高速公路上行驶时的最高速度不能超过多少?', 1, '110公里/小时', '120公里/小时', '90公里 /小时', '100公里/小时', '请看右侧的标志,此为限速标志,所以最高车速不能超过110公里每小时.', 'http://images.juheapi.com/jztk/c1c2subject1/1104.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1105, 1105, '在这条公路上行驶的最高速度不能超过多少?', 4, '30公里/小时', '40公里/小时', '50公里/小时', '70公里/小时', '根据《中华人民共和国道路交通安全法实施条例》第四十五条:……在没有限速标志、标线的道路上,机动车不得超过下列最高行驶速度:　　(一)没有道路中心线的道路,城市道路为每小时30公里,公路为每小时40公里；　　(二)同方向只有1条机动车道的道路,城市道路为每小时50公里,公路为每小时70公里.图为公路,所以最高时速为70公里/每小时.', 'http://images.juheapi.com/jztk/c1c2subject1/1105.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1106, 1106, '交通标志和交通标线不属于交通信号.', 2, '正确', '错误', , , '交通信号分为:交通信号灯、交通标志、交通标线和交通警察的指挥.', , 0);
INSERT INTO ""."examination_question1" VALUES (1107, 1107, '前方路口这种信号灯亮表示什么意思?', 4, '准许通行', '提醒注意', '路口警示', '禁止通行', '红灯停.', 'http://images.juheapi.com/jztk/c1c2subject1/1107.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1108, 1108, '机动车仪表板上（如图所示）亮表示什么?', 2, '前后雾灯开启', '前后位置灯开启', '前照灯开启', '危险报警闪光灯开启', '位置灯就是车辆示宽灯,也就是当你开启双跳灯后,前后左右四个闪的灯 宽灯就是转向信号灯同时闪烁,用于警告作用.', 'http://images.juheapi.com/jztk/c1c2subject1/1108.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1109, 1109, '机动车仪表板上（如图所示）亮表示什么?', 4, '后雾灯打开', '前照灯近光打开', '前照灯远光打开', '前雾灯打开', '前雾灯朝左,后雾灯朝右.', 'http://images.juheapi.com/jztk/c1c2subject1/1109.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1110, 1110, '前方路口这种信号灯亮表示什么意思?', 4, '路口警示', '禁止通行', '提醒注意', '准许通行', '绿灯行.', 'http://images.juheapi.com/jztk/c1c2subject1/1110.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1111, 1111, '打开后雾灯开关,（如图所示）亮起.', 2, '正确', '错误', , , '前雾灯是朝左的,后雾灯是朝右的.', 'http://images.juheapi.com/jztk/c1c2subject1/1111.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1112, 1112, '车辆发生爆胎后,驾驶人在尚未控制住车速前,不要冒险使用行车制动器停车,以避免车辆横甩发生更大的险情.', 1, '正确', '错误', , , '行车制动=制动踏板=脚刹,爆胎后,驾驶人在尚未控制住车速的时候,是不能急踩刹车的,要不然会横甩或者翻车已经后面来车追尾.正确的做法应该是紧握方向盘,控制方向,极力保持直线行驶,利用发动机制动,减档或抢档减速度,轻踏制动踏板缓慢减速,平稳靠边停车.请注意试题中的“驾驶人在尚未控制住车速前”此时是不可以使用行车制动器的.', , 0);
INSERT INTO ""."examination_question1" VALUES (1113, 1113, '夜间驾驶机动车通过人行横道时需要交替使用远近光灯.', 1, '正确', '错误', , , , , 0);
INSERT INTO ""."examination_question1" VALUES (1114, 1114, '打开前雾灯开关,（如图所示）亮起.', 2, '正确', '错误', , , '前雾灯是朝左的,后雾灯是朝右的.', 'http://images.juheapi.com/jztk/c1c2subject1/1114.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1115, 1115, '机动车仪表板上（如图所示）这个符号表示什么?', 1, '行李舱开启', '一侧车门开启', '发动机舱开启', '燃油箱盖开启', '后边为行李舱,前边为发动机舱.', 'http://images.juheapi.com/jztk/c1c2subject1/1115.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1116, 1116, '驾驶机动车在道路上向右变更车道可以不使用转向灯.', 2, '正确', '错误', , , '大家要养成良好的行车习惯.', , 0);
INSERT INTO ""."examination_question1" VALUES (1117, 1117, '机动车发生故障时,（如图所示）闪烁.', 2, '正确', '错误', , , '危险报警闪光灯俗称双闪,不会自动闪烁的,这个是要我们驾驶员操作的.', 'http://images.juheapi.com/jztk/c1c2subject1/1117.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1118, 1118, '在这种急弯道路上行车应交替使用远近光灯.', 1, '正确', '错误', , , '交替使用远近光灯,先变远光灯再回近光灯的包括以下情况:急弯、坡路、超车（同时打左灯）、拱桥、人行横道、没有信号灯的路口.', 'http://images.juheapi.com/jztk/c1c2subject1/1118.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1119, 1119, '在这条城市道路上行驶的最高速度不能超过多少?', 3, '30公里/小时', '40公里/小时', '50公里/小时', '70公里/小时', '根据《中华人民共和国道路交通安全法实施条例》第四十五条:……在没有限速标志、标线的道路上,机动车不得超过下列最高行驶速度:　　(一)没有道路中心线的道路,城市道路为每小时30公里,公路为每小时40公里；　　(二)同方向只有1条机动车道的道路,城市道路为每小时50公里,公路为每小时70公里.图为同方向只有1条机动车道的城市道路,所以最高每小时50公里.', 'http://images.juheapi.com/jztk/c1c2subject1/1119.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1120, 1120, '机动车仪表板上（如图所示）亮时,防抱死制动系统处于打开状态.', 2, '正确', '错误', , , '此图为防抱死制动系统发生故障.', 'http://images.juheapi.com/jztk/c1c2subject1/1120.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1121, 1121, '小型客车行驶在平坦的高速公路上,突然有颠簸感觉时,应迅速降低车速,防止爆胎.', 1, '正确', '错误', , , '此题没问题,在平坦的高速公路上,突然有颠簸感觉时只有轮胎出现问题', , 0);
INSERT INTO ""."examination_question1" VALUES (1122, 1122, '机动车仪表板上（如图所示）亮表示什么?', 1, '防抱死制动系统故障', '驻车制动器处于解除状态', '行车制动系统故障', '安全气囊处于故障状态', 'ABS就是防抱死制动系统', 'http://images.juheapi.com/jztk/c1c2subject1/1122.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1123, 1123, '机动车在环形路口内行驶,遇有其他车辆强行驶入时,只要有优先权就可以不避让', 2, '正确', '错误', , , '当然错拉,有优先权就了不起呀?有优先权也要考虑到安全啊,不然容易发生两车相撞.', , 0);
INSERT INTO ""."examination_question1" VALUES (1124, 1124, '交通信号包括交通信号灯、交通标志、交通标线和交通警察的指挥.', 1, '正确', '错误', , , '交通信号分为:交通信号灯、交通标志、交通标线和交通警察的指挥.', , 0);
INSERT INTO ""."examination_question1" VALUES (1125, 1125, '在路口遇到这种情形时怎样做?', 2, '停在网状线区域内等待', '停在路口以外等待', '跟随前车通过路口', '停在路口内等待', '首先看前边是已经堵车了（请仔细看图片,前边不只一辆车）,你若跟过去只能停在网状线区域,但是规定网状线区域是禁止停车的,所以虽然是绿灯那也得停在路口以外等待.', 'http://images.juheapi.com/jztk/c1c2subject1/1125.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1126, 1126, '行至这种情况的铁路道口要停车观察.', 1, '正确', '错误', , , '是正确的,你看到火车的标志没,你要没停车观察,万一火车开过来肿么办.', 'http://images.juheapi.com/jztk/c1c2subject1/1126.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1127, 1127, '驾驶机动车上坡时,在将要到达坡道顶端时要加速并鸣喇叭.', 2, '正确', '错误', , , '肯定是错的,鸣喇叭对,但是加速是错误的.', , 0);
INSERT INTO ""."examination_question1" VALUES (1128, 1128, '行车中当车辆突然爆胎时,驾驶人切忌慌乱中急踏制动踏板,尽量采用“抢挡”的方法,利用发动机制动使车辆减速.', 1, '正确', '错误', , , '由于车胎爆胎,紧急制动会侧滑.所以只能用抢挡,用发动机制动.', , 0);
INSERT INTO ""."examination_question1" VALUES (1129, 1129, '机动车仪表板上（如图所示）亮,提示左侧车门未关闭.', 2, '正确', '错误', , , '很明显他是右侧的', 'http://images.juheapi.com/jztk/c1c2subject1/1129.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1130, 1130, '在这个弯道上行驶时的最高速度不能超过多少?', 2, '40公里/小时', '30公里/小时', '50公里/小时', '70公里/小时', '规定:驾驶机动车通过急弯路时,最高速度不能超过30公里/小时', 'http://images.juheapi.com/jztk/c1c2subject1/1130.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1131, 1131, '车辆前轮胎爆裂,危险较大,方向会立刻向爆胎车轮一侧跑偏,直接影响驾驶人对转向盘的控制.', 1, '正确', '错误', , , '如果车速过快,很有可能造成事故.碰到这种情况的时候,切忌不要慌张,不要向相反的方向猛打方向盘,要尽量保持车子直线行驶,同时降低车速,切记不可急刹车,要缓慢降低车速,还有打开安全警告灯,注意后方来车,条件允许的话,把车子驶离行车道,靠右边车道停车.碰到这种情况,切记不要慌,首要的就是先稳住车辆.', , 0);
INSERT INTO ""."examination_question1" VALUES (1132, 1132, '这个地面标记的含义是预告前方设有交叉路口.', 2, '正确', '错误', , , '道路路面上的白色菱形图案,是中华人民共和国国家标准（GB5768/1999）《道路交通标志和标线》中新设置的一种交通标线,正式名称为人行横道预告标线.这种标线在到达人行横道前的道路30-50米处设置,它的作用是用来提示驾驶员,前方已接近人行横道,应减速慢行,并须注意行人横过马路', 'http://images.juheapi.com/jztk/c1c2subject1/1132.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1133, 1133, '驾驶机动车遇雾、雨、雪等能见度在50米以内时,最高速度不能超过多少?', 4, '70公里/小时', '50公里/小时', '40公里/小时', '30公里/小时', '规定:驾驶机动车遇雾、雨、雪等能见度在50米以内时,最高速度不能超过30公里/小时', , 0);
INSERT INTO ""."examination_question1" VALUES (1134, 1134, '在同向4车道高速公路上行车,车速高于每小时110公里的车辆应在哪条车道上行驶?', 1, '最左侧', '第二条', '最右侧', '第三条', '交通安全法实施条例第七十八条规定了同向两车道、三车道的限速规范:二车道左侧车道最低限速100,三车道左到右依次110、90、60.同向四车道记着培训老师讲的:左到右最低限速依次110、90、90、60.', , 0);
INSERT INTO ""."examination_question1" VALUES (1135, 1135, '按下这个开关,后风窗玻璃除霜器开始工作.', 2, '正确', '错误', , , '这个开关是管前挡风窗户的,下边那个按钮才是管后风窗的.告诉大家一个小技巧,你看半弧形的图标是前挡风,四四方方的图标就是后风窗,嘘,别告诉别人.', 'http://images.juheapi.com/jztk/c1c2subject1/1135.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1136, 1136, '驾驶机动车遇到漫水桥时要察明水情确认安全后再低速通过.', 1, '正确', '错误', , , '这个是对的因为不知道水深,小心盲目的冲过去,然后汽车飘起来.', , 0);
INSERT INTO ""."examination_question1" VALUES (1137, 1137, '在大雨天行车,为避免发生“水滑”而造成危险,要控制速度行驶.', 1, '正确', '错误', , , '控制速度行驶:减速或者停车都是为了安全.', , 0);
INSERT INTO ""."examination_question1" VALUES (1138, 1138, '这是什么操纵装置?', 4, '节气门操纵杆', '变速器操纵杆', '离合器操纵杆', '驻车制动器操纵杆', '驻车制动器操纵杆俗称手刹,能使汽车本体固定在地面或某平面上而不出现相对移动的总成的操纵装置.', 'http://images.juheapi.com/jztk/c1c2subject1/1138.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1139, 1139, '驾驶机动车遇到前方车辆停车排队等候或缓慢行驶时怎么办?', 3, '可借道超车', '占用对面车道', '依次行驶', '穿插等候的车辆', '肯定不能占用对面车道,大家排队吧,不要做让别人讨厌的事.', , 0);
INSERT INTO ""."examination_question1" VALUES (1140, 1140, '驾驶机动车在没有中心线的道路上遇相对方向来车时怎样行驶?', 3, '紧靠路边行驶', '靠路中心行驶', '减速靠右行驶', '借非机动车道行驶', '我们国家都是靠右行驶的,所以要尽快靠右避让.', , 0);
INSERT INTO ""."examination_question1" VALUES (1141, 1141, '车辆因故障必须在高速公路停车时,应在车后方多少米以外设置故障警告标志?', 2, 25, 150, 100, 50, '车辆因故障必须在高速公路停车时,应在车后方（150米）处设置故障警告标志,夜间还需开启示廓灯和后位灯. 机动车在速公路上发生故障时,警告标志应当设置在故障车来车方向（150米）以外. 机动车在白天在道路上发生故障,妨碍交通又难以移动的,应当按规定开启危险报警闪光灯,并在车后（50米至100米）处设置警告标志.', , 0);
INSERT INTO ""."examination_question1" VALUES (1142, 1142, '同车道行驶的车辆前方遇到下列哪种车辆不得超车?', 3, '大型客货车', '出租汽车', '执行任务的救护车', '公共汽车', '亲,人命关天,您就别超了.', , 0);
INSERT INTO ""."examination_question1" VALUES (1143, 1143, '如图所示,在高速公路同方向三条机动车道最左侧车道行驶,应保持什么车速?', 1, '110 公里/小时～120公里/小时', '100 公里/小时～120公里/小时', '90 公里/小时～110公里/小时', '60 公里/小时～120公里 /小时', '《中华人民共和国道路交通安全法实施条例》第七十八条:同方向有2条车道的,左侧车道的最低车速为每小时100公里；同方向有3条以上车道的,最左侧车道的最低车速为每小时110公里,中间车道的最低车速为每小时90公里.道路限速标志标明的车速与上述车道行驶车速的规定不一致的,技照道路限速标志标明的车速行驶.', 'http://images.juheapi.com/jztk/c1c2subject1/1143.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1144, 1144, '车辆通过铁道路口时,应用低速挡安全通过,中途不得换挡,以避免发动机熄火.', 1, '正确', '错误', , , '通过铁路道口,安全第一.中途换挡如果发生熄火,万一这时火车刚开过来了,那不是完蛋了?', , 0);
INSERT INTO ""."examination_question1" VALUES (1145, 1145, '下长坡连续使用行车制动会导致什么?', 4, '会缩短发动机寿命', '增加驾驶人的劳动强度', '容易造成车辆倾翻', '会使制动器温度升高而使制动效果急剧下降', '下长坡,车子向下的冲力很大,采取制动,刹车垫与车轮轴就摩擦起热,刹车垫也磨损严重！', , 0);
INSERT INTO ""."examination_question1" VALUES (1146, 1146, '行人参与道路交通的主要特点是什么?', 3, '行动迟缓', '喜欢聚集、围观', '行走随意性大,方向多变', '以上都是', '行人不带转向灯无号牌,如果没有分透出对方的轨迹那就注意行人的72变化.', , 0);
INSERT INTO ""."examination_question1" VALUES (1147, 1147, '超车时应从前车的左侧超越,是因为左侧超车便于观察,有利于安全.', 1, '正确', '错误', , , '左侧更便于观察前后方的情况,有利于安全.', , 0);
INSERT INTO ""."examination_question1" VALUES (1148, 1148, '这属于哪一种标志?', 1, '警告标志', '指路标志', '指示标志', '禁令标志', '交叉路口:用以警告车辆驾驶人谨慎慢行,注意横向来车.设在平面交叉路口驶入路段的适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/1148.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1149, 1149, '这个标志的含义是警告前方出现向左的急转弯路.', 1, '正确', '错误', , , '向左急弯路:用以警告车辆驾驶人减速慢行.设置位置为曲线起点的外面,但不应进入相邻的圆曲线内.', 'http://images.juheapi.com/jztk/c1c2subject1/1149.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1150, 1150, '机动车驾驶证遗失、损毁无法辨认时,机动车驾驶人应当向机动车驾驶证核发地车辆管理所申请补发.', 1, '正确', '错误', , , '没有问题,施主牢记吧.', , 0);
INSERT INTO ""."examination_question1" VALUES (1151, 1151, '这个导向箭头是何含义?', 1, '提示前方有左弯或需向左合流', '提示前方有右弯或需向右合流', '提示前方右侧有障碍需向左合流', '提示前方有左弯或或许向左绕行', '导向箭头:提示前方道路有左弯或需向左合流.', 'http://images.juheapi.com/jztk/c1c2subject1/1151.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1152, 1152, '科目三道路驾驶技能和安全文明驾驶常识考试满分分别为100分,成绩分别达到80和90分的为合格.', 2, '正确', '错误', , , '应该都为90分.123号令第三十条  各科目考试的合格标准为:（一）科目一考试满分为100分,成绩达到90分的为合格；（三）科目三道路驾驶技能和安全文明驾驶常识考试满分分别为100分,成绩分别达到90分的为合格.', , 0);
INSERT INTO ""."examination_question1" VALUES (1153, 1153, '驾驶机动车遇到这种信号灯不断闪烁时怎样行驶?', 3, '尽快加速通过', '靠边停车等待', '注意?望安全通过', '紧急制动', '路口黄灯持续闪烁,警示驾驶人要注意?望,确认安全通过.', 'http://images.juheapi.com/jztk/c1c2subject1/1153.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1154, 1154, '这属于哪一种标志?', 3, '警告标志', '禁令标志', '指示标志', '指路标志', '指示标志是交通标志中主要标志的一种,,用以指示车辆和行人按规定方向、地点行驶. 指示标志的颜色为蓝底、白图案；形状分为圆形、长方形和正方形.', 'http://images.juheapi.com/jztk/c1c2subject1/1154.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1155, 1155, '机动车仪表板上（如图所示）亮,提示行李舱开启.', 2, '正确', '错误', , , '前盖是发动机舱.', 'http://images.juheapi.com/jztk/c1c2subject1/1155.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1156, 1156, '这个标志的含义是指示此处设有室内停车场.', 1, '正确', '错误', , , '你看那个P字上边还有个遮挡,说明是室内的,如果没有上边那个遮挡就是露天停车场.', 'http://images.juheapi.com/jztk/c1c2subject1/1156.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1157, 1157, '驾驶机动车跨越双实线行驶属于什么行为?', 2, '违章行为', '违法行为', '过失行为', '违规行为', '现在都是违法行为,没有违章,违规行为一说.', , 0);
INSERT INTO ""."examination_question1" VALUES (1158, 1158, '这属于哪一种标志?', 1, '旅游区标志', '作业区标志', '告示标志', '高速公路标志', '旅游区方向:设在通往旅游区各连接道路的交叉口处.', 'http://images.juheapi.com/jztk/c1c2subject1/1158.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1159, 1159, '这个标志是何含义?', 3, '提醒车辆驾驶人前方道路沿水库、湖泊、河流', '提醒车辆驾驶人前方有向上的陡坡路段', '提醒车辆驾驶人前方有向下的陡坡路段', '提醒车辆驾驶人前方有两个及以上的连续上坡路段', '下陡坡:用以提醒车辆驾驶人小心驾驶.', 'http://images.juheapi.com/jztk/c1c2subject1/1159.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1160, 1160, '这个标志的含义是警告前方道路有障碍物,车辆减速绕行.', 2, '正确', '错误', , , '向右急弯路:用以警告车辆驾驶人减速慢行.设置位置为曲线起点的外面,但不应进入相邻的圆曲线内.', 'http://images.juheapi.com/jztk/c1c2subject1/1160.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1161, 1161, '这个标志的含义是警告前方道路易滑,注意慢行.', 2, '正确', '错误', , , '反向弯路:用以警告车辆驾驶人减速慢行.设置位置为两反向圆曲线起点的外面,但不应进入相邻的圆曲线内.', 'http://images.juheapi.com/jztk/c1c2subject1/1161.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1162, 1162, '机动车登记证书、号牌、行驶证灭失、丢失或者损毁的,机动车所有人应当向居住地车辆管理所申请补领、换领.', 2, '正确', '错误', , , '应当向登记地车辆管理所申请补领、换领.', , 0);
INSERT INTO ""."examination_question1" VALUES (1163, 1163, '这是什么交通标志?', 3, '易滑路段', '急转弯路', '反向弯路', '连续弯路', '反向弯路:用以警告车辆驾驶人减速慢行.设置位置为两反向圆曲线起点的外面,但不应进入相邻的圆曲线内.', 'http://images.juheapi.com/jztk/c1c2subject1/1163.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1164, 1164, '这个标志的含义是提醒前方道路变为不分离双向行驶路段.', 1, '正确', '错误', , , '双向交通:用以提醒车辆驾驶人注意会车.设在由双向分离行驶,因某种原因出现临时性或永久性的不分离双向行驶的路段,或由单向行驶进人双向行驶的路段以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/1164.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1165, 1165, '这种标志的作用是警告车辆驾驶人前方有危险,谨慎通行.', 1, '正确', '错误', , , '前方这么多状况,要谨慎驾驶.', 'http://images.juheapi.com/jztk/c1c2subject1/1165.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1166, 1166, '这个标志的含义是警告前方有两个相邻的反向转弯道路.', 2, '正确', '错误', , , '连续弯路:用以警告车辆驾驶人减速慢行.设置位置为连续弯路起点的外面,当连续弯路总长度大于500m时,应重复设置.', 'http://images.juheapi.com/jztk/c1c2subject1/1166.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1167, 1167, '这个标志的含义是前方即将行驶至Y型交叉路口?', 2, '正确', '错误', , , '交叉路口:用以警告车辆驾驶人谨慎慢行,注意横向来车.设在平面交叉路口驶入路段的适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/1167.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1168, 1168, '这是什么交通标志?', 3, '两侧变窄', '右侧变窄', '左侧变窄', '桥面变窄', '左侧变窄:用以警告车辆驾驶人注意前方车行道或路面狭窄情况,遇有来车应予减速避让.设在双车道路面宽度缩减为6 m以下的路段起点前方.', 'http://images.juheapi.com/jztk/c1c2subject1/1168.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1169, 1169, '这个标志是何含义?', 2, '提醒车辆驾驶人前方道路沿水库、湖泊、河流', '提醒车辆驾驶人前方有向上的陡坡路段', '.提醒车辆驾驶人前方有两个及以上的连续上坡路段', '提醒车辆驾驶人前方有向下的陡坡路段', '上陡坡:用以提醒车辆驾驶人小心驾驶.', 'http://images.juheapi.com/jztk/c1c2subject1/1169.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1170, 1170, '这个标志的含义是提醒前方桥面宽度变窄.', 2, '正确', '错误', , , '两侧变窄:用以警告车辆驾驶人注意前方车行道或路面狭窄情况,遇有来车应予减速避让.设在双车道路面宽度缩减为6 m以下的路段起点前方.', 'http://images.juheapi.com/jztk/c1c2subject1/1170.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1171, 1171, '这是什么交通标志?', 4, '易滑路段', '急转弯路', '反向弯路', '连续弯路', '连续弯路:用以警告车辆驾驶人减速慢行.设置位置为连续弯路起点的外面,当连续弯路总长度大于500m时,应重复设置.', 'http://images.juheapi.com/jztk/c1c2subject1/1171.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1172, 1172, '在驾驶技能准考证明的有效期内,科目二和科目三道路驾驶技能考试预约次数不得超过多少次?', 3, '3次', '4次', '5次', '6次', '123号令第三十七条:在驾驶技能准考证明有效期内,科目二和科目三道路驾驶技能考试预约考试的次数不得超过五次.第五次预约考试仍不合格的,已考试合格的其他科目成绩作废.', , 0);
INSERT INTO ""."examination_question1" VALUES (1173, 1173, '机动车发生正面碰撞时,安全气囊加上安全带的双重保护才能充分发挥作用.', 1, '正确', '错误', , , '研究表明,假设气囊加安全带是100%安全的话,安全带的作用是70%,气囊的作用是30%.', , 0);
INSERT INTO ""."examination_question1" VALUES (1174, 1174, '这是什么交通标志?', 1, '两侧变窄', '右侧变窄', '左侧变窄', '桥面变窄', '两侧变窄:用以警告车辆驾驶人注意前方车行道或路面狭窄情况,遇有来车应予减速避让.设在双车道路面宽度缩减为6 m以下的路段起点前方.', 'http://images.juheapi.com/jztk/c1c2subject1/1174.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1175, 1175, '驾驶机动车在人行横道上临时停车属于违法行为.', 1, '正确', '错误', , , '现在都是违法行为,没有违章,违规行为一说.', , 0);
INSERT INTO ""."examination_question1" VALUES (1176, 1176, '科目三考试分为道路驾驶技能考试和安全文明驾驶常识考试两部分.', 1, '正确', '错误', , , '123号令第二十二条  机动车驾驶人考试内容分为道路交通安全法律、法规和相关知识考试科目（以下简称“科目一”）、场地驾驶技能考试科目（以下简称“科目二”）、道路驾驶技能和安全文明驾驶常识考试科目（以下简称“科目三”）.', , 0);
INSERT INTO ""."examination_question1" VALUES (1177, 1177, '这个标志的作用是用以警告车辆驾驶人谨慎慢行,注意横向来车.', 1, '正确', '错误', , , '交叉路口:用以警告车辆驾驶人谨慎慢行,注意横向来车.设在平面交叉路口驶入路段的适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/1177.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1178, 1178, '这属于哪一种标志?', 2, '警告标志', '禁令标志', '指示标志', '指路标志', '禁令标志是交通标志中主要标志的一种,对车辆加以禁止或限制的标志,如禁止通行、禁止停车、禁止左转弯、禁止鸣喇叭、限制速度、限制重量等. 禁令标志的颜色,除个别标志外,为白底,红圈,红杠,黑图案.图案压杠.', 'http://images.juheapi.com/jztk/c1c2subject1/1178.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1179, 1179, '小型汽车科目二考试内容包括倒车入库、坡道定点停车和起步、侧方停车、曲线行驶、直角转弯.', 1, '正确', '错误', , , '123号令第二十五条  科目二考试内容包括:小型汽车、小型自动挡汽车、残疾人专用小型自动挡载客汽车和低速载货汽车考试倒车入库、坡道定点停车和起步、侧方停车、曲线行驶、直角转弯；', , 0);
INSERT INTO ""."examination_question1" VALUES (1180, 1180, '驾驶技能准考证明的有效期是多久?', 3, '1年', '2年', '3年', '4年', '学员报名驾考后,就会获得“准考证”.但是,这张“准考证”的有效期,要从该学员通过科目一考试之日算起.学员通过科目一考试后,三年内都属于准考证明有效期内.如果超过三年,那么公安部门会将该学员学籍取消,学员必须重新报名开始培训.', , 0);
INSERT INTO ""."examination_question1" VALUES (1181, 1181, '这个标志是何含义?', 4, '禁止左转', '此路不通', '禁止通行', '超高绕行', '4M高度,因为超过所以只能绕行.', 'http://images.juheapi.com/jztk/c1c2subject1/1181.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1182, 1182, '这个标志的含义是提醒前方两侧行车道或路面变窄.', 2, '正确', '错误', , , '窄桥:用以警告车辆驾驶人注意前方桥面宽度变窄,应谨慎驾驶.设在桥面净宽较两端路面宽度变窄,且桥的净宽小于6 m的桥梁以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/1182.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1183, 1183, '申请人以欺骗、贿赂等不正当手段取得机动车驾驶证的（被撤销的）,申请人在多长时间内不得再次申领机动车驾驶证.', 4, '6个月', '1年', '2年', '3年', , , 0);
INSERT INTO ""."examination_question1" VALUES (1184, 1184, '机动车购买后尚未注册登记,需要临时上道路行驶的,可以凭什么临时上道路行驶?', 2, '合法来源凭证', '临时行驶车号牌', '借用的机动车号牌', '法人单位证明', '准予机动车临时上道路行驶的纸质机动车号牌,也称临时行驶车号牌.', , 0);
INSERT INTO ""."examination_question1" VALUES (1185, 1185, '正在执行社区戒毒、强制隔离戒毒、社区康复措施,车辆管理所将注销其驾驶证.', 1, '正确', '错误', , , '被查获有吸食、注射毒品后驾驶机动车行为,正在执行社区戒毒、强制隔离戒毒、社区康复措施,或者长期服用依赖性精神药品成瘾尚未戒除的,车辆管理所应当注销其机动车驾驶证.三年内有吸食、注射毒品行为,或者解除强制隔离戒毒措施未满三年,或者长期服用依赖性精神药品成瘾尚未戒除的,不得申请机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (1186, 1186, '这个标志的含义是提醒前方左侧行车道或路面变窄.', 1, '正确', '错误', , , '左侧变窄:用以警告车辆驾驶人注意前方车行道或路面狭窄情况,遇有来车应予减速避让.设在双车道路面宽度缩减为6 m以下的路段起点前方.', 'http://images.juheapi.com/jztk/c1c2subject1/1186.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1187, 1187, '这个标志是何含义?', 4, '提醒车辆驾驶人前方有两个及以上的连续上坡路段', '提醒车辆驾驶人前方有向上的陡坡路段', '提醒车辆驾驶人前方有向下的陡坡路段', '提醒车辆驾驶人前方有两个及以上的连续下坡路段', '连续下坡:用以提醒车辆驾驶人小心驾驶.当连续下坡总长大于3 km后,应重复设置.', 'http://images.juheapi.com/jztk/c1c2subject1/1187.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1188, 1188, '这个标志的含义是提醒车辆驾驶人前方是无人看守铁路道口.', 2, '正确', '错误', , , '有人看守铁路道口:用以警告车辆驾驶人注意慢行或及时停车.设在车辆驾驶人不易发现的道口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/1188.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1189, 1189, '夜间通过没有路灯或路灯照明不良时,应将近光灯转换为远光灯,但同向行驶的后车不得使用远光灯.', 1, '正确', '错误', , , '后面如果打远光灯的话,你通过后视镜就什么都看不清了！如果你硬看的话,容易出现短暂失明,很危险！', , 0);
INSERT INTO ""."examination_question1" VALUES (1190, 1190, '这个标志的含义是告示前方是拥堵路段,注意减速慢行.', 2, '正确', '错误', , , '事故易发路段:用以告示前方道路为事故易发路段,谨慎驾驶.设在交通事故易发路段以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/1190.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1191, 1191, '这个标志的含义是提醒车辆驾驶人前方是傍山险路路段.', 2, '正确', '错误', , , '注意落石:用以提醒车辆驾驶人注意落石.设在有落石危险的傍山路段以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/1191.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1192, 1192, '这个标志的含义是提醒前方右侧行车道或路面变窄.', 1, '正确', '错误', , , '右侧变窄:用以警告车辆驾驶人注意前方车行道或路面狭窄情况,遇有来车应予减速避让.设在双车道路面宽度缩减为6 m以下的路段起点前方.', 'http://images.juheapi.com/jztk/c1c2subject1/1192.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1193, 1193, '申请人在考试过程中有贿赂、舞弊行为的,取消考试资格,已经通过考试的其他科目成绩无效.', 1, '正确', '错误', , , '这个没有疑问的吧.', , 0);
INSERT INTO ""."examination_question1" VALUES (1194, 1194, '这个标志的含义是提醒车辆驾驶人前方是非机动车道.', 2, '正确', '错误', , , '注意非机动车:用以提醒车辆驾驶人注意慢行.设在经常有非机动车横穿、出入的地点前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/1194.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1195, 1195, '申请人因故不能按照预约时间参加考试的,应当提前一日申请取消预约,对申请人未按照预约考试时间参加考试的,判定该次考试不合格.', 1, '正确', '错误', , , '2013年新交规第三十六条  申请人因故不能按照预约时间参加考试的,应当提前一日申请取消预约.对申请人未按照预约考试时间参加考试的,判定该次考试不合格.', , 0);
INSERT INTO ""."examination_question1" VALUES (1196, 1196, '机动车登记证书、号牌、行驶证灭失、丢失或者损毁的,机动车所有人应当向哪个部门申请补领、换领.', 3, '住地交警支队车辆管理所', '驾驶证核发地车辆管理所', '登记地车辆管理所', '当地公安局', '应当向登记地车辆管理所申请补领、换领.', , 0);
INSERT INTO ""."examination_question1" VALUES (1197, 1197, '这个标志的含义是提醒车辆驾驶人前方是无人看守铁路道口.', 1, '正确', '错误', , , '无人看守铁路道口:用以警告车辆驾驶人注意慢行或及时停车.设在无人看守铁路道口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/1197.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1198, 1198, '这个标志的含义是告示前方道路施工,车辆左右绕行.', 2, '正确', '错误', , , '注意障碍物标志左右绕行:用以告示前方道路有障碍物,车辆应按标志指示减速慢行.设在道路障碍物前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/1198.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1199, 1199, '这是什么交通标志?', 2, '两侧变窄', '右侧变窄', '左侧变窄', '桥面变窄', '右侧变窄:用以警告车辆驾驶人注意前方车行道或路面狭窄情况,遇有来车应予减速避让.设在双车道路面宽度缩减为6 m以下的路段起点前方.', 'http://images.juheapi.com/jztk/c1c2subject1/1199.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1200, 1200, '打开位置灯开关,（如图所示）亮起.', 1, '正确', '错误', , , '如图表示全车灯都亮了,包括位置灯.', 'http://images.juheapi.com/jztk/c1c2subject1/1200.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1201, 1201, '机动车仪表板上（如图所示）亮表示什么?', 3, '侧面出风', '空气外循环', '迎面出风', '空气内循环', '直吹人脸表示迎面出风,垂直向下表示吹地板.', 'http://images.juheapi.com/jztk/c1c2subject1/1201.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1202, 1202, '驾驶的车辆正在被其他车辆超越时,若此时后方有跟随行驶的车辆,应怎样做?', 2, '继续加速行驶', '稍向右侧行驶,保证横向安全距离', '靠道路中心行驶', '加速向右侧让路', '我们都应该让,安全行驶,一定要和谐.', , 0);
INSERT INTO ""."examination_question1" VALUES (1203, 1203, '机动车仪表板上（如图所示）这个符号表示什么?', 4, '雪地起步模式', '空气循环', '空调制冷', '冷风暖气风扇', '看它长的多像风扇.', 'http://images.juheapi.com/jztk/c1c2subject1/1203.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1204, 1204, '驾驶机动车应当随身携带哪种证件?', 3, '职业资格证', '身份证', '驾驶证', '工作证', '驾驶证是必须的.', , 0);
INSERT INTO ""."examination_question1" VALUES (1205, 1205, '发动机起动后仪表板上（如图所示）亮表示什么?', 4, '燃油泵出现异常或者故障', '发动机点火系统出现故障', '发动机供油系统出现异常', '油箱内燃油已到最低液面', '这个是汽油不足了,多像加油站加油的那个机器,记住机油和汽油是不一样的.', 'http://images.juheapi.com/jztk/c1c2subject1/1205.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1206, 1206, '机动车仪表板上（如图所示）亮时提醒发动机需要加注机油.', 2, '正确', '错误', , , '这个是提醒发动机冷却液可能不足.', 'http://images.juheapi.com/jztk/c1c2subject1/1206.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1207, 1207, '这是什么操纵装置?', 1, '刮水器开关', '前照灯开关', '转向灯开关', '除雾器开关', '左手边转向灯开关,右手边刮水器开关.', 'http://images.juheapi.com/jztk/c1c2subject1/1207.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1208, 1208, '在实习期内驾驶机动车的,应当在车身后部粘贴或者悬挂哪种标志?', 3, '注意避让标志', '注意新手标志', '统一式样的实习标志', '注意车距标志', '规定在实习期内驾驶机动车的,应当在车身后部粘贴或者悬挂统一式样的实习标志.', , 0);
INSERT INTO ""."examination_question1" VALUES (1209, 1209, '将转向灯开关向下拉,右转向灯亮.', 2, '正确', '错误', , , '转向灯操作:上右下左.', 'http://images.juheapi.com/jztk/c1c2subject1/1209.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1210, 1210, '以欺骗、贿赂等不正当手段取得驾驶证被依法撤销驾驶许可的,多长时间不得重新申请驾驶许可?', 1, '3年内', '终身', '1年内', '5年内', '以欺骗、贿赂等不正当手段取得驾驶证被依法撤销驾驶许可的,3年内不得重新考取驾驶本.', , 0);
INSERT INTO ""."examination_question1" VALUES (1211, 1211, '准驾车型为小型汽车的,可以驾驶下列哪种车辆?', 1, '低速载货汽车', '中型客车', '三轮摩托车', '轮式自行机械', '小型汽车（C1）准驾车型:小型、微型载客汽车以及轻型、微型载货汽车；轻型、微型专项作业车,小型、微型自动挡载客汽车以及轻型、微型自动挡载货汽车,低速载货汽车,三轮汽车.', , 0);
INSERT INTO ""."examination_question1" VALUES (1212, 1212, '驾驶机动车上路前应当检查车辆安全技术性能.', 1, '正确', '错误', , , '常识题.', , 0);
INSERT INTO ""."examination_question1" VALUES (1213, 1213, '同车道行驶的车辆前方遇到下列哪种车辆不得超车?', 1, '执行任务的警车', '大型客货车', '出租汽车', '城市公交车', '如果是真的执行任务的警车（此处不好判断）,一定要让行.', , 0);
INSERT INTO ""."examination_question1" VALUES (1214, 1214, '雪天行车中,在有车辙的路段应循车辙行驶.', 1, '正确', '错误', , , '驾驶人看不到路面的状况,容易陷入水坑等.冰雪路同样如此,你看不到冰雪下的路面,很容易陷入坑坑洼洼.而沿着前车的车辙行驶,可以保证安全. 行驶过程中所遇情况是复杂的,我们要注意的是主要矛盾而不是次要矛盾！！！', , 0);
INSERT INTO ""."examination_question1" VALUES (1215, 1215, '在路口遇这种情形要减速让行.', 1, '正确', '错误', , , '右侧有减速让行的标志,故正确.', 'http://images.juheapi.com/jztk/c1c2subject1/1215.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1216, 1216, '机动车驾驶证有效期分为6年、10年、20年.', 2, '正确', '错误', , , '123号令第十条:机动车驾驶证有效期分为六年、十年和长期.', , 0);
INSERT INTO ""."examination_question1" VALUES (1217, 1217, '机动车仪表板上（如图所示）亮表示什么?', 4, '车前后位置灯亮起', '车前后示宽灯亮起', '左转向指示灯闪烁', '右转向指示灯闪烁', '这个没有问题吧...老衲也不知如何解释这个题.', 'http://images.juheapi.com/jztk/c1c2subject1/1217.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1218, 1218, '机动车仪表板上（如图所示）亮表示什么?', 1, '危险报警闪光灯闪烁', '右转向指示灯闪烁', '左转向指示灯闪烁', '车前后位置灯闪烁', '危险报警闪光灯,通常称为“双蹦”（红三角里有个！的标志按扭开关,俗称双闪灯 或双跳灯）,是一种提醒其他车辆与行人注意本车发生了特殊情况的信号灯.', 'http://images.juheapi.com/jztk/c1c2subject1/1218.jpeg', 0);
INSERT INTO ""."examination_question1" VALUES (1219, 1219, '准驾车型为小型汽车的,可以驾驶小型自动挡载客汽车.', 1, '正确', '错误', , , '小型汽车（C1）准驾车型:小型、微型载客汽车以及轻型、微型载货汽车；轻型、微型专项作业车,小型、微型自动挡载客汽车以及轻型、微型自动挡载货汽车,低速载货汽车,三轮汽车.', , 0);
INSERT INTO ""."examination_question1" VALUES (1220, 1220, '驾驶机动车违反道路交通安全法律法规发生交通事故属于交通违章行为.', 2, '正确', '错误', , , '应该是属于违法行为.', , 0);
INSERT INTO ""."examination_question1" VALUES (1221, 1221, '在这种情况下可以加速通过人行横道.', 2, '正确', '错误', , , '前方为人行横道并且有人通过路口,要小心驾驶,不可以加速通过人行横道.', 'http://images.juheapi.com/jztk/c1c2subject1/1221.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1222, 1222, '驾驶人在驾驶证核发地车辆管理所管辖区以外居住的,可以向居住地车辆管理所申请换证.', 1, '正确', '错误', , , '这个没问题的,你无论在哪里居住都可以向当地的车管所申请换证.', , 0);
INSERT INTO ""."examination_question1" VALUES (1223, 1223, '提拉这个开关控制机动车哪个部位?', 1, '左右转向灯', '倒车灯', '示廓灯', '报警闪光灯', '此为控制左右转向灯.顺便回忆一下转向灯操作:上右下左.', 'http://images.juheapi.com/jztk/c1c2subject1/1223.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1224, 1224, '驾驶机动车超车时,前方车辆不减速让路,应停止超车并适当减速,与前方车辆保持安全距离.', 1, '正确', '错误', , , '不让超车,就别超了,安全第一.', , 0);
INSERT INTO ""."examination_question1" VALUES (1225, 1225, '闪光警告信号灯为持续闪烁的黄灯,其作用是提示车辆、行人需要快速通过.', 2, '正确', '错误', , , '黄灯闪烁提示行人车辆注意,确认安全后通过,快速通过是不对的.', , 0);
INSERT INTO ""."examination_question1" VALUES (1226, 1226, '如图所示,驾驶机动车遇到这种情况时,A车应当注意避让.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法》第五十四条条明确规定:道路养护车辆、工程作业车进行作业时,在不影响过往车辆通行的前提下,其行驶路线和方向不受交通标志、标线限制,过往车辆和人员应当注意避让.', 'http://images.juheapi.com/jztk/c1c2subject1/1226.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1227, 1227, '如图所示,驾驶过程中遇到这种情况时,A车可以长鸣喇叭提醒道路养护车辆暂停喷水.', 2, '正确', '错误', , , '《中华人民共和国道路交通安全法》第五十四条条明确规定:道路养护车辆、工程作业车进行作业时,在不影响过往车辆通行的前提下,其行驶路线和方向不受交通标志、标线限制,过往车辆和人员应当注意避让.', 'http://images.juheapi.com/jztk/c1c2subject1/1227.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (1228, 1228, '驾驶机动车在道路上违反交通安全法规的行为属于违法行为.', 1, '正确', '错误', , , '常识题,现在都是违法行为,没有违章行为和违规行为一说.', , 0);
INSERT INTO ""."examination_question1" VALUES (1229, 1229, '驾驶人连续驾驶4小时以上,停车休息的时间不得少于多少?', 3, '10分钟', '15分钟', '20分钟', '5分钟', '《中华人民共和国道路交通安全法实施条例》第六十二条 驾驶机动车不得有下列行为:　　（七）连续驾驶机动车超过４小时未停车休息或者停车休息时间少于２０分钟； ', , 0);
