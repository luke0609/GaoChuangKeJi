INSERT INTO ""."examination_question1" VALUES (455, 455, '下列哪种证件是驾驶机动车上路行驶应当随车携带?', 2, '机动车保险单', '机动车行驶证', '出厂合格证明', '机动车登记证', '常识题.', , 0);
INSERT INTO ""."examination_question1" VALUES (456, 456, '车辆在高速公路匝道上可以停车.', 2, '正确', '错误', , , '规定:驶入确定匝道后,迅速提高车速,但不得超过规定速度,在匝道上,不准超车、掉头、停车、和倒车.', , 0);
INSERT INTO ""."examination_question1" VALUES (457, 457, '车辆驶入匝道后,迅速将车速提高到每小时60公里以上.', 2, '正确', '错误', , , '应该是从匝道进入高速公路加速车道,在加速车道里迅速将车速提高到每小时60公里以上.', , 0);
INSERT INTO ""."examination_question1" VALUES (458, 458, '机动车发生财产损失交通事故,对应当自行撤离现场而未撤离的,交通警察不可以责令当事人撤离现场.', 2, '正确', '错误', , , '当然不是,警察叔叔来了首先就是疏导好交通在处理案子.', , 0);
INSERT INTO ""."examination_question1" VALUES (459, 459, '雾天行车时,应及时开启什么灯?', 3, '倒车灯', '近光灯', '雾灯', '远光灯', '都说是雾天了,当然要用雾灯了,不然要那玩意儿干吗！', , 0);
INSERT INTO ""."examination_question1" VALUES (460, 460, '点火开关在ON位置,车用电器不能使用.', 2, '正确', '错误', , , '点火开关在ON位置的时候全车的电源是连通的,可以用车载电器听音乐等等.', 'http://images.juheapi.com/jztk/c1c2subject1/460.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (461, 461, '驾驶机动车通过窄路、窄桥时的最高速度不能超过每小时30公里.', 1, '正确', '错误', , , '这个是规定,记住吧.', , 0);
