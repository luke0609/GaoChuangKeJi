INSERT INTO ""."examination_question1" VALUES (1, 1, '这个标志是何含义?', 4, '小型车车道', '小型车专用车道', '多乘员车辆专用车道', '机动车车道', '此为机动车车道,比多乘员车辆专用车道少俩人.', 'http://images.juheapi.com/jztk/c1c2subject1/1.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (2, 2, '这个标志是何含义?', 1, '线形诱导标志', '合流诱导标志', '分流诱导标志', '转弯诱导标志', '线型诱导标线型诱导标志,用于引导车辆驾驶人改变行驶方向,促使安全运行.视需要设于易肇事之弯道路段,小半径匝道曲线或中央隔离设施及渠化设施的端部.线形诱导标的颜色规定为:指示性线形诱导标一般道路为蓝底白图案,高速公路为绿底白图案,用以提供一般性行驶指示;警告性线形诱导标为红底白图案,可使车辆驾驶人提高警觉,并准备防范应变之措施.', 'http://images.juheapi.com/jztk/c1c2subject1/2.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (3, 3, '这个标志是何含义?', 1, '车道数变少', '合流处', '应急车道', '向左变道', '三车道变成二车道故车道数变少,选A.', 'http://images.juheapi.com/jztk/c1c2subject1/3.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (4, 4, '这个标志是何含义?', 2, '禁止自行车通行车道', '非机动车车道', '自行车专用车道', '停放自行车路段', '此图为非机动车车道,别误以为自行车专用车道,没有自行车专用车道这一说.', 'http://images.juheapi.com/jztk/c1c2subject1/4.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (5, 5, '这个标志是何含义?', 3, '停车让行', '单行路', '会车先行', '对向先行', '表示会车先行,此标志设在车道以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/5.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (6, 6, '这个标志是何含义?', 4, '右转车道', '掉头车道', '左转车道', '直行车道', '表示只准一切车辆直行.此标志设在直行的路口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/6.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (7, 7, '这个标志是何含义?', 2, '减速慢行', '注意危险', '拥堵路段', '事故多发路段', '注意危险:用以促使车辆驾驶员谨慎慢行.', 'http://images.juheapi.com/jztk/c1c2subject1/7.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (8, 8, '这个标志是何含义?', 2, '车道方向预告', '交叉路口预告', '分道信息预告', '分岔处预告', '此为交叉路口预告,记住吧.', 'http://images.juheapi.com/jztk/c1c2subject1/8.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (9, 9, '这个标志是何含义?', 2, '距有人看守铁路道口50米', '距无人看守铁路道口100米', '距有人看守铁路道口100米', '距无人看守铁路道口50米', '一道红线是50米,二道100米.', 'http://images.juheapi.com/jztk/c1c2subject1/9.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (10, 10, '这个标志是何含义?', 2, '分向行驶车道', '掉头和左转合用车道', '禁止左转和掉头车道', '直行和左转合用车道', '左转和掉头合并在一个标志里,你应该能看到的.', 'http://images.juheapi.com/jztk/c1c2subject1/10.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (11, 11, '这个标志是何含义?', 3, '省道编号', '国道编号', '县道编号', '乡道编号', '乡道是白底Y字开头的,县道是白底X开头的,省道是黄底的,国道是红底的.', 'http://images.juheapi.com/jztk/c1c2subject1/11.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (12, 12, '这个标志是何含义?', 4, '前方40米减速', '最低时速40公里', '限制40吨轴重', '限制最高时速40公里', '限制最高时速40公里:表示该标志至前方限制速度标志的路段内,机动车行驶速度不得超过标志所示数值.此标志设在需要限制车辆速度的路段的起点.以图为例:限制行驶时速不得超过40公里.', 'http://images.juheapi.com/jztk/c1c2subject1/12.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (13, 13, '这个标志是何含义?', 4, '十字交叉路口预告', '互通立体交叉预告', 'Y型交叉路口预告', '环行交叉路口预告', '环形交叉口是在几条相交道路的平面交叉口中央设置一个半径较大的中心岛,使所有经过交叉口的直行和左转车辆都绕着中心岛作逆时针方向行驶,在其行驶过程中将车流的冲突点变  为交织点,从而保证交叉口的行车安全,提高交叉口的通行能力.', 'http://images.juheapi.com/jztk/c1c2subject1/13.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (14, 14, '这个标志是何含义?', 3, '专用停车场', '露天停车场', '室内停车场', '内部停车场', '你看那个P字上边还有个遮挡,说明是室内的,如果没有上边那个遮挡就是露天停车场.', 'http://images.juheapi.com/jztk/c1c2subject1/14.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (15, 15, '这个标志是何含义?', 4, '道路流量监测', '减速拍照区', '全路段抓拍', '交通监控设备', '交通监控设备标志属于指路标志,蓝底、白图形、白边框、蓝色衬边.用于告知机动车驾驶人该区域设置有固定式交通监控设备.', 'http://images.juheapi.com/jztk/c1c2subject1/15.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (16, 16, '这个标志是何含义?', 4, '靠道路左侧停车', '左侧是下坡路段', '只准向左转弯', '靠左侧道路行驶', '靠左侧道路行驶:表示只准一切车辆靠左侧道路行驶.此标志设在车辆必须靠左侧行驶的路口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/16.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (17, 17, '这个标志是何含义?', 2, '大型客车专用车道', '公交线路专用车道', '快速公交系统专用车道', '多乘员车辆专用车道', '此为公交线路专用车道,与快速公交系统专用车道的区别是少了“快速公交”四个字.', 'http://images.juheapi.com/jztk/c1c2subject1/17.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (18, 18, '这个标志是何含义?', 4, '小型汽车专用车道', '机动车专用车道', '出租汽车专用车道', '多乘员车辆专用车道', '此标志为多乘员车辆专用车道,比机动车专用车道标志上的小车多了2个小人,所以它是多乘员车辆专用车道.', 'http://images.juheapi.com/jztk/c1c2subject1/18.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (19, 19, '这个标志是何含义?', 1, '休息区', '服务区', '停车场', '观景台', '这个是休息区,服务区是有加油,通车,吃饭的组合标志,停车场就是一个P,图中没有加油站和吃饭的,所以它只是一个休息区.', 'http://images.juheapi.com/jztk/c1c2subject1/19.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (20, 20, '这个标志是何含义?', 2, '直行和掉头合用车道', '直行和左转合用车道', '直行和右转车道', '分向行驶车道', '表示只准一切车辆直行和向左转弯.此标志设在车辆必须直行和向左转弯的路口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/20.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (21, 21, '这个标志是何含义?', 4, '平面交叉路口', '环行平面交叉', '注意交互式道路', '注意分离式道路', '注意分离式道路:用以警告车辆驾驶人注意前方平面交叉的被交道路是分离式道路.', 'http://images.juheapi.com/jztk/c1c2subject1/21.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (22, 22, '这个标志是何含义?', 1, '丁字交叉路口预告', '道路分叉处预告', 'Y型交叉路口预告', '十字交叉路口预告', '你看它长的多像丁字.', 'http://images.juheapi.com/jztk/c1c2subject1/22.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (23, 23, '这个标志是何含义?', 1, '地点距离', '行驶路线', '终点地名', '行驶方向', '你看那个地点名和KM公里多明显,所以本题的答案是地点距离.如果有箭头的一半是行驶方向.', 'http://images.juheapi.com/jztk/c1c2subject1/23.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (24, 24, '指示标志的作用是什么?', 4, '告知方向信息', '警告前方危险', '限制车辆、行人通行', '指示车辆、行人行进', '指示标志是交通标志中主要标志的一种,用以指示车辆和行人按规定方向、地点行驶. 指示标志的颜色为蓝底、白图案；形状分为圆形、长方形和正方形.', , 0);
INSERT INTO ""."examination_question1" VALUES (25, 25, '这个标志是何含义?', 2, '塌方路段', '施工路段', '前方工厂', '道路堵塞', '施工路段:用以告示前方道路施工,车辆应减速慢行或绕道行驶.', 'http://images.juheapi.com/jztk/c1c2subject1/25.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (26, 26, '这个标志是何含义?', 3, '注意危险', '右侧绕行', '左侧绕行', '单向通行', '左侧绕行:用以告示前方道路有障碍物,车辆应按标志指示减速慢行,左侧绕行通过.', 'http://images.juheapi.com/jztk/c1c2subject1/26.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (27, 27, '这个标志是何含义?', 3, '直行和左转车道', '直行和辅路出口车道', '直行和右转合用车道', '分向行驶车道', '表示只准一切车辆直行和向右转弯.此标志设在车辆必须直行和向右转弯）的路口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/27.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (28, 28, '这个标志是何含义?', 4, '限制宽度为3.5米', '解除3.5米限高', '限制车距为3.5米', '限制高度为3.5米', '限制高度为3.5米:表示禁止装载高度超过标志所示数值的车辆通行.此标志设在最大允许高度受限制的地方. 以图为例:装载高度不得超过3.5米.', 'http://images.juheapi.com/jztk/c1c2subject1/28.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (29, 29, '这个标志是何含义?', 1, '禁止通行', '减速行驶', '限时进入', '禁止驶入', '禁止通行:表示禁止一切车辆和行人通行.此标志设在禁止通行的道路入口处.', 'http://images.juheapi.com/jztk/c1c2subject1/29.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (30, 30, '这个标志是何含义?', 2, '十字交叉路口预告', '互通式立体交叉预告', 'Y型交叉路口预告', '环行交叉路口预告', '互通式立体交叉:路线交叉中的一种重要类型.交叉道路之间立体交叉并以匝道相互连通以实现交通转换的交叉.', 'http://images.juheapi.com/jztk/c1c2subject1/30.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (31, 31, '这个标志是何含义?', 2, '右侧是下坡路段', '靠右侧道路行驶', '靠道路右侧停车', '只准向右转弯', '靠右侧道路行驶:表示只准一切车辆靠右侧道路行驶.此标志设在车辆必须靠右侧行驶的路口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/31.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (32, 32, '这个标志是何含义?', 4, '禁止变道', '禁止左转', '禁止直行', '禁止掉头', '禁止掉头:表示前方路口禁止一切车辆掉头.此标志设在禁止掉头的路口前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/32.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (33, 33, '这个标志是何含义?', 3, '注意双向行驶', '靠两侧行驶', '注意潮汐车道', '可变车道', '“潮汐车道”是指根据早晚交通流量不同情况,对有条件的道路,试点开辟潮汐车道,通过车道灯的指示方向变化,控制主干道车道行驶方向,来调整车道数.', 'http://images.juheapi.com/jztk/c1c2subject1/33.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (34, 34, '这个标志是何含义?', 2, '直行和向左转弯', '直行和向右转弯', '禁止直行和向右转弯', '只准向左和向右转弯', '直行和向右转弯:表示只准一切车辆直行和向右转弯.此标志设在车辆必须直行和向右转弯）的路口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/34.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (35, 35, '这个标志是何含义?', 2, '禁止直行和向左变道', '禁止直行和向左转弯', '允许直行和向左变道', '禁止直行和向右转弯', '禁止直行和向左转弯:表示前方路口禁止一切车辆直行和向左转弯.此标志设在禁止直行和向左转弯的路口前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/35.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (36, 36, '这个标志是何含义?', 1, '观景台', '停车场', '休息区', '停车位', '观景台那三个字多明显,你再不选它,叫它情何以堪.', 'http://images.juheapi.com/jztk/c1c2subject1/36.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (37, 37, '这属于哪一类标志?', 1, '指路标志', '指示标志', '禁令标志', '警告标志', '他是指方向的故为指路标志.', 'http://images.juheapi.com/jztk/c1c2subject1/37.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (38, 38, '这个标志是何含义?', 4, '乡道编号', '县道编号', '省道编号', '国道编号', '乡道是白底Y字开头的,县道是白底X开头的,省道是黄底的,国道是红底的.', 'http://images.juheapi.com/jztk/c1c2subject1/38.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (39, 39, '这个标志是何含义?', 1, '距无人看守铁路道口150米', '距无人看守铁路道口100米', '距有人看守铁路道口100米', '距有人看守铁路道口150米', '一道红线是50米,三道150米.', 'http://images.juheapi.com/jztk/c1c2subject1/39.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (40, 40, '这个标志是何含义?', 2, '两侧街道', '干路先行', '停车让行', '单行路', '表示干路先行,此标志设在车道以前适当位置.答题技巧:记住哥一句话,谁粗谁先行.', 'http://images.juheapi.com/jztk/c1c2subject1/40.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (41, 41, '这个标志是何含义?', 1, '向右转弯', '单行路', '只准直行', '直行车道', '向右转弯:表示只准一切车辆向右转弯.此标志设在车辆必须向右转弯的路口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/41.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (42, 42, '这个标志是何含义?', 4, '允许临时停车', '允许长时停车', '禁止长时停车', '禁止停放车辆', '禁止停放车辆:表示在限定的范围内,禁止一切车辆临时或长时停放.此标志设在禁止车辆停放的地方.禁止车辆停放的时间、车种和范围可用辅助标志说明.', 'http://images.juheapi.com/jztk/c1c2subject1/42.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (43, 43, '这个标志是何含义?', 4, '左转行驶车道', '直线行驶车道', '右转行驶车道', '分向行驶车道', '同向多车道的公路一般都设计了分向行驶车道,正常行驶过程中可以变更车道,当你行驶至路口时要变换到你要行驶的车道.例如要左转弯时要提前变更车道至左转弯车道；要右转弯时要提前变更车道至右转弯车道,不能占用他人行车道,以最大限度的保障通行.', 'http://images.juheapi.com/jztk/c1c2subject1/43.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (44, 44, '这个标志是何含义?', 3, '有人看守铁路道口', '无人看守铁路道口', '多股铁路与道路相交', '注意避让火车', '多股铁路与道路相交,则应在铁路道口标志上方设置叉形符号.叉形符号交叉点到警告标志三角形顶点的距离为40cm.', 'http://images.juheapi.com/jztk/c1c2subject1/44.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (45, 45, '这个标志是何含义?', 3, '禁止向右转弯', '禁止掉头', '禁止直行', '禁止向左转弯', '禁止直行:表示前方路口禁止一切车辆直行.此标志设在禁止直行的路口前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/45.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (46, 46, '这个标志是何含义?', 2, '禁止临时停车', '禁止长时停车', '禁止停放车辆', '允许长时停车', '禁止长时停车:禁止车辆长时停放,临时停放不受限制.禁止车辆停放的时间、车种和范围可用辅助标志说明.', 'http://images.juheapi.com/jztk/c1c2subject1/46.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (47, 47, '这个标志是何含义?', 3, '隧道减速', '隧道开远光灯', '隧道开灯', '隧道开示宽灯', '这个应该能看懂吧?很明显那个小车开灯的效果.', 'http://images.juheapi.com/jztk/c1c2subject1/47.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (48, 48, '驾驶机动车在前方路口不能右转弯.', 2, '正确', '错误', , , '黄灯闪烁是警示的意思,没有不能右转的意思.', 'http://images.juheapi.com/jztk/c1c2subject1/48.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (49, 49, '这个标志是何含义?', 4, 'Y型交叉口', '主路让行', '注意分流', '注意合流', '黄底为警告标志,所以排除A和B,图很明显的为合流标志,所以选D', 'http://images.juheapi.com/jztk/c1c2subject1/49.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (50, 50, '这个标志是何含义?', 1, '事故易发路段', '施工路段', '减速慢行路段', '拥堵路段', '事故易发路段:此标志设在交通事故易发路段以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/50.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (51, 51, '这个标志是何含义?', 2, '两侧通行', '右侧通行', '左侧通行', '不准通行', '用于引导行车方向,提示道路使用者前方线形变化,注意谨慎驾驶.设于中央隔离设施端部、渠化设施的端部、桥头等.答题技巧:∧表示俩侧通行,/这个方向的表示左侧通行,这个方向表示右侧通行.交通指路标志大全:http://www.jszks.com/onepage/16752.html', 'http://images.juheapi.com/jztk/c1c2subject1/51.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (52, 52, '这个标志是何含义?', 1, '掉头', '倒车', '左转', '绕行', '允许掉头标志', 'http://images.juheapi.com/jztk/c1c2subject1/52.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (53, 53, '禁令标志的作用是什么?', 3, '指示车辆行进', '警告前方危险', '禁止或限制行为', '告知方向信息', '禁令标志是交通标志中主要标志的一种,对车辆加以禁止或限制的标志,如禁止通行、禁止停车、禁止左转弯、禁止鸣喇叭、限制速度、限制重量等.', , 0);
INSERT INTO ""."examination_question1" VALUES (54, 54, '驾驶机动车在这种情况下可以右转弯.', 2, '正确', '错误', , , '看信号灯只有左转的灯是绿色,所以这种情况下可以左转.', 'http://images.juheapi.com/jztk/c1c2subject1/54.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (55, 55, '这个标志是何含义?', 1, '停车让行', '不准临时停车', '不准车辆驶入', '不准长时间停车', '停车让行:表示车辆必须在停止线以外停车了望,确认安全后,才准许通行.停车让行标志在下列情况下设置:(1)与交通流量较大的干路平交的支路路口；(2)无人看守的铁路道口；(3)其他需要设置的地方.', 'http://images.juheapi.com/jztk/c1c2subject1/55.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (56, 56, '这个标志是何含义?', 2, '直行和向右转弯', '直行和向左转弯', '禁止直行和向左转弯', '只准向右和向左转弯', '直行和向左转弯:表示只准一切车辆直行和向左转弯.此标志设在车辆必须直行和向左转弯的路口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/56.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (57, 57, '这个标志是何含义?', 4, '40米减速行驶路段', '最低时速40公里', '最高时速40公里', '解除时速40公里限制', '解除时速40公里限制:表示限制速度路段结束.此标志设在限制车辆速度路段的重点.', 'http://images.juheapi.com/jztk/c1c2subject1/57.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (58, 58, '这个标志是何含义?', 4, '右侧通行', '左侧通行', '向右行驶', '环岛行驶', '环岛行驶:表示只准车辆靠右环行.此标志设在环岛面向路口来车方向适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/58.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (59, 59, '红色圆圈内标线含义是什么?', 3, '临时停靠站', '大客车停靠站', '公交车停靠站', '应急停车带', '都写了公交站了,不许选错了', 'http://images.juheapi.com/jztk/c1c2subject1/59.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (60, 60, '这个标志是何含义?', 2, '车多路段', '慢行', '拥堵路段', '施工路段', '慢行:此标志设在前方需要减速慢行的路段以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/60.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (61, 61, '这个标志是何含义?', 1, '有人看守铁路道口', '多股铁路与道路相交', '立交式的铁路道口', '无人看守铁路道口', '有火车形状的,为无人看守的铁路口,此图为有人看守的铁路口.', 'http://images.juheapi.com/jztk/c1c2subject1/61.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (62, 62, '这个标志是何含义?', 3, '路面低洼', '驼峰桥', '路面不平', '路面高突', '路面不平:用以提醒车辆驾驶人减速慢行.设在路面颠簸路段或桥头跳车较严重的地点以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/62.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (63, 63, '这个标志是何含义?', 3, '不准小型车通行', '只准小型车行驶', '机动车行驶', '禁止小型车行驶', '看到小汽车你应该想到的是机动车行驶.', 'http://images.juheapi.com/jztk/c1c2subject1/63.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (64, 64, '这个标志是何含义?', 3, '环行交叉路口预告', '十字交叉路口预告', 'Y型交叉路口预告', '丁字交叉路口预告', '俩个箭头加上你来的道路形成一个Y字.', 'http://images.juheapi.com/jztk/c1c2subject1/64.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (65, 65, '这个路面标记是何含义?', 1, '非机动车道', '摩托车专用道', '电瓶车专用道', '自行车专用道', '这是非机动车（就是不烧油的）不要把四轮的开到他们的道上', 'http://images.juheapi.com/jztk/c1c2subject1/65.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (66, 66, '这个标志是何含义?', 1, '易滑路段', '急转弯路', '试车路段', '曲线路段', '易滑路段:此标志设在路面的摩擦系数不能满足相应行驶速度下要求紧急刹车距离的路段前适当位置.行驶至此路段必须减速慢行.', 'http://images.juheapi.com/jztk/c1c2subject1/66.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (67, 67, '这个标志是何含义?', 1, '两侧通行', '不准通行', '左侧通行', '右侧通行', '用于引导行车方向,提示道路使用者前方线形变化,注意谨慎驾驶.设于中央隔离设施端部、渠化设施的端部、桥头等.∧表示俩侧通行,/这个方向的表示左侧通行,这个方向表示右侧通行.交通指路标志大全:http://www.jszks.com/onepage/16752.html', 'http://images.juheapi.com/jztk/c1c2subject1/67.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (68, 68, '这个标志是何含义?', 1, '隧道出口距离', '隧道入口距离', '隧道跟车距离', '隧道总长度', '用于指示到前方隧道出口的距离.设在长度超过3 000 m的特长隧道内,从距离隧道出口2 000m处开始每500 m设置一块,直至隧道出口.该标志一般设置在隧道侧壁上.不要纠结选D,正确答案就是A,牢记吧.交通指路标志大全:http://www.jszks.com/onepage/16752.html', 'http://images.juheapi.com/jztk/c1c2subject1/68.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (69, 69, '这个标志是何含义?', 3, '高速公路右侧出口预告', '高速公路下一出口预告', '高速公路地点、方向预告', '高速公路左侧出口预告', '高速公路地点、方向预告:用于指示高速公路或城市快速路两个行驶方向,设在驶入高速公路或城市快速路的匝道分岔点处.在目的地信息之上,可增加前往高速公路的编号信息.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/69.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (70, 70, '这个标志是何含义?', 3, '高速公路下一出口预告', '高速公路右侧出口预告', '高速公路左侧出口预告', '高速公路目的地预告', '高速公路左侧出口预告:用于预告前方出口,在距离高速公路或城市快速路减速车道的渐变段起点处.出口预告标志应同时附着出口编号标志.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/70.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (71, 71, '这个标志是何含义?', 1, '掉头车道', '绕行车道', '分向车道', '左转车道', '常识题.', 'http://images.juheapi.com/jztk/c1c2subject1/71.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (72, 72, '机动车号牌损毁,机动车所有人要向登记地车辆管理所申请补领、换领.', 1, '正确', '错误', , , '常识题,机动车号牌损毁上路的直接扣12分.', , 0);
INSERT INTO ""."examination_question1" VALUES (73, 73, '绿灯亮表示前方路口允许机动车通行.', 1, '正确', '错误', , , '常识题', , 0);
INSERT INTO ""."examination_question1" VALUES (74, 74, '路面上的白色标线是何含义?', 2, '道路施工提示标线', '车行道横向减速标线', '车行道纵向减速标线', '车道变少提示标线', '唉,横着的线 这么多 肯定得减速慢待多观察了.', 'http://images.juheapi.com/jztk/c1c2subject1/74.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (75, 75, '这个标志是何含义?', 4, '高速公路报警电话', '高速公路公用电话', '高速公路紧急电话', '高速公路救援电话', '高速公路救援电话:用于指示救援电话号码,没有设置紧急电话的高速公路上应设置救援电话标志.标志上号码为示例.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/75.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (76, 76, '这个标志是何含义?', 4, '大型客车专用车道', '多乘员车专用车道', '公交车专用车道', 'BRT车辆专用车道', '此图为BRT车辆专用车道,与公交车专用车道的区别是多了“快速公交”四个字.', 'http://images.juheapi.com/jztk/c1c2subject1/76.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (77, 77, '这个标志是何含义?', 3, '傍山险路', '悬崖路段', '注意落石', '危险路段', '注意落石:此标志设在左侧有落石危险的傍山路段之前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/77.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (78, 78, '驾驶机动车不按照规定避让校车的,一次记6分.', 1, '正确', '错误', , , '记住吧.', , 0);
INSERT INTO ""."examination_question1" VALUES (79, 79, '路面上的黄色填充标线是何含义?', 3, '接近移动障碍物标线', '远离狭窄路面标线', '接近障碍物标线', '接近狭窄路面标线', '双黄线就是警告注意越界,“打叉的物体”就是不可移动障碍物.因此选 接近障碍物标线.', 'http://images.juheapi.com/jztk/c1c2subject1/79.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (80, 80, '路面上导向箭头是何含义?', 3, '指示前方道路是Y型交叉口', '指示前方道路是分离式道路', '指示前方道路仅可左右转弯', '指示前方道路需向左右合流', '可以左转也可以右转,三叉路口', 'http://images.juheapi.com/jztk/c1c2subject1/80.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (81, 81, '这个标志是何含义?', 4, '高速公路救援电话号码', '高速公路服务电话号码', '高速公路报警电话号码', '高速公路交通广播频率', '高速公路交通广播频率:用以指示收听高速公路或城市快速路交通信息广播的频率,可在适当地点设置,根据需要可重复设置.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/81.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (82, 82, '这个标志是何含义?', 4, '会车时停车让右侧车先行', '右侧道路禁止车通行', '前方是双向通行路段', '会车时停车让对方车先行', '会车时停车让对方车先行:表示车辆会车时,必须停车让对方车先行.设置在会车有困难的狭窄路段的一端或由于某种原因只能开放一条车道作双向通行路段的一端.', 'http://images.juheapi.com/jztk/c1c2subject1/82.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (83, 83, '这个标志是何含义?', 1, '高速公路停车区预告', '高速公路避险处预告', '高速公路停车场预告', '高速公路服务区预告', '高速公路停车区是有茶杯的,高速公路停车场是只有一个P.', 'http://images.juheapi.com/jztk/c1c2subject1/83.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (84, 84, '醉酒驾驶机动车在道路上行驶会受到什么处罚?', 3, '处管制,并处罚金', '处2年以上徒刑', '处拘役,并处罚金', '处2年以下徒刑', '醉酒驾驶,由公安机关约束至酒醒.吊销机驾驶证,依法追究刑事责任,5年内不得重新取得驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (85, 85, '这个标志是何含义?', 3, '禁止鸣高音喇叭', '禁止鸣低音喇叭', '应当鸣喇叭', '禁止鸣喇叭', '表示机动车行至该标志处必须鸣喇叭.此标志设在公路的急转弯处、陡坡等视线不良路段的起点.', 'http://images.juheapi.com/jztk/c1c2subject1/85.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (86, 86, '这个标志是何含义?', 4, '左转让行', '直行单行路', '向右单行路', '向左单行路', '表示一切车辆向左单向行驶.此标志设在单行路的路口和入口处的适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/86.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (87, 87, '这个路面标记是何含义?', 4, '平均速度为100公里/小时', '最低限速为100公里/小时', '解除100公里/小时限速', '最高限速为100公里/小时', '黄色是警示你别超了,一越你就黄了', 'http://images.juheapi.com/jztk/c1c2subject1/87.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (88, 88, '这个标志是何含义?', 2, '禁止向左转弯', '向左和向右转弯', '禁止向左右转弯', '禁止向右转弯', '向左和向右转弯:表示只准一切车辆向左和向右转弯.此标志设在车辆必须向左和向右转弯的路口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/88.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (89, 89, '路缘石上的黄色实线是何含义?', 4, '仅允许上下人员', '仅允许装卸货物', '禁止长时间停车', '禁止停放车辆', '好人记住吧', 'http://images.juheapi.com/jztk/c1c2subject1/89.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (90, 90, '这个标志是何含义?', 2, '路面不平', '路面高突', '路面低洼', '驼峰桥', '路面高突:用以提醒车辆驾驶人减速慢行.设在路面突然高突以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/90.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (91, 91, '这个标志是何含义?', 4, '露天停车场', '紧急停车带', '停车位', '错车道', '错车道指的是在单车道道路上,可通视的一定距离内,供车辆交错避让用的一段加宽车道.', 'http://images.juheapi.com/jztk/c1c2subject1/91.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (92, 92, '图中圈内黄色虚线是什么标线?', 2, '非机动车引导线', '路口导向线', '车道连接线', '小型车转弯线', '转向时按黄虚线行驶', 'http://images.juheapi.com/jztk/c1c2subject1/92.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (93, 93, '这个标志是何含义?', 3, '直行和左转弯行驶', '直行和右转弯行驶', '立体交叉直行和右转弯行驶', '立体交叉直行和左转弯行驶', '立体交叉直行和右转弯行驶:表示车辆在立交处可以直行和按图示路线右转弯行驶.此标志设在立交右转弯出口处适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/93.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (94, 94, '指示标线的作用是什么?', 2, '禁止通行', '指示通行', '限制通行', '警告提醒', '指示标线主要就是引导你正确的行驶,安全守法', , 0);
INSERT INTO ""."examination_question1" VALUES (95, 95, '这个标志是何含义?', 1, '十字交叉路口', '环行交叉路口', 'T型交叉路口', 'Y型交叉路口', '十字交叉路口:除了基本形十字路口外,还有部分变异的十字路口,如:五路交叉路口、变形十字路口、变形五路交叉路口等.五路以上的路口均按十字路口对待.', 'http://images.juheapi.com/jztk/c1c2subject1/95.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (96, 96, '图中圈内白色虚线是什么标线?', 4, '小型车转弯线', '车道连接线', '非机动车引导线', '路口导向线', '路口导向线:车辆左转弯一律线内行驶.', 'http://images.juheapi.com/jztk/c1c2subject1/96.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (97, 97, '路面上白色虚线和三角地带标线组成的是什么标线?', 1, '道路入口标线', '可跨越式分道线', '道路入口减速线', '道路出口标线', '道路入口标线为白色虚线,主要用于高速公路和其他采用立体交叉道路.该标线作用为,高速公路出入口横向标线或三角地带标线是为驶入或驶出匝道车辆提供安全交汇引导,减少与突出部缘石碰撞.', 'http://images.juheapi.com/jztk/c1c2subject1/97.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (98, 98, '驾驶人违反交通运输管理法规发生重大事故后,逃逸或者有其他特别恶劣情节的,处7年以上有期徒刑.', 2, '正确', '错误', , , '应该是除以3年以上7年以下有期徒刑.', , 0);
INSERT INTO ""."examination_question1" VALUES (99, 99, '路中心白色实线是何含义?', 2, '单侧可跨越同向车道分界线', '禁止跨越同向车行道分界线', '双侧可跨越同向车道分界线', '禁止跨越对向车行道分界线', '实线不能压,如果两个同向车道之间有虚线就可以换道,如果是实线则规规矩矩的别跑偏了', 'http://images.juheapi.com/jztk/c1c2subject1/99.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (100, 100, '这个地面标记是什么标线?', 1, '人行横道预告', '交叉路口预告', '减速让行预告', '停车让行预告', '这种标线在到达人行横道前的道路30-50米处设置,它的作用是用来提示驾驶员,前方已接近人行横道,应减速慢行,并须注意行人横过马路', 'http://images.juheapi.com/jztk/c1c2subject1/100.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (101, 101, '使用其他机动车号牌、行驶证的一次记3分.', 2, '正确', '错误', , , '使用伪造、变造的机动车号牌、行驶证、驾驶证、校车标牌或者使用其他机动车号牌、行驶证的一次记12分.', , 0);
INSERT INTO ""."examination_question1" VALUES (102, 102, '雾天对安全行车的主要影响是什么?', 3, '发动机易熄火', '易发生侧滑', '能见度低,视线不清', '行驶阻力增大', '能见度低,视线不清；你看不见我,我看不见你；如果车速快,可能就“吻”一起了,要刹得牢就得开慢点,特别注意前后左右突然窜出来大神们！！！', , 0);
INSERT INTO ""."examination_question1" VALUES (103, 103, '这个路面数字标记是何含义?', 3, '保持车距标记', '最小间距标记', '速度限制标记', '道路编号标记', '图中的意思是最低60,最高80,用来提醒你速度应该限制在什么范围里', 'http://images.juheapi.com/jztk/c1c2subject1/103.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (104, 104, '这个路口允许车辆怎样行驶?', 1, '直行或向右转弯', '向左转弯', '直行或向左转弯', '向左、向右转弯', '请看灯,直行和右转灯是绿的.', 'http://images.juheapi.com/jztk/c1c2subject1/104.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (105, 105, '遇到这种情况时怎样行驶?', 4, '加速进入两侧车道行驶', '进入右侧车道行驶', '减速进入两侧车道行驶', '禁止车辆在两侧车道通行', '俩侧车道都是红灯都不可以通行所以是禁止车辆在两侧车道通行.', 'http://images.juheapi.com/jztk/c1c2subject1/105.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (106, 106, '这个标志是何含义?', 1, '高速公路右侧出口预告', '高速公路目的地预告', '高速公路左侧出口预告', '高速公路下一出口预告', '高速公路右侧出口预告:用于预告前方出口,设置在距离高速公路或城市快速路减速车道的渐变段起点处.出口预告标志应同时附着出口编号标志.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/106.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (107, 107, '这个标志是何含义?', 2, '向左单行路', '向右单行路', '直行单行路', '右转让行', '表示一切车辆向右单向行驶.此标志设在单行路的路口和入口处的适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/107.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (108, 108, '驾驶机动车发生财产损失交通事故后,当事人对事实及成因无争议移动车辆时需要对现场拍照或者标划停车位置.', 1, '正确', '错误', , , '这个是正确的,移动车子使得交通顺畅,拍照等是为了留下证据.', , 0);
INSERT INTO ""."examination_question1" VALUES (109, 109, '交通警察对未放置保险标志上道路行驶的车辆可依法扣留行驶证.', 2, '正确', '错误', , , '应该是扣留车辆,不是扣留行驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (110, 110, '这辆红色轿车可以在该车道行驶.', 2, '正确', '错误', , , '看红色轿车行驶的车道的信号灯是红色,所以是不能在该道行驶的.', 'http://images.juheapi.com/jztk/c1c2subject1/110.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (111, 111, '路中白色虚线是什么标线?', 4, '禁止跨越对向车道中心线', '限制跨越对向车道中心线', '单向行驶车道分界中心线', '可跨越同向车道中心线', '同向两车道之间的标线,没线怕乱开', 'http://images.juheapi.com/jztk/c1c2subject1/111.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (112, 112, '路右侧车行道边缘白色虚线是什么含义?', 1, '车辆可临时越线行驶', '车辆禁止越线行驶', '应急车道分界线', '人行横道分界线', '白虚线可压', 'http://images.juheapi.com/jztk/c1c2subject1/112.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (113, 113, '这个标志是何含义?', 2, 'Y型交叉路口', 'T型交叉路口', '十字交叉路口', '环行交叉路口', 'T型交叉路口:丁字形标志原则上设在与交叉口形状相符的道路上.右侧丁字路口,此标志设在进入T字路口以前的适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/113.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (114, 114, '路两侧的车行道边缘白色实线是什么含义?', 2, '车辆可临时跨越', '禁止车辆跨越', '机动车可临时跨越', '非机动车可临时跨越', '白虚线不可以跨越', 'http://images.juheapi.com/jztk/c1c2subject1/114.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (115, 115, '这个路口允许车辆怎样行驶?', 3, '直行或向右转弯', '向右转弯', '向左转弯', '直行', '左转的灯是绿色,所以可以向左转弯.', 'http://images.juheapi.com/jztk/c1c2subject1/115.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (116, 116, '冰雪道路对安全行车的主要影响是什么?', 4, '电器设备易受潮短路', '能见度降低,视野模糊', '行驶阻力增大', '制动性能差,方向易跑偏', '因汽车轮胎与路面的摩擦系数减小、附着力大大降低,汽车驱动轮很容易打滑或空转,尤其是上坡、起步、停车时还会出现后溜车的现象；车辆在行驶中如果突然加速或减速,很容易造成侧滑及方向跑偏现象；遇情况紧急制动时,制动距离会大大延长.', , 0);
INSERT INTO ""."examination_question1" VALUES (117, 117, '图中圈内的白色折线是什么标线?', 1, '车距确认线', '减速行驶线', '车速确认线', '路口减速线', '车距确认标线作为车辆驾驶人保持行车安全距离的参考,视需要设于较长直线段、易发生追尾事故或其他需要的路段,应与车距确认标志配合使用.', 'http://images.juheapi.com/jztk/c1c2subject1/117.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (118, 118, '图中路口中央黄色路面标记是什么标线?', 3, '中心圈', '导流线', '网状线', '停车区', '路面黄色网状线表示严格禁止一切车辆长时或临时停车,防止交通阻塞.当黄色网状线前方有车辆停驶时,后车必须在黄色网状线外等候,直到确认黄色网状线前方有足够空间停驶本车时,方可驶过黄色网状线. 另有黄色网状线是施画在重要国家机关、军队、大型企事业单位和公交枢纽门前,确保车辆进出时不致被门前拥堵车辆堵住的措施.网状线是黄色线,停车时禁止占用.', 'http://images.juheapi.com/jztk/c1c2subject1/118.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (119, 119, '这个标志是何含义?', 1, '堤坝路', '临崖路', '傍水路', '易滑路', '堤坝路:用以提醒车辆驾驶人小心驾驶,设在沿水库、湖泊、河流等堤坝道路以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/119.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (120, 120, '水淹路面影响行车安全,不易通行的原因是什么?', 2, '路面附着力增大', '无法观察到暗坑和凸起的路面', '能见度低,视野模糊', '日光反射阻挡视线', '因为水淹没了路面,所以看不清是暗坑和凸起的路面', , 0);
INSERT INTO ""."examination_question1" VALUES (121, 121, '夜间行车,驾驶人视距变短,影响观察,同时注意力高度集中,易产生疲劳.', 1, '正确', '错误', , , '高度集中注意力,很费神的,久了就会疲倦.', , 0);
INSERT INTO ""."examination_question1" VALUES (122, 122, '这个导向箭头是何含义?', 4, '指示前方可左转或掉头', '指示前方可直行或左转', '指示前方直行向左变道', '指示前方可直行或掉头', '直行或者掉头都可以', 'http://images.juheapi.com/jztk/c1c2subject1/122.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (123, 123, '图中圈内两条黄色虚线间的区域是何含义?', 4, '营运客车专用车道', '大客车专用车道', '出租车专用车道', '公交专用车道', '黄色虚线中的车道里有写字,黄色虚线是想突出引动驾驶员注意的,知道选啥了没?看图就懂！', 'http://images.juheapi.com/jztk/c1c2subject1/123.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (124, 124, '这个标志是何含义?', 4, '减速让行', '潮汐车道', '分离式道路', '双向交通', '双向交通:用以提醒车辆驾驶人注意会车.设在由双向分离行驶,因某种原因出现临时性或永久性的不分离双向行驶的路段,或由单向行驶进人双向行驶的路段以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/124.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (125, 125, '这个标志是何含义?', 1, '人行横道', '学生通道', '注意行人', '儿童通道', '表示该处为专供行人横穿马路的通道.此标志设在人行横道的两侧.', 'http://images.juheapi.com/jztk/c1c2subject1/125.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (126, 126, '这个标志预告什么?', 4, '高速公路服务区预告', '高速公路避险处预告', '高速公路客车站预告', '高速公路停车场预告', '高速公路停车区和高速公路停车场的区别是停车区是有一个茶杯的,停车场只有一个P.', 'http://images.juheapi.com/jztk/c1c2subject1/126.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (127, 127, '驾驶人记分没有达到满分,有罚款尚未缴纳的,记分转入下一记分周期.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法实施条例》第二十四条 机动车驾驶人在一个记分周期内记分未达到12分,所处罚款已经缴纳的,记分予以清除；记分虽未达到12分,但尚有罚款未缴纳的,记分转入下一记分周期.', , 0);
INSERT INTO ""."examination_question1" VALUES (128, 128, '黄灯持续闪烁,表示机动车可以加速通过.', 2, '正确', '错误', , , '黄灯持续闪烁的意思是,提示用户谨慎通行,此刻应该放慢速度谨慎通过.', , 0);
INSERT INTO ""."examination_question1" VALUES (129, 129, '使用伪造、变造的机动车号牌一次记几分?', 3, '2分', '3分', '12分', '6分', '使用伪造、变造的机动车号牌的一次记12分.', , 0);
INSERT INTO ""."examination_question1" VALUES (130, 130, '这一组交通警察手势是什么信号?', 4, '减速慢行信号', '靠边停车信号', '停止信号', '右转弯信号', '手势题详解:http://www.jszks.com/onepage/16203.html', 'http://images.juheapi.com/jztk/c1c2subject1/130.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (131, 131, '这个标志是何含义?', 3, '省道编号', '县道编号', '乡道编号', '国道编号', '乡道是白底Y字开头的,县道是白底X开头的,省道是黄底的,国道是红底的.', 'http://images.juheapi.com/jztk/c1c2subject1/131.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (132, 132, '这一组交通警察手势是什么信号?', 4, '右转弯信号', '减速慢行信号', '靠边停车信号', '变道信号', '手势题详解:http://www.jszks.com/onepage/16203.html', 'http://images.juheapi.com/jztk/c1c2subject1/132.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (133, 133, '这个标志是何含义?', 4, '向右变道', '分流处', '路面变宽', '车道数增加', '俩个车道变成三个,表示车道数增加.', 'http://images.juheapi.com/jztk/c1c2subject1/133.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (134, 134, '这个导向箭头是何含义?', 3, '指示向左变道', '指示前方直行', '指示前方左转弯', '指示前方右转弯', '请记住了哦,不难的吧', 'http://images.juheapi.com/jztk/c1c2subject1/134.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (135, 135, '这个标志是何含义?', 4, '国界', '边防', '边界', '海关', '海关那俩个大字那么清晰你再不选D,D得有多伤心.', 'http://images.juheapi.com/jztk/c1c2subject1/135.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (136, 136, '驾驶机动车在铁路道口看到这种信号灯时怎样行驶?', 2, '在火车到来前通过', '不得越过停止线', '边观察边缓慢通过', '不换挡加速通过', '红灯,你就停了吧.', 'http://images.juheapi.com/jztk/c1c2subject1/136.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (137, 137, '这个标志是何含义?', 1, '立体交叉直行和左转弯行驶', '立体交叉直行和右转弯行驶', '直行和左转弯行驶', '直行和右转弯行驶', '立体交叉直行和左转弯行驶:表示车辆在立交处可以直行和按图示路线左转弯行驶.此标志设在立交左转弯出口处适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/137.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (138, 138, '这个导向箭头是何含义?', 3, '指示向左变道', '指示前方直行', '指示前方掉头', '指示前方右转', '请记住了 so easy!!', 'http://images.juheapi.com/jztk/c1c2subject1/138.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (139, 139, '夜间道路环境对安全行车的主要影响是什么?', 3, '驾驶人体力下降', '驾驶人易产生冲动、幻觉', '能见度低、不利于观察道路交通情况', '路面复杂多变', '夜间行车视线差,在没有照明的路段,驾驶人视线普遍受到影响,不能清楚看到前方事物,或者不能看清和判断前方事物的距离,尤其是在会车的情况下,由于受对向车辆灯光的影响,会产生视觉的盲点,导致看不清或看不见前方事物,夜间特大道路交通事故明显高于白天.夜间行车由于不能见到道路两旁的景观,对驾驶员兴奋性刺激物小,因此最易产生驾驶疲劳,如稍有感觉就应振作精神或停车休息片刻.', , 0);
INSERT INTO ""."examination_question1" VALUES (140, 140, '这个标志是何含义?', 3, '内部停车场', '专用停车场', '露天停车场', '室内停车场', '如果P字上边没有遮挡就是露天停车场.', 'http://images.juheapi.com/jztk/c1c2subject1/140.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (141, 141, '这个标志是何含义?', 2, '直行车道', '只准直行', '单行路', '禁止直行', '只准直行:表示只准一切车辆直行.此标志设在直行的路口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/141.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (142, 142, '这个标志是何含义?', 1, '驼峰桥', '路面高突', '路面低洼', '不平路面', '驼峰桥:用以提醒车辆驾驶人谨慎驾驶.设在拱度很大,影响视距的驼峰桥以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/142.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (143, 143, '这个标志是何含义?', 3, '禁止在路口掉头', '禁止向左向右变道', '禁止向左向右转弯', '禁止车辆直行', '禁止向左向右转弯:表示前方路口禁止一切车辆向左向右转弯.此标志设在禁止向左向右转弯的路口前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/143.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (144, 144, '这个标志提示哪种车型禁止通行?', 2, '各种车辆', '小型客车', '中型客车', '小型货车', '一看图片就是小型客车.', 'http://images.juheapi.com/jztk/c1c2subject1/144.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (145, 145, '这个标志是何含义?', 1, '最低限速50公里/小时', '高度限速50公里/小时', '水平高度50米', '海拔高度50米', '表示机动车驶入前方道路之最低时速限制.此标志设在高速公路或其他道路限速路段的起点.高度限速标志是白底红字.', 'http://images.juheapi.com/jztk/c1c2subject1/145.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (146, 146, '这个标志是何含义?', 1, '避险车道', '应急车道', '路肩', '急弯道', '避险车道是指在长陡下坡路段行车道外侧增设的供速度失控车辆驶离正线安全减速的专用车道.', 'http://images.juheapi.com/jztk/c1c2subject1/146.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (147, 147, '这个标志是何含义?', 1, '应急避难场所', '生活服务区', '行人专用通道', '横过道路设施', '你看他的动作时在跑,所以是紧急的情况,所以答案是A.应急避难场所是应对突发公共事件的一项灾民安置措施,是现代化大城市用于民众躲避火灾、爆炸、洪水、地震、疫情等重大突发公共事件的安全避难场所.', 'http://images.juheapi.com/jztk/c1c2subject1/147.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (148, 148, '这个标志是何含义?', 3, '禁止车辆掉头', '禁止向左变道', '禁止向左转弯', '禁止驶入左车道', '禁止向左转弯:表示前方路口禁止一切车辆向左转弯.此标志设在禁止向左转弯的路口前适当位置', 'http://images.juheapi.com/jztk/c1c2subject1/148.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (149, 149, '这个标志是何含义?', 3, '右转车道', '掉头车道', '左转车道', '分向车道', '表示只准一切车辆向左转弯.此标志设在车辆必须向左转弯的路口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/149.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (150, 150, '初次申领驾驶证的驾驶人在实习期内可以单独驾驶机动车上高速公路行驶.', 2, '正确', '错误', , , '驾驶人在实习期内驾驶机动车上高速公路行驶,应当由持相应或者更高准驾车型驾驶证三年以上的驾驶人陪同.其中,驾驶残疾人专用小型自动挡载客汽车的,可以由持有小型自动挡载客汽车以上准驾车型驾驶证的驾驶人陪同.', , 0);
INSERT INTO ""."examination_question1" VALUES (151, 151, '驾驶人将机动车交给驾驶证被吊销的人驾驶的,交通警察依法扣留驾驶证.', 1, '正确', '错误', , , '规定是:驾驶人将机动车交给驾驶证被吊销的人驾驶的将被处200元以上2000元以下罚款,可以并处吊销机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (152, 152, '驾驶机动车遇到这种信号灯亮时,如果已越过停止线,可以继续通行.', 1, '正确', '错误', , , '规定:当黄灯亮起的时候,车身已越过停止线,可以继续通行.', 'http://images.juheapi.com/jztk/c1c2subject1/152.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (153, 153, '驾驶人因服兵役、出国（境）等原因无法办理审验时,延期审验期限最长不超过多长时间?', 1, '3年', '5年', '1年', '2年', '第六十三条  机动车驾驶人因服兵役、出国（境）等原因,无法在规定时间内办理驾驶证期满换证、审验、提交身体条件证明的,可以向机动车驾驶证核发地车辆管理所申请延期办理.申请时应当填写申请表,并提交机动车驾驶人的身份证明、机动车驾驶证和延期事由证明.延期期限最长不超过三年.延期期间机动车驾驶人不得驾驶机动车.', , 0);
INSERT INTO ""."examination_question1" VALUES (154, 154, '这一组交通警察手势是什么信号?', 4, '左转弯待转信号', '靠边停车信号', '减速慢行信号', '左转弯信号', '手势题详解:http://www.jszks.com/onepage/16203.html', 'http://images.juheapi.com/jztk/c1c2subject1/154.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (155, 155, '泥泞道路对安全行车的主要影响是什么?', 2, '行驶阻力变小', '车轮极易滑转和侧滑', '能见度低,视野模糊', '路面附着力增大', '泥泞路面特别松软和粘稠,车辆行驶阻力大,附着力减小,车轮极易划转和侧滑.', , 0);
INSERT INTO ""."examination_question1" VALUES (156, 156, '路面上的菱形块虚线是何含义?', 2, '道路施工提示标线', '车行道纵向减速标线', '车行道横向减速标线', '车道变少提示标线', '车行道纵向减速标线为一组平行于车行道分界线的菱形块虚线.在车行道纵向减速标线的起始位置,设置30 m的渐变段,菱形块虚线由窄变宽－－from 专业控.', 'http://images.juheapi.com/jztk/c1c2subject1/156.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (157, 157, '行车中突遇对向车辆强行超车,占据自己车道时,可不予避让,迫使对方让路.', 2, '正确', '错误', , , '开车一般是依安全为主 避让为主 和谐为主.', , 0);
INSERT INTO ""."examination_question1" VALUES (158, 158, '图中圈内三角填充区域是什么标线?', 3, '停车线', '减速线', '导流线', '网状线', '导流线的形式主要为一个或几个根据路口地形设置的白色V形线或斜纹线区域,表示车辆必须按规定的路线行驶,不得压线或越线行驶. 主要用于过宽、不规则或行驶条件比较复杂的交叉路口,立体交叉的匝道口或其他特殊地点.', 'http://images.juheapi.com/jztk/c1c2subject1/158.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (159, 159, '夜间驾驶人对物体的观察明显比白天差,视距会有什么变化?', 4, '不变', '无规律', '变长', '变短', '这是我们眼睛的硬伤,所以我们都是晚上睡觉~~推理:晚上偷袭小日本效果扛扛的', , 0);
INSERT INTO ""."examination_question1" VALUES (160, 160, '有下列哪种违法行为的机动车驾驶人将被一次记12分?', 4, '机动车驾驶证被暂扣期间驾驶机动车的', '以隐瞒、欺骗手段补领机动车驾驶证的', '驾驶机动车不按照规定避让校车的', '驾驶故意污损号牌的机动车上道路行驶', 'A,B,C都是扣6分,只有D是扣12分.', , 0);
INSERT INTO ""."examination_question1" VALUES (161, 161, '图中圈内白色横实线是何含义?', 4, '待转线', '减速线', '让行线', '停止线', '红绿灯前的停止线呗,基础的基础', 'http://images.juheapi.com/jztk/c1c2subject1/161.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (162, 162, '这一组交通警察手势是什么信号?', 3, '左转弯待转信号', '靠边停车信号', '右转弯信号', '减速慢行信号', '手势题详解:http://www.jszks.com/onepage/16203.html', 'http://images.juheapi.com/jztk/c1c2subject1/162.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (163, 163, '路中心黄色虚线属于哪一类标线?', 1, '指示标线', '禁止标线', '警告标志', '辅助标线', '道路交通标线是同标划于路面上的各种线条、箭头、文字、立面标记、突起路标和轮廓标等所构成的交通安全设施.它的作用是管制和引导交通.可以与标志配合使用,也可单独使用.', 'http://images.juheapi.com/jztk/c1c2subject1/163.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (164, 164, '在狭窄的路段会车时,应做到礼让三先:先慢、先让、先停.', 1, '正确', '错误', , , '这样做是友好的行为,很善意的做法,注意不要鸣太多次.因此本题正确.', , 0);
INSERT INTO ""."examination_question1" VALUES (165, 165, '驾驶人有哪种情形,交通警察可依法扣留机动车驾驶证?', 4, '超过规定速度10%', '疲劳后驾驶机动车', '行车中未系安全带', '饮酒后驾驶机动车', '其他的都不足以扣留驾驶证只有D最严重.', , 0);
INSERT INTO ""."examination_question1" VALUES (166, 166, '在铁路道口遇到两个红灯交替闪烁时要停车等待.', 1, '正确', '错误', , , '两个红灯交替闪烁时表示火车要经过了,赶紧停车等待吧.', 'http://images.juheapi.com/jztk/c1c2subject1/166.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (167, 167, '这个标志是何含义?', 1, '旅游区距离', '旅游区方向', '旅游区符号', '旅游区类别', '遇此类标志是指旅游区的距离还有多远（亲不要激动 安全第一 慢行', 'http://images.juheapi.com/jztk/c1c2subject1/167.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (168, 168, '伪造、变造或者使用伪造、变造驾驶证的驾驶人构成犯罪的,将依法追究刑事责任.', 1, '正确', '错误', , , '《机动车驾驶证申领和使用规定》（公安部令第123号）第八十一条  伪造、变造或者使用伪造、变造的机动车驾驶证的,由公安机关交通管理部门予以收缴,依法拘留,并处二千元以上五千元以下罚款；构成犯罪的,依法追究刑事责任.', , 0);
INSERT INTO ""."examination_question1" VALUES (169, 169, '驾驶证记载的驾驶人信息发生变化的要在多长时间内申请换证?', 1, '30日', '40日', '50日', '60日', '应当在30日内到驾驶证核发地车管所申请换证.填写申请表、带一寸彩色数码照片、身份证、驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (170, 170, '路面上的黄色标记是何含义?', 3, '禁止掉头', '禁止左转', '禁止右转', '禁止直行', '一个X加上一个右转,加一起就是禁止右转,爆弱了', 'http://images.juheapi.com/jztk/c1c2subject1/170.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (171, 171, '路中两条双黄色虚线是什么标线?', 2, '双向分道线', '潮汐车道线', '可跨越分道线', '单向分道线', '“潮汐车道线”是指根据早晚交通流量不同情况,对有条件的道路,试点开辟潮汐车道,通过车道灯的指示方向变化,控制主干道车道行驶方向,来调整车道数,提高车道使用效率.', 'http://images.juheapi.com/jztk/c1c2subject1/171.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (172, 172, '驾驶机动车在路口看到这种信号灯亮时,要加速通过.', 2, '正确', '错误', , , '路口黄灯持续闪烁,警示驾驶人要注意?望,确认安全通过.', 'http://images.juheapi.com/jztk/c1c2subject1/172.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (173, 173, '行车中遇有前方发生交通事故,需要帮助时,应怎样做?', 3, '尽量绕道躲避', '立即报警,停车观望', '协助保护现场,并立即报警', '加速通过,不予理睬', '请伸出您的援手,你不能保证下次需要帮助的人不是你.', , 0);
INSERT INTO ""."examination_question1" VALUES (174, 174, '路面上的黄色标记是何含义?', 3, '禁止直行', '允许掉头', '禁止掉头', '禁止转弯', '一个X加上一个向回掉头的标记,加一起就是禁止掉头', 'http://images.juheapi.com/jztk/c1c2subject1/174.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (175, 175, '对未按照国家规定投保交强险的车辆,交通警察可依法予以扣留.', 1, '正确', '错误', , , '强制险是必须交的.', , 0);
INSERT INTO ""."examination_question1" VALUES (176, 176, '驾驶机动车在路口遇到这种信号灯禁止通行.', 2, '正确', '错误', , , '绿灯行.不是禁止通行.', 'http://images.juheapi.com/jztk/c1c2subject1/176.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (177, 177, '路面上的黄色标线是何含义?', 1, '路面宽度渐变标线', '车行道变多标线', '接近障碍物标线', '施工路段提示线', '车道宽度紧缩,注意慢行', 'http://images.juheapi.com/jztk/c1c2subject1/177.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (178, 178, '雨天对安全行车的主要影响是什么?', 1, '路面湿滑,视线受阻', '发动机易熄火', '行驶阻力增大', '电器设备易受潮短路', '呐,给您罗列下:1、雨水影响视线,要开适度的雨刮频次；2、空气潮湿容易在车的前档内表面形成雾气,要注意除雾；3、雨天路面易打滑,要保持安全的低速；4、特殊情况下,紧急制动时,务必保持方向稳定,防止侧翻；5、雨水易形成路面沟、洞表面积水,易造成驾驶员判断失误,谨慎避开；', , 0);
INSERT INTO ""."examination_question1" VALUES (179, 179, '驶离高速公路可以从这个位置直接驶入匝道.', 2, '正确', '错误', , , '不能压实线直接过去的啊,应该提前换到驶离的车道,在进入匝道.', 'http://images.juheapi.com/jztk/c1c2subject1/179.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (180, 180, '这一组交通警察手势是什么信号?', 3, '左转弯待转信号', '靠边停车信号', '左转弯信号', '右转弯信号', '手势题详解:http://www.jszks.com/onepage/16203.html', 'http://images.juheapi.com/jztk/c1c2subject1/180.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (181, 181, '驾驶与准驾车型不符的机动车一次记几分?', 4, '2分', '3分', '6分', '12分', '驾驶与准驾车型不符的机动车的一次记12分.', , 0);
INSERT INTO ""."examination_question1" VALUES (182, 182, '驾驶机动车行驶到这个位置时,如果车前轮已越过停止线可以继续通过.', 2, '正确', '错误', , , '都红灯了,赶紧停车吧.', 'http://images.juheapi.com/jztk/c1c2subject1/182.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (183, 183, '这个标志是何含义?', 1, '高速公路紧急停车带', '高速公路避让处所', '高速公路客车站', '高速公路停车区', '高速公路紧急停车带:用于指示紧急停车的位置,设在紧急停车带的前端.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/183.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (184, 184, '图中圈内两条白色虚线是什么标线?', 2, '交叉路停车线', '左弯待转区线', '掉头引导线', '小型车转弯线', '直行灯变绿后,要左转的可以行驶进白色虚线内,等灯变成左转时就可以通行了', 'http://images.juheapi.com/jztk/c1c2subject1/184.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (185, 185, '驾驶机动车在这种情况下不能直行和左转弯', 1, '正确', '错误', , , '直行和左转都是红灯,所以不能直行和左转.', 'http://images.juheapi.com/jztk/c1c2subject1/185.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (186, 186, '驾驶机动车要选择绿色箭头灯亮的车道行驶.', 1, '正确', '错误', , , '绿灯行,没有问题.', 'http://images.juheapi.com/jztk/c1c2subject1/186.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (187, 187, '驶近没有人行横道的交叉路口时,发现有人横穿道路,应怎样做?', 1, '减速或停车让行', '鸣喇叭示意其让道', '立即变道绕过行人', '抢在行人之前通过', '安全第一,以人为本.', , 0);
INSERT INTO ""."examination_question1" VALUES (188, 188, '图中圈内白色实线是什么标线?', 1, '导向车道线', '可变导向车道线', '方向引导线', '单向行驶线', '具有固定行驶方向的导向车道线', 'http://images.juheapi.com/jztk/c1c2subject1/188.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (189, 189, '路中黄色分界线的作用是什么?', 2, '允许在左侧车道行驶', '分隔对向行驶的交通流', '分隔同向行驶的交通流', '禁止跨越对向行车道', '分隔相向车道的分界线', 'http://images.juheapi.com/jztk/c1c2subject1/189.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (190, 190, '在这条车道行驶的最低车速是多少?', 4, '100公里/小时', '110公里/小时', '60公里/小时', '90公里/小时', '高速路同方向有3条车道,最左侧的为110公里到120公里,中间的为90公里到110公里,最右侧的速度范围为60公里到90公里.', 'http://images.juheapi.com/jztk/c1c2subject1/190.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (191, 191, '驾驶人在超车时,前方车辆不减速、不让道,应怎样做?', 2, '紧跟其后,伺机再超', '停止继续超车', '加速继续超越', '连续鸣喇叭加速超越', '不让咱就别超了,安全第一.', , 0);
INSERT INTO ""."examination_question1" VALUES (192, 192, '驾驶机动车在路口直行遇到这种信号灯应该怎样行驶?', 4, '进入路口等待', '左转弯行驶', '加速直行通过', '不得越过停止线', '红灯请停在停止线内.', 'http://images.juheapi.com/jztk/c1c2subject1/192.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (193, 193, '图中圈内的白色半圆状标记是什么标线?', 4, '减速行驶线', '车速确认线', '路口减速线', '车距确认线', '车距确认标线作为车辆驾驶人保持行车安全距离的参考,视需要设于较长直线段、易发生追尾事故或其他需要的路段,应与车距确认标志配合使用. 白色半圆状车距确认标线设置于气象条件复杂,影响安全行车的路段两侧.', 'http://images.juheapi.com/jztk/c1c2subject1/193.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (194, 194, '驾驶机动车在路口遇到这种信号灯亮时,要在停止线前停车?望.', 2, '正确', '错误', , , '绿灯,赶紧的走你.', 'http://images.juheapi.com/jztk/c1c2subject1/194.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (195, 195, '路口最前端的双白实线是什么含义?', 1, '停车让行线', '减速让行线', '左弯待转线', '等候放行线', '停车让行线是在没有信号灯状态下.停止线是遇有信号灯红灯时表示车辆等候放行信号的位置,不能超越或压此线.', 'http://images.juheapi.com/jztk/c1c2subject1/195.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (196, 196, '这个标志是何含义?', 3, '旅游区距离', '旅游区类别', '旅游区方向', '旅游区符号', '快到云居寺了,箭头指的是方向', 'http://images.juheapi.com/jztk/c1c2subject1/196.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (197, 197, '驾驶人一边驾车,一边打手持电话是违法行为.', 1, '正确', '错误', , , '以后所有“违章”行为变为“违法”行为.', , 0);
INSERT INTO ""."examination_question1" VALUES (198, 198, '大型客车、牵引车、城市公交车、中型客车、大型货车驾驶人应当每两年提交一次身体条件证明.', 2, '正确', '错误', , , '2013年1月1日起施行的公安部123号令,已明确规定,大、中型客货车驾驶人每年参加审验,审验时自己申报身体条件情况', , 0);
INSERT INTO ""."examination_question1" VALUES (199, 199, '车辆在雨天临时停车时,应开启什么灯?', 2, '前后雾灯', '危险报警闪光灯', '前大灯', '倒车灯', '雨天汽车的刹车距离会比晴天的长,为了防止追尾,你要提前告知后车,所以要开启危险警报灯,夜间和雾天的临时停车也是一样滴！', , 0);
INSERT INTO ""."examination_question1" VALUES (200, 200, '驾驶人在道路上醉酒驾驶机动车的处3年以上有期徒刑.', 2, '正确', '错误', , , '醉酒驾驶,由公安机关约束至酒醒.吊销机驾驶证,依法追究刑事责任,5年内不得重新取得驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (201, 201, '已注册登记的小型载客汽车有下列哪种情形,所有人不需要办理变更登记?', 2, '机动车更换发动机', '加装前后防撞装置', '改变车身颜色', '更换车身或者车架', '只有前后防撞装置不备案,可以加装.', , 0);
INSERT INTO ""."examination_question1" VALUES (202, 202, '驾驶人驾驶机动车违反道路交通信号灯通行一次记多少分?', 3, '2分', '3分', '6分', '12分', '驾驶机动车违反道路交通信号灯通行的一次记6分.', , 0);
INSERT INTO ""."examination_question1" VALUES (203, 203, '这个标志是何含义?', 4, '高速公路左侧出口预告', '高速公路目的地预告', '高速公路右侧出口预告', '高速公路下一出口预告', '高速公路下一出口预告:预告下一出口可到达地点.当互通式立体交叉间距大于或等于3 km、小于5 km时,应设置下一出口预告标志,预告下一出口的信息和距离.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/203.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (204, 204, '这个标志是何含义?', 2, '高速公路出口', '高速公路起点', '高速公路入口', '高速公路终点', '高速公路起点:设在高速公路或城市快速路的起点.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/204.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (205, 205, '会车中遇到对方来车行进有困难需借道时,应怎样做?', 2, '靠右侧加速行驶', '尽量礼让对方先行', '不侵占对方道路,正常行驶', '示意对方停车让行', '尽量礼让一下吧,我们也会遇到这样的困难.', , 0);
INSERT INTO ""."examination_question1" VALUES (206, 206, '驾驶机动车在这种情况下不能左转弯.', 1, '正确', '错误', , , '左转红灯,所以不能左转,正确.', 'http://images.juheapi.com/jztk/c1c2subject1/206.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (207, 207, '车速超过规定时速50%以上的一次记12分.', 1, '正确', '错误', , , '超速驾驶一次记12分的情形有:驾驶中型以上载客载货汽车、校车、危险物品运输车辆在高速公路、城市快速路上行驶超过规定时速20％以上或者在高速公路、城市快速路以外的道路上行驶超过规定时速50％以上；驾驶其他机动车行驶超过规定时速50%以上的.', , 0);
INSERT INTO ""."examination_question1" VALUES (208, 208, '一个合格的驾驶人,不仅表现在技术的娴熟上,更重要的是应该具有良好的驾驶行为习惯和道德修养.', 1, '正确', '错误', , , '此题道德修养就是说明要遵守交通规则！', , 0);
INSERT INTO ""."examination_question1" VALUES (209, 209, '这一组交通警察手势是什么信号?', 1, '直行信号', '转弯信号', '停止信号', '靠边停车信号', '手势题详解:http://www.jszks.com/onepage/16203.html', 'http://images.juheapi.com/jztk/c1c2subject1/209.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (210, 210, '这个标志是何含义?', 4, '分道信息预告', '道路分岔处预告', '地点和距离预告', '十字交叉路口预告', '上边有十字能看到吧?', 'http://images.juheapi.com/jztk/c1c2subject1/210.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (211, 211, '驾驶机动车发生财产损失交通事故,当事人对事实及成因无争议的可先撤离现场.', 1, '正确', '错误', , , '也就是私了.', , 0);
INSERT INTO ""."examination_question1" VALUES (212, 212, '距离交叉路口50米以内的路段不能停车.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法实施条例》第六十三条 机动车在道路上临时停车,应当遵守下列规定:（二）交叉路口、铁路道口、急弯路、宽度不足４米的窄路、桥梁、陡坡、隧道以及距离上述地点５０米以内的路段,不得停车.', , 0);
INSERT INTO ""."examination_question1" VALUES (213, 213, '可以从这个位置直接驶入高速公路行车道.', 2, '正确', '错误', , , '应该按照标线行驶,不要乱开.', 'http://images.juheapi.com/jztk/c1c2subject1/213.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (214, 214, '遇到这种情况的路口,以下做法正确的是什么?', 2, '沿左侧车道掉头', '该路口不能掉头', '选择中间车道掉头', '在路口内掉头', '此处有禁止掉头的标志,所以是不可以掉头的.', 'http://images.juheapi.com/jztk/c1c2subject1/214.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (215, 215, '会车前选择的交会位置不理想时,应怎样做?', 1, '减速、低速会车或停车让行', '向左占道,让对方减速让行', '打开前照灯,示意对方停车让行', '加速选择理想位置', '礼貌行车嘛,既然即将交会的地方不理想,就应减速找个适合的地方避让,否则就将堵车,谁也通不过.', , 0);
INSERT INTO ""."examination_question1" VALUES (216, 216, '驾驶机动车在铁路道口、桥梁、陡坡、隧道或者容易发生危险的路段不能掉头.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法实施条例》第四十九条第一款“机动车在有禁止掉头或者禁止左转弯标志、标线的地点以及在铁路道口、人行横道、桥梁、急弯、陡坡、隧道或者容易发生危险的路段,不得掉头.”', , 0);
INSERT INTO ""."examination_question1" VALUES (217, 217, '驾驶车辆行至道路急转弯处,应怎样做?', 3, '急剧制动低速通过', '靠弯道外侧行驶', '充分减速并靠右侧行驶', '借对向车道行驶', '安全第一,过急转弯当然要减低车速,靠右行驶是为了防止急转弯另一边有车过来导致两车相撞.', , 0);
INSERT INTO ""."examination_question1" VALUES (218, 218, '路缘石上的黄色虚线是何含义?', 4, '禁止临时停车', '禁止上下人员', '禁止装卸货物', '禁止长时停车', '请查看详细题解:http://www.jszks.com/jieshi/15128.html', 'http://images.juheapi.com/jztk/c1c2subject1/218.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (219, 219, '这个标志是何含义?', 4, 'T型路口', '分流路口', '减速通行', '此路不通', '用以指示前方道路为死胡同,无出口、不能通行.该标志为蓝底白色街区红色图案.', 'http://images.juheapi.com/jztk/c1c2subject1/219.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (220, 220, '这个标志是何含义?', 3, '高速公路公用电话', '高速公路报警电话', '高速公路紧急电话', '高速公路救援电话', '高速公路紧急电话:用于指示高速公路紧急电话的位置,可单独设置也可设在紧急电话的立柱上或箱体上.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/220.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (221, 221, '这个标志是何含义?', 3, '高速公路客车站预告', '高速公路避险处预告', '高速公路服务区预告', '高速公路收费站预告', '高速公路服务区预告:用于预告服务区的位置,设在减速车道起点处.表示一般服务区预告标志.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/221.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (222, 222, '发现前方道路堵塞,正确的做法是什么?', 1, '按顺序停车等候', '鸣喇叭示意前方车辆快速行驶', '选择空当逐车超越', '继续穿插绕行', '文明行车,礼让行车.', , 0);
INSERT INTO ""."examination_question1" VALUES (223, 223, '这个标志是何含义?', 1, '停车领卡', '停车缴费', 'ETC通道', '停车检查', '停车领卡:用以提示停车领卡,设在进入高速公路或城市快速路收费站入口一侧的适当位置.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/223.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (224, 224, '在这条车道行驶的最低车速是多少?', 3, '60公里/小时', '90公里/小时', '100公里/小时', '110公里/小时', '高速路同方向有2条车道,左侧车的速度范围为100公里到120公里,右侧车的速度范围为60公里到100公里.', 'http://images.juheapi.com/jztk/c1c2subject1/224.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (225, 225, '这样在路边临时停放机动车有什么违法行为?', 3, '停车占用机动车道', '距离路边超过30厘米', '在有禁停标线路段停车', '在非机动车道停车', '图中右侧路边是黄色的禁止停车标线,所以此处是不能停车的.', 'http://images.juheapi.com/jztk/c1c2subject1/225.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (226, 226, '这个标志是何含义?', 2, '残疾人出入口', '注意残疾人', '残疾人休息处', '残疾人专用通道', '黄底标志为警告标志,所以是注意残疾人.', 'http://images.juheapi.com/jztk/c1c2subject1/226.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (227, 227, '这辆小型载客汽车进入高速公路行车道的行为是正确的.', 2, '正确', '错误', , , '此车压实线,不按标线要求行车.', 'http://images.juheapi.com/jztk/c1c2subject1/227.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (228, 228, '这个标志是何含义?', 1, '向右急转弯', '向右绕行', '连续弯路', '向左急转弯', '向右急转弯:向右急弯路标志,设在右急转弯的道路前方适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/228.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (229, 229, '在距这段路多少米以内的路段不能停放机动车?', 3, '5米以内', '10米以内', '50米以内', '30米以内', '图片为急弯路.《中华人民共和国道路交通安全法实施条例》第六十三条 机动车在道路上临时停车,应当遵守下列规定:　　（二）交叉路口、铁路道口、急弯路、宽度不足４米的窄路、桥梁、陡坡、隧道以及距离上述地点５０米以内的路段,不得停车.', 'http://images.juheapi.com/jztk/c1c2subject1/229.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (230, 230, '在标志、标线齐全的高速公路上行车,应当按照什么规定的车道和车速行驶?', 1, '标志或标线', '《道路交通安全法》', '车辆说明书', '地方法规', '题目这么强调“标志、标线”,你再不选A,你让A情何以堪.', , 0);
INSERT INTO ""."examination_question1" VALUES (231, 231, '驾驶机动车在路口遇到这种信号灯表示什么意思?', 2, '禁止右转', '路口警示', '准许直行', '加速通过', '路口黄灯持续闪烁,警示驾驶人要注意?望,确认安全通过.', 'http://images.juheapi.com/jztk/c1c2subject1/231.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (232, 232, '指路标志的作用是什么?', 3, '限制车辆通行', '提示限速信息', '提供方向信息', '警告前方危险', '指路标志嘛就是指路的,指路就是指方向的啊.', , 0);
INSERT INTO ""."examination_question1" VALUES (233, 233, '机动车在道路上发生轻微交通事故且妨碍交通时,不需移动.', 2, '正确', '错误', , , '机动车在道路上发生轻微交通事故就赶紧协商一下花不了几百块,不要挡着路了就,不然大家都过不去堵在一起了.', , 0);
INSERT INTO ""."examination_question1" VALUES (234, 234, '在这种路口怎样进行掉头?', 1, '从中心线虚线处掉头', '从右侧车道掉头', '进入路口后掉头', '在人行横道上掉头', '从中心线虚线处是可以掉头的,其他的选项都不对.', 'http://images.juheapi.com/jztk/c1c2subject1/234.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (235, 235, '社会车辆距离消防栓或者消防队(站)门前30米以内的路段不能停车.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法实施条例》第六十三条 机动车在道路上临时停车,应当遵守下列规定:　　（三）公共汽车站、急救站、加油站、消防栓或者消防队（站）门前以及距离上述地点３０米以内的路段,除使用上述设施的以外,不得停车；', , 0);
INSERT INTO ""."examination_question1" VALUES (236, 236, '行车中发现前方道路拥堵时,应怎样做?', 3, '寻找机会超越前车', '从车辆空间穿插通过', '减速停车,依次排队等候', '鸣喇叭催促', '基于和谐第一位,一定要减速停车依次排队.', , 0);
INSERT INTO ""."examination_question1" VALUES (237, 237, '机动车行驶中,车上少年儿童可不使用安全带.', 2, '正确', '错误', , , '当然不是,儿童的体格比我们大人差远了所以儿童更应该使用安全带,有孩子的朋友一定要买儿童安全座椅,不然出事哭都来不及.', , 0);
INSERT INTO ""."examination_question1" VALUES (238, 238, '这位驾驶人违反法律规定的行为是什么?', 3, '没按规定握转向盘', '座椅角度不对', '没系安全带', '驾驶姿势不正确', '这个妹子没系安全带.', 'http://images.juheapi.com/jztk/c1c2subject1/238.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (239, 239, '这样临时停放红色轿车有什么违法行为?', 3, '距离路边超过30厘米', '在有禁停标线路段停车', '距离加油站不到30米', '停车占用非机动车道', '图为加油站,规定加油站50米内不得停车.公共汽车站、急救站、加油站、消防栓或者消防队（站）门前以及距离上述地点３０米以内的路段,除使用上述设施的以外,不得停车；', 'http://images.juheapi.com/jztk/c1c2subject1/239.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (240, 240, '这个标志是何含义?', 3, '注意行人', '有人行横道', '村庄或集镇', '有小学校', '村庄:用以提醒车辆驾驶人小心驾驶.设在紧靠村庄、集镇且视线不良的路段以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/240.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (241, 241, '路中心黄色虚实线是何含义?', 2, '虚线一侧禁止越线', '实线一侧禁止越线', '实线一侧允许越线', '两侧均可越线行驶', '虚线可压,实线不可压,黄色是引起你注意,如图就是实线一侧禁止越线', 'http://images.juheapi.com/jztk/c1c2subject1/241.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (242, 242, '行车中超越同向行驶的自行车时,应怎样做?', 4, '连续鸣喇叭提醒其让路', '持续鸣喇叭并加速超越', '让自行车先行', '注意观察动态,减速慢行,留有足够的安全距离', '安全第一,不抢一秒.', , 0);
INSERT INTO ""."examination_question1" VALUES (243, 243, '这个标志是何含义?', 3, '水渠', '桥梁', '隧道', '涵洞', '隧道:用以提醒车辆驾驶人注意慢行.设在双向行驶并且照明不好的隧道口前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/243.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (244, 244, '这个标志是何含义?', 1, '高速公路入口预告', '高速公路终点预告', '高速公路起点预告', '高速公路出口预告', '高速公路入口预告:用于指示进入高速公路或城市快速路的入口,设在进入高速公路或城市快速路前的被交道路上适当位置.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/244.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (245, 245, '这个标志是何含义?', 4, '非机动车道', '避让非机动车', '禁止非机动车通行', '注意非机动车', '黄底标志为警告标志,自行车优势非机动车所以答案是注意非机动车.', 'http://images.juheapi.com/jztk/c1c2subject1/245.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (246, 246, '这个标志是何含义?', 3, '隧道入口', '气象台', '注意横风', '风向标', '注意横风:此标志设在经常有很强的侧风并有必要引起注意的路段前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/246.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (247, 247, '这个标志是何含义?', 3, '高速公路收费处', '高速公路检查站', '设有ETC的收费站', '高速公路领卡处', '设有ETC的收费站:用以指示前方收费站.设在高速公路、城市快速路主线上渐变段起点处.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/247.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (248, 248, '驾驶小型载客汽车在高速公路上时速低于100公里时的最小跟车距离是多少?', 3, '不得少于20米', '不得少于10米', '不得少于50米', '不得少于30米', '规定,记住吧.', , 0);
INSERT INTO ""."examination_question1" VALUES (249, 249, '这个标志是何含义?', 3, '两侧通行', '右侧通行', '左侧通行', '不准通行', '用于引导行车方向,提示道路使用者前方线形变化,注意谨慎驾驶.设于中央隔离设施端部、渠化设施的端部、桥头等.答题技巧:∧表示俩侧通行,/这个方向的表示左侧通行,这个方向表示右侧通行.交通指路标志大全:http://www.jszks.com/onepage/16752.html', 'http://images.juheapi.com/jztk/c1c2subject1/249.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (250, 250, '这个路面标记是何含义?', 1, '最低限速为80公里/小时', '平均速度为80公里/小时', '最高限速为80公里/小时', '解除80公里/小时限速', '白色为最低限速,黄色为最高限速.', 'http://images.juheapi.com/jztk/c1c2subject1/250.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (251, 251, '这个标志是何含义?', 1, '高速公路特殊天气建议速度', '高速公路特殊天气最低速度', '高速公路特殊天气平均速度', '高速公路特殊天气最高速度', '高速公路特殊天气建议速度:用以提醒驾驶员在雨、雪、雾等特殊天气下,以建议速度行驶,和白色半圆状车距确认线配合使用.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/251.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (252, 252, '这个标志是何含义?', 2, '堤坝路', '傍山险路', '落石路', '临崖路', '傍山险路:用以提醒车辆驾驶人小心驾驶.设在傍山险路路段以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/252.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (253, 253, '这个标志是何含义?', 3, '禁止借道', '禁止变道', '禁止超车', '禁止掉头', '禁止超车:表示该标志至前方解除禁止超车标志的路段内,不准机动车超车.此标志设在禁止超车的起点.', 'http://images.juheapi.com/jztk/c1c2subject1/253.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (254, 254, '这个标志是何含义?', 2, '漫水桥', '渡口', '船用码头', '过水路面', '渡口:用以提醒车辆驾驶人谨慎驾驶.设在车辆渡口以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/254.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (255, 255, '机动车在道路上发生故障,需要停车排除时,驾驶人应该怎么办?', 2, '将车停在道路中间', '将车停到不妨碍交通的地方', '开启近光灯或雾灯', '就地停车排除故障', '只有B最合理,首先应该是不妨碍交通的地方,放在路中间,搞不好后边的看不到撞上来.', , 0);
INSERT INTO ""."examination_question1" VALUES (256, 256, '这个标志是何含义?', 3, '高速公路里程编号', '高速公路界牌编号', '高速公路命名编号', '高速公路路段编号', '高速公路命名编号:用于指示高速公路的编号,作为指路标志的路线信息,出现在高速公路、城市快速路、一般道路指路标志版面中.查看高速公路标志大全:http://www.jszks.com/onepage/16721.html', 'http://images.juheapi.com/jztk/c1c2subject1/256.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (257, 257, '这个标志是何含义?', 3, '低速行驶', '注意行人', '步行', '行人先行', '表示该街道只供步行.此标志设在步行街的两端.', 'http://images.juheapi.com/jztk/c1c2subject1/257.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (258, 258, '行车中遇到执行紧急任务的消防车、救护车、工程救险车时要及时让行.', 1, '正确', '错误', , , '看题干,都说是执行紧急任务了,当然是应当让行啦！', , 0);
INSERT INTO ""."examination_question1" VALUES (259, 259, '驾驶机动车行经城市没有列车通过的铁路道口时允许超车.', 2, '正确', '错误', , , '安全第一.', , 0);
INSERT INTO ""."examination_question1" VALUES (260, 260, '在道路上遇到这种情况可以从两侧超车.', 2, '正确', '错误', , , '不要影响警察叔叔抓坏人.', 'http://images.juheapi.com/jztk/c1c2subject1/260.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (261, 261, '在这个路口不能掉头.', 2, '正确', '错误', , , '此处可以掉头有掉头的标志,还有黄色的虚线.', 'http://images.juheapi.com/jztk/c1c2subject1/261.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (262, 262, '驾驶机动车在高速公路要按照限速标志标明的车速行驶.', 1, '正确', '错误', , , '是的,否则超速的后果很严重,在高速公路上超速不足50%的,按照以下第九十条处罚,罚款20到200元(记3分).超速50%以上的(记6分),处二百元以上二千元以下罚款,并处吊销机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (263, 263, '行车中遇儿童时,应怎样做?', 1, '减速慢行,必要时停车避让', '长鸣喇叭催促', '迅速从一侧通过', '加速绕行', '请尊老爱幼.', , 0);
INSERT INTO ""."examination_question1" VALUES (264, 264, '驾驶车辆驶入铁路道口前减速降挡,进入道口后应怎样做?', 1, '不能变换挡位', '可以变换挡位', '可换为高挡', '停车观察', '变档时容易熄火,万一熄火,火车来了不把你撞飞', , 0);
INSERT INTO ""."examination_question1" VALUES (265, 265, '驾驶车辆向右变更车道时,应提前开启右转向灯,注意观察,在确保安全的情况下,驶入要变更的车道.', 1, '正确', '错误', , , '变更车道就是即将转弯的车道 所以要提前开启转向灯 不是说变道就得开转向 而是随后要转弯', , 0);
INSERT INTO ""."examination_question1" VALUES (266, 266, '这辆停在路边的机动车没有违法行为.', 2, '正确', '错误', , , '警告标志设置的太近了,应该在150米处设置.', 'http://images.juheapi.com/jztk/c1c2subject1/266.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (267, 267, '车辆在拥挤路段低速行驶时,遇其他车辆强行插队,应怎样做?', 4, '鸣喇叭警告,不得进入', '加速行驶,紧跟前车,不让其进入', '挤靠“加塞”车辆,逼其离开', '主动礼让,确保行车安全', '和谐,和谐第一.', , 0);
INSERT INTO ""."examination_question1" VALUES (268, 268, '饮酒后驾驶机动车的一次记12分.', 1, '正确', '错误', , , '不尊重自己和别人的生命,这么严重的事情肯定是一次把分扣光光.', , 0);
INSERT INTO ""."examination_question1" VALUES (269, 269, '在一般道路倒车时,若发现有过往车辆通过,应怎样做?', 2, '鸣喇叭示意', '主动停车避让', '加速倒车', '继续倒车', '和谐第一,请立即变身圣人,就知道答案了.', , 0);
INSERT INTO ""."examination_question1" VALUES (270, 270, '遇到这种情形时,应怎么办?', 1, '停车让行人先行', '从行人前方绕行', '鸣喇叭提醒行人', '从行人后方绕行', '以人为本,没有问题.', 'http://images.juheapi.com/jztk/c1c2subject1/270.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (271, 271, '在道路上临时停车不得妨碍其他车辆和行人通行.', 1, '正确', '错误', , , '和谐第一,这个错了不应该了亲.', , 0);
INSERT INTO ""."examination_question1" VALUES (272, 272, '驾驶车辆在道路上行驶时,应当按照规定的速度安全行驶.', 1, '正确', '错误', , , '注意“安全行驶”四个字.有安全两个字就不会错了', , 0);
INSERT INTO ""."examination_question1" VALUES (273, 273, '驾驶机动车驶离高速公路时,在这个位置怎样行驶?', 1, '驶入减速车道', '继续向前行驶', '车速保持100公里/小时', '车速降到40公里/小时以下', '下一步肯定是驶入减速车道,在减速,再驶离高速.', 'http://images.juheapi.com/jztk/c1c2subject1/273.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (274, 274, '山区道路车辆进入弯道前,在对面没有来车的情况下,应怎样做?', 4, '可靠弯道外侧行驶', '可短时间借用对方的车道', '可加速沿弯道切线方向通过', '应“减速、鸣喇叭、靠右行”', '题目是驶入弯道前,你必须要鸣笛啊 你知道弯道那边有没有车马? 减速也是必须的！万一有车不撞了了吗?', , 0);
INSERT INTO ""."examination_question1" VALUES (275, 275, '在泥泞路上制动时,车轮易发生侧滑或甩尾,导致交通事故.', 1, '正确', '错误', , , '常识题,泥泞路摩擦系数低.', , 0);
INSERT INTO ""."examination_question1" VALUES (276, 276, '驾驶人在发生交通事故后因抢救伤员变动现场时要标明位置.', 1, '正确', '错误', , , '对的,要留下证据.', , 0);
INSERT INTO ""."examination_question1" VALUES (277, 277, '驾驶机动车行经市区下列哪种道路时不得超车?', 3, '主要街道', '单向行驶路段', '交通流量大的路段', '单向两条行车道', '同车道行驶的机动车,后车应当与前车保持足以采取紧急制动措施的安全距离.有下列情形之一的,不得超车:  ①前车正在左转弯、掉头、超车的;  ②与对面来车有会车可能的;  ③前车为执行紧急任务的警车、消防车、救护车、工程救险车的;  ④行经铁路道口、交叉路口、窄桥、弯道、陡坡、隧道、人行横道、《《市区交通流量大的路段》》等没有超车条件的.', , 0);
INSERT INTO ""."examination_question1" VALUES (278, 278, '上道路行驶的机动车故意遮挡、污损、不按规定安装机动车号牌的一次记几分?', 4, '2分', '3分', '6分', '12分', '上道路行驶的机动车未悬挂机动车号牌的,或者故意遮挡、污损、不按规定安装机动车号牌的一次记12分.', , 0);
INSERT INTO ""."examination_question1" VALUES (279, 279, '这样在路边临时停放机动车有什么违法行为?', 1, '在人行横道上停车', '距离路边超过30厘米', '在有禁停标线路段停车', '在非机动车道停车', '图中汽车停在人行横道.规定是:在设有禁停标志、标线的路段,在机动车道与非机动车道、人行道之间设有隔离设施的路段以及人行横道、施工地段,不得停车；', 'http://images.juheapi.com/jztk/c1c2subject1/279.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (280, 280, '这个路段可以在非机动车道上临时停车.', 2, '正确', '错误', , , '这个是非机动车道,当然不能占道,不然人家骑车的怎么过去.', 'http://images.juheapi.com/jztk/c1c2subject1/280.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (281, 281, '变更车道时只需开启转向灯,便可迅速转向驶入相应的行车道.', 2, '正确', '错误', , , '先开转向灯后观察才可以使入车道', , 0);
INSERT INTO ""."examination_question1" VALUES (282, 282, '同车道行驶的车辆遇前车有下列哪种情形时不得超车?', 3, '正在停车', '减速让行', '正在掉头', '正常行驶', '同车道行驶的机动车,后车应当与前车保持足以采取紧急制动措施的安全距离.有下列情形之一的,不得超车:  ①《《前车正在左转弯、掉头、超车的》》;  ②与对面来车有会车可能的;  ③前车为执行紧急任务的警车、消防车、救护车、工程救险车的;  ④行经铁路道口、交叉路口、窄桥、弯道、陡坡、隧道、人行横道、市区交通流量大的路段等没有超车条件的.', , 0);
INSERT INTO ""."examination_question1" VALUES (283, 283, '驾驶机动车在高速公路遇到能见度低于50米的气象条件时,车速不得超过20公里/小时,还应怎么做?', 2, '进入应急车道行驶', '尽快驶离高速公路', '尽快在路边停车', '在路肩低速行驶', '能见度只有50米,尽快驶离高速,连50米都看不到说明太危险了.', , 0);
INSERT INTO ""."examination_question1" VALUES (284, 284, '漫水道路行车时,应挂高速档,快速通过.', 2, '正确', '错误', , , '不应该挂高速挡,而是要“应选用低速挡,以保证汽车有足够的驱动力;入水时应平稳,以防溅起的水花落 入电器设备,造成电器短路;发动机中速运转,保持车速,中途不得停车、换挡和急转转向盘”这样才对.', , 0);
INSERT INTO ""."examination_question1" VALUES (285, 285, '夜间车辆通过照明条件良好的路段时,应使用什么灯?', 3, '危险报警闪光灯', '远光灯', '近光灯', '雾灯', '直接解释大家可能难以转过弯来,我们先反过来看这题！夜间车辆通过照明条件不良好的路段时应使用--? 答案:远光灯.照明不良好就是看不见远近路段,当然是打开远光灯,看的更远更清楚,相反照明良好时,也就是说远近处,我们都能看的见,当然是打开近光灯最为保险.', , 0);
INSERT INTO ""."examination_question1" VALUES (286, 286, '找出这辆故障车有哪种违法行为?', 4, '没有开启危险报警闪光灯', '没有将车停到路边', '没有立即排除故障', '没有设置警告标志', '没有看到警告标志,所以D选项是对的.', 'http://images.juheapi.com/jztk/c1c2subject1/286.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (287, 287, '驾驶人未携带哪种证件驾驶机动车上路,交通警察可依法扣留车辆?', 1, '机动车行驶证', '从业资格证', '居民身份证', '机动车通行证', '《道路交通安全违法行为处理程序规定》第十三条 “有下列情形之一的,因无其他机动车驾驶人代替驾驶、违法行为尚未消除、需要调查或者证据保全等原因不能立即放行的,可以扣留车辆:    （一）未悬挂机动车号牌,未放置检验合格标志、保险标志,或者未携带行驶证、机动车驾驶证的；', , 0);
INSERT INTO ""."examination_question1" VALUES (288, 288, '在这个位置时怎样使用灯光?', 2, '开启右转向灯', '开启左转向灯', '开启危险报警闪光灯', '开启前照灯', '因为你要从直行并入左侧的车道所以要开启左转向灯.', 'http://images.juheapi.com/jztk/c1c2subject1/288.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (289, 289, '对未放置检验合格标志上道路行驶的车辆,交通警察可依法予以扣留.', 1, '正确', '错误', , , '没有问题.', , 0);
INSERT INTO ""."examination_question1" VALUES (290, 290, '行车中遇列队横过道路的学生时,应怎样做?', 2, '提前加速抢行', '停车让行', '降低车速、缓慢通过', '连续鸣喇叭催促', '祖国的未来啊,不让公务车也要让祖国的未来.', , 0);
INSERT INTO ""."examination_question1" VALUES (291, 291, '机动车在夜间道路上发生故障难以移动时要开启危险报警闪光灯、示廓灯、后位灯.', 1, '正确', '错误', , , '这个没有问题,危险报警闪光灯、示廓灯、后位灯都是为了晚上提示周围的车你的位置,以防发生碰撞和刮蹭.', , 0);
INSERT INTO ""."examination_question1" VALUES (292, 292, '车辆驶近停在车站的公交车辆时,为预防公交车突然起步或行人从车前穿出,应怎样做?', 1, '减速,保持足够间距,随时准备停车', '保持正常车速行驶', '鸣喇叭提醒,加速通过', '随时准备紧急制动', '安全第一,以人为本.', , 0);
INSERT INTO ""."examination_question1" VALUES (293, 293, '提供虚假材料申领驾驶证的申请人会承担下列哪种法律责任?', 3, '处20元以上200元以下罚款', '取消申领驾驶证资格', '1年内不得再次申领驾驶证', '2年内不能再次申领驾驶证', '《机动车驾驶证申领和使用规定》（公安部令第123号）第七十八条  隐瞒有关情况或者提供虚假材料申领机动车驾驶证的,申请人在一年内不得再次申领机动车驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (294, 294, '造成交通事故后逃逸构成犯罪的人不能申请机动车驾驶证.', 1, '正确', '错误', , , '你被列入“马路杀手”行列.', , 0);
INSERT INTO ""."examination_question1" VALUES (295, 295, '车辆驶近人行横道时,应怎样做?', 3, '加速通过', '立即停车', '先减速注意观察行人、非机动车动态,确认安全后再通过', '鸣喇叭示意行人让道', '过人行横道要先观察,因为行人具有随意性说不清超那边走.', , 0);
INSERT INTO ""."examination_question1" VALUES (296, 296, '驾驶机动车需要在路边停车时怎样选择停车地点?', 2, '靠左侧路边逆向停放', '在停车泊位内停放', '在路边随意停放', '在人行道上停放', '只有停车泊位是可以停放的.', , 0);
INSERT INTO ""."examination_question1" VALUES (297, 297, '在道路上超车时,应尽量加大横向距离,必要时可越实线超车.', 2, '正确', '错误', , , '不能越实线超车,因此本题错误.', , 0);
INSERT INTO ""."examination_question1" VALUES (298, 298, '距离宽度不足4米的窄路50米以内的路段不能停车.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法实施条例》第六十三条 机动车在道路上临时停车,应当遵守下列规定:　　（二）交叉路口、铁路道口、急弯路、宽度不足４米的窄路、桥梁、陡坡、隧道以及距离上述地点５０米以内的路段,不得停车；', , 0);
INSERT INTO ""."examination_question1" VALUES (299, 299, '行车中遇到对向来车占道行驶,应怎样做?', 3, '逼对方靠右行驶', '用大灯警示对方', '主动给对方让行', '紧靠道路中心行驶', '请立即变身圣人答题.', , 0);
INSERT INTO ""."examination_question1" VALUES (300, 300, '距离桥梁、陡坡、隧道50米以内的路段不能停车.', 1, '正确', '错误', , , '《中华人民共和国道路交通安全法实施条例》第六十三条 机动车在道路上临时停车,应当遵守下列规定:　　（二）交叉路口、铁路道口、急弯路、宽度不足４米的窄路、桥梁、陡坡、隧道以及距离上述地点５０米以内的路段,不得停车；', , 0);
INSERT INTO ""."examination_question1" VALUES (301, 301, '驾驶车辆在交叉路口前变更车道时,应怎样驶入要变更的车道?', 4, '在路口前实线区内根据需要', '进入路口实线区内', '在路口停止线前', '在虚线区按导向箭头指示', '当然是在未到人行道前按导向标识了,要不都到人行道边上了,怎么改变车道.', , 0);
INSERT INTO ""."examination_question1" VALUES (302, 302, '当驾驶车辆行经两侧有行人且有积水的路面时,应怎样做?', 4, '加速通过', '正常行驶', '连续鸣喇叭', '减速慢行', '常识题,减速可以防止溅水,按喇叭有提醒的作用,但不能防止溅水. 想想我们没车的时候是不是很讨厌那些在积水路面开快车的?好吧,我们还是不要做让别人讨厌的人了吧！', , 0);
INSERT INTO ""."examination_question1" VALUES (303, 303, '行车中遇交通事故受伤者需要抢救时,应怎样做?', 1, '及时将伤者送医院抢救或拨打急救电话', '尽量避开,少惹麻烦', '绕过现场行驶', '借故避开现场', '冷漠的社会,请您站出来.', , 0);
INSERT INTO ""."examination_question1" VALUES (304, 304, '谨慎驾驶的三原则是集中注意力、仔细观察和提前预防.', 1, '正确', '错误', , , '黄金原则,牢记,对我们以后驾车是有很大的帮助的.', , 0);
INSERT INTO ""."examination_question1" VALUES (305, 305, '驾驶车辆变更车道时,应提前开启转向灯,注意观察,保持安全距离,驶入要变更的车道.', 1, '正确', '错误', , , '常识题,安全第一.', , 0);
INSERT INTO ""."examination_question1" VALUES (306, 306, '打开机动车车门时,不得妨碍其他车辆和行人通行.', 1, '正确', '错误', , , '这个没有问题.', , 0);
INSERT INTO ""."examination_question1" VALUES (307, 307, '在这段高速公路上行驶的最低车速是多少?', 3, '100公里/小时', '80公里/小时', '60公里/小时', '50公里/小时', '图片上那个蓝色的牌子60就是最低车速.', 'http://images.juheapi.com/jztk/c1c2subject1/307.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (308, 308, '驾驶人违反交通运输管理法规发生重大事故致人重伤的可能判处3年以下徒刑或拘役.', 1, '正确', '错误', , , '第一百三十三条　违反交通运输管理法规,因而发生重大事故,致人重伤、死亡或者使公私财产遭受重大损失的,处三年以下有期徒刑或者拘役；', , 0);
INSERT INTO ""."examination_question1" VALUES (309, 309, '上道路行驶的机动车有哪种情形交通警察可依法扣留车辆?', 1, '未放置检验合格标志', '未携带身份证', '未放置城市环保标志', '未携带机动车登记证书', '未放置检验合格标志,未悬挂机动车号牌,未放置保险标志的,未携带机动车行驶证的都是可依法扣留车辆的.', , 0);
INSERT INTO ""."examination_question1" VALUES (310, 310, '这个标志是何含义?', 1, '连续下坡', '下陡坡', '上陡坡', '连续上坡', '连续下坡:用以提醒车辆驾驶人小心驾驶.当连续下坡总长大于3 km后,应重复设置.', 'http://images.juheapi.com/jztk/c1c2subject1/310.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (311, 311, '驾驶人在道路上驾驶机动车追逐竞驶,情节恶劣的处3年以下有期徒刑.', 2, '正确', '错误', , , '2011年《中华人民共和国刑法修正案（八）》第二十二条:在刑法第一百三十三条后增加一条,作为第一百三十三条之一:“在道路上驾驶机动车追逐竞驶,情节恶劣的,或者在道路上醉酒驾驶机动车的,处拘役,并处罚金.”不至于处3年以下有期徒刑.', , 0);
INSERT INTO ""."examination_question1" VALUES (312, 312, '行车中遇到正在进行作业的道路养护车辆、工程作业车时要注意避让.', 1, '正确', '错误', , , '他在为人民服务,我们还是要让一让的.', , 0);
INSERT INTO ""."examination_question1" VALUES (313, 313, '驾驶小型汽车下陡坡时允许熄火滑行.', 2, '正确', '错误', , , '当然不行,一定要用着火并且挂着档下陡坡.', , 0);
INSERT INTO ""."examination_question1" VALUES (314, 314, '在堵车的交叉路口绿灯亮时,车辆应怎样做?', 2, '可直接驶入交叉路口', '不能驶入交叉路口', '可借对向车道通过路口', '在保证安全的情况下驶入交叉路口', '已经堵了,进入路口的还没走完,你再进去只有添堵,这样大家都走不了.', , 0);
INSERT INTO ""."examination_question1" VALUES (315, 315, '机动车发生财产损失交通事故,对应当自行撤离现场而未撤离造成交通堵塞的,可以对驾驶人处以200元罚款.', 1, '正确', '错误', , , '这个是正确的.', , 0);
INSERT INTO ""."examination_question1" VALUES (316, 316, '这个标志是何含义?', 4, '堤坝路', '上陡坡', '连续上坡', '下陡坡', '下陡坡:用以提醒车辆驾驶人小心驾驶.', 'http://images.juheapi.com/jztk/c1c2subject1/316.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (317, 317, '这个标志是何含义?', 1, '注意行人', '注意儿童', '学校区域', '人行横道', '注意行人:用以警告车辆驾驶人减速慢行,注意行人.设在行人密集,或不易被驾驶员发现的人行横道线以前适当位置.', 'http://images.juheapi.com/jztk/c1c2subject1/317.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (318, 318, '女驾驶人穿高跟鞋驾驶车辆,不利于安全行车.', 1, '正确', '错误', , , '穿高跟鞋本身就是错的.', , 0);
INSERT INTO ""."examination_question1" VALUES (319, 319, '不要在驾驶室的前后窗范围内悬挂和放置妨碍驾驶人视线的物品.', 1, '正确', '错误', , , '视线是咱开车的第一位的.', , 0);
INSERT INTO ""."examination_question1" VALUES (320, 320, '车辆临时靠边停车后准备起步时,应先怎样做?', 4, '加油起步', '鸣喇叭', '提高发动机转速', '观察周围交通情况', '要是车边刚好有一脾气不好的牛,但你没有先观察到,一鸣喇叭,它一角给你的车顶上,不就麻烦了吗?所以先观察,后喇叭是对的.', , 0);
INSERT INTO ""."examination_question1" VALUES (321, 321, '驾驶人违反交通运输管理法规发生重大事故后,因逃逸致人死亡的,处多少年有期徒刑?', 4, '2年以下', '3年以下', '7年以下', '7年以上', '要看清题目致人死亡的是不是因为逃逸造成的,如果是那就是7年以上.', , 0);
INSERT INTO ""."examination_question1" VALUES (322, 322, '在道路与铁路道口遇到一个红灯亮时要尽快通过道口.', 2, '正确', '错误', , , '红灯停啊亲.', 'http://images.juheapi.com/jztk/c1c2subject1/322.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (323, 323, '冰雪道路行车,由于积雪对光线的反射,极易造成驾驶人目眩而产生错觉.', 1, '正确', '错误', , , '因为太阳光通过冰的折射到人的眼睛里,冰在太阳光下就好像一面镜子.', , 0);
INSERT INTO ""."examination_question1" VALUES (324, 324, '上道路行驶的机动车有哪种情形交通警察可依法扣留车辆?', 4, '未携带机动车登记证书', '未携带保险合同', '未放置城市环保标志', '未放置保险标志', '未放置检验合格标志,未悬挂机动车号牌,未放置保险标志的,未携带机动车行驶证的都是可依法扣留车辆的.', , 0);
INSERT INTO ""."examination_question1" VALUES (325, 325, '路口黄灯持续闪烁,警示驾驶人要注意?望,确认安全通过.', 1, '正确', '错误', , , '路口黄灯持续闪烁,警示驾驶人要注意?望,确认安全通过.', , 0);
INSERT INTO ""."examination_question1" VALUES (326, 326, '这一组交通警察手势是什么信号?', 2, '左转弯信号', '左转弯待转信号', '减速慢行信号', '右转弯信号', '手势题详解:http://www.jszks.com/onepage/16203.html', 'http://images.juheapi.com/jztk/c1c2subject1/326.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (327, 327, '道路最左侧白色虚线区域是何含义?', 1, '多乘员车辆专用车道', '小型客车专用车道', '未载客出租车专用车道', '大型客车专用车道', '图上有字,“多乘员专用”这是送分题', 'http://images.juheapi.com/jztk/c1c2subject1/327.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (328, 328, '路口最前端的双白虚线是什么含义?', 3, '等候放行线', '停车让行线', '减速让行线', '左弯待转线', '虚线可以压线,就是可以过去,边上有倒三角“让”字,所以是减速让行 ORZ', 'http://images.juheapi.com/jztk/c1c2subject1/328.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (329, 329, '这一组交通警察手势是什么信号?', 4, '右转弯信号', '靠边停车信号', '左转弯信号', '停止信号', '手势题详解:http://www.jszks.com/onepage/16203.html', 'http://images.juheapi.com/jztk/c1c2subject1/329.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (330, 330, '行车中在道路情况良好的条件下可以观看车载视频.', 2, '正确', '错误', , , '当然不可以,开车是一个注意力极为集中的工作,出错就是人命.', , 0);
INSERT INTO ""."examination_question1" VALUES (331, 331, '驾驶人有使用其他车辆检验合格标志嫌疑的,交通警察可依法扣留车辆.', 1, '正确', '错误', , , '交通警察可依法扣留 车辆,等他们查清楚车的来由,当场教导让其车在指定时间内接受车辆检验并放行.', , 0);
INSERT INTO ""."examination_question1" VALUES (332, 332, '驾驶人因服兵役、出国（境）等原因延期审验期间不得驾驶机动车.', 1, '正确', '错误', , , '规定', , 0);
INSERT INTO ""."examination_question1" VALUES (333, 333, '对使用其他车辆号牌、行驶证的车辆,交通警察可依法予以扣留.', 1, '正确', '错误', , , '一定得号牌和行驶证等对上,这个很重要,这样才好查你的信息,不然都乱掉了.', , 0);
INSERT INTO ""."examination_question1" VALUES (334, 334, '冰雪路行车时应注意什么?', 1, '制动距离延长', '抗滑能力变大', '制动性能没有变化', '路面附着力增大', '车辆在冰雪路面上行驶,因汽车轮胎与路面的摩擦系数减小、附着力大大降低,所以遇有冰雪路面,车辆在行驶中最重要的是车辆制动问题,要采取点刹、并和前后车保持车距离.遇情况紧急制动时,制动距离会大大延长.', , 0);
INSERT INTO ""."examination_question1" VALUES (335, 335, '道路交通违法行为累积记分周期是多长时间?', 2, '14个月', '12个月', '6个月', '10个月', '常识题,积分周期为一年', , 0);
INSERT INTO ""."examination_question1" VALUES (336, 336, '驾驶人有使用其他车辆号牌、行驶证嫌疑的,交通警察可依法扣留车辆.', 1, '正确', '错误', , , '这个规定是可以的.万一是犯罪的车辆所以可以扣留车辆.', , 0);
INSERT INTO ""."examination_question1" VALUES (337, 337, '进入减速车道时怎样使用灯光?', 4, '开启危险报警闪光灯', '开启前照灯', '开启左转向灯', '开启右转向灯', '因为你要先进入减速车道,所以要打开右转向进入减速车道.', 'http://images.juheapi.com/jztk/c1c2subject1/337.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (338, 338, '在道路上跟车行驶时,跟车距离不是主要的,只须保持与前车相等的速度,即可防止发生追尾事故.', 2, '正确', '错误', , , '举个例子吧:你们都是以100速前进,只隔了1米,前面猛的刹车,你觉得你能在1米内刹住车吗?明显不能,所以此题错了！', , 0);
INSERT INTO ""."examination_question1" VALUES (339, 339, '驾驶机动车时可以向道路上抛撒物品.', 2, '正确', '错误', , , '不要影响其他人在道路上正常驾驶么.', , 0);
INSERT INTO ""."examination_question1" VALUES (340, 340, '驾驶人驾驶有达到报废标准嫌疑机动车上路的,交通警察依法予以拘留.', 2, '正确', '错误', , , '交警只能扣留车辆,还没达到拘留的地步.', , 0);
INSERT INTO ""."examination_question1" VALUES (341, 341, '道路交通事故中,机动车无号牌、检验合格标志、保险标志时,要保护现场并立即报警.', 1, '正确', '错误', , , '这样的人违规在先,先报警,责任在他.', , 0);
INSERT INTO ""."examination_question1" VALUES (342, 342, '对有使用伪造或变造检验合格标志嫌疑的车辆,交通警察只进行罚款处罚.', 2, '正确', '错误', , , '交警要扣留车的.', , 0);
INSERT INTO ""."examination_question1" VALUES (343, 343, '道路交通安全违法行为累积记分一个周期满分为12分.', 1, '正确', '错误', , , '这个是常识和规定,记住.', , 0);
INSERT INTO ""."examination_question1" VALUES (344, 344, '这个标志是何含义?', 3, '下陡坡', '连续上坡', '上陡坡', '堤坝路', '上陡坡:用以提醒车辆驾驶人小心驾驶.', 'http://images.juheapi.com/jztk/c1c2subject1/344.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (345, 345, '下雨后路面湿滑,车辆行驶中紧急制动时,容易导致什么?', 1, '发生侧滑、引发交通事故', '因视线模糊而撞车', '不被其他车辆驾驶人发现', '引起发动机熄火', '给大伙上个关于水的物理课:在粗糙的物体表面有能摸的着的水时,水会减小摩擦；在粗糙的物体表面有极少的（摸不到,有湿润感觉）水时,会增加物体表面的粗糙程度增大摩擦力－－如:数钞票时,在手上沾上些水,数钱数的又快又准等.！！！下雨天不是数钱,别急刹了我个亲！急刹加打方向－－你会成为漂移达人的！！', , 0);
INSERT INTO ""."examination_question1" VALUES (346, 346, '遇到这种情形时要停车避让行人.', 1, '正确', '错误', , , '答题以人为本,此题正确.', 'http://images.juheapi.com/jztk/c1c2subject1/346.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (347, 347, '驾驶机动车在道路上追逐竞驶,情节恶劣,会受到什么处罚?', 4, '处6个月徒刑', '处1年以上徒刑', '处管制,并处罚金', '处拘役,并处罚金', '2011年《中华人民共和国刑法修正案（八）》第二十二条:在刑法第一百三十三条后增加一条,作为第一百三十三条之一:“在道路上驾驶机动车追逐竞驶,情节恶劣的,或者在道路上醉酒驾驶机动车的,处拘役,并处罚金.”', , 0);
INSERT INTO ""."examination_question1" VALUES (348, 348, '驾驶机动车在道路上发生交通事故要立即将车移到路边.', 2, '正确', '错误', , , '要先判断事故责任,双方无争议后将车移动到路边.', , 0);
INSERT INTO ""."examination_question1" VALUES (349, 349, '上道路行驶的机动车有哪种情形交通警察可依法扣留车辆?', 1, '未悬挂机动车号牌', '未携带身份证', '未携带保险合同', '未放置城市环保标志', '未放置检验合格标志,未悬挂机动车号牌,未放置保险标志的,未携带机动车行驶证的都是可依法扣留车辆的.', , 0);
INSERT INTO ""."examination_question1" VALUES (350, 350, '驾驶人将机动车交由什么样的人驾驶的,交通警察可依法扣留机动车驾驶证?', 3, '实习期驾驶人', '取得驾驶证的人', '驾驶证被吊销的人', '驾驶证记分达到6分的人', '驾驶证被吊销的人,驾驶证被在暂扣的人,无证驾驶的人', , 0);
INSERT INTO ""."examination_question1" VALUES (351, 351, '驾驶人违反交通运输管理法规发生重大事故致人死亡且逃逸的,处多少年有期徒刑?', 3, '7年以上', '3年以下', '3年以上7年以下', '10年以上', '因为逃逸致死亡的才是7年以上,致人死亡且逃逸的是3年以上7年以下.', , 0);
INSERT INTO ""."examination_question1" VALUES (352, 352, '驾驶人违反交通运输管理法规发生重大事故后,因逃逸致人死亡的,处3年以上7年以下有期徒刑.', 2, '正确', '错误', , , '看清题目是因逃逸致人死亡的,不是先死亡再逃逸的,所以应该是7年以上.', , 0);
INSERT INTO ""."examination_question1" VALUES (353, 353, '小型汽车驾驶人发生交通事故造成人员死亡,承担同等以上责任未被吊销驾驶证的,应当在记分周期结束后30日内接受审验.', 1, '正确', '错误', , , '《机动车驾驶证申领和使用规定》（公安部令第123号）第六十条 小型汽车驾驶人发生交通事故造成人员死亡承担同等以上责任未被吊销机动车驾驶证的,应当在本记分周期结束后三十日内到公安机关交通管理部门接受审验.', , 0);
INSERT INTO ""."examination_question1" VALUES (354, 354, '夜间驾驶车辆遇自行车对向驶来时,应怎样做?', 3, '连续变换远、近光灯', '不断鸣喇叭', '使用近光灯,减速或停车避让', '使用远光灯', '安全第一.如果使用远光灯会使对方看不清,变换远近光灯也是一样,不断鸣喇叭会使对方产生抵触情绪,也会使对方慌张,结果适得其反.', , 0);
INSERT INTO ""."examination_question1" VALUES (355, 355, '高速公路因发生事故造成堵塞时,可在右侧紧急停车带或路肩行驶.', 2, '正确', '错误', , , '在高速公路因发生事故造成堵塞的情况下,右侧紧急停车带是供救援车辆使用的.', , 0);
INSERT INTO ""."examination_question1" VALUES (356, 356, '对发生道路交通事故需要收集证据的事故车,交通警察可以依法扣留.', 1, '正确', '错误', , , '警察叔叔是有这个权利滴.', , 0);
INSERT INTO ""."examination_question1" VALUES (357, 357, '驾驶人违反交通运输管理法规发生重大事故使公私财产遭受重大损失的可能处3年以下徒刑或拘役.', 1, '正确', '错误', , , '朋友,要安全驾驶啊.', , 0);
INSERT INTO ""."examination_question1" VALUES (358, 358, '这辆小轿车不能在这个位置停车.', 1, '正确', '错误', , , '人家人行横道你还停.', 'http://images.juheapi.com/jztk/c1c2subject1/358.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (359, 359, '3年内有下列哪种行为的人不得申请机动车驾驶证?', 4, '注射胰岛素', '酒醉经历', '吸烟成瘾', '注射毒品', '第十二条有下列情形之一的,不得申请机动车驾驶证:三年内有吸食、注射毒品行为或者解除强制隔离戒毒措施未满三年,或者长期服用依赖性精神药品成瘾尚未戒除的；', , 0);
INSERT INTO ""."examination_question1" VALUES (360, 360, '驾驶人未携带哪种证件驾驶机动车上路,交通警察可依法扣留车辆?', 1, '机动车驾驶证', '居民身份证', '机动车通行证', '从业资格证', '《道路交通安全违法行为处理程序规定》第十三条 “有下列情形之一的,因无其他机动车驾驶人代替驾驶、违法行为尚未消除、需要调查或者证据保全等原因不能立即放行的,可以扣留车辆:    （一）未悬挂机动车号牌,未放置检验合格标志、保险标志,或者未携带行驶证、机动车驾驶证的；', , 0);
INSERT INTO ""."examination_question1" VALUES (361, 361, '对有伪造或变造号牌、行驶证嫌疑的车辆,交通警察可依法予以扣留', 1, '正确', '错误', , , '这个没有问题.', , 0);
INSERT INTO ""."examination_question1" VALUES (362, 362, '车辆在路边起步后应尽快提速,并向左迅速转向驶入正常行驶道路.', 2, '正确', '错误', , , '你要先观察阿！安全了你再开过去阿！不是一起步就迅速的往正常行驶道行驶,万一后面有车怎么办呢? 所以“迅速”是错的', , 0);
INSERT INTO ""."examination_question1" VALUES (363, 363, '发生交通事故造成人员受伤时,要保护现场并立即报警.', 1, '正确', '错误', , , '造成人员受伤了,不能私了,要保护现场,并报警和叫救护车.', , 0);
INSERT INTO ""."examination_question1" VALUES (364, 364, '图中圈内的锯齿状白色实线是什么标线?', 3, '导向车道线', '方向引导线', '可变导向车道线', '单向行驶线', '这道儿左转右转还是直行都是可以的,关键看信号灯或者路面上的指标牌', 'http://images.juheapi.com/jztk/c1c2subject1/364.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (365, 365, '夜间行车,驾驶人的视野受限,很难观察到灯光照射区域以外的交通情况,因此要减速行驶.', 1, '正确', '错误', , , '晚上开车的时候受到光线暗的局限,相对观察事物的距离也就受到局限,所以视距比较短,因此很难观察到灯光照射区域以外的交通情况.为了安全考虑,减速行驶是必须的.', , 0);
INSERT INTO ""."examination_question1" VALUES (366, 366, '车辆起步前,驾驶人应对车辆周围交通情况进行观察,确认安全时再开始起步.', 1, '正确', '错误', , , '这道题里的话并没有错,观察周围只是起步前准备工作的一部分,并没有说是不用打转向灯的.', , 0);
INSERT INTO ""."examination_question1" VALUES (367, 367, '在交叉路口遇到这种情况享有优先通行权.', 2, '正确', '错误', , , '右转让左转.', 'http://images.juheapi.com/jztk/c1c2subject1/367.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (368, 368, '通过经常发生塌方、泥石流的山区地段,避免停车.', 1, '正确', '错误', , , '是对的,谨慎驾驶,有塌方 就避免停车,又不是说不能停车,只是避免.', , 0);
INSERT INTO ""."examination_question1" VALUES (369, 369, '驾驶机动车在车道减少的路口,遇到前方车辆依次停车或缓慢行驶时怎么办?', 3, '从前车右侧路肩进入路口', '从有空隙一侧进入路口', '每车道一辆依次交替驶入路口', '向左变道穿插进入路口', '永远选最和谐的答案.', , 0);
INSERT INTO ""."examination_question1" VALUES (370, 370, '这个标志是何含义?', 1, '注意野生动物', '注意牲畜', '动物公园', '开放的牧区', '这个是头鹿啊,是野生动物啊.', 'http://images.juheapi.com/jztk/c1c2subject1/370.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (371, 371, '立交桥上一般都是单向行驶,车辆不必减速行驶.', 2, '正确', '错误', , , '立交桥上车多,路窄,弯多,所以应减速！', , 0);
INSERT INTO ""."examination_question1" VALUES (372, 372, '机动车在高速公路行驶,下列做法正确的是?', 1, '非紧急情况时不得在应急车道行驶或者停车', '可在减速车道或加速车道上超车、停车', '可在紧急停车带停车装卸货物', '可在路肩停车上下人员', '只有A最和谐.', , 0);
INSERT INTO ""."examination_question1" VALUES (373, 373, '车辆在高速公路行驶时,可以仅凭感觉确认车速.', 2, '正确', '错误', , , '有个“仅”看见没?~这话说得太绝对了,所以是错的.', , 0);
INSERT INTO ""."examination_question1" VALUES (374, 374, '这个导向箭头是何含义?', 4, '指示禁行', '指示车道', '指示合流', '指示直行', '亲,直行哦~~', 'http://images.juheapi.com/jztk/c1c2subject1/374.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (375, 375, '驾驶车辆通过人行横道线时,应注意礼让行人.', 1, '正确', '错误', , , '所有题里遇到行人都是让行人先行的', , 0);
INSERT INTO ""."examination_question1" VALUES (376, 376, '变更车道时,应开启转向灯,迅速驶入侧方车道.', 2, '正确', '错误', , , '注意迅速两字,使入前先观察后慢慢变道', , 0);
INSERT INTO ""."examination_question1" VALUES (377, 377, '车辆行至交叉路口,遇有转弯的车辆抢行,应怎样做?', 3, '提高车速抢先通过', '鸣喇叭抢先通过', '停车避让', '保持正常车速行驶', '答题的时候请立即变身,变身为圣人,你就知道这些题的答案了.', , 0);
INSERT INTO ""."examination_question1" VALUES (378, 378, '驾驶机动车遇有前方交叉路口交通阻塞时怎么办?', 2, '可借对向车道通过', '依次停在路口外等候', '从前车两侧穿插通过', '进入路口内等候', '只有B最和谐.', , 0);
INSERT INTO ""."examination_question1" VALUES (379, 379, '在冰雪道路上行车时,车辆的稳定性降低,加速过急时车轮极易空转或溜滑.', 1, '正确', '错误', , , '轮胎与地面的摩擦系数降低,轮胎不易附着在地面上,只要一加速,就极易空转或溜滑.', , 0);
INSERT INTO ""."examination_question1" VALUES (380, 380, '车辆在高速公路上行车,可以频繁地变更车道.', 2, '正确', '错误', , , '车辆在高速公路上行车,可以变更车道,但不可以“频繁”的变更车道.', , 0);
INSERT INTO ""."examination_question1" VALUES (381, 381, '掉头过程中,应严格控制车速,仔细观察道路前后方情况,确认安全后方可前进或倒车.', 1, '正确', '错误', , , '在非繁忙路段或非主干道,又没有其它限制时,可以倒车.有时路窄,并不能一次掉头到位！', , 0);
INSERT INTO ""."examination_question1" VALUES (382, 382, '驾驶机动车不能进入红色叉形灯或者红色箭头灯亮的车道.', 1, '正确', '错误', , , '红灯停嘛.', , 0);
INSERT INTO ""."examination_question1" VALUES (383, 383, '车辆转弯时应沿道路右侧行驶,不要侵占对方的车道,做到“左转转大弯,右转转小弯”.', 1, '正确', '错误', , , '左转转大弯,但当心身后非机动车.右转转小弯,当心对面右行驶和身后的车吧！', , 0);
INSERT INTO ""."examination_question1" VALUES (384, 384, '车辆驶离高速公路时,应当经减速车道减速后进入匝道.', 1, '正确', '错误', , , '匝道:立交桥和高架路上下两条道路相连接的路段,也指高速公路与邻近的辅路相连接的路段.高架路的匝道,进口路和出口路是分开的,只能顺行,不准掉头,车辆错过了下匝道,就不能从上匝道下路,只能从下一个下匝道下路. 立交桥的匝道,也是按照设定的标志行驶,谁也不能各行其是.', , 0);
INSERT INTO ""."examination_question1" VALUES (385, 385, '驾驶小型载客汽车在高速公路行驶的最低车速为90公里/小时.', 2, '正确', '错误', , , '不对,每条车道的最低车速是不一样的,不是统一的最低90公里/小时', , 0);
INSERT INTO ""."examination_question1" VALUES (386, 386, '行车中遇抢救伤员的救护车从本车道逆向驶来时,应怎样做?', 1, '靠边减速或停车让行', '占用其他车道行驶', '加速变更车道避让', '在原车道内继续行驶', '救护车有优先权,当然得让行,如果觉得A有问题的,请看清A的答案,有“靠边”两个字.', , 0);
INSERT INTO ""."examination_question1" VALUES (387, 387, '车辆通过凹凸路面时,应怎样做?', 4, '依靠惯性加速冲过', '挂空挡滑行驶过', '保持原速通过', '低速缓慢平稳通过', '凹凸不平的路面减速慢行不仅对车没有有好处 而且不会因颠簸影响驾驶.', , 0);
INSERT INTO ""."examination_question1" VALUES (388, 388, '车辆行至交叉路口时,左转弯车辆在任何时段都可以进入左弯待转区.', 2, '正确', '错误', , , '直行车辆放行时才可进入左转待转区.你到路口观察一下就明白了！', , 0);
INSERT INTO ""."examination_question1" VALUES (389, 389, '驾驶小型载客汽车在高速公路上时速超过100公里时的跟车距离是多少?', 3, '保持50米以上', '保持60米以上', '保持100米以上', '保持80米以上', '规定,记住吧.', , 0);
INSERT INTO ""."examination_question1" VALUES (390, 390, '驾驶车辆通过无人看守的铁路道口时,应怎样做?', 3, '减速通过', '匀速通过', '一停、二看、三通过', '加速通过', '一停,是停在未到铁道前,不是停在铁路上二看,是在铁道前观察是否有火车要经过三通过,才是安全通过', , 0);
INSERT INTO ""."examination_question1" VALUES (391, 391, '遇到这种情况时,要加速从红车前变更车道.', 2, '正确', '错误', , , '礼貌让行,不然出事故更麻烦.', 'http://images.juheapi.com/jztk/c1c2subject1/391.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (392, 392, '通过隧道时,不得超车.', 1, '正确', '错误', , , '安全第一还是不要超车！', , 0);
INSERT INTO ""."examination_question1" VALUES (393, 393, '驾驶机动车通过漫水路时要加速行驶.', 2, '正确', '错误', , , '要先观察水深情况.', , 0);
INSERT INTO ""."examination_question1" VALUES (394, 394, '行驶在高速公路上遇大雾视线受阻时,应当立即紧急制动停车.', 2, '正确', '错误', , , '错在“紧急制动”上,要逐渐减速停车', , 0);
INSERT INTO ""."examination_question1" VALUES (395, 395, '遇到前方车辆缓慢行驶时怎样行驶?', 1, '依次排队行驶', '占对向车道超越', '从右侧借道超越', '从两侧随意超越', '请选最和谐的答案.', 'http://images.juheapi.com/jztk/c1c2subject1/395.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (396, 396, '驾驶人持超过有效期的驾驶证可以在1年内驾驶机动车.', 2, '正确', '错误', , , '机动车驾驶人应当于机动车驾驶证有效期满前九十日内,向机动车驾驶证核发地车辆管理所申请换证.过了有效期就不能用了.', , 0);
INSERT INTO ""."examination_question1" VALUES (397, 397, '行车中遇有非机动车准备绕过停放的车辆时,应怎样做?', 2, '鸣喇叭示意其让道', '让其先行', '紧随其后鸣喇叭', '加速绕过', '应该是这样,你在行车中,道路前方有非机动车正在绕过或准备绕过另外停放的机动车.所以应让其先行.', , 0);
INSERT INTO ""."examination_question1" VALUES (398, 398, '行车中超越右侧停放的车辆时,为预防其突然起步或开启车门,应怎样做?', 4, '加速通过', '长鸣喇叭', '保持正常速度行驶', '预留出横向安全距离,减速行驶', '留出安全距离、并减速,万一右侧车辆启动才有足够的空间和时间避让！', , 0);
INSERT INTO ""."examination_question1" VALUES (399, 399, '在路口直行时,遇这种情形如何通行?', 3, '开启危险报警闪光灯通行', '直接加速直行通过', '让右方道路车辆先行', '让左方道路车辆先行', '右方的车直行,左方的车左转,左转让直行.', 'http://images.juheapi.com/jztk/c1c2subject1/399.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (400, 400, '驾驶机动车在没有中心线的城市道路上,最高速度不能超过每小时50公里.', 2, '正确', '错误', , , '根据《中华人民共和国道路交通安全法实施条例》第四十五条:……在没有限速标志、标线的道路上,机动车不得超过下列最高行驶速度:　　(一)没有道路中心线的道路,城市道路为每小时30公里,公路为每小时40公里；　　(二)同方向只有1条机动车道的道路,城市道路为每小时50公里,公路为每小时70公里.', , 0);
INSERT INTO ""."examination_question1" VALUES (401, 401, '这个标志是何含义?', 3, '人行横道灯', '注意行人', '注意信号灯', '交叉路口', '注意信号灯:用以警告车辆驾驶人注意前方路段设有信号灯,应依信号灯指示行车.', 'http://images.juheapi.com/jztk/c1c2subject1/401.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (402, 402, '行车中,遇非机动车抢行时,应怎样做?', 3, '加速通过', '鸣喇叭警告', '减速让行', '临近时突然加速', '一定要让,以人为本.', , 0);
INSERT INTO ""."examination_question1" VALUES (403, 403, '车辆上坡行驶,要提前观察路况、坡道长度,及时减挡使车辆保持充足的动力.', 1, '正确', '错误', , , '高档属于快速档,而低档却用于起步或者作有重力的运动而使用,上坡本然就形成一种重力,需要足够的马力才可以行驶,所以需要将档位降低才可以,如果使用高档,很可能导至功率不够形成熄火,甚至溜车！', , 0);
INSERT INTO ""."examination_question1" VALUES (404, 404, '驾驶的车辆正在被其他车辆超越时,应怎样做?', 4, '靠道路中心行驶', '加速让路', '继续加速行驶', '减速,靠右侧行驶', '我们都应该让.安全行驶.', , 0);
INSERT INTO ""."examination_question1" VALUES (405, 405, '在山区道路遇对向来车时,应怎样会车?', 4, '紧靠道路中心', '不减速', '加速', '减速或停车让行', '是双方选择怎么交会,两个车可以等到安全路段再减速交会', , 0);
INSERT INTO ""."examination_question1" VALUES (406, 406, '在这种情况下可以借右侧公交车道超车.', 2, '正确', '错误', , , '不要占用公交车道,小心被拍罚款扣分.', 'http://images.juheapi.com/jztk/c1c2subject1/406.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (407, 407, '驾驶机动车遇到这种桥时首先怎样办?', 3, '保持匀速通过', '尽快加速通过', '停车察明水情', '低速缓慢通过', '遇到漫水桥首先观察情况.', 'http://images.juheapi.com/jztk/c1c2subject1/407.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (408, 408, '车辆在山区道路行车下陡坡时,不得超车.', 1, '正确', '错误', , , '下坡时不易控制车速,安全第一,怎么能超车呢?', , 0);
INSERT INTO ""."examination_question1" VALUES (409, 409, '在这条车道行驶的最高车速是多少?', 2, '100公里/小时', '90公里/小时', '120公里/小时', '110公里/小时', '右边那个牌子就是告诉你最高时速是90公里/小时,安标志行驶.', 'http://images.juheapi.com/jztk/c1c2subject1/409.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (410, 410, '机动车在道路上发生故障难以移动时要在车后50米以内设置警告标志.', 2, '正确', '错误', , , '机动车在道路上发生故障或者发生交通事故,妨碍交通又难以移动的,应当按照规定开启危险报警闪光灯并在车后50米至100米处设置警告标志,夜间还应当同时开启示廓灯和后位灯. 所以不是在50米内,应该是50米以外100米内.扩展提示:记不记得在高速公路上要在车后150米外设置警告标志.', , 0);
INSERT INTO ""."examination_question1" VALUES (411, 411, '驾驶机动车行经下列哪种路段不得超车?', 3, '主要街道', '高架路', '人行横道', '环城高速', '《中华人民共和国道路交通管理条例》第六章 第五十条 机动车超车,必须遵守下列规定:行经交叉路口、人行横道、漫水路、漫水桥等,不准超车.', , 0);
INSERT INTO ""."examination_question1" VALUES (412, 412, '遇有浓雾或特大雾天能见度过低,行车困难时,应怎样做?', 4, '开启前照灯,继续行驶', '开启示廓灯、雾灯,靠右行驶', '开启危险报警闪光灯,继续行驶', '开启危险报警闪光灯和雾灯,选择安全地点停车', '浓雾或特大雾天,你都看不见了,还能开车?', , 0);
INSERT INTO ""."examination_question1" VALUES (413, 413, '驾驶人出现下列哪种情况,不得驾驶机动车?', 3, '记分达到10分', '记分达到6分', '驾驶证丢失、损毁', '驾驶证接近有效期', '驾驶证丢失、损毁的算是没有驾驶证,此时驾驶就是无证驾驶.', , 0);
INSERT INTO ""."examination_question1" VALUES (414, 414, '机动车在道路边临时停车时,应怎样做?', 1, '不得逆向或并列停放', '只要出去方便,可随意停放', '可逆向停放', '可并列停放', '答案是排除法.不让逆向停放,你只有顺方向停放.不让并列停放,只有纵向停放.顺行驶方向纵向停放可以最大限度少占用道路的宽度,对道路通行的影响最小.', , 0);
INSERT INTO ""."examination_question1" VALUES (415, 415, '雨天路面湿滑,车辆制动距离增大,行车中尽量使用紧急制动减速.', 2, '正确', '错误', , , '因为路面比较滑,如果紧急制动（急刹车）,容易造成车辆失控,所以用发动机制动.', , 0);
INSERT INTO ""."examination_question1" VALUES (416, 416, '遇到这种情况不能超车.', 1, '正确', '错误', , , '前车已经在超车了,为了安全不要再超了.', 'http://images.juheapi.com/jztk/c1c2subject1/416.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (417, 417, '在高速公路上遇分流交通管制时,可不驶出高速公路,就地靠边停靠等待管制结束后继续前行.', 2, '正确', '错误', , , '高速公路的分流交通管制就是指在高速公路上遇到紧急情况后,将后续来车在匝道口进行管制通行,避免次生事故和交通拥堵,亦或是有紧急交通勤务警卫,需要提前封闭道路,故提前分流车辆.', , 0);
INSERT INTO ""."examination_question1" VALUES (418, 418, '驾驶机动车在夜间超车时怎样使用灯光?', 4, '关闭前大灯', '开启远光灯', '开启雾灯', '变换远、近光灯', '高速上超车时候也尽量使用变换远、近光灯,变换远、近光灯是提醒前车的意思.', , 0);
INSERT INTO ""."examination_question1" VALUES (419, 419, '车辆在雪天临时停车时,应开启什么灯?', 4, '前后雾灯', '倒车灯', '前大灯', '危险报警闪光灯', '请看清是停车,开启危险报警闪光灯防止没看清追尾等事故.', , 0);
INSERT INTO ""."examination_question1" VALUES (420, 420, '在交叉路口遇到这种情况时,要在红灯亮以前加速通过路口.', 2, '正确', '错误', , , '黄灯亮了,车身没过白线,请停车等待.', 'http://images.juheapi.com/jztk/c1c2subject1/420.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (421, 421, '驾驶机动车在下列哪种路段不得超车?', 4, '山区道路', '城市高架路', '城市快速路', '窄桥、弯道', '按照规定窄桥、弯道是不可超车的.', , 0);
INSERT INTO ""."examination_question1" VALUES (422, 422, '行车中需要借道绕过前方障碍物,但对向来车已接近障碍物时,应怎样做?', 4, '加速提前抢过', '鸣喇叭示意对向车辆让道', '迅速占用车道,迫使对向来车停车让道', '降低速度或停车,让对向来车优先通行', '是相向的才是有阻碍的一方让无阻碍的一方啊,我们也应做到礼让啊,又不会吃亏.', , 0);
INSERT INTO ""."examination_question1" VALUES (423, 423, '行经这种交通标线的路段要加速行驶.', 2, '正确', '错误', , , '要减速通过,看到那个白色的菱形没,那是人行横道预告,所以要减速防止撞到行人.', 'http://images.juheapi.com/jztk/c1c2subject1/423.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (424, 424, '这种情况超车时,从前车的哪一侧超越?', 3, '左右两侧均可超越', '从前车的右侧超越', '从前车的左侧超越', '从无障碍一侧超越', '任何超车都是从左侧超车的.', 'http://images.juheapi.com/jztk/c1c2subject1/424.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (425, 425, '遇到这种情况的路段,可以进入网状线区域内停车等候.', 2, '正确', '错误', , , '网状线区域是禁止停车的.', 'http://images.juheapi.com/jztk/c1c2subject1/425.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (426, 426, '进入左侧道路超车,无法保证与正常行驶前车的横向安全间距时,应怎样做?', 2, '谨慎超越', '放弃超车', '并行一段距离后再超越', '加速超越', '亲,那就别超了,安全第一嘛.', , 0);
INSERT INTO ""."examination_question1" VALUES (427, 427, '在路口这个位置时可以加速通过路口.', 2, '正确', '错误', , , '施主,请看清前边是红灯,还不赶快停下.', 'http://images.juheapi.com/jztk/c1c2subject1/427.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (428, 428, '车辆在山区道路跟车行驶时,应怎样做?', 1, '适当加大安全距离', '紧随前车之后', '适当减小安全距离', '尽可能寻找超车机会', '为了安全,适当加大安全距离.', , 0);
INSERT INTO ""."examination_question1" VALUES (429, 429, '车辆通过学校和小区应注意观察标志标线,低速行驶,不要鸣喇叭.', 1, '正确', '错误', , , '减少噪音,以免影响居民生活和学生学习！', , 0);
INSERT INTO ""."examination_question1" VALUES (430, 430, '在高速公路上行驶感觉疲劳时,应立即停车休息.', 2, '正确', '错误', , , '在高速公路上行驶感觉疲困时,应该找到最近的服务区停车休息,高速上哪能立即停车休息.', , 0);
INSERT INTO ""."examination_question1" VALUES (431, 431, '驾驶机动车在没有交通信号的路口要尽快通过.', 2, '正确', '错误', , , '一定要注意观察,在通过啊,哪能尽快通过.', , 0);
INSERT INTO ""."examination_question1" VALUES (432, 432, '车辆在较窄的山路上行驶时,如果靠山体的一方不让行,应怎样做?', 3, '鸣喇叭催其让行', '保持正常车速行驶', '提前减速或停车避让', '向左占道,谨慎驶过', '如果没特别指明我们有优先权,选事事让着别人就对了,做人和开车,都要学会淡定.', , 0);
INSERT INTO ""."examination_question1" VALUES (433, 433, '这个路口允许车辆怎样行驶?', 2, '直行', '向右转弯', '向左转弯', '直行或向左转弯', '右转的灯是绿的,所以只能向右转弯.', 'http://images.juheapi.com/jztk/c1c2subject1/433.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (434, 434, '驾驶人吸食或注射毒品后驾驶机动车的,一经查获,其驾驶证将被注销.', 1, '正确', '错误', , , '严禁机动车驾驶员酒驾或者毒驾,驾驶人吸食或注射毒品后驾驶机动车的,一经查获,将注销其驾驶证.', , 0);
INSERT INTO ""."examination_question1" VALUES (435, 435, '夜间行车中,前方出现弯道时,灯光照射会发生怎样的变化?', 2, '离开路面', '由路中移到路侧', '距离不变', '由高变低', '要是没有实地操作,个人为了避免混淆是这样记的:在平直的路面时,灯光距离不变；出现一般的弯道时,灯光也跟着转弯,从路中移至路侧；当上坡时,灯光随着车的上下导致灯光也由高变低；当出现急弯或大坑时,灯光随着车的颠簸也离开路面.大家试着记记,遇到这题绝对不错！', , 0);
INSERT INTO ""."examination_question1" VALUES (436, 436, '有下列哪种违法行为的机动车驾驶人将被一次记6分?', 1, '驾驶机动车违反道路交通信号灯', '未取得校车驾驶资格驾驶校车', '驾驶与准驾车型不符的机动车', '饮酒后驾驶机动车', '闯灯的一次6分.B,C,D都是一次记12分.', , 0);
INSERT INTO ""."examination_question1" VALUES (437, 437, '驾驶人行车中看到注意儿童标志的时候,应怎样做?', 4, '加速行驶', '绕道行驶', '保持正常车速行驶', '谨慎选择行车速度', '祖国的花朵,当然要保护了.', , 0);
INSERT INTO ""."examination_question1" VALUES (438, 438, '在这种路段如何行驶?', 4, '占对方道路转弯', '在弯道中心转弯', '加速鸣喇叭通过', '减速鸣喇叭示意', '因为是弯道,实线变窄,所以要减速鸣喇叭示意提示对方来的车,安全第一.', 'http://images.juheapi.com/jztk/c1c2subject1/438.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (439, 439, '驾驶机动车通过没有交通信号的交叉路口怎样行驶?', 1, '减速慢行', '加速通过', '大型车先行', '左侧车辆先行', '减速慢行并且观察路况,搞不好哪里飞出辆车来.', , 0);
INSERT INTO ""."examination_question1" VALUES (440, 440, '这是什么操纵装置?', 2, '空调开关', '点火开关', '雨刷开关', '灯光开关', '这个是点火装置没错.灯光开关在左手边,雨刷开关在右手边.', 'http://images.juheapi.com/jztk/c1c2subject1/440.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (441, 441, '高速公路上行车,如果因疏忽驶过出口,应怎样做?', 4, '立即停车', '在原地掉头', '在原地倒车驶回', '继续向前行驶,寻找下一个出口', '高速路上掉头或者停车是非常非常危险的动作.', , 0);
INSERT INTO ""."examination_question1" VALUES (442, 442, '遇有这种排队等候的情形怎么做?', 3, '从左侧跨越实线超越', '从两侧随意超越', '依次排队等候', '从右侧借道超越', '大家都排队你凭什么不排队.', 'http://images.juheapi.com/jztk/c1c2subject1/442.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (443, 443, '车辆行至急转弯处时,应减速并靠右侧行驶,防止与越过弯道中心线的对方车辆相撞.', 1, '正确', '错误', , , '本题只说是急转弯,但没有说清楚是左转还是右转.因此若是右转,那么靠右边行驶没错,但若是左转,那么就应该靠左边行驶.', , 0);
INSERT INTO ""."examination_question1" VALUES (444, 444, '这个导向箭头是何含义?', 2, '指示直行或掉头', '指示直行或左转弯', '指示直行或向左变道', '指示向左转弯或掉头', '左或者直行', 'http://images.juheapi.com/jztk/c1c2subject1/444.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (445, 445, '在这种环境下通过路口如何使用灯光?', 3, '关闭远光灯', '使用危险报警闪光灯', '交替使用远近光灯', '使用远光灯', '路过没有路灯和信号灯的路口,应该交替使用远近光灯,提示周围的车辆,以免发生事故.', 'http://images.juheapi.com/jztk/c1c2subject1/445.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (446, 446, '这两辆车发生追尾的主要原因是什么?', 4, '前车采取制动时没看后视镜', '前车采取制动过急', '后车超车时距离前车太近', '后车未与前车保持安全距离', '追尾都是后车的责任,因为你没有保持足有的安全距离.', 'http://images.juheapi.com/jztk/c1c2subject1/446.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (447, 447, '造成交通事故后逃逸,尚不构成犯罪的一次记几分?', 3, '3分', '2分', '12分', '6分', '造成交通事故后逃逸,尚不构成犯罪的一次记12分.', , 0);
INSERT INTO ""."examination_question1" VALUES (448, 448, '驾驶机动车在泥泞道路行驶时,最高速度不能超过多少?', 4, '15公里/小时', '20公里/小时', '40公里/小时', '30公里/小时', '规定是:驾驶机动车在泥泞道路行驶时,最高速度不能超过30公里/小时', , 0);
INSERT INTO ""."examination_question1" VALUES (449, 449, '机动车在高速公路上发生故障时,在来车方向50至100米处设置警告标志.', 2, '正确', '错误', , , '请看清道路,高速公路是150米是最佳距离,普通道路是50-100米.', , 0);
INSERT INTO ""."examination_question1" VALUES (450, 450, '车辆长时间停放时,应选择停车场停车.', 1, '正确', '错误', , , '车辆长时间停放时,应选择停车场停车.', , 0);
INSERT INTO ""."examination_question1" VALUES (451, 451, '夜间行车,要尽量避免超车,确需超车时,可变换远近光灯向前车示意.', 1, '正确', '错误', , , '只要在确保安全和不违规的情况下都可以超车.变换远近灯光是为了提醒前方车辆的注意,虽然只是一个动作,但对于保证安全来说却是很重要的.', , 0);
INSERT INTO ""."examination_question1" VALUES (452, 452, '在这段道路上一定要减少鸣喇叭的频率.', 2, '正确', '错误', , , '右侧的标志明显是禁止鸣喇叭.', 'http://images.juheapi.com/jztk/c1c2subject1/452.jpg', 0);
INSERT INTO ""."examination_question1" VALUES (453, 453, '机动车驾驶人违法驾驶造成重大交通事故构成犯罪的,依法追究什么责任?', 1, '刑事责任', '民事责任', '直接责任', '经济责任', '因为你造成重大交通事故构成犯罪了,所以得追求刑事责任.', , 0);
